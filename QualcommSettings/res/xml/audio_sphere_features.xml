<PreferenceScreen
   xmlns:android="http://schemas.android.com/apk/res/android">

   <CheckBoxPreference
     android:key="asphere_enable"
     android:title="@string/asphere_enable_title"
     android:summary="@string/asphere_enable_summary"
     android:defaultValue="false"/>

   <EditTextPreference
     android:key="asphere_strength"
     android:title="@string/asphere_strength_title"
     android:summary="@string/asphere_strength_summary"
     android:inputType="number"
     android:defaultValue="1000"/>

</PreferenceScreen>
