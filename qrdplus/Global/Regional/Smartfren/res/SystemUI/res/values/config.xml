<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The number of columns in the QuickSettings -->
    <integer name="quick_settings_num_columns">3</integer>

    <!-- style of status bar -->
    <integer name="status_bar_style">1</integer>

    <bool name="config_showDataConnectionView">false</bool>
    <bool name="config_enableDataSwitch">true</bool>
    <bool name="config_showRingerModeSwitch">true</bool>
    <bool name="config_enableTaskManager">true</bool>
    <bool name="config_showSdcardAbsentIndicator">true</bool>
    <bool name="config_showWifiIndicatorWhenEnabled">true</bool>
    <bool name="config_showGpsIndicator">true</bool>
    <bool name="config_showTogglesLayout">true</bool>
    <bool name="config_showBrightnessToggle">true</bool>
    <bool name="quick_settings_show_rotation_lock">true</bool>
    <bool name="config_showRingerToggle">true</bool>

    <integer name="config_max_unlock_countdown_times">10</integer>

    <!-- The default tiles to display in QuickSettings -->
    <string name="quick_settings_tiles_default" translatable="false">
        wifi,bt,inversion,cell,airplane,rotation,flashlight,location,cast,hotspot,data,sync
    </string>

    <!-- Whether to enable to show task manager or not -->
    <bool name="config_showTaskManagerSwitcher">true</bool>

    <!-- Show operator name on status bar -->
    <bool name="enable_operator_name">true</bool>

    <!-- Hide the no-sim signal icon -->
    <bool name="hide_nosim_signal_icon">true</bool>
</resources>
