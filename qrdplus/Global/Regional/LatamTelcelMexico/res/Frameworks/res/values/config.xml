<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Controlling respond call via sms feature via config item -->
    <bool name="config_reject_call_via_sms_enabled">false</bool>
    <!-- Configured default voice mail number-->
    <string-array translatable="false" name="config_default_vm_number" >
        <item>*86</item>
    </string-array>

    <!-- Enabled pdp data reject config-->
    <bool name="config_pdp_retry_for_29_33_enabled">true</bool>
    <!-- Data retry delay for reject cause 29/33 is set to 45sec-->
    <integer name="data_retry_delay">45000</integer>
    <!-- Data reject idle time delay is set to 2hrs-->
    <integer name="data_retry_idle_delay">7200000</integer>
    <!-- Enabled to show Auth failure message to user-->
    <bool name="config_show_auth_reject">true</bool>

    <!-- Zero Balance redirect URL config -->
    <string name="operator_config_url" translatable="false">http://pcautivo.telcel.com</string>
    <!-- Zero Balance redirect URL config -->
    <string name="operator_ping_url" translatable="false">http://www.google.com/</string>
    <!-- Zero Balance feature enable config -->
    <bool name="config_zero_balance_operator">true</bool>

    <!-- custom system sound or not -->
    <bool name="def_custom_sys_sound">true</bool>
    <!--Default ringtone for SIM1,the value is ringtone file name. -->
    <string name="def_custom_sys_ringtone">TELCEL.mp3</string>

</resources>

