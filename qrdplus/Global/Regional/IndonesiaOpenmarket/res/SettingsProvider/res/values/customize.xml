<resources>

    <!-- Time format,default vlaue is 24 : 24 format,other value is 12 format -->
    <string name="def_time_format" translatable="false">24</string>

    <!-- Date format,yyyy-MM-dd: 2013/07/30; MM-dd-yyyy:07/30/2013; dd-MM-yyyy:30/07/2013 -->
    <string name="def_date_format" translatable="false">dd-MM-yyyy</string>

    <!-- Default Input Method, its value is from inputmethod's package name and main class name
              such as "com.android.inputmethod.pinyin/.PinyinIME" -->
    <string name="def_input_method" translatable="false">com.android.inputmethod.latin/.LatinIME</string>



    <!-- Default Enable InputMethods, its value is from inputmethod's package name and main class name
              such as "com.android.inputmethod.pinyin/.PinyinIME" ;This is a string
         containing the IDs of all enabled input methods, each ID separated by ':'.-->
    <string name="def_enable_input_methods" translatable="false"></string>

    <!-- NITZ (network time), use network provided time from NITZ or not, default value is true, other value is false -->
    <bool name="def_auto_time">true</bool>

    <!-- NITZ (network time), use network provided time zone from NITZ or not, default value is true, other value is false -->
    <bool name="def_auto_time_zone">false</bool>

    <!-- default wifi on, false - Off, true - On, default value is false.-->
    <bool name="def_wifi_on">true</bool>

    <!-- Keep Wi-Fi on during sleep,0 == never, 1 == only when plugged in, 2 == always, default value is 2. -->
    <integer name="def_wifi_sleep_policy">1</integer>

    <!-- default location allowed,the value may be "","gps","network","gps,network",default value is gps-->
    <string name="def_location_providers_allowed" translatable="false">gps,network</string>

    <!-- Screen Brightness, Default for brightness automatic, true will open the automatic mode,
         false will close the automatic mode, default value is false -->
    <bool name="def_screen_brightness_automatic_mode">true</bool>

    <!--  Alarm sounds play or not when low battery, 1 : play, other value will not paly, default value is 1 -->
    <integer name="def_power_sounds_enabled">1</integer>

</resources>
