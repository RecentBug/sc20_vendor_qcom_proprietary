<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Default launcher, its value is from launcher's package name -->
    <string name="default_launcher"></string>

    <!-- Used in LocalePicker, default language must be contained -->
    <string name="locale_codes" translatable="false">en_US,en_IN,th_TH,bn_IN,pt_BR,hi_IN,ta_IN,te_IN</string>

    <!-- Default wi-fi direct name -->
    <string name="def_wifi_direct_name" translatable="false">IndiaCommon</string>

    <!-- custome date format or not, if set the value as true, please set the correct fromat in settingprovider overlay package, the reference key is "def_date_format" -->
    <bool name="config_dateformat">false</bool>

    <!-- Set customize default bluetooth name -->
    <string name="def_custom_bt_defname">IndiaCommon</string>

    <!-- Set Wifi hotspot security type NONE: 0 ,WPA2 PSK: 4 -->
    <integer name="wifi_hotspot_security_type">0</integer>

</resources>
