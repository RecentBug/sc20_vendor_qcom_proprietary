<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Default launcher, its value is from launcher's package name -->
    <string name="default_launcher"></string>

    <!-- show country in language -->
    <bool name="config_display_country_for_locale_codes">true</bool>

    <!-- Used in LocalePicker, default language must be contained -->
    <string name="locale_codes" translatable="false">en_US,en_IN,bn_IN,pt_BR,hi_IN,ta_IN,te_IN</string>

    <!-- Default wi-fi direct name -->
    <string name="def_wifi_direct_name" translatable="false">8909-Xolo</string>

    <!-- custom date format or not, if set the value as true, please set the correct fromat in settingprovider overlay package, the reference key is "def_date_format" -->
    <bool name="def_custom_dateformat">true</bool>

    <!-- Whether to make default hotspot security NONE-->
    <bool name="set_wifi_hotspot_security_none">true</bool>
</resources>
