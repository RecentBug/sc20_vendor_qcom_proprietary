<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Used in LocalePicker, default language must be contained,split by ",", such as "en-GB,zh-CN". -->
    <string name="locale_codes" translatable="false">pt-PT,en-US,es-US</string>

   <!-- custom date format or not, if set the value as true, please set the correct fromat in settingprovider overlay package, the reference key is "def_date_format" -->
    <bool name="def_custom_dateformat">true</bool>

    <!-- Whether to make default hotspot security NONE-->
    <bool name="set_wifi_hotspot_security_none">true</bool>

    <!--Default ringtone for mms,the value is ringtone file name. -->
    <string name="def_custom_sys_mms">InstitucionalRubrica.mp3</string>
    <!--Default ringtone for system notification,the value is ringtone file name. -->
    <string name="def_custom_sys_notification">InstitucionalRubrica.mp3</string>

</resources>
