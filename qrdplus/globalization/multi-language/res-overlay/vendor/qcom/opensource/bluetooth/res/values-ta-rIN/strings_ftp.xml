<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="ftp_acceptance_dialog_title"/>
    <string name="ftp_acceptance_dialog_title">"%1$s உங்கள் தொலைபேசி கோப்புறைகளை அணுக விரும்புகிறார். %2$s க்கு அணுகலை வழங்கவா?"</string>
    <add-resource type="string" name="ftp_acceptance_dialog_header"/>
    <string name="ftp_acceptance_dialog_header">"தொலைபேசி கோப்புறை அணுகல்"</string>
    <add-resource type="string" name="ftp_session_key_dialog_title"/>
    <string name="ftp_session_key_dialog_title">"%1$s க்கு அமர்வு விசை தட்டச்சுசெய்"</string>
    <add-resource type="string" name="ftp_session_key_dialog_header"/>
    <string name="ftp_session_key_dialog_header">"புளூடூத் அமர்வு விசை தேவை"</string>
    <add-resource type="string" name="ftp_acceptance_timeout_message"/>
    <string name="ftp_acceptance_timeout_message">"%1$s உடனான இணைப்பை ஏற்பதற்கு நேரம் முடிந்தது"</string>
    <add-resource type="string" name="ftp_authentication_timeout_message"/>
    <string name="ftp_authentication_timeout_message">"%1$s உடனான அமர்வு விசையை உள்ளிட நேரம் முடிந்தது"</string>
    <add-resource type="string" name="ftp_notif_ticker"/>
    <string name="ftp_notif_ticker">"புளூடூத் இணைப்புக் கோரிக்கை"</string>
    <!-- Notification title when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="ftp_notif_title"/>
    <string name="ftp_notif_title">"FTP இற்கான அமர்வு விசைக் கோரிக்கை"</string>
    <!-- Notification message when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="ftp_notif_message"/>
    <string name="ftp_notif_message">"\"<xliff:g id="device_name">%1$s</xliff:g>\" உடன் இணைக்க தொடவும்."</string>
    <!-- Activity label of BluetoothFtpPermissionActivity, also used as Strings in the permission dialog [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_ftp_request"/>
    <string name="bluetooth_ftp_request">"Ftp கோரிக்கை"</string>

    <!-- Bluetooth FTP permission Alert Activity text [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_ftp_acceptance_dialog_text"/>
    <string name="bluetooth_ftp_acceptance_dialog_text">"%1$s உங்கள் கோப்புகளையும் கோப்புறைகளையும் அணுக வேண்டுகிறது. %2$s க்கு அணுகல் வழங்க வேண்டுமா?"</string></resources>
