<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="sap_notif_ticker"/>
    <string name="sap_notif_ticker">"Bluetooth SIM প্ৰৱেশৰ অনুৰোধ"</string>
    <!-- Notification title when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="sap_notif_title"/>
    <string name="sap_notif_title">"SAP-ৰ বাবে ছেচন কীৰ অনুৰোধ"</string>
    <!-- Notification message when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="sap_notif_message"/>
    <string name="sap_notif_message">"\"<xliff:g id="device_name">%1$s</xliff:g>\"-লৈ সংযোগ কৰিবলৈ স্পৰ্শ কৰক৷"</string>
    <add-resource type="string" name="sap_acceptance_timeout_message"/>
    <string name="sap_acceptance_timeout_message">"%1$s-ৰ সৈতে সংযোগ গ্ৰহণ কৰিবলৈ সময়সীমা উকলিছিল"</string>
    <!-- Activity label of BluetoothSapPermissionActivity, also used as Strings in the permission dialog [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_sap_request"/>
    <string name="bluetooth_sap_request">"SAP অনুৰোধ"</string>
    <!-- Bluetooth SAP permission Alert Activity text [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_sap_acceptance_dialog_text"/>
    <string name="bluetooth_sap_acceptance_dialog_text">"%1$s-এ আপোনাৰ SIM প্ৰৱেশ কৰিবলৈ বিচাৰিছে। %2$s-ত প্ৰৱেশ কৰিবলৈ দিবনে?"</string></resources>
