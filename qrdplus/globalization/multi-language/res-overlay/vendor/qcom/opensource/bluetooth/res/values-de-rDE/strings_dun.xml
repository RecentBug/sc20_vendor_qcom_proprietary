    <string name="bluetooth_dun_request">"DUN-Anforderung"</string>
    <!-- Bluetooth DUN permission Alert Activity text [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_dun_acceptance_dialog_text"/>
    <string name="bluetooth_dun_acceptance_dialog_text">"%1$s möchte DUN verwenden. Zugriff auf %2$s erteilen?"</string></resources>
