    <add-resource type="string" name="dun_notif_ticker"/>
    <string name="dun_notif_ticker">"Bluetooth-DUN-Anfrage"</string>
    <!-- Notification title when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="dun_notif_title"/>
    <string name="dun_notif_title">"Sitzungsschlüsselanforderung für DUN"</string>
