<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="dun_notif_ticker"/>
    <string name="dun_notif_ticker">"Bluetooth DUN അഭ്യര്ത്ഥന"</string>
    <!-- Notification title when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="dun_notif_title"/>
    <string name="dun_notif_title">"DUN-നായുള്ള സെഷൻ കീ അഭ്യർത്ഥന"</string>
    <!-- Notification message when a Bluetooth device wants to pair with us -->
    <add-resource type="string" name="dun_notif_message"/>
    <string name="dun_notif_message">"\"<xliff:g id="device_name">%1$s</xliff:g>\"-ലേക്ക് ബന്ധിപ്പിക്കുന്നതിന് സ്പർശിക്കുക."</string>
    <add-resource type="string" name="dun_acceptance_timeout_message"/>
    <string name="dun_acceptance_timeout_message">"%1$s-മൊത്തുള്ള കണക്ഷൻ സ്വീകരിക്കുന്നതിന് സമയം കഴിയൽ സംഭവിച്ചു"</string>
    <!-- Activity label of BluetoothDunPermissionActivity, also used as Strings in the permission dialog [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_dun_request"/>
    <string name="bluetooth_dun_request">"DUN അഭ്യർത്ഥന"</string>
    <!-- Bluetooth DUN permission Alert Activity text [CHAR LIMIT=none] -->
    <add-resource type="string" name="bluetooth_dun_acceptance_dialog_text"/>
    <string name="bluetooth_dun_acceptance_dialog_text">"DUN ഉപയോഗിക്കാൻ %1$s താൽപ്പര്യപ്പെടുന്നു. %2$s-ന് പ്രവേശനം നൽകണോ?"</string></resources>
