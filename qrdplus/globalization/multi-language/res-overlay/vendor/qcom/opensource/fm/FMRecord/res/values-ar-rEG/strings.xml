<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- alert to the user that USB storage must be available before using FM recording  [CHAR LIMIT=NONE] -->
   <add-resource type="string" name="no_storage"/>
    <string name="no_storage">"قم بتثبيت بطاقة SD قبل التسجيل."</string>
   <!-- alert to the user that the USB storage is being disk-checked [CHAR LIMIT=30] -->
   <add-resource type="string" name="preparing_sd"/>
    <string name="preparing_sd">"تحضير بطاقة SD"</string>
   <!-- alert to the user that the FM fails to read or write the USB storage. [CHAR LIMIT=NONE] -->
   <add-resource type="string" name="access_sd_fail"/>
    <string name="access_sd_fail">"تعذر الدخول إلى بطاقة SD."</string>
   <!-- Low-memory dialog message [CHAR LIMT=NONE] -->
   <add-resource type="string" name="spaceIsLow_content"/>
    <string name="spaceIsLow_content">"المساحة في بطاقة وحدة تخزين بطاقة SD تنفد. غيِّر إعداد الجودة أو احذف بعض الصور أو الملفات الأخرى."</string>
   <!-- The messsage shown when FM record reaches size limit. -->
   <add-resource type="string" name="FMRecording_reach_size_limit"/>
    <string name="FMRecording_reach_size_limit">"تم بلوغ الحد الأقصى للحجم."</string>
    <!-- the name under which recordings will be visible in the media database is formatted like this -->
    <add-resource type="string" name="audio_db_title_format"/>
    <string name="audio_db_title_format"><xliff:g id="format">yyyy-MM-dd HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this 'artist' name -->
    <add-resource type="string" name="audio_db_artist_name"/>
    <string name="audio_db_artist_name">"تسجيلات FM الخاصة بي"</string>
    <!-- all recordings will show up in the media database with this 'album' name -->
    <add-resource type="string" name="audio_db_album_name"/>
    <string name="audio_db_album_name">"تسجيلات FM"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <add-resource type="string" name="audio_db_playlist_name"/>
    <string name="audio_db_playlist_name">"تسجيلات FM"</string>
    <add-resource type="string" name="save_record_file"/>
    <string name="save_record_file">"تم حفظ الملف المُسجّل على نطاق FM في <xliff:g id="record_file">%1$s</xliff:g>"</string>
    <add-resource type="string" name="fm_record_progress"/>
    <string name="fm_record_progress">"aيتم التسجيل على نطاق FM"</string>
    <add-resource type="string" name="unable_to_store"/>
    <string name="unable_to_store">"يتعذر حفظ الصوت المسجل"</string></resources>
