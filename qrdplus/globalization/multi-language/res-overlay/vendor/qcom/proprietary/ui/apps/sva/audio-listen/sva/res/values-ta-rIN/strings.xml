    <add-resource type="string" name="user_tutorial1_top"/>
    <string name="user_tutorial1_top">"குரல் செயலாக்கம் ஆன் செய்யப்படும் போது, திரை உறக்க நிலையில் இருந்தாலும் கூட உங்கள் குரலை ஃபோன் கேட்கும்."</string>
    <add-resource type="string" name="user_tutorial1_bottom"/>
    <string name="user_tutorial1_bottom">"ஃபோனை வேக் அப் செய்ய மற்றும் பயன்பாட்டை துவக்க, உங்கள் பிரதான செற்றொடரில் ஒன்றை கூறலாம்."</string>
    <add-resource type="string" name="user_tutorial2_title"/>
    <string name="user_tutorial2_title">"குரல் செயலாக்கம் ஆன் மற்றும் ஆஃப்"</string>
    <add-resource type="string" name="user_tutorial2_body"/>
    <string name="user_tutorial2_body">"நீங்கள் விரும்பினால், அனைத்து நேரத்திலும் நீங்கள் குரல் செயலாக்கத்தை வைத்திருக்கலாம்.\n\nஅல்லது,  நேரத்திற்கேற்றவாறு நீங்கள் குரல் செயலாக்கத்தை ஆன் அல்லது ஆஃப் செய்யலாம் நீங்கள் விருப்பப்படும் போது குரல் செயலாக்கம் உங்கள் குரலை கேட்குமாறு தேர்வுசெய்யலாம்.\n\n குரல் செயலாக்க பிரதான திரையின் மேல் பிரதான ஆன்/ஆஃப் கட்டுப்பாட்டை கண்டறியலாம்."</string>
    <add-resource type="string" name="user_tutorial3_title"/>
    <string name="user_tutorial3_title">"உங்கள் சொந்த பிரதான செற்றொடரை சேர்க்கவும்"</string>
    <add-resource type="string" name="user_tutorial3_top"/>
    <string name="user_tutorial3_top">"எந்த நேரத்திலும், நீங்கள் புதிய செற்றொடரை சேர்க்கலாம் மற்றும் சொந்த வார்த்தையை தேர்வுசெய்யலாம்."</string>
    <add-resource type="string" name="user_tutorial3_bottom"/>
    <string name="user_tutorial3_bottom">"உங்கள் சொந்த பிரதான செற்றொடரை உருவாக்க, பிரதான திரையின் மேலுள்ள \"+\" சின்னத்தை தேர்வுசெய்து விதிமுறைகளை பின்பற்றவும். நீங்கள் பிரதான செற்றொடரை அடிக்கடி பல முறைகள் சொல்லும்படி கூறுவீர்கள் ஆதலால் உங்கள் குரலை குரல் செயலாக்கம் கண்டறிந்து கற்றுக்கொள்ளலாம்."</string>
    <add-resource type="string" name="user_tutorial4_title"/>
    <string name="user_tutorial4_title">"முயல்க!"</string>
    <add-resource type="string" name="user_tutorial4_top"/>
    <string name="user_tutorial4_top">"ஒன்றுக்கும் மேற்பட்ட பிரதான செற்றொடரை உருவாக்கி, பல பயன்பாட்டிற்கேற்றவாறு ஒவ்வொன்றையும் துவக்குக."</string>
    <add-resource type="string" name="user_tutorial4_bottom"/>
    <string name="user_tutorial4_bottom">"எடுத்துக்காட்டு, நீங்கள் ஒரு பிரதான செற்றொடரை உருவாக்கலாம் அது உங்கள் கேமராவை துவக்கும் மற்றும் மற்றொரு செற்றொடரானது, உங்களுக்கு பிடித்த தகவல் பயன்பாட்டை துவக்கும்."</string>
    <add-resource type="string" name="user_editkeyphrase_pdknodeletion"/>
    <string name="user_editkeyphrase_pdknodeletion">"முன்ஏற்றப்பட்ட பிரதான செற்றொடர் நீக்கப்பட மாட்டாது."</string>
    <add-resource type="string" name="user_editkeyphrase_dialog_confirmdelete_title"/>
    <string name="user_editkeyphrase_dialog_confirmdelete_title">"நீக்க உறுதிசெய்க"</string>
    <add-resource type="string" name="user_editkeyphrase_dialog_confirmdelete_message"/>
    <string name="user_editkeyphrase_dialog_confirmdelete_message">"நீங்கள் உறுதியாக பிரதான செற்றொடரை நீக்க விரும்புகிறீர்கள் என்பதை உறுதிசெய்க"</string>
    <add-resource type="string" name="user_editkeyphrase_dialog_pdknotfound_message"/>
    <string name="user_editkeyphrase_dialog_pdknotfound_message">"பிரதான செற்றொடரை கண்டறிய இயலவில்லை."</string>
    <add-resource type="string" name="user_main_toast_turnkeyphraseoffbeforeediting"/>
    <string name="user_main_toast_turnkeyphraseoffbeforeediting">"திருத்துவதற்கு முன் பிரதான சொற்றொடரை நிறுத்தவும்."</string>
    <add-resource type="string" name="user_main_toast_turnonvatoselectkeyphrases"/>
    <string name="user_main_toast_turnonvatoselectkeyphrases">"பிரதான சொற்றொடர்களை தேர்வுசெய்ய குரல் இயக்குதலை இயக்கவும்."</string>
    <add-resource type="string" name="user_main_toast_8keyphrasesatoncewarning"/>
    <string name="user_main_toast_8keyphrasesatoncewarning">"ஒரே நேரத்தில் 8 முக்கிய சொற்றொடர்களுக்கு மேல் தேர்வுசெய்ய முடியாது."</string>
