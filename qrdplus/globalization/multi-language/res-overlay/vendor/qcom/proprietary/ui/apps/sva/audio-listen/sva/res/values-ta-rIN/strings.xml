    <add-resource type="string" name="user_addkeyphrase_instruction"/>
    <string name="user_addkeyphrase_instruction">"உங்கள் ஃபோனுடன் செயல்பட விரும்பும் செற்றொடரை உள்ளிடவும்."</string>
