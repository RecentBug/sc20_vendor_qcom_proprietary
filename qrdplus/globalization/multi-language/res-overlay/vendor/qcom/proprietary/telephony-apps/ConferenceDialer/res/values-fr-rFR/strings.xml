<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Directory partition name -->
    <add-resource type="string" name="applicationLabel"/>
    <string name="applicationLabel">"Numéroteur de conférence"</string>

    <!--  Title of start conference call -->
    <add-resource type="string" name="conference_call_activity_title"/>
    <string name="conference_call_activity_title">"Appel de conférence 4G"</string>
    <!-- add for 4g conference call, Menu item used to initiate 4g conference call from dialer -->
    <add-resource type="string" name="menu_4g_conference_call"/>
    <string name="menu_4g_conference_call">"Appel de conférence 4G"</string>
    <add-resource type="string" name="add_to_4g_conference_call"/>
    <string name="add_to_4g_conference_call">"Ajouter à un appel de conférence 4G"</string>
    <add-resource type="string" name="button_start_conference_call"/>
    <string name="button_start_conference_call">"Démarrer l\'appel"</string>
    <add-resource type="string" name="contact_app_not_found"/>
    <string name="contact_app_not_found">"Impossible de trouver l\'application des contacts"</string>
    <add-resource type="string" name="number_of_recipients"/>
    <string name="number_of_recipients">5</string>
    <add-resource type="string" name="too_many_recipients"/>
    <string name="too_many_recipients">"Le nombre maximal de participants autorisés pendant un appel de conférence 4G est de <xliff:g id="maxLimit">%1$s</xliff:g>."</string>
    <add-resource type="string" name="repeated_recipients"/>
    <string name="repeated_recipients">"Certains destinataires sont répétés"</string>
    <add-resource type="string" name="button_add_participant"/>
    <string name="button_add_participant">"Ajouter des participants"</string>
    <!-- In the title of the compose message activity, when there are multiple recipients, we show
         the number of recipients in the subtitle of the action bar. There's never a case of one. [CHAR LIMIT=50] -->
    <add-resource type="plurals" name="recipient_count"/>
    <plurals name="recipient_count">
        <!-- Case of 0 or 2 or more results. -->
        <item quantity="other">"<xliff:g id="NUMBER">%1$s</xliff:g> personnes"</item>
    </plurals>
    <!-- Title of progress dialog for picker -->
    <add-resource type="string" name="pick_too_many_recipients"/>
    <string name="pick_too_many_recipients">"Trop de destinataires"</string>
    <!-- Message of progress dialog for picker -->
    <add-resource type="string" name="adding_recipients"/>
    <string name="adding_recipients">"Ajout de destinataires"</string>
    <!-- add for 4g conference call, Menu item used to initiate 4g conference call from dialer -->

    <add-resource type="string" name="add_up_five_numbers"/>
    <string name="add_up_five_numbers">"Ajouter jusqu\'à 5 numéros"</string>
    <add-resource type="string" name="tab_font_family"/>
    <string name="tab_font_family">sans-serif</string>
    <!-- File Authority for AOSP Contacts files -->
    <add-resource type="string" name="contacts_file_provider_authority"/>
    <string name="contacts_file_provider_authority">"com.android.contacts.files"</string></resources>
