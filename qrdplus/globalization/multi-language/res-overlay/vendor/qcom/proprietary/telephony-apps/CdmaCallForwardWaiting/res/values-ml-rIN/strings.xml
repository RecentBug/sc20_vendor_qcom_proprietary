    <add-resource type="string" name="CFB_ACT"/>
    <string name="CFB_ACT"></string>
    <!-- Deactivate call forward when busy -->
    <add-resource type="string" name="CFB_DEACT"/>
    <string name="CFB_DEACT"></string>
    <!-- Activate call forward when no reply -->
    <add-resource type="string" name="CFNRY_ACT"/>
    <string name="CFNRY_ACT"></string>
    <!-- Deactivate call forward when no reply -->
    <add-resource type="string" name="CFNRY_DEACT"/>
    <string name="CFNRY_DEACT"></string>
    <!-- Activate call forward when not reachable -->
    <add-resource type="string" name="CFNRC_ACT"/>
    <string name="CFNRC_ACT"></string>
    <!-- Deactivate call forward when not reachable -->
    <add-resource type="string" name="CFNRC_DEACT"/>
    <string name="CFNRC_DEACT"></string>
    <!-- Deactivate all call forward -->
    <add-resource type="string" name="CFA_DEACT"/>
    <string name="CFA_DEACT"></string>
    <!-- Activate call waiting -->
    <add-resource type="string" name="CW_ACT"/>
    <string name="CW_ACT"></string>
    <!-- Deactivate call waiting -->
    <add-resource type="string" name="CW_DEACT"/>
    <string name="CW_DEACT"></string></resources>
