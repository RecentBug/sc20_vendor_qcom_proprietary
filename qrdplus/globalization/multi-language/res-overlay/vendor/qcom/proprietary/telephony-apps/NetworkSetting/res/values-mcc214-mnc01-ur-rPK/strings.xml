<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Mobile network settings screen, setting summary text when check box is selected -->
    <add-resource type="string" name="roaming_enable"/>
    <string name="roaming_enable">"ڈیٹا رومنگ بیرون ملک رہتے ہوئے آپ کو ویب براؤز کرنے، ای میل، MMS اور دیگر ڈیٹا سروسز استعمال کرنے کا اہل بناتا ہے۔ اپنے نیٹ ورک فراہم کنندہ سے قیمت معلوم کریں"</string>
    <!-- Mobile network settings screen, setting summary text when check box is clear -->
    <add-resource type="string" name="roaming_disable"/>
    <string name="roaming_disable">"ڈیٹا رومنگ بیرون ملک رہتے ہوئے آپ کو ویب براؤز کرنے، ای میل، MMS اور دیگر ڈیٹا سروسز استعمال کرنے کا اہل بناتا ہے۔ اپنے نیٹ ورک فراہم کنندہ سے قیمت معلوم کریں"</string>

    <!-- Mobile network settings screen, dialog message when user selects the Data roaming check box -->
    <add-resource type="string" name="roaming_warning"/>
    <string name="roaming_warning">"رومنگ کے وقت ڈیٹا استعمال کرنے کے لیے &lt;OK&gt; پر کلک کریں۔ اپنے نیٹ ورک فراہم کنندہ سے قیمت چیک کریں۔"</string></resources>
