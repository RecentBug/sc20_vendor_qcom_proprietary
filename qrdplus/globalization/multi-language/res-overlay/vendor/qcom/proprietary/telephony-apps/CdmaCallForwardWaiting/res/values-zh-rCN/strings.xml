    <add-resource type="string" name="call_forwarding_settings"/>
    <string name="call_forwarding_settings">"来电转接设置"</string>
    <!-- Call forwarding settings screen, setting option name -->
    <add-resource type="string" name="labelCFU"/>
    <string name="labelCFU">"始终转接"</string>
    <!-- Call forwarding dialog box, text field label -->
    <add-resource type="string" name="messageCFU"/>
    <string name="messageCFU">"始终使用此号码"</string>
