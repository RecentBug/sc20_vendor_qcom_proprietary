    <add-resource type="string" name="labelCW"/>
    <string name="labelCW">"来电等待"</string>

    <add-resource type="string" name="labelCFNRc_cdma"/>
    <string name="labelCFNRc_cdma">"隐含呼叫转移"</string>
    <add-resource type="string" name="CFActivate"/>
    <string name="CFActivate">"激活"</string>
    <add-resource type="string" name="CFDeactivate"/>
    <string name="CFDeactivate">"禁用"</string>
