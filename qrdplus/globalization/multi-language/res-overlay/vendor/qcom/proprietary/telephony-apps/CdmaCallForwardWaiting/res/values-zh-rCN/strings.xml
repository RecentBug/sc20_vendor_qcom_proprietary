    <add-resource type="string" name="labelCFDeactAll"/>
    <string name="labelCFDeactAll">"全部禁用"</string>

    <!-- Button label used in several settings-related dialogs -->
    <add-resource type="string" name="enable"/>
    <string name="enable">"开启"</string>
    <!-- Button label used in several settings-related dialogs -->
    <add-resource type="string" name="disable"/>
    <string name="disable">"关闭"</string>
    <!-- Button label which indicates the user wants to update a stored
