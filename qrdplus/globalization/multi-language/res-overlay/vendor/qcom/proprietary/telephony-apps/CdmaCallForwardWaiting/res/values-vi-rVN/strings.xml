    <add-resource type="string" name="CFNRC_DEACT"/>
    <string name="CFNRC_DEACT"></string>
    <!-- Deactivate all call forward -->
    <add-resource type="string" name="CFA_DEACT"/>
    <string name="CFA_DEACT"></string>
