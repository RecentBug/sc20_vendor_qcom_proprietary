    <add-resource type="array" name="switch_set"/>
    <string-array name="switch_set">
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na manj kot 1 minuto od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> in <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> in <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> in <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> od zdaj."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> je nastavljeno na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>, <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> in <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> od zdaj."</item>
    </string-array>

