    <add-resource type="string" name="wifiap_settings_indication_step2"/>
    <string name="wifiap_settings_indication_step2">"2. WLAN नेटवर्कच्या सूचीमध्ये “<xliff:g id="ssid_name">%s</xliff:g>” शोधा."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_ssid"/>
    <string name="wifiap_settings_indication_step3_ssid">"3. “<xliff:g id="ssid_name">%s</xliff:g>” निवडा."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_all"/>
    <string name="wifiap_settings_indication_step3_all">"3. पासवर्ड फिल्डमध्ये “<xliff:g id="ssid_name">%s</xliff:g>” निवडा आणि “<xliff:g id="ssid_pwd">%s</xliff:g>” प्रविष्ट करा."</string>
