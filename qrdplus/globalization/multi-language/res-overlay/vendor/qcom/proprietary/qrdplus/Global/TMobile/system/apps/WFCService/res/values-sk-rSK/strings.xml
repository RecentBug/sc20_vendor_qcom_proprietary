    <add-resource type="string" name="question_what_is_wifi_calling"/>
    <string name="question_what_is_wifi_calling">" Čo predstavujú Wi-Fi hovory?"</string>
    <add-resource type="string" name="answer_question_what_is_wifi_calling"/>
    <string name="answer_question_what_is_wifi_calling">"Hovory Wi-Fi umožňujú uskutočňovať a prijímať hlasové hovory a textové/multimediálne správy cez Wi-Fi, ak je telefón pripojený k sieti Wi-Fi a súčasne zaregistrovaný v sieti T-Mobile na Wi-Fi hovory."</string>
    <add-resource type="string" name="question_how_enable_wifi"/>
    <string name="question_how_enable_wifi">"Ako zapnem Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_how_enable_wifi"/>
    <string name="answer_question_how_enable_wifi">"Na zapnutie funkcie Wi-Fi použite časť ovládania bezdrôtových sietí v ponuke nastavení systému Android."</string>
    <add-resource type="string" name="question_how_enable_wifi_calling"/>
    <string name="question_how_enable_wifi_calling">"Ako zapnem Wi-Fi hovory?"</string>
    <add-resource type="string" name="answer_question_how_enable_wifi_calling"/>
    <string name="answer_question_how_enable_wifi_calling">"Predvolene sa Wi-Fi hovory zapnú ihneď po zapnutí Wi-Fi rádia. Wi-Fi hovory možno ovládať v nastaveniach Wi-Fi hovorov."</string>
    <add-resource type="string" name="question_if_use_Wifi"/>
    <string name="question_if_use_Wifi">"Ako zistím, že na hovory používam Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_if_use_Wifi"/>
    <string name="answer_question_if_use_Wifi">"Keď sa váš telefón spojí so sieťou Wi-Fi, automaticky sa pokúsi zaregistrovať v sieti T-Mobile na Wi-Fi hovory. Po úspešnom dokončení registrácie sa v stavovom riadku zobrazí ikona Wi-Fi hovorov. Táto ikona sa zobrazí aj vo Vytáčaní (na mieste, kde je ikona bežného volania) a nakrátko sa v hornej časti obrazovky objaví hlásenie o umožnení hovorov cez Wi-Fi."</string>
    <add-resource type="string" name="question_charge_diff_about_wifi_call"/>
    <string name="question_charge_diff_about_wifi_call">"Budú sa hovory či SMS/MMS cez Wi-Fi účtovať inak, než bežné mobilné hovory?"</string>
    <add-resource type="string" name="answer_question_charge_diff_about_wifi_call"/>
    <string name="answer_question_charge_diff_about_wifi_call">"Hlavnou výhodou Wi-Fi hovorov je zvýšenie pokrytia na miestach s Wi-Fi. Hovory a správy uskutočnené cez Wi-Fi sa budú účtovať ako bežné mobilné hovory v rámci vášho paušálu."</string>
    <add-resource type="string" name="question_what_happen_without_wifi_coverage"/>
    <string name="question_what_happen_without_wifi_coverage">"Čo sa stane, ak sa dostanem mimo pokrytia Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_what_happen_without_wifi_coverage"/>
    <string name="answer_question_what_happen_without_wifi_coverage">"V prípade, že stratíte pokrytie Wi-Fi, všetky aktívne hovory sa ukončia a telefón sa pokúsi pripojiť k bežnej mobilnej sieti. V prípade, že hovor spadne v dôsledku straty pokrytia Wi-Fi, objaví sa upozornenie."</string>
    <add-resource type="string" name="wifi_calling_wizard_learn_more"/>
    <string name="wifi_calling_wizard_learn_more">"Ďalšie informácie"</string>
    <add-resource type="string" name="wifi_calling_help_wizard_title"/>
    <string name="wifi_calling_help_wizard_title">"Zistite informácie o Wi-Fi hovoroch"</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_1"/>
    <string name="wifi_calling_wizard_content_step_1">"Toto je školenie k Wi-Fi hovorom. \n Tlačidlo \"Preskočiť\" tentoraz sprievodcu preskočí. \n Tlačidlo \"Ďalšie informácie\" vám ukáže viac"</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_2"/>
    <string name="wifi_calling_wizard_content_step_2">"Vďaka Wi-Fi hovorom sa môžete rozlúčiť s prerušeným pokrytím. Wi-Fi hovory vám umožňujú odosielanie a príjímanie hovorov a správ cez sieť Wi-Fi použitím vášho súčasného paušálu T-Mobile.  \n  Pre Wi-Fi hovory sa nevyžaduje žiadne špeciálne povolenie.  V ponuke Nastavenia prejdite na možnosť Bezdrôtové a Siete a pripojenia a začnite s pripojením k sieti Wi-Fi."</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_3"/>
    <string name="wifi_calling_wizard_content_step_3">"Po tom, čo sa v stavovom riadku zobrazí ikona Wi-Fi hovorov je váš telefón pripravený na Wi-fi hovory. "</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_4"/>
    <string name="wifi_calling_wizard_content_step_4">"Aby sa Wi-Fi hovor neprerušil, zostaňte v oblasti pokrytia siete Wi-Fi. Po spadnutí hovoru v dôsledku opustenia oblasti pokrytia začujete zvukové upozornenie a na obrazovke sa zobrazí hlásenie."</string>

