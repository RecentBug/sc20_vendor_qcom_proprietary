    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"Kuona miunganisho ya Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"Inaruhusu programu kuona taarifa kuhusu mtandao wa Wi-Fi, kama vile ikiwa Wi-Fi imewezeshwa mna jina la vifaa vya Wi-Fi vilivyounganishwa."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"unganisha na utenganishe kutoka kwa Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"Inaruhusu programu kuunganisha kwenye au kukata kutoka pointi za ufikivu wa Wi-Fi na kufanya mabadiliko kwenye usanidi wa kifaa cha mitandao ya Wi-Fi."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"ruhusu upokeaji wa Wi-Fi Multicast"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Inaruhusu programu kupokea pakiti zilizotumwa katika vifaa vyote kwenye mtandao wa Wi-Fi kwa kutumia anwani za upeperushaji anuwai, sio tu kompyuta yako kibao. Inatumia nguvu zaidi kuliko mfumo wa upeperushaji usio anuwai."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"Inaruhusu programu kupokea pakiti zilizotumwa katika vifaa vyote kwenye mtandao wa Wi-Fi kwa kutumia anwani za upeperushaji anuwai, sio tu simu yako. Inatumia nguvu zaidi kuliko mfumo wa upeperushaji usio anuwai."</string>

