    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"Len tiesňové hovory"</string>
    <!-- Strings for tethered notification -->
    <!-- Shown when the device is tethered -->

    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"zobraziť pripojenia Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"Umožňuje aplikácii zobraziť informácie o sieťach Wi-Fi. Napríklad o tom, či je sieť Wi-Fi povolená alebo názvy pripojených zariadení Wi-Fi."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"pripojiť a odpojiť od siete Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"Umožňuje aplikácii pripojiť sa na prístupové body siete Wi-Fi, odpojiť sa od nich a meniť konfiguráciu zariadení pre siete Wi-Fi."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"Povoliť príjem viacsmerového vysielania Wi-Fi"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Umožňuje aplikácii prijímať pakety odoslané na všetky zariadenia v sieti Wi-Fi pomocou viacsmerových adries, nielen pomocou vášho tabletu. Spotrebuje viac energie ako režim bez viacsmerového vysielania."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"Umožňuje aplikácii prijímať pakety odoslané na všetky zariadenia v sieti Wi-Fi pomocou viacsmerových adries, nielen pomocou vášho telefónu. Spotrebuje viac energie ako režim bez viacsmerového vysielania."</string>

    <add-resource type="string" name="wifi_available_sign_in"/>
    <string name="wifi_available_sign_in">"Prihlásenie do siete Wi-Fi"</string>
    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"Nepodarilo sa pripojiť k sieti Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"Priame pripojenie Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"Spustiť priame pripojenie siete Wi-Fi. Táto možnosť vypne sieť Wi-Fi v režime klient alebo hotspot."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Priame pripojenie siete Wi-Fi sa nepodarilo spustiť"</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"Priame pripojenie siete Wi-Fi je zapnuté"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Tablet bude počas pripojenia k zariadeniu <xliff:g id="DEVICE_NAME">%1$s</xliff:g> od siete Wi-Fi dočasne odpojený."</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Telefón bude počas pripojenia k zariadeniu <xliff:g id="DEVICE_NAME">%1$s</xliff:g> od siete Wi-Fi dočasne odpojený."</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_title"/>
    <string name="data_usage_wifi_limit_title">"Dáta Wi-Fi sú vypnuté"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"Dát. limit Wi-Fi bol prekročený"</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <add-resource type="plurals" name="wifi_available"/>
    <plurals name="wifi_available">
        <item quantity="one">"K dispozícii je sieť Wi-Fi"</item>
        <item quantity="other">"K dispozícii sú siete Wi-Fi."</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"K dispozícii je verejná sieť Wi-Fi"</item>
        <item quantity="other">"K dispozícii sú verejné siete Wi-Fi"</item>
    </plurals></resources>
