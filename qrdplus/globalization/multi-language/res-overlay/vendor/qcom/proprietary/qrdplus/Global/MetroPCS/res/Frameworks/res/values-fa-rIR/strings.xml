<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"‏مشاهدهٔ اتصالات Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"‏به برنامه امکان می‌دهد اطلاعات مربوط به شبکه Wi-Fi را مشاهده کند، به‌عنوان مثال فعال بودن Wi-Fi و نام دستگاه‌های Wi-Fi متصل."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"‏اتصال به Wi-Fi و قطع اتصال از آن"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"‏به برنامه اجازه می‎دهد تا به نقاط دسترسی Wi-Fi وصل شود و ارتباط خود را با آن‌ها قطع کند و تغییراتی را در پیکربندی دستگاه برای شبکه‎های Wi-Fi ایجاد کند."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"‏دریافت چندگانه Wi-Fi را مجاز می‌کند"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"‏به برنامه اجازه می‌دهد به دریافت بسته‌های ارسالی به همه دستگاه‌های موجود در شبکه Wi-Fi با استفاده از آدرس‌های پخش چندگانه و نه فقط به رایانهٔ لوحی شما بپردازند. این از توان مصرف بیشتری نسبت به حالت پخش غیرچندگانه استفاده می‌کند."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"‏به برنامه اجازه می‌دهد به دریافت بسته‌های ارسالی به همه دستگاه‌های موجود در شبکه Wi-Fi با استفاده از آدرس‌های پخش چندگانه و نه فقط به تلفن شما بپردازند. این از توان مصرف بیشتری نسبت به حالت پخش غیرچندگانه استفاده می‌کند."</string>

    <add-resource type="string" name="wifi_available_sign_in"/>
    <string name="wifi_available_sign_in">"ورود به سیستم شبکه Wi-Fi"</string>
    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"‏اتصال به Wi-Fi ممکن نیست"</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"Wi-Fi Direct"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"‏Wi-Fi Direct را شروع کنید. این کار نقطه اتصال/سرویس گیرنده Wi-Fi را غیرفعال خواهد کرد."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"‏Wi-Fi Direct شروع نشد."</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"‏Wi-Fi Direct روشن است"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"‏در حین اتصال به <xliff:g id="DEVICE_NAME">%1$s</xliff:g> ارتباط این رایانه لوحی با Wi-Fi موقتاً قطع خواهد شد."</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"‏این گوشی به‌طور موقت از Wi-Fi قطع خواهد شد، در حالی که به <xliff:g id="DEVICE_NAME">%1$s</xliff:g> وصل است"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_title"/>
    <string name="data_usage_wifi_limit_title">"داده Wi-Fi خاموش است"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"‏از محدوده مجاز داده‌های Wi-Fi بیشتر شد"</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <add-resource type="plurals" name="wifi_available"/>
    <plurals name="wifi_available">
        <item quantity="one">"‏شبکه Wi-Fi موجود است"</item>
        <item quantity="other">"‏شبکه‌های Wi-Fi موجود هستند"</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"‏شبکه Wi-Fi موجود را باز کنید"</item>
        <item quantity="other">"‏شبکه‌های Wi-Fi موجود را باز کنید"</item>
    </plurals>

    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"فقط تماس‌های اضطراری"</string>
    <add-resource type="string" name="mobile_tether_help_dialog_title"/>
    <string name="mobile_tether_help_dialog_title">"راهنمای اتصال به اینترنت با تلفن همراه ازطریق USB"</string>
    <add-resource type="string" name="learn_usb_dialog_text"/>
    <string name="learn_usb_dialog_text">"می‌خواهید راجع به اتصال اینترنت USB در تلفن خود بیشتر بدانید؟"</string>
    <add-resource type="string" name="mobile_usb_help_dialog_text"/>
    <string name="mobile_usb_help_dialog_text">"می‌توانید دستگاه Android خود را به یک نقطه اتصال همراه قابل‌حمل تبدیل کنید و اتصال اینترنت دستگاه Android خود را با یک یا چند رایانه یا دستگاه‌های دیگر به اشتراک بگذارید. \n\n۱. وقتی دستگاهتان به‌عنوان نقطه اتصال همراه درحال استفاده است، می‌توانید از برنامه‌های آن برای دسترسی به اینترنت ازطریق اتصال Wi-Fi استفاده کنید (اما همچنان از طریق شبکه داده همراهتان به اینترنت متصل می‌مانید). \n\n۲.نقطه اتصال را با تنظیمات نقطه اتصال همراه پیکربندی کنید"</string></resources>
