    <add-resource type="array" name="switch_set"/>
    <string-array name="switch_set">
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na mniej niż 1 min począwszy od tej chwili."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> i <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> i <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> i <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> począwszy od teraz."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ustawiono na <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>, <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> i <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> począwszy od teraz."</item>
    </string-array>

