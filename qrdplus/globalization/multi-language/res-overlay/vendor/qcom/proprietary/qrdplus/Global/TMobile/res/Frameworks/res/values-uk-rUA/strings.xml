    <string name="wifi_p2p_dialog_title">"Wi-Fi Direct"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"Запустити Wi-Fi Direct. Це вимкне з\'єднання Wi-Fi клієнт/точка доступу."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Не вдалося запустити Wi-Fi Direct."</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"Wi-Fi Direct увімкнено"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Під час з\'єднання з пристроєм <xliff:g id="DEVICE_NAME">%1$s</xliff:g> планшетний ПК тимчасово від\'єднається від мережі Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Під час з\'єднання з пристроєм <xliff:g id="DEVICE_NAME">%1$s</xliff:g> телефон тимчасово від\'єднається від мережі Wi-Fi"</string>
