    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"Только экстр. вызовы"</string>
    <!-- Strings for tethered notification -->
    <!-- Shown when the device is tethered -->

    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"Просмотр подключений Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"Приложение сможет просматривать информацию о сети Wi-Fi: состояние сети и имена подключенных устройств."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"Подключение/отключение от сети Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"Приложение сможет подключаться к точкам доступа Wi-Fi и отключаться от них, а также изменять конфигурацию сетей Wi-Fi на устройстве."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"Получение данных по многоадресной рассылке через Wi-Fi"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Приложение сможет получать пакеты, отправленные всем устройствам в сети Wi-Fi (не только вашему планшетному ПК) в многоадресной рассылке. При этом расходуется больше энергии, чем в одноадресном режиме."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"Приложение сможет получать пакеты, отправленные всем устройствам в сети Wi-Fi (не только вашему телефону) в многоадресной рассылке. При этом расходуется больше энергии, чем в одноадресном режиме."</string>

    <add-resource type="string" name="wifi_available_sign_in"/>
    <string name="wifi_available_sign_in">"Вход в сеть Wi-Fi"</string>
    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"Не удалось подключиться к сети Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"Wi-Fi Direct"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"Начать соединение через Wi-Fi Direct. Модуль Wi-Fi будет отключен."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Не удалось запустить Wi-Fi Direct."</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"Wi-Fi Direct включен"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"При подключении к устройству <xliff:g id="DEVICE_NAME">%1$s</xliff:g> планшетный ПК будет временно отключаться от сети Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"При подключении к устройству <xliff:g id="DEVICE_NAME">%1$s</xliff:g> телефон будет временно отключаться от сети Wi-Fi"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_title"/>
    <string name="data_usage_wifi_limit_title">"Данные Wi-Fi отключены"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"Превышен лимит трафика Wi-Fi"</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <add-resource type="plurals" name="wifi_available"/>
    <plurals name="wifi_available">
        <item quantity="one">"Доступна сеть Wi-Fi"</item>
        <item quantity="other">"Доступна сеть Wi-Fi"</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"Найдена доступная сеть Wi-Fi"</item>
        <item quantity="other">"Найдены доступные сети Wi-Fi"</item>
