    <add-resource type="string" name="wifiap_settings_desc"/>
    <string name="wifiap_settings_desc">"Wifi aplikácia WLAN dokáže internetové pripojenie tohto telefónu zdieľať s inými PC/mobilnými zariadeniami cez WLAN."</string>
    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"Po zapnutí wifi aplikácie WLAN sa k telefónu môžu pripojiť iné PC/mobilné zariadenia a začať využívať internet nasledujúcim spôsobom:"</string>
    <add-resource type="string" name="wifiap_settings_indication_step1"/>
    <string name="wifiap_settings_indication_step1">"1. Na PC/mobilnom zariadení zapnite WLAN."</string>
    <add-resource type="string" name="wifiap_settings_indication_step2"/>
    <string name="wifiap_settings_indication_step2">"2. V zozname sietí WLAN vyhľadajte „<xliff:g id="ssid_name">%s</xliff:g>”."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_ssid"/>
    <string name="wifiap_settings_indication_step3_ssid">"3. Vyberte možnosť „<xliff:g id="ssid_name">%s</xliff:g>”."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_all"/>
    <string name="wifiap_settings_indication_step3_all">"3. Vyberte „<xliff:g id="ssid_name">%s</xliff:g>“ a do poľa na heslo zadajte „<xliff:g id="ssid_pwd">%s</xliff:g>“."</string>
    <add-resource type="string" name="wifiap_settings_indication_step4"/>
    <string name="wifiap_settings_indication_step4">"4. Na PC/mobilnom zariadení začnite používať internet."</string>
