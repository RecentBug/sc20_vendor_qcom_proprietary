    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"Po włączeniu punktu dostępowego WLAN w telefonie można zapewnić innym komputerom/urządzeniom mobilnym połączenie z Internetem, wykonując poniższe czynności:"</string>
    <add-resource type="string" name="wifiap_settings_indication_step1"/>
