    <string name="wifiap_settings_desc">"Aplikacija WLAN wifiap omogoča skupno rabo internetne povezave z drugimi računalniki/mobilnimi napravami prek WLAN."</string>
    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"Ko je aplikacija WLAN wifiap vklopljena, se lahko z njo povežejo drugi računalniki/mobilne naprave in prek nje dostopajo do spleta tako:"</string>
    <add-resource type="string" name="wifiap_settings_indication_step1"/>
