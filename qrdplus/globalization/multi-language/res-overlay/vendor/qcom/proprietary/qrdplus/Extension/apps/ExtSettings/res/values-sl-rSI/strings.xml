    <add-resource type="string" name="wifiap_settings_indication_step2"/>
    <string name="wifiap_settings_indication_step2">"2. Na seznamu omrežij WLAN poiščite <xliff:g id="ssid_name">%s</xliff:g>”."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_ssid"/>
    <string name="wifiap_settings_indication_step3_ssid">"3. Izberite “<xliff:g id="ssid_name">%s</xliff:g>”."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_all"/>
