    <add-resource type="string" name="wifiap_settings_desc"/>
    <string name="wifiap_settings_desc">"Wifiap WLAN có thể chia sẻ kết nối internet dữ liệu của điện thoại này với các thiết bị PC/di động khác qua WLAN."</string>
    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"Sau khi bật wifiap WLAN, các thiết bị PC/di động khác có thể kết nối và bắt đầu sử dụng internet bằng cách làm theo các bước sau:"</string>
    <add-resource type="string" name="wifiap_settings_indication_step1"/>
    <string name="wifiap_settings_indication_step1">"1. Bật WLAN trên PC/Thiết bị di động."</string>
    <add-resource type="string" name="wifiap_settings_indication_step2"/>
    <string name="wifiap_settings_indication_step2">"2. Tìm “<xliff:g id="ssid_name">%s</xliff:g>” trong danh sách mạng WLAN."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_ssid"/>
    <string name="wifiap_settings_indication_step3_ssid">"3. Chọn “<xliff:g id="ssid_name">%s</xliff:g>”."</string>
    <add-resource type="string" name="wifiap_settings_indication_step3_all"/>
    <string name="wifiap_settings_indication_step3_all">"3. Chọn “<xliff:g id="ssid_name">%s</xliff:g>” và nhập “<xliff:g id="ssid_pwd">%s</xliff:g>” vào trường mật khẩu."</string>
    <add-resource type="string" name="wifiap_settings_indication_step4"/>
    <string name="wifiap_settings_indication_step4">"4. Bắt đầu sử dụng Internet trên PC/Thiết bị di động."</string>
