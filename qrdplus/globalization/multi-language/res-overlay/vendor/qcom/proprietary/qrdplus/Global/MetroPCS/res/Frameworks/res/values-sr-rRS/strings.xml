    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"Прекорачење преноса Wi-Fi подат."</string>

