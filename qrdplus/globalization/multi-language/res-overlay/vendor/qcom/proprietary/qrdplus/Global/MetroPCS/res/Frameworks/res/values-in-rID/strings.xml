    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Sambungan tablet akan diputuskan dari Wi-Fi untuk sementara saat tersambung dengan <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Ponsel akan terputus sementara dari Wi-Fi saat tersambung ke <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
