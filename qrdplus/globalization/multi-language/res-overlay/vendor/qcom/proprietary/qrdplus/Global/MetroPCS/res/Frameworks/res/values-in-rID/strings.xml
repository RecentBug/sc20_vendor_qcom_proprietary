    <string name="wifi_p2p_turnon_message">"Memulai Wi-Fi Direct. Opsi ini akan mematikan hotspot/klien Wi-Fi."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Tidak dapat memulai Wi-Fi Direct."</string>
