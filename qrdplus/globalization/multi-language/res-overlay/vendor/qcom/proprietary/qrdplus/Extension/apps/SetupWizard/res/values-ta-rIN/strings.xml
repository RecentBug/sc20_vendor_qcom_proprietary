    <add-resource type="string" name="skip_protect_dialog_msg"/>
    <string name="skip_protect_dialog_msg">"சாதனப் பாதுகாப்பு அம்சங்கள் செயல்படுத்தப்படவில்லை. இந்த ஃபோன் தொலைக்கப்பட்டால், திருடப்பட்டால் அல்லது காணாமல் போனால் மற்றவர்கள் இதை பயன்படுத்துவதை உங்களால் தடுக்க முடியாது."</string>
    <add-resource type="string" name="dont_skip"/>
    <string name="dont_skip">"தவிர்க்க வேண்டாம்"</string>
    <add-resource type="string" name="skip_anyway"/>
    <string name="skip_anyway">"எப்படியேனும் தவிர்"</string>

