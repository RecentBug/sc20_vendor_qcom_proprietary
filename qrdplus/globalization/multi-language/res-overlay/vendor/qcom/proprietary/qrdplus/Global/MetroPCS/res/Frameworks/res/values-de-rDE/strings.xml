<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"WLAN-Verbindungen abrufen"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"Ermöglicht der App, Informationen  zu WLANs abzufrufen, zum Beispiel die Namen verbundener WLAN-Geräte oder ob ein WLAN aktiviert ist."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"WLAN-Verbindungen herstellen und trennen"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"Ermöglicht der App, eine Verbindung zu WLAN-Zugriffspunkten herzustellen und solche zu trennen und Änderungen an der Gerätekonfiguration für WLAN-Netzwerke vorzunehmen."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"WLAN-Multicast-Empfang zulassen"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Ermöglicht der App, Pakete zu empfangen, die mithilfe von Multicast-Adressen an sämtliche Geräte in einem WLAN versendet wurden, nicht nur an Ihr Tablet. Dies kostet mehr Leistung als der Nicht-Multicast-Modus."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"Ermöglicht der App, Pakete zu empfangen, die mithilfe von Multicast-Adressen an sämtliche Geräte in einem WLAN versendet wurden, nicht nur an Ihr Telefon. Dies kostet mehr Leistung als der Nicht-Multicast-Modus."</string>

    <add-resource type="string" name="wifi_available_sign_in"/>
    <string name="wifi_available_sign_in">"Bei WLAN-Netzwerk anmelden"</string>
    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"Es konnte keine WLAN-Verbindung hergestellt werden."</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"Wi-Fi Direct"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"Wi-Fi Direct-Betrieb starten. Hierdurch wird der WLAN-Client-/-Hotspot-Betrieb deaktiviert."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Starten von Wi-Fi Direct nicht möglich"</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"Wi-Fi Direct ist aktiviert."</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Das Tablet wird vorübergehend vom WLAN getrennt, während eine Verbindung mit <xliff:g id="DEVICE_NAME">%1$s</xliff:g> besteht."</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Das Telefon wird vorübergehend vom WLAN getrennt, während eine Verbindung mit <xliff:g id="DEVICE_NAME">%1$s</xliff:g> hergestellt wird."</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_title"/>
    <string name="data_usage_wifi_limit_title">"WLAN-Daten sind deaktiviert"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"WLAN-Datenlimit überschritten"</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <add-resource type="plurals" name="wifi_available"/>
    <plurals name="wifi_available">
        <item quantity="one">"WLAN-Netzwerk verfügbar"</item>
        <item quantity="other">"WLAN-Netzwerke verfügbar"</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"Verfügbares WLAN-Netzwerk öffnen"</item>
        <item quantity="other">"Verfügbare WLAN-Netzwerke öffnen"</item>
    </plurals>

    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"Nur Notrufe"</string>
    <add-resource type="string" name="mobile_tether_help_dialog_title"/>
    <string name="mobile_tether_help_dialog_title">"Hilfe zum USB-Tethering"</string>
    <add-resource type="string" name="learn_usb_dialog_text"/>
    <string name="learn_usb_dialog_text">"Sie möchten mehr über USB-Tethering auf Ihrem Telefon erfahren?"</string>
    <add-resource type="string" name="mobile_usb_help_dialog_text"/>
    <string name="mobile_usb_help_dialog_text">"Sie können Ihr Android-Gerät in einen mobilen Hotspot verwandeln, um die Internetverbindung des Android-Geräts für einen oder mehrere Computer oder andere Geräte freizugeben. \n\n1. Während das Gerät als mobiler Hotspot agiert, können Sie mit den Apps Ihres Geräts über die WLAN-Verbindung nicht auf das Internet zugreifen (Sie bleiben aber natürlich über das mobile Datennetz mit dem Internet verbunden). \n\n2. Sie konfigurieren den Hotspot über die Einstellungen für den mobilen Hotspot."</string></resources>
