    <add-resource type="string" name="hotspot_settings_desc"/>
    <string name="hotspot_settings_desc">"Hotspotul mobil poate partaja conexiunea la internet a datelor acestui telefon cu alte PC-uri/dispozitive mobile prin Wi-Fi."</string>
    <add-resource type="string" name="hotspot_settings_indication"/>
    <string name="hotspot_settings_indication">"După activarea hotspotului mobil, alte PC-uri/dispozitive mobile se pot conecta şi pot începe să utilizeze internetul urmând aceşti paşi:"</string>
    <add-resource type="string" name="hotspot_turn_on_hint"/>
