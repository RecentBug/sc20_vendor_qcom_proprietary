    <add-resource type="array" name="switch_set"/>
    <string-array name="switch_set">
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài dưới 1 phút kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> và <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> và <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> và <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> kể từ bây giờ."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> được cài <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>, <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> và <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> kể từ bây giờ."</item>
