    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"Nu se poate conecta la Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"Wi-Fi Direct"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"Porniţi Wi-Fi Direct. Acest lucru va dezactiva clientul/hotspotul Wi-Fi."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"Wi-Fi Direct nu a putut porni."</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"Wi-Fi Direct este activat"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"Tableta se va deconecta temporar de la rețeaua Wi-Fi cât timp este conectată la <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"Telefonul se va deconecta temporar de la reţeaua Wi-Fi cât timp este conectat la <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
