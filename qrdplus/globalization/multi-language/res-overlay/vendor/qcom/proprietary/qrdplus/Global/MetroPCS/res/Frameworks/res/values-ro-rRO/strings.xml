<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"vizualizează conexiunile Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"Permite aplicaţiei să vadă informaţiile despre reţelele Wi-Fi, de ex. dacă o reţea Wi-Fi este activată, precum şi numele dispozitivelor conectate la reţeaua Wi-Fi."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"se conectează şi se deconectează de la Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"Permite aplicaţiei să se conecteze şi să se deconecteze de la punctele de acces Wi-Fi, precum şi să efectueze modificări în configuraţia dispozitivului pentru reţelele Wi-Fi."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"permitere recepţionare difuzare multiplă Wi-Fi"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"Permite aplicaţiei să primească pachetele trimise către toate dispozitivele dintr-o reţea Wi-Fi, utilizând adrese cu difuzare multiplă, nu doar tableta dvs. Această funcţie utilizează mai multă energie decât modul fără difuzare multiplă."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"Permite aplicaţiei să primească pachetele trimise către toate dispozitivele dintr-o reţea Wi-Fi, utilizând adrese cu difuzare multiplă, nu doar telefonul dvs. Această funcţie utilizează mai multă energie decât modul fără difuzare multiplă."</string>

