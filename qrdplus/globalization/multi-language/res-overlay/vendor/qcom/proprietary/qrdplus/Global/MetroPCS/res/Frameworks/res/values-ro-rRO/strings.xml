    <plurals name="wifi_available">
        <item quantity="one">"Reţea Wi-Fi disponibilă"</item>
        <item quantity="other">"Reţele Wi-Fi disponibile"</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"Reţea Wi-Fi deschisă disponibilă"</item>
        <item quantity="other">"Reţele Wi-Fi deschise disponibile"</item>
    </plurals>

    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"Numai apeluri de urgenţă"</string>
    <add-resource type="string" name="mobile_tether_help_dialog_title"/>
    <string name="mobile_tether_help_dialog_title">"Ajutor pentru tethering prin USB"</string>
    <add-resource type="string" name="learn_usb_dialog_text"/>
    <string name="learn_usb_dialog_text">"Doriți să știți mai multe despre Tetheringul prin USB de pe telefonul dvs.?"</string>
    <add-resource type="string" name="mobile_usb_help_dialog_text"/>
    <string name="mobile_usb_help_dialog_text">"Puteţi transforma dispozitivul dvs. Android într-un Hotspot mobil pentru a partaja conexiunea la internet a dispozitivului Android cu unul sau mai multe computere sau alte dispozitive. \n\n1.Când dispozitivul are rolul de Hotspot mobil, nu puteţi utiliza aplicaţiile dispozitivului pentru a accesa internetul prin conexiunea Wi-Fi (dar rămâneţi conectat la internet prin reţeaua de date mobile). \n\n2.Configuraţi hotspotul cu setările pentru Hotspotul mobil"</string></resources>
