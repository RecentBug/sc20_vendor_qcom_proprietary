<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Overlay WLAN to Wi-Fi -->
    <add-resource type="string" name="permlab_accessWifiState"/>
    <string name="permlab_accessWifiState">"‏عرض اتصالات Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_accessWifiState"/>
    <string name="permdesc_accessWifiState">"‏للسماح للتطبيق بعرض معلومات حول شبكات Wi-Fi، كعرض معلومات حول ما إذا تم تمكين Wi-Fi واسم أجهزة Wi-Fi المتصلة."</string>

    <add-resource type="string" name="permlab_changeWifiState"/>
    <string name="permlab_changeWifiState">"‏التوصيل والفصل من Wi-Fi"</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiState"/>
    <string name="permdesc_changeWifiState">"‏للسماح للتطبيق بالاتصال بنقاط الوصول إلى Wi-Fi وقطع الاتصال بها، وإجراء تغييرات على تهيئة الجهاز لشبكات Wi-Fi."</string>
    <add-resource type="string" name="permlab_changeWifiMulticastState"/>
    <string name="permlab_changeWifiMulticastState">"‏السماح باستقبال بث Wi-Fi متعدد"</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="tablet">"‏للسماح للتطبيق بتلقي الحزم التي يتم إرسالها إلى جميع الأجهزة على شبكة Wi-Fi باستخدام عناوين بث متعدد، وليس باستخدام جهازك اللوحي فقط. ويؤدي ذلك إلى استخدام قدر أكبر من الطاقة يفوق وضع البث غير المتعدد."</string>
    <add-resource type="string" name="permdesc_changeWifiMulticastState"/>
    <string name="permdesc_changeWifiMulticastState" product="default">"‏للسماح للتطبيق بتلقي الحزم التي يتم إرسالها إلى جميع الأجهزة على شبكة Wi-Fi باستخدام عناوين بث متعدد، وليس باستخدام هاتفك فقط. ويؤدي ذلك إلى استخدام قدر أكبر من الطاقة يفوق وضع البث غير المتعدد."</string>

    <add-resource type="string" name="wifi_available_sign_in"/>
    <string name="wifi_available_sign_in">"تسجيل الدخول إلى شبكة Wi-Fi"</string>
    <add-resource type="string" name="wifi_watchdog_network_disabled"/>
    <string name="wifi_watchdog_network_disabled">"‏تعذر الاتصال بـ Wi-Fi"</string>
    <add-resource type="string" name="wifi_p2p_dialog_title"/>
    <string name="wifi_p2p_dialog_title">"‏اتصال Wi-Fi مباشر"</string>
    <add-resource type="string" name="wifi_p2p_turnon_message"/>
    <string name="wifi_p2p_turnon_message">"‏ابدأ Wi-Fi Direct. يؤدي هذا إلى إيقاف عميل/نقطة اتصال Wi-Fi."</string>
    <add-resource type="string" name="wifi_p2p_failed_message"/>
    <string name="wifi_p2p_failed_message">"‏تعذر بدء Wi-Fi Direct."</string>
    <add-resource type="string" name="wifi_p2p_enabled_notification_title"/>
    <string name="wifi_p2p_enabled_notification_title">"‏تم تشغيل اتصال Wi-Fi المباشر"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">"‏سيتم قطع اتصال الجهاز اللوحي مؤقتًا بشبكة Wi-Fi في الوقت الذي يكون فيه متصلاً بـ <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <add-resource type="string" name="wifi_p2p_frequency_conflict_message"/>
    <string name="wifi_p2p_frequency_conflict_message" product="default">"‏سيتم قطع اتصال الهاتف مؤقتًا بشبكة Wi-Fi في الوقت الذي يكون فيه متصلاً بـ <xliff:g id="DEVICE_NAME">%1$s</xliff:g>"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_title"/>
    <string name="data_usage_wifi_limit_title">"بيانات Wi-Fi قيد الإيقاف"</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <add-resource type="string" name="data_usage_wifi_limit_snoozed_title"/>
    <string name="data_usage_wifi_limit_snoozed_title">"‏تم تجاوز حد بيانات شبكة Wi-Fi"</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <add-resource type="plurals" name="wifi_available"/>
    <plurals name="wifi_available">
        <item quantity="one">"‏شبكة Wi-Fi متاحة"</item>
        <item quantity="other">"‏شبكات Wi-Fi متاحة"</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <add-resource type="plurals" name="wifi_available_detailed"/>
    <plurals name="wifi_available_detailed">
        <item quantity="one">"‏هناك شبكة Wi-Fi مفتوحة متاحة"</item>
        <item quantity="other">"‏هناك شبكات Wi-Fi مفتوحة متاحة"</item>
    </plurals>

    <add-resource type="string" name="lockscreen_emergency_call"/>
    <string name="lockscreen_emergency_call">"مكالمات طوارئ فقط"</string>
    <add-resource type="string" name="mobile_tether_help_dialog_title"/>
    <string name="mobile_tether_help_dialog_title">"تعليمات ربط USB"</string>
    <add-resource type="string" name="learn_usb_dialog_text"/>
    <string name="learn_usb_dialog_text">"هل تريد معرفة المزيد عن ربط USB على الهاتف؟"</string>
    <add-resource type="string" name="mobile_usb_help_dialog_text"/>
    <string name="mobile_usb_help_dialog_text">"يمكنك تحويل جهاز Android الخاص بك إلى نقطة اتصال محمول، لمشاركة اتصال الإنترنت الخاص بجهاز Android الخاص بك مع جهاز كمبيوتر واحد أو أكثر أو مع أجهزة أخرى. \n\n1. عندما يعمل جهازك كنقطة اتصال محمول، فلا يمكنك استخدام التطبيقات الموجودة عليه للوصول إلى الإنترنت عبر اتصال Wi-Fi الخاص به (لكن ستظل بالطبع متصلاً بالإنترنت عبر شبكة بيانات المحمول الخاصة بك). \n\n2. يمكنك تكوين نقطة الاتصال باستخدام إعدادات نقطة اتصال المحمول"</string></resources>
