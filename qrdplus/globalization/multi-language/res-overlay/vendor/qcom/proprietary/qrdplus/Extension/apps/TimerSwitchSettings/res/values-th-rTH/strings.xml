    <add-resource type="array" name="switch_set"/>
    <string-array name="switch_set">
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็นน้อยกว่า 1 นาทีนับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> และ <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> และ <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> and <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> นับแต่นี้ไป"</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> ตั้งเป็น <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>, <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> และ <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> นับแต่นี้ไป"</item>
    </string-array>

