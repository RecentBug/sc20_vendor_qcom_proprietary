    <add-resource type="string" name="wifiap_settings_desc"/>
    <string name="wifiap_settings_desc">"WLAN wifiap 可透過 WLAN 與其他電腦/流動裝置共享此手機的數據互聯網連線。 "</string>
    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"開啟 WLAN wifiap 後，其他電腦/流動裝置可透過下列步驟進行連線並開始使用互聯網:"</string>
    <add-resource type="string" name="wifiap_settings_indication_step1"/>
