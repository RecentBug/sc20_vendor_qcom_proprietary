<resources>

    <!-- Match this with drawable.wifi_signal. -->
    <!-- Wi-Fi settings. The signal strength a Wi-Fi network has. -->
    <add-resource type="array" name="wifi_signal"/>
    <string-array name="wifi_signal">
        <item>"ਖ਼ਰਾਬ"</item>
        <item>"ਉਚਿਤ"</item>
        <item>"ਵਧੀਆ"</item>
        <item>"ਸ਼ਾਨਦਾਰ"</item>
    </string-array>

    <!-- Phase 2 options for PEAP -->
    <add-resource type="array" name="wifi_peap_phase2_entries"/>
    <string-array name="wifi_peap_phase2_entries">
        <item>"ਕੋਈ ਨਹੀਂ"</item>
        <item>MSCHAPV2</item>
        <item>GTC</item>
    </string-array>

    <!-- Phase 2 options for rest of EAP methods -->
    <add-resource type="array" name="wifi_phase2_entries"/>
    <string-array name="wifi_phase2_entries">
        <item>"ਕੋਈ ਨਹੀਂ"</item>
        <item>PAP</item>
        <item>MSCHAP</item>
        <item>MSCHAPV2</item>
        <item>GTC</item>
    </string-array>

    <!-- Wi-Fi IP settings. -->
    <!-- Note that adding/removing/moving the items will need wifi settings code change. -->
    <add-resource type="array" name="wifi_ip_settings"/>
    <string-array name="wifi_ip_settings">
        <!-- Use DHCP (Dynamic Host Configuration Protocol) for obtaining IP settings [CHAR LIMIT=25] -->
        <item>DHCP</item>
        <!-- Use statically defined IP settings [CHAR LIMIT=25]-->
        <item>"ਸਥਿਰ"</item>
    </string-array>

    <!-- Wi-Fi proxy settings. -->
    <!-- Note that adding/removing/moving the items will need wifi settings code change. -->
    <add-resource type="array" name="wifi_proxy_settings"/>
    <string-array name="wifi_proxy_settings">
        <!-- No HTTP proxy is used for the current wifi network [CHAR LIMIT=25] -->
        <item>"ਕੋਈ ਨਹੀਂ"</item>
        <!-- Manual HTTP proxy settings are used for the current wifi network [CHAR LIMIT=25] -->
        <item>"ਮੈਨੁਅਲ"</item>
        <!-- Proxy Auto-Config URL that is used for the current wifi network [CHAR LIMIT=25] -->
        <item>"ਪ੍ਰੌਕਸੀ ਆਟੋ-ਕੌਂਫਿਗਰ"</item>
    </string-array>

</resources>
