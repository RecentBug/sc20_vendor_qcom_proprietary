    <string-array name="switch_set">
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk kurang dari 1 menit dari sekarang."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> dari sekarang."</item>
