        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> dari sekarang."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> dan <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> dari sekarang."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> dan <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> dari sekarang."</item>
        <item>"<xliff:g id="SWITCH">%1$s</xliff:g> atur untuk <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>, <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g>, dan <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> dari sekarang."</item>
