    <add-resource type="string" name="wifiap_settings_indication"/>
    <string name="wifiap_settings_indication">"Baada ya wifiap ya WLAN kuwashwa, kompyuta/vifaa vingine vya mkononi vinaweza kuunganisha kwenye kifaa hiki na kuanza kutumia intaneti kwa kufuata hatua zifuatazo:"</string>
