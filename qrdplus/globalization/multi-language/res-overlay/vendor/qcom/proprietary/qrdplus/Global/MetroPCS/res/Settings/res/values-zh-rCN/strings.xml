    <add-resource type="string" name="hotspot_settings_desc"/>
    <string name="hotspot_settings_desc">"移动热点可以通过 WLAN 与其他电脑/移动设备分享此手机的移动互联网连接。"</string>
    <add-resource type="string" name="hotspot_settings_indication"/>
    <string name="hotspot_settings_indication">"打开移动热点后，其他电脑/移动设备可以通过以下步骤进行连接并使用互联网："</string>
    <add-resource type="string" name="hotspot_turn_on_hint"/>
