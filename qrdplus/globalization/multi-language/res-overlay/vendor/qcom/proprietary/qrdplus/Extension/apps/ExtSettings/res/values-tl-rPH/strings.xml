    <add-resource type="string" name="question_what_is_wifi_calling"/>
    <string name="question_what_is_wifi_calling">" Ano ang Pagtawag Gamit ang Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_what_is_wifi_calling"/>
    <string name="answer_question_what_is_wifi_calling">"Pinahihintulutan ka ng Pagtawag Gamit ang Wi-Fi na tumawag at tumanggap ng mga voice call at text/mga mensaheng multimedia sa Wi-Fi kapag nakakonekta ang iyong telepono sa isang Wi-Fi network at nakarehistro sa MetroPCS network para sa Pagtawag Gamit ang Wi-Fi."</string>
    <add-resource type="string" name="question_how_enable_wifi"/>
    <string name="question_how_enable_wifi">"Paano ko i-enable ang Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_how_enable_wifi"/>
    <string name="answer_question_how_enable_wifi">"Upang i-enable ang Wi-Fi, gamitin ang seksyon ng wireless control sa iyong Android settings menu."</string>
    <add-resource type="string" name="question_how_enable_wifi_calling"/>
    <string name="question_how_enable_wifi_calling">"Paano ko i-enable ang Pagtawag Gamit ang Wi-Fi?"</string>
    <add-resource type="string" name="answer_question_how_enable_wifi_calling"/>
    <string name="answer_question_how_enable_wifi_calling">"Ayon sa default, naka-enable ang iyong telepono para sa Pagtawag Gamit ang Wi-Fi sa sandaling i-enable mo ang Wi-Fi radio. Ang Pagtawag Gamit ang Wi-Fi ay maaaring makontrol mula sa mga Setting ng Pagtawag Gamit ang Wi-Fi."</string>
    <add-resource type="string" name="question_if_use_Wifi"/>
    <string name="question_if_use_Wifi">"Paano ko malalaman kung gumagamit ako ng Wi-Fi para sa aking mga tawag?"</string>
    <add-resource type="string" name="answer_question_if_use_Wifi"/>
    <string name="answer_question_if_use_Wifi">"Kapag nakaugnay ang iyong telepono sa isang Wi-Fi network,awtomatiko nitong susubukang magrehistro sa MetroPCS network para sa Pagtawag Gamit ang Wi-Fi. Sa sandaling tagumpay na makumpleto ang pagrerehistro makikita mo ang icon ng Pagtawag Gamit ang Wi-Fi sa Status Bar.Makikita mo rin ang icon ng Pagtawag Gamit ang Wi-Fi na nagpapakita sa dialer(sa halip na ang iyong regular na icon ng pagtawag) at saglit mong makikita ang isang mensahe sa taas ng screen na nagsasabing,Gagawin ang mga tawag sa Wi-Fi."</string>
    <add-resource type="string" name="question_charge_diff_about_wifi_call"/>
    <string name="question_charge_diff_about_wifi_call">"Sisingilin ba ang mga tawag o SMS/MMS gamit ang Wi-Fi nang iba sa aking mga tawag sa cellular?"</string>
    <add-resource type="string" name="answer_question_charge_diff_about_wifi_call"/>
    <string name="answer_question_charge_diff_about_wifi_call">"Ang pangunahing benepisyo ng Pagtawag Gamit ang Wi-Fi ay upang palawakin ang iyong coverage sa mga lokasyon na may Wi-Fi.Ang mga tawag at mensahe gamit ang Wi-Fi ay sisingilin tulad ng mga tawag sa cellular para sa mga partikular na plano."</string>
    <add-resource type="string" name="question_what_happen_without_wifi_coverage"/>
    <string name="question_what_happen_without_wifi_coverage">"Ano ang mangyayari kung mawalan ako ng Wi-Fi coverage?"</string>
    <add-resource type="string" name="answer_question_what_happen_without_wifi_coverage"/>
    <string name="answer_question_what_happen_without_wifi_coverage">"Kapag nawala ng handset ang Wi-Fi coverage,anumang kasalukuyang mga tawag ay mapuputol at magkokonekta ang telepono sa isang cellular network kung maaari. Makakatanggap ka ng prompt kung maputol ang iyong tawag dahil sa pagkawala ng Wi-Fi coverage."</string>
    <add-resource type="string" name="wifi_calling_wizard_learn_more"/>
    <string name="wifi_calling_wizard_learn_more">"Matuto nang higit pa"</string>
    <add-resource type="string" name="wifi_calling_help_wizard_title"/>
    <string name="wifi_calling_help_wizard_title">"Alamin ang lahat tungkol sa Pagtawag Gamit ang Wi-Fi"</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_1"/>
    <string name="wifi_calling_wizard_content_step_1">"Ito ay tutorial sa pagtawag gamit ang wi-fi. \n \"Laktawan ang\" ay lalaktawan ang wizard sa panahong ito. \n \"Alamin ang higit pa\" ay magpapakita ng higit pang impormasyon tungkol dito"</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_2"/>
    <string name="wifi_calling_wizard_content_step_2">"Pinahihintulutan ka ng pagtawag gamit ang Wi-Fi na magpaalam sa nawawala-walang coverage. Sa pagtawag gamit ang Wi-Fi maaari kang tumawag at tumanggap ng mga tawag at mensahe sa Wi-Fi network gamit ang iyong kasalukuyang T-Mobile plan.  \n  Walang kailangang espesyal na setup para sa pagtawag gamit ang Wi-Fi.  Mula sa Settings menu,pumunta sa Wireless at Mga Network at kumonekta sa isang Wi-Fi network upang magsimula."</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_3"/>
    <string name="wifi_calling_wizard_content_step_3">"Sa sandaling nakikita mo na ang icon ng Pagtawag Gamit ang Wi-Fi sa status bar, handa na ang iyong telepono para sa Pagtawag Gamit ang Wi-Fi. "</string>
    <add-resource type="string" name="wifi_calling_wizard_content_step_4"/>
    <string name="wifi_calling_wizard_content_step_4">"Upang manatiling nasa isang Tawag sa Wi-Fi, manatili sa loob ng range ng Wi-Fi network. Makakarinig ka ng beep at makakakuha ka ng mensahe sa screen na nagpapaalam sa iyo kung naputol ang isang tawag dahil lumabas ka sa range."</string>
