    <!-- String referring to the device being connected when there is no name for the device -->
    <add-resource type="string" name="device"/>
    <string name="device">"perangkat"</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connecting_peripheral"/>
    <string name="connecting_peripheral">"Menghubungkan <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connected_peripheral"/>
    <string name="connected_peripheral">"Menghubungkan <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connect_peripheral_failed"/>
    <string name="connect_peripheral_failed">"Tidak dapat menghubungkan <xliff:g id="device_name">%1$s</xliff:g>"</string>
