    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_pay"/>
    <string name="tap_again_to_pay">"Klepnutím zaplatíte pomocou aplikácie <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_complete"/>
    <string name="tap_again_to_complete">"Klepnutím akciu dokončíte pomocou aplikácie <xliff:g id="APP">%1$s</xliff:g>"</string>
