    <add-resource type="string" name="transaction_failure"/>
    <string name="transaction_failure">"Túto transakciu nie je možné dokončiť pomocou aplikácie <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn't be completed with the
         indicated app -->
    <add-resource type="string" name="could_not_use_app"/>
    <string name="could_not_use_app">"Aplikáciu <xliff:g id="APP">%1$s</xliff:g> nie je možné použiť."</string>
