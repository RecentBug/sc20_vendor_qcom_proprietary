    <string name="open_external_dialog_request">"<xliff:g id="STORAGE"><i>^3</i></xliff:g> ರಲ್ಲಿ <xliff:g id="DIRECTORY"><i>^2</i></xliff:g> ಡೈರೆಕ್ಟರಿಗೆ <xliff:g id="APPNAME"><b>^1</b></xliff:g> ಪ್ರವೇಶ ನೀಡುವುದೇ?"</string>
    <!-- Text in an alert dialog asking user to grant app access to a given directory in the internal storage -->
    <add-resource type="string" name="open_external_dialog_request_primary_volume"/>
    <string name="open_external_dialog_request_primary_volume">"<xliff:g id="APPNAME"><b>^1</b></xliff:g><xliff:g id="DIRECTORY"><i>^2</i></xliff:g> ಡೈರೆಕ್ಟರಿ ಪ್ರವೇಶಿಸಲು ಅನುಮತಿಸುವುದೇ?"</string>
    <!-- Text in an alert dialog asking user to grant app access to all data in an external storage volume -->
    <add-resource type="string" name="open_external_dialog_root_request"/>
    <string name="open_external_dialog_root_request">"<xliff:g id="STORAGE"><i>^2</i></xliff:g> ಸಂಗ್ರಹಣೆಯಲ್ಲಿನ ಪೋಟೋಗಳು ಮತ್ತು ವೀಡಿಯೊಗಳು ಸೇರಿದಂತೆ ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಪ್ರವೇಶಿಸಲು <xliff:g id="APPNAME"><b>^1</b></xliff:g> ಅಪ್ಲಿಕೇಶನ್‌ಗೆ ಅನುಮತಿಸುವುದೇ?"</string>
    <!-- Checkbox that allows user to not be questioned about the directory access request again -->
    <add-resource type="string" name="never_ask_again"/>
    <string name="never_ask_again">"ಮತ್ತೆ ಕೇಳಬೇಡಿ"</string>
    <!-- Text in the button asking user to allow access to a given directory. -->
    <add-resource type="string" name="allow"/>
    <string name="allow">"ಅನುಮತಿಸು"</string>
    <!-- Text in the button asking user to deny access to a given directory. -->
    <add-resource type="string" name="deny"/>
    <string name="deny">"ನಿರಾಕರಿಸು"</string>
