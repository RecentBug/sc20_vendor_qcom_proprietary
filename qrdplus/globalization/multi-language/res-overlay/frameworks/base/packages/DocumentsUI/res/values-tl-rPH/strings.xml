    <add-resource type="string" name="notification_copy_files_converted_title"/>
    <string name="notification_copy_files_converted_title">"Na-convert ang ilang file"</string>

    <!-- Text in an alert dialog asking user to grant app access to a given directory in an external storage volume -->
    <add-resource type="string" name="open_external_dialog_request"/>
    <string name="open_external_dialog_request">"Bigyan ang <xliff:g id="APPNAME"><b>^1</b></xliff:g> ng access sa directory ng <xliff:g id="DIRECTORY"><i>^2</i></xliff:g> sa <xliff:g id="STORAGE"><i>^3</i></xliff:g>?"</string>
    <!-- Text in an alert dialog asking user to grant app access to a given directory in the internal storage -->
    <add-resource type="string" name="open_external_dialog_request_primary_volume"/>
    <string name="open_external_dialog_request_primary_volume">"Bibigyan ang <xliff:g id="APPNAME"><b>^1</b></xliff:g> ng access sa direktoryong <xliff:g id="DIRECTORY"><i>^2</i></xliff:g>?"</string>
    <!-- Text in an alert dialog asking user to grant app access to all data in an external storage volume -->
    <add-resource type="string" name="open_external_dialog_root_request"/>
    <string name="open_external_dialog_root_request">"Bigyan ang <xliff:g id="APPNAME"><b>^1</b></xliff:g> ng access sa iyong data, kabilang ang mga larawan at video, sa <xliff:g id="STORAGE"><i>^2</i></xliff:g>?"</string>
    <!-- Checkbox that allows user to not be questioned about the directory access request again -->
    <add-resource type="string" name="never_ask_again"/>
    <string name="never_ask_again">"Huwag nang tatanunging muli"</string>
    <!-- Text in the button asking user to allow access to a given directory. -->
    <add-resource type="string" name="allow"/>
    <string name="allow">"Payagan"</string>
    <!-- Text in the button asking user to deny access to a given directory. -->
    <add-resource type="string" name="deny"/>
    <string name="deny">"Tanggihan"</string>
    <!-- Dialog text shown to users when asking if they want to delete files (a confirmation). -->
    <!-- Label text showing user how many items are selected. Can be one or more elements. -->
    <add-resource type="plurals" name="elements_selected"/>
    <plurals name="elements_selected">
        <item quantity="one">"<xliff:g id="COUNT_1">%1$d</xliff:g> ang napili"</item>
        <item quantity="other">"<xliff:g id="COUNT_1">%1$d</xliff:g> ang napili"</item>
    </plurals>

    <!-- Label text showing user how many items are being dragged. Can be one or more elements. -->
    <add-resource type="plurals" name="elements_dragged"/>
    <plurals name="elements_dragged">
        <item quantity="one">"<xliff:g id="COUNT_1">%1$d</xliff:g> item"</item>
        <item quantity="other">"<xliff:g id="COUNT_1">%1$d</xliff:g> na item"</item>
    </plurals>

    <!-- Dialog text shown to users when asking if they want to delete a file (a confirmation) -->
    <add-resource type="string" name="delete_filename_confirmation_message"/>
    <string name="delete_filename_confirmation_message">"Gusto mo bang i-delete ang \"<xliff:g id="NAME">%1$s</xliff:g>?\""</string>
    <!-- Dialog text shown to users when asking if they want to delete a folder (a confirmation) -->
    <add-resource type="string" name="delete_foldername_confirmation_message"/>
    <string name="delete_foldername_confirmation_message">"Gusto mo bang i-delete ang folder na \"<xliff:g id="NAME">%1$s</xliff:g>\" at ang mga content nito?"</string>
    <!-- Dialog text shown to users when asking if they want to delete files (a confirmation). -->
    <add-resource type="plurals" name="delete_files_confirmation_message"/>
    <plurals name="delete_files_confirmation_message">
        <item quantity="one">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> file?"</item>
        <item quantity="other">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> (na) file?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete folders (a confirmation). -->
    <add-resource type="plurals" name="delete_folders_confirmation_message"/>
    <plurals name="delete_folders_confirmation_message">
        <item quantity="one">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> folder at mga content ng mga ito?"</item>
        <item quantity="other">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> na folder at mga content ng mga ito?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete mixed type items: files and folders (a confirmation). -->
    <add-resource type="plurals" name="delete_items_confirmation_message"/>
    <plurals name="delete_items_confirmation_message">
        <item quantity="one">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> item?"</item>
        <item quantity="other">"Gusto mo bang i-delete ang <xliff:g id="COUNT_1">%1$d</xliff:g> na item?"</item>
    </plurals>
    <!-- Snackbar shown to users who wanted to select more than 1000 items (files or directories). -->
    <add-resource type="string" name="too_many_selected"/>
    <string name="too_many_selected">"Paumanhin, maaari ka lang pumili ng hanggang 1000 item sa isang pagkakataon"</string>
    <!-- Snackbar shown to users who wanted to select all, but there were too many items (files or directories).
         Only the first 1000 items are selected in such case. -->
    <add-resource type="string" name="too_many_in_select_all"/>
    <string name="too_many_in_select_all">"Maaari lang pumili ng 1000 item"</string></resources>
