    <add-resource type="string" name="delete_failure_alert_content"/>
    <string name="delete_failure_alert_content">"Hindi na-delete ang mga file na ito: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Contents of the copying warning dialog due to converted files. [CHAR LIMIT=64] -->
    <add-resource type="string" name="copy_converted_warning_content"/>
    <string name="copy_converted_warning_content">"Na-convert ang mga file na ito sa ibang format: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Toast shown when a user copies files to clipboard. -->
    <add-resource type="plurals" name="clipboard_files_clipped"/>
    <plurals name="clipboard_files_clipped">
        <item quantity="one">"Nakopya ang <xliff:g id="COUNT_1">%1$d</xliff:g> file sa clipboard."</item>
        <item quantity="other">"Nakopya ang <xliff:g id="COUNT_1">%1$d</xliff:g> na file sa clipboard."</item>
    </plurals>
