    <add-resource type="string" name="clipboard_files_cannot_paste"/>
    <string name="clipboard_files_cannot_paste">"선택한 파일을 이 위치에 붙여넣을 수 없습니다."</string>
    <!-- Menu item that renames the selected document [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_rename"/>
    <string name="menu_rename">"이름 바꾸기"</string>
