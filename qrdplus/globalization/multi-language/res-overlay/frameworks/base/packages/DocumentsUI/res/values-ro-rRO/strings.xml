    <add-resource type="string" name="menu_select_all"/>
    <string name="menu_select_all">"Selectați tot"</string>
    <!-- Menu item title that copies the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_copy"/>
    <string name="menu_copy">"Copiați în\u2026"</string>
    <!-- Menu item title that moves the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_move"/>
    <string name="menu_move">"Mutați în\u2026"</string>

    <!-- Menu item title that creates a new window in the activity [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_new_window"/>
