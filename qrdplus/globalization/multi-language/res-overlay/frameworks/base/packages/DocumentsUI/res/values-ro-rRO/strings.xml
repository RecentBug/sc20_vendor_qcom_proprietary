    <add-resource type="string" name="delete_filename_confirmation_message"/>
    <string name="delete_filename_confirmation_message">"Ștergeți „<xliff:g id="NAME">%1$s</xliff:g>”?"</string>
    <!-- Dialog text shown to users when asking if they want to delete a folder (a confirmation) -->
    <add-resource type="string" name="delete_foldername_confirmation_message"/>
    <string name="delete_foldername_confirmation_message">"Ștergeți dosarul „<xliff:g id="NAME">%1$s</xliff:g>” și conținutul acestuia?"</string>
    <!-- Dialog text shown to users when asking if they want to delete files (a confirmation). -->
    <add-resource type="plurals" name="delete_files_confirmation_message"/>
    <plurals name="delete_files_confirmation_message">
        <item quantity="one">"Ștergeți <xliff:g id="COUNT_0">%1$d</xliff:g> fișier?"</item>
        <item quantity="other">"Ștergeți <xliff:g id="COUNT_1">%1$d</xliff:g> de fișiere?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete folders (a confirmation). -->
    <add-resource type="plurals" name="delete_folders_confirmation_message"/>
    <plurals name="delete_folders_confirmation_message">
        <item quantity="one">"Ștergeți <xliff:g id="COUNT_0">%1$d</xliff:g> dosar și conținutul acestuia?"</item>
        <item quantity="other">"Ștergeți <xliff:g id="COUNT_1">%1$d</xliff:g> de dosare și conținutul acestora?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete mixed type items: files and folders (a confirmation). -->
    <add-resource type="plurals" name="delete_items_confirmation_message"/>
    <plurals name="delete_items_confirmation_message">
        <item quantity="one">"Ștergeți <xliff:g id="COUNT_0">%1$d</xliff:g> element?"</item>
        <item quantity="other">"Ștergeți <xliff:g id="COUNT_1">%1$d</xliff:g> de elemente?"</item>
    </plurals>
    <!-- Snackbar shown to users who wanted to select more than 1000 items (files or directories). -->
    <add-resource type="string" name="too_many_selected"/>
    <string name="too_many_selected">"Ne pare rău, puteți selecta cel mult 1000 de elemente odată"</string>
    <!-- Snackbar shown to users who wanted to select all, but there were too many items (files or directories).
         Only the first 1000 items are selected in such case. -->
    <add-resource type="string" name="too_many_in_select_all"/>
    <string name="too_many_in_select_all">"S-au putut selecta numai 1000 de elemente"</string></resources>
