    <add-resource type="string" name="delete_failure_alert_content"/>
    <string name="delete_failure_alert_content">"Aceste fișiere nu au fost șterse: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Contents of the copying warning dialog due to converted files. [CHAR LIMIT=64] -->
    <add-resource type="string" name="copy_converted_warning_content"/>
    <string name="copy_converted_warning_content">"Aceste fișiere au fost convertite în alt format: <xliff:g id="LIST">%1$s</xliff:g>"</string>
