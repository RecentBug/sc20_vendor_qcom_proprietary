<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the documents application [CHAR LIMIT=32] -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"File"</string>

    <!-- Title of the standalone downloads activity. [CHAR LIMIT=32] -->
    <add-resource type="string" name="downloads_label"/>
    <string name="downloads_label">"Download"</string>

    <!-- Action bar title prompting user to choose a location to open a document from [CHAR LIMIT=32] -->
    <add-resource type="string" name="title_open"/>
    <string name="title_open">"Apri da"</string>
    <!-- Action bar title prompting user to choose a location to save a document to [CHAR LIMIT=32] -->
    <add-resource type="string" name="title_save"/>
    <string name="title_save">"Salva in"</string>

    <!-- Menu item that creates a new directory/folder at the current location [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_create_dir"/>
    <string name="menu_create_dir">"Nuova cartella"</string>
    <!-- Menu item that switches view to show documents as a large-format grid of thumbnails [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_grid"/>
    <string name="menu_grid">"Visualizzazione griglia"</string>
    <!-- Menu item that switches view to show documents as a list [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_list"/>
    <string name="menu_list">"Visualizzazione elenco"</string>
    <!-- Menu item that switches the criteria with which documents are sorted [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_sort"/>
    <string name="menu_sort">"Ordina per"</string>
    <!-- Menu item that enters a mode to search for documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_search"/>
    <string name="menu_search">"Cerca"</string>
    <!-- Menu item that enters activity to change settings for current root [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_settings"/>
    <string name="menu_settings">"Impostazioni memoria"</string>

    <!-- Menu item title that opens the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_open"/>
    <string name="menu_open">"Apri"</string>
    <!-- Menu item title that saves the current document [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_save"/>
    <string name="menu_save">"Salva"</string>
    <!-- Menu item title that shares the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_share"/>
    <string name="menu_share">"Condividi"</string>
    <!-- Menu item title that deletes the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_delete"/>
    <string name="menu_delete">"Elimina"</string>
    <!-- Menu item title that selects all documents in the current directory [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_select_all"/>
    <string name="menu_select_all">"Seleziona tutto"</string>
    <!-- Menu item title that copies the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_copy"/>
    <string name="menu_copy">"Copia in\u2026"</string>
    <!-- Menu item title that moves the selected documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_move"/>
    <string name="menu_move">"Sposta in..."</string>

    <!-- Menu item title that creates a new window in the activity [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_new_window"/>
    <string name="menu_new_window">"Nuova finestra"</string>
    <!-- Menu item title that copies the selected documents to clipboard [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_copy_to_clipboard"/>
    <string name="menu_copy_to_clipboard">"Copia"</string>
    <!-- Menu item title that pastes files from the clipboard [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_paste_from_clipboard"/>
    <string name="menu_paste_from_clipboard">"Incolla"</string>

    <!-- Menu item that reveals internal storage built into the device [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_advanced_show"/>
    <string name="menu_advanced_show">"Mostra memoria interna"</string>
    <!-- Menu item that hides internal storage built into the device [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_advanced_hide"/>
    <string name="menu_advanced_hide">"Nascondi memoria interna"</string>

    <!-- Menu item that reveals the sizes of displayed files [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_file_size_show"/>
    <string name="menu_file_size_show">"Mostra dimensioni file"</string>
    <!-- Menu item that hides the sizes of displayed files [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_file_size_hide"/>
    <string name="menu_file_size_hide">"Nascondi dimensioni file"</string>

    <!-- Button label that select the current directory [CHAR LIMIT=24] -->
    <add-resource type="string" name="button_select"/>
    <string name="button_select">"Seleziona"</string>
    <!-- Button label that copies files to the current directory [CHAR LIMIT=24] -->
    <add-resource type="string" name="button_copy"/>
    <string name="button_copy">"Copia"</string>
    <!-- Button label that moves files to the current directory [CHAR LIMIT=24] -->
    <add-resource type="string" name="button_move"/>
    <string name="button_move">"Sposta"</string>
    <!-- Button label that hides the error bar [CHAR LIMIT=24] -->
    <add-resource type="string" name="button_dismiss"/>
    <string name="button_dismiss">"Ignora"</string>
    <add-resource type="string" name="button_retry"/>
    <string name="button_retry">"Riprova"</string>

    <!-- Mode that sorts documents by their display name alphabetically [CHAR LIMIT=24] -->
    <add-resource type="string" name="sort_name"/>
    <string name="sort_name">"Per nome"</string>
    <!-- Mode that sorts documents by their last modified time in descending order; most recent first [CHAR LIMIT=24] -->
    <add-resource type="string" name="sort_date"/>
    <string name="sort_date">"Per data di modifica"</string>
    <!-- Mode that sorts documents by their file size in descending order; largest first [CHAR LIMIT=24] -->
    <add-resource type="string" name="sort_size"/>
    <string name="sort_size">"Per dimensioni"</string>

    <!-- Accessibility title to open the drawer showing all roots where documents can be stored [CHAR LIMIT=32] -->
    <add-resource type="string" name="drawer_open"/>
    <string name="drawer_open">"Mostra nodi principali"</string>
    <!-- Accessibility title to close the drawer showing all roots where documents can be stored [CHAR LIMIT=32] -->
    <add-resource type="string" name="drawer_close"/>
    <string name="drawer_close">"Nascondi nodi principali"</string>

    <!-- Toast shown when saving a document failed with an error [CHAR LIMIT=48] -->
    <add-resource type="string" name="save_error"/>
    <string name="save_error">"Impossibile salvare il documento"</string>
    <!-- Toast shown when creating a folder failed with an error [CHAR LIMIT=48] -->
    <add-resource type="string" name="create_error"/>
    <string name="create_error">"Impossibile creare la cartella"</string>
    <!-- Error message shown when querying for a list of documents failed [CHAR LIMIT=48] -->
    <add-resource type="string" name="query_error"/>
    <string name="query_error">"Impossibile caricare i contenuti al momento"</string>

    <!-- Title of storage root location that contains recently modified or used documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_recent"/>
    <string name="root_recent">"Recenti"</string>
    <!-- Subtitle of storage root indicating the total free space available, in bytes [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_available_bytes"/>
    <string name="root_available_bytes">"<xliff:g id="SIZE">%1$s</xliff:g> liberi"</string>

    <!-- Header title for list of storage roots that contains cloud services [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_type_service"/>
    <string name="root_type_service">"Servizi di archiviazione"</string>
    <!-- Header title for list of storage roots that contains shortcuts to documents that may be available elsewhere [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_type_shortcut"/>
    <string name="root_type_shortcut">"Scorciatoie"</string>
    <!-- Header title for list of storage roots that contains physical devices [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_type_device"/>
    <string name="root_type_device">"Dispositivi"</string>
    <!-- Header title for list of additional apps that can provide documents [CHAR LIMIT=24] -->
    <add-resource type="string" name="root_type_apps"/>
    <string name="root_type_apps">"Altre app"</string>

    <!-- Text shown when a directory of documents is empty [CHAR LIMIT=24] -->
    <add-resource type="string" name="empty"/>
    <string name="empty">"Nessun articolo"</string>
    <!-- Text shown when a file search returns no items [CHAR LIMIT=32] -->
    <add-resource type="string" name="no_results"/>
    <string name="no_results">"Nessuna corrispondenza in %1$s"</string>

    <!-- Toast shown when no app can be found to open the selected document [CHAR LIMIT=48] -->
    <add-resource type="string" name="toast_no_application"/>
    <string name="toast_no_application">"Impossibile aprire il file"</string>
    <!-- Toast shown when some of the selected documents failed to be deleted [CHAR LIMIT=48] -->
    <add-resource type="string" name="toast_failed_delete"/>
    <string name="toast_failed_delete">"Impossibile eliminare alcuni documenti"</string>

    <!-- Title of dialog when prompting user to select an app to share documents with [CHAR LIMIT=32] -->
    <add-resource type="string" name="share_via"/>
    <string name="share_via">"Condividi via"</string>

    <!-- Title of the copy notification [CHAR LIMIT=24] -->
    <add-resource type="string" name="copy_notification_title"/>
    <string name="copy_notification_title">"Copia di file in corso"</string>
    <!-- Title of the move notification [CHAR LIMIT=24] -->
    <add-resource type="string" name="move_notification_title"/>
    <string name="move_notification_title">"Spostamento di file"</string>
    <!-- Title of the move notification [CHAR LIMIT=24] -->
    <add-resource type="string" name="delete_notification_title"/>
    <string name="delete_notification_title">"Eliminazione dei file"</string>
    <!-- Text shown on the copy notification to indicate remaining time, in minutes [CHAR LIMIT=24] -->
    <add-resource type="string" name="copy_remaining"/>
    <string name="copy_remaining">"<xliff:g id="DURATION">%s</xliff:g> rimanenti"</string>
    <!-- Toast shown when a file copy is kicked off -->
    <add-resource type="plurals" name="copy_begin"/>
    <plurals name="copy_begin">
        <item quantity="one">"Copia di <xliff:g id="COUNT">%1$d</xliff:g> file in corso."</item>
        <item quantity="other">"Copia di <xliff:g id="COUNT">%1$d</xliff:g> file in corso."</item>
    </plurals>
    <add-resource type="plurals" name="move_begin"/>
    <plurals name="move_begin">
        <item quantity="one">"Spostamento di <xliff:g id="COUNT_0">%1$d</xliff:g> file."</item>
        <item quantity="other">"Spostamento di <xliff:g id="COUNT_1">%1$d</xliff:g> file."</item>
    </plurals>
    <!-- Text shown when files are deleted -->
    <add-resource type="plurals" name="deleting"/>
    <plurals name="deleting">
        <item quantity="one">"Eliminazione di <xliff:g id="COUNT_0">%1$d</xliff:g> file."</item>
        <item quantity="other">"Eliminazione di <xliff:g id="COUNT_1">%1$d</xliff:g> file."</item>
    </plurals>
    <!-- Text shown for the undo button -->
    <add-resource type="string" name="undo"/>
    <string name="undo">"Annulla"</string>
    <!-- Text shown on the notification while DocumentsUI performs setup in preparation for copying files [CHAR LIMIT=32] -->
    <add-resource type="string" name="copy_preparing"/>
    <string name="copy_preparing">"Preparazione alla copia\u2026"</string>
    <!-- Text shown on the notification while DocumentsUI performs setup in preparation for moving files [CHAR LIMIT=32] -->
    <add-resource type="string" name="move_preparing"/>
    <string name="move_preparing">"Preparazione dello spostamento\u2026"</string>
    <!-- Text shown on the notification while DocumentsUI performs setup in preparation for deleting files [CHAR LIMIT=32] -->
    <add-resource type="string" name="delete_preparing"/>
    <string name="delete_preparing">"Preparazione eliminazione\u2026"</string>
    <!-- Title of the copy error notification [CHAR LIMIT=48] -->
    <add-resource type="plurals" name="copy_error_notification_title"/>
    <plurals name="copy_error_notification_title">
        <item quantity="one">"Impossibile copiare <xliff:g id="COUNT">%1$d</xliff:g> file"</item>
        <item quantity="other">"Impossibile copiare <xliff:g id="COUNT">%1$d</xliff:g> file"</item>
    </plurals>
    <!-- Title of the move error notification [CHAR LIMIT=48] -->
    <add-resource type="plurals" name="move_error_notification_title"/>
    <plurals name="move_error_notification_title">
        <item quantity="one">"Impossibile spostare <xliff:g id="COUNT_0">%1$d</xliff:g> file"</item>
        <item quantity="other">"Impossibile spostare <xliff:g id="COUNT_1">%1$d</xliff:g> file"</item>
    </plurals>
    <!-- Title of the delete error notification [CHAR LIMIT=48] -->
    <add-resource type="plurals" name="delete_error_notification_title"/>
    <plurals name="delete_error_notification_title">
        <item quantity="one">"Impossibile eliminare <xliff:g id="COUNT_0">%1$d</xliff:g> file"</item>
        <item quantity="other">"Impossibile eliminare <xliff:g id="COUNT_1">%1$d</xliff:g> file"</item>
    </plurals>
    <!-- Second line for notifications saying that more information will be shown after touching [CHAR LIMIT=48] -->
    <add-resource type="string" name="notification_touch_for_details"/>
    <string name="notification_touch_for_details">"Tocca per vedere i dettagli"</string>
    <!-- Label of the close dialog button.[CHAR LIMIT=24] -->
    <add-resource type="string" name="close"/>
    <string name="close">"Chiudi"</string>
    <!-- Contents of the copying failure alert dialog. [CHAR LIMIT=48] -->
    <add-resource type="string" name="copy_failure_alert_content"/>
    <string name="copy_failure_alert_content">"I seguenti file non sono stati copiati: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Contents of the moving failure alert dialog. [CHAR LIMIT=48] -->
    <add-resource type="string" name="move_failure_alert_content"/>
    <string name="move_failure_alert_content">"I seguenti file non sono stati spostati: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Message shown to users when an operation to delete one or more files has failed. Presented in a dialog. [CHAR LIMIT=48] -->
    <add-resource type="string" name="delete_failure_alert_content"/>
    <string name="delete_failure_alert_content">"I seguenti file non sono stati eliminati: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Contents of the copying warning dialog due to converted files. [CHAR LIMIT=64] -->
    <add-resource type="string" name="copy_converted_warning_content"/>
    <string name="copy_converted_warning_content">"I file sono stati convertiti in un altro formato: <xliff:g id="LIST">%1$s</xliff:g>"</string>
    <!-- Toast shown when a user copies files to clipboard. -->
    <add-resource type="plurals" name="clipboard_files_clipped"/>
    <plurals name="clipboard_files_clipped">
        <item quantity="one">"<xliff:g id="COUNT_0">%1$d</xliff:g> file copiato negli appunti."</item>
        <item quantity="other">"<xliff:g id="COUNT_1">%1$d</xliff:g> file copiati negli appunti."</item>
    </plurals>
    <!-- Toast shown when a user tries to paste files into an unsupported location. -->
    <add-resource type="string" name="clipboard_files_cannot_paste"/>
    <string name="clipboard_files_cannot_paste">"Impossibile incollare i file selezionati in questa posizione."</string>
    <!-- Menu item that renames the selected document [CHAR LIMIT=24] -->
    <add-resource type="string" name="menu_rename"/>
    <string name="menu_rename">"Rinomina"</string>
    <!-- Toast shown when renaming document failed with an error [CHAR LIMIT=48] -->
    <add-resource type="string" name="rename_error"/>
    <string name="rename_error">"Ridenominazione documento non riuscita"</string>
    <!-- First line for notifications saying that some files were converted to a different format
         during a copy. [CHAR LIMIT=48] -->
    <add-resource type="string" name="notification_copy_files_converted_title"/>
    <string name="notification_copy_files_converted_title">"Alcuni file sono stati convertiti"</string>

    <!-- Text in an alert dialog asking user to grant app access to a given directory in an external storage volume -->
    <add-resource type="string" name="open_external_dialog_request"/>
    <string name="open_external_dialog_request">"Concedere all\'app <xliff:g id="APPNAME"><b>^1</b></xliff:g> l\'accesso alla directory <xliff:g id="DIRECTORY"><i>^2</i></xliff:g> su <xliff:g id="STORAGE"><i>^3</i></xliff:g>?"</string>
    <!-- Text in an alert dialog asking user to grant app access to a given directory in the internal storage -->
    <add-resource type="string" name="open_external_dialog_request_primary_volume"/>
    <string name="open_external_dialog_request_primary_volume">"Concedere all\'app <xliff:g id="APPNAME"><b>^1</b></xliff:g> l\'accesso alla directory <xliff:g id="DIRECTORY"><i>^2</i></xliff:g>?"</string>
    <!-- Text in an alert dialog asking user to grant app access to all data in an external storage volume -->
    <add-resource type="string" name="open_external_dialog_root_request"/>
    <string name="open_external_dialog_root_request">"Concedere all\'app <xliff:g id="APPNAME"><b>^1</b></xliff:g> l\'accesso ai tuoi dati, inclusi video e foto, sull\'unità <xliff:g id="STORAGE"><i>^2</i></xliff:g>?"</string>
    <!-- Checkbox that allows user to not be questioned about the directory access request again -->
    <add-resource type="string" name="never_ask_again"/>
    <string name="never_ask_again">"Non chiedermelo più"</string>
    <!-- Text in the button asking user to allow access to a given directory. -->
    <add-resource type="string" name="allow"/>
    <string name="allow">"Consenti"</string>
    <!-- Text in the button asking user to deny access to a given directory. -->
    <add-resource type="string" name="deny"/>
    <string name="deny">"Nega"</string>
    <!-- Dialog text shown to users when asking if they want to delete files (a confirmation). -->
    <!-- Label text showing user how many items are selected. Can be one or more elements. -->
    <add-resource type="plurals" name="elements_selected"/>
    <plurals name="elements_selected">
        <item quantity="one">"<xliff:g id="COUNT_0">%1$d</xliff:g> file selezionato"</item>
        <item quantity="other">"<xliff:g id="COUNT_1">%1$d</xliff:g> file selezionati"</item>
    </plurals>

    <!-- Label text showing user how many items are being dragged. Can be one or more elements. -->
    <add-resource type="plurals" name="elements_dragged"/>
    <plurals name="elements_dragged">
        <item quantity="one">"<xliff:g id="COUNT_0">%1$d</xliff:g> elemento"</item>
        <item quantity="other">"<xliff:g id="COUNT_1">%1$d</xliff:g> elementi"</item>
    </plurals>

    <!-- Dialog text shown to users when asking if they want to delete a file (a confirmation) -->
    <add-resource type="string" name="delete_filename_confirmation_message"/>
    <string name="delete_filename_confirmation_message">"Eliminare \"<xliff:g id="NAME">%1$s</xliff:g>\"?"</string>
    <!-- Dialog text shown to users when asking if they want to delete a folder (a confirmation) -->
    <add-resource type="string" name="delete_foldername_confirmation_message"/>
    <string name="delete_foldername_confirmation_message">"Eliminare la cartella \"<xliff:g id="NAME">%1$s</xliff:g>\" e i relativi contenuti?"</string>
    <!-- Dialog text shown to users when asking if they want to delete files (a confirmation). -->
    <add-resource type="plurals" name="delete_files_confirmation_message"/>
    <plurals name="delete_files_confirmation_message">
        <item quantity="one">"Eliminare <xliff:g id="COUNT_0">%1$d</xliff:g> file?"</item>
        <item quantity="other">"Eliminare <xliff:g id="COUNT_1">%1$d</xliff:g> file?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete folders (a confirmation). -->
    <add-resource type="plurals" name="delete_folders_confirmation_message"/>
    <plurals name="delete_folders_confirmation_message">
        <item quantity="one">"Eliminare <xliff:g id="COUNT_0">%1$d</xliff:g> cartella e i relativi contenuti?"</item>
        <item quantity="other">"Eliminare <xliff:g id="COUNT_1">%1$d</xliff:g> cartelle e i relativi contenuti?"</item>
    </plurals>
    <!-- Dialog text shown to users when asking if they want to delete mixed type items: files and folders (a confirmation). -->
    <add-resource type="plurals" name="delete_items_confirmation_message"/>
    <plurals name="delete_items_confirmation_message">
        <item quantity="one">"Eliminare <xliff:g id="COUNT_0">%1$d</xliff:g> elemento?"</item>
        <item quantity="other">"Eliminare <xliff:g id="COUNT_1">%1$d</xliff:g> elementi?"</item>
    </plurals>
    <!-- Snackbar shown to users who wanted to select more than 1000 items (files or directories). -->
    <add-resource type="string" name="too_many_selected"/>
    <string name="too_many_selected">"Spiacenti, puoi selezionare massimo 1000 elementi alla volta"</string>
    <!-- Snackbar shown to users who wanted to select all, but there were too many items (files or directories).
         Only the first 1000 items are selected in such case. -->
    <add-resource type="string" name="too_many_in_select_all"/>
    <string name="too_many_in_select_all">"Sono stati selezionati solo 1000 elementi"</string></resources>
