<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title of the PrintSpooler application. [CHAR LIMIT=50] -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"列印多工緩衝處理器"</string>

    <!-- Label of the print dialog's button for more print options. [CHAR LIMIT=25] -->
    <add-resource type="string" name="more_options_button"/>
    <string name="more_options_button">"更多選項"</string>

    <!-- Label of the destination widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_destination"/>
    <string name="label_destination">"目的地"</string>

    <!-- Label of the copies count widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_copies"/>
    <string name="label_copies">"份數"</string>

    <!-- Label of the copies count for the print options summary. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_copies_summary"/>
    <string name="label_copies_summary">"份數:"</string>

    <!-- Label of the paper size widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_paper_size"/>
    <string name="label_paper_size">"紙張大小"</string>

    <!-- Label of the paper size for the print options summary. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_paper_size_summary"/>
    <string name="label_paper_size_summary">"紙張大小:"</string>

    <!-- Label of the color mode widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_color"/>
    <string name="label_color">"顏色"</string>

    <!-- Label of the printer mode to print on both sides of paper. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_duplex"/>
    <string name="label_duplex">"雙面"</string>

    <!-- Label of the orientation widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_orientation"/>
    <string name="label_orientation">"方向"</string>

    <!-- Label of the page selection widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="label_pages"/>
    <string name="label_pages">"頁數"</string>

    <!-- Label of the destination widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="destination_default_text"/>
    <string name="destination_default_text">"選擇打印機"</string>

    <!-- Template for the all pages option in the page selection widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="template_all_pages"/>
    <string name="template_all_pages">"全部 <xliff:g id="PAGE_COUNT">%1$s</xliff:g> 頁"</string>

    <!-- Template for the page range option in the page selection widget. [CHAR LIMIT=20] -->
    <add-resource type="string" name="template_page_range"/>
    <string name="template_page_range">"<xliff:g id="PAGE_COUNT">%1$s</xliff:g> 頁"</string>

    <!-- Page range exmple used as a hint of how to specify such. [CHAR LIMIT=20] -->
    <add-resource type="string" name="pages_range_example"/>
    <string name="pages_range_example">"例如: 1-5、8、11-13"</string>

    <!-- Title for the pring preview button .[CHAR LIMIT=30] -->
    <add-resource type="string" name="print_preview"/>
    <string name="print_preview">"預覽列印"</string>

    <!-- Title for the pring preview button if there is no PDF viewer isntalled. [CHAR LIMIT=50] -->
    <add-resource type="string" name="install_for_print_preview"/>
    <string name="install_for_print_preview">"安裝預覽所需的 PDF 檢視器"</string>

    <!-- Title of the message that the printing application crashed. [CHAR LIMIT=50] -->
    <add-resource type="string" name="printing_app_crashed"/>
    <string name="printing_app_crashed">"列印應用程式當機了"</string>

    <!-- Title for the temporary dialog show while an app is generating a print job. [CHAR LIMIT=30] -->
    <add-resource type="string" name="generating_print_job"/>
    <string name="generating_print_job">"正在產生列印工作"</string>

    <!-- Title for the save as PDF option in the printer list. [CHAR LIMIT=30] -->
    <add-resource type="string" name="save_as_pdf"/>
    <string name="save_as_pdf">"儲存為 PDF"</string>

    <!-- Title for the open all printers UI option in the printer list. [CHAR LIMIT=30] -->
    <add-resource type="string" name="all_printers"/>
    <string name="all_printers">"所有打印機\u2026"</string>

    <!-- Title for the print dialog announced to the user for accessibility. Not shown in the UI. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_dialog"/>
    <string name="print_dialog">"列印對話方塊"</string>

    <!-- Template for the message that shows the current page out of the total number of pages [CHAR LIMIT=none] -->
    <add-resource type="string" name="current_page_template"/>
    <string name="current_page_template">"<xliff:g id="CURRENT_PAGE">%1$d</xliff:g> /<xliff:g id="PAGE_COUNT">%2$d</xliff:g>"</string>

    <!-- Description of the current page - spoken to the user [CHAR LIMIT=none] -->
    <add-resource type="string" name="page_description_template"/>
    <string name="page_description_template">"第 <xliff:g id="CURRENT_PAGE">%1$d</xliff:g> 頁，共 <xliff:g id="PAGE_COUNT">%2$d</xliff:g> 頁"</string>

    <!-- Template for the message to announce the print options summary - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="summary_template"/>
    <string name="summary_template">"摘要，<xliff:g id="COPIES">%1$s</xliff:g> 份，<xliff:g id="PAPER_SIZE">%2$s</xliff:g> 紙張大小"</string>

    <!-- Description for the handle to expand all print options - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="expand_handle"/>
    <string name="expand_handle">"展開控點"</string>

    <!-- Description for the handle to collapse all print options - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="collapse_handle"/>
    <string name="collapse_handle">"收合控點"</string>

    <!-- Description for the print button - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_button"/>
    <string name="print_button">"列印"</string>

    <!-- Description for the save to PDF button - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="savetopdf_button"/>
    <string name="savetopdf_button">"儲存為 PDF"</string>

    <!-- Message to announce print options are expanded - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_options_expanded"/>
    <string name="print_options_expanded">"列印選項已展開"</string>

    <!-- Message to announce print options are collapsed - spoken to the user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_options_collapsed"/>
    <string name="print_options_collapsed">"列印選項已收合"</string>

    <!-- Select printer activity -->

    <!-- Title for the share action bar menu item. [CHAR LIMIT=20] -->
    <add-resource type="string" name="search"/>
    <string name="search">"搜尋"</string>

    <!-- Title for the select printer activity. [CHAR LIMIT=30] -->
    <add-resource type="string" name="all_printers_label"/>
    <string name="all_printers_label">"所有打印機"</string>

    <!-- Title of the button to install a print service. [CHAR LIMIT=25] -->
    <add-resource type="string" name="add_print_service_label"/>
    <string name="add_print_service_label">"新增服務"</string>

    <!-- Utterance to announce that the search box is shown. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_search_box_shown_utterance"/>
    <string name="print_search_box_shown_utterance">"搜尋框已顯示"</string>

    <!-- Utterance to announce that the search box is hidden. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <add-resource type="string" name="print_search_box_hidden_utterance"/>
    <string name="print_search_box_hidden_utterance">"搜尋框已隱藏"</string>

    <!-- Label of add printers button when no printers are found. [CHAR LIMIT=25] -->
    <add-resource type="string" name="print_add_printer"/>
    <string name="print_add_printer">"新增打印機"</string>

    <!-- Title of the menu item to select a printer. [CHAR LIMIT=25] -->
    <add-resource type="string" name="print_select_printer"/>
    <string name="print_select_printer">"選取打印機"</string>

    <!-- Title of the menu item to forget a printer. [CHAR LIMIT=25] -->
    <add-resource type="string" name="print_forget_printer"/>
    <string name="print_forget_printer">"移除打印機"</string>

    <!-- Utterance to announce a change in the number of matches during a search. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <add-resource type="plurals" name="print_search_result_count_utterance"/>
    <plurals name="print_search_result_count_utterance">
        <item quantity="one">"找到 <xliff:g id="COUNT">%1$s</xliff:g> 部打印機"</item>
        <item quantity="other">"找到 <xliff:g id="COUNT">%1$s</xliff:g> 部打印機"</item>
    </plurals>

    <!-- Template for an extended description of a printer. [CHAR LIMIT=50] -->
    <add-resource type="string" name="printer_extended_description_template"/>
    <string name="printer_extended_description_template">"<xliff:g id="PRINT_SERVICE_LABEL">%1$s</xliff:g> - <xliff:g id="PRINTER_DESCRIPTION">%2$s</xliff:g>"</string>

    <!-- Description of printer info icon. [CHAR LIMIT=50] -->
    <add-resource type="string" name="printer_info_desc"/>
    <string name="printer_info_desc">"有關此打印機的詳細資訊"</string>

    <!-- Notification that we could not create a file name for the printed PDF. [CHAR LIMIT=50] -->
    <add-resource type="string" name="could_not_create_file"/>
    <string name="could_not_create_file">"無法建立檔案"</string>

    <!-- Notification that print services as disabled. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_services_disabled_toast"/>
    <string name="print_services_disabled_toast">"部分列印服務已停用"</string>

    <!-- Title for the prompt shown as a placeholder if no printers are found while not searching. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_searching_for_printers"/>
    <string name="print_searching_for_printers">"正在搜尋打印機"</string>

    <!-- Title for the prompt shown as a placeholder if there are no print services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_no_print_services"/>
    <string name="print_no_print_services">"沒有已啟用的列印服務"</string>

    <!-- Title for the prompt shown as a placeholder if there are no printers while searching. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_no_printers"/>
    <string name="print_no_printers">"找不到打印機"</string>

    <!-- Add printer activity -->

    <!-- Subtitle for services that cannot add printers. [CHAR LIMIT=50] -->
    <add-resource type="string" name="cannot_add_printer"/>
    <string name="cannot_add_printer">"無法新增印表機"</string>

    <!-- Subtitle for services that can add printers. [CHAR LIMIT=50] -->
    <add-resource type="string" name="select_to_add_printers"/>
    <string name="select_to_add_printers">"選擇即可新增印表機"</string>

    <!-- Subtitle for disabled services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="enable_print_service"/>
    <string name="enable_print_service">"選取即可啟用"</string>

    <!-- Header for the list of enabled print services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="enabled_services_title"/>
    <string name="enabled_services_title">"已啟用的服務"</string>

    <!-- Header for the list of recommended print services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="recommended_services_title"/>
    <string name="recommended_services_title">"推薦服務"</string>

    <!-- Header for the list of disabled print services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="disabled_services_title"/>
    <string name="disabled_services_title">"已停用的服務"</string>

    <!-- Label for the list item that links to the list of all print services. [CHAR LIMIT=50] -->
    <add-resource type="string" name="all_services_title"/>
    <string name="all_services_title">"所有服務"</string>

    <!-- Subtitle for a print service recommendation. [CHAR LIMIT=50] -->
    <add-resource type="plurals" name="print_services_recommendation_subtitle"/>
    <plurals name="print_services_recommendation_subtitle">
        <item quantity="one">"安裝即可使用 <xliff:g id="COUNT_0">%1$s</xliff:g> 部印表機"</item>
        <item quantity="other">"安裝即可使用 <xliff:g id="COUNT_1">%1$s</xliff:g> 部印表機"</item>
    </plurals>

    <!-- Notifications -->

    <!-- Template for the notification label for a printing print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="printing_notification_title_template"/>
    <string name="printing_notification_title_template">"正在列印 <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a cancelling print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="cancelling_notification_title_template"/>
    <string name="cancelling_notification_title_template">"正在取消 <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a failed print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="failed_notification_title_template"/>
    <string name="failed_notification_title_template">"打印機錯誤: <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a blocked print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="blocked_notification_title_template"/>
    <string name="blocked_notification_title_template">"打印機已封鎖 <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Label for the notification button for cancelling a print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="cancel"/>
    <string name="cancel">"取消"</string>

    <!-- Label for the notification button for restarting a filed print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="restart"/>
    <string name="restart">"重新開始"</string>

    <!-- Message that there is no connection to a printer. [CHAR LIMIT=40] -->
    <add-resource type="string" name="no_connection_to_printer"/>
    <string name="no_connection_to_printer">"尚未與打印機連線"</string>

    <!-- Label for an unknown reason for failed or blocked print job. [CHAR LIMIT=25] -->
    <add-resource type="string" name="reason_unknown"/>
    <string name="reason_unknown">"不明"</string>

    <!-- Title for a warning message about security implications of using a print service,
         displayed as a dialog message when the user prints using a print service that has not been
         used before. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="print_service_security_warning_title"/>
    <string name="print_service_security_warning_title">"使用「<xliff:g id="SERVICE">%1$s</xliff:g>」？"</string>

    <!-- Summary for a warning message about security implications of using a print service,
         displayed as a dialog message when the user prints using a print service that has not been
         used before. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="print_service_security_warning_summary"/>
    <string name="print_service_security_warning_summary">"您的文件可能會通過一部或多部伺服器才傳送至打印機。"</string>

    <!-- Arrays -->

    <!-- Color mode labels. -->
    <add-resource type="array" name="color_mode_labels"/>
    <string-array name="color_mode_labels">
        <!-- Color mode label: Monochrome color scheme, e.g. one color is used. [CHAR LIMIT=20] -->
        <item>"黑白"</item>
        <!-- Color mode label: Color color scheme, e.g. many colors are used. [CHAR LIMIT=20] -->
        <item>"彩色"</item>
    </string-array>

    <!-- Duplex mode labels. -->
    <add-resource type="array" name="duplex_mode_labels"/>
    <string-array name="duplex_mode_labels">
        <!-- Duplex mode label: No duplex supported. [CHAR LIMIT=20] -->
        <item>"無"</item>
        <!-- Duplex mode label: Turn page sideways along the long edge like a book. [CHAR LIMIT=20] -->
        <item>"長邊"</item>
        <!-- Duplex mode label: Turn page upwards along the short edge like a notepad. [CHAR LIMIT=20] -->
        <item>"短邊"</item>
    </string-array>

    <!-- Orientation labels. -->
    <add-resource type="array" name="orientation_labels"/>
    <string-array name="orientation_labels">
        <!-- Orientation label: Portrait page orientation. [CHAR LIMIT=30] -->
        <item>"直向"</item>
        <!-- Orientation label: Landscape page orientation [CHAR LIMIT=30] -->
        <item>"橫向"</item>
    </string-array>

    <!-- Error messages -->

    <!-- Message for an error when trying to print to a PDF file. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_write_error_message"/>
    <string name="print_write_error_message">"無法寫入檔案"</string>

    <!-- Default message for an error while generating a print job. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_error_default_message"/>
    <string name="print_error_default_message">"很抱歉，行不通。請再試一次。"</string>

    <!-- Label for the retry button in the error message. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_error_retry"/>
    <string name="print_error_retry">"重試"</string>

    <!-- Message for the currently selected printer being unavailable. [CHAR LIMIT=100] -->
    <add-resource type="string" name="print_error_printer_unavailable"/>
    <string name="print_error_printer_unavailable">"這部打印機目前無法使用。"</string>

    <!-- Message for the case when a preview of a page cannot be loaded because the printing app
         provided a broken print preview rendering for this page. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_cannot_load_page"/>
    <string name="print_cannot_load_page">"無法顯示預覽"</string>

    <!-- Long running operations -->

    <!-- Message long running operation when preparing print preview. [CHAR LIMIT=50] -->
    <add-resource type="string" name="print_preparing_preview"/>
    <string name="print_preparing_preview">"正在準備預覽\u2026"</string></resources>
