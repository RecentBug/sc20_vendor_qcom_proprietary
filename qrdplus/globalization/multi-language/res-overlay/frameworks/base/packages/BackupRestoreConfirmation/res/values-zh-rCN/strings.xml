<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <add-resource type="string" name="backup_confirm_title"/>
    <string name="backup_confirm_title">"完全备份"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <add-resource type="string" name="restore_confirm_title"/>
    <string name="restore_confirm_title">"完全还原"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <add-resource type="string" name="backup_confirm_text"/>
    <string name="backup_confirm_text">"系统请求将所有数据完整备份至已连接的桌面计算机。允许此操作吗？\n\n如果您本人未要求备份，请阻止该操作。"</string>
    <!-- Button to allow a requested full backup to occur -->
    <add-resource type="string" name="allow_backup_button_label"/>
    <string name="allow_backup_button_label">"备份我的数据"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <add-resource type="string" name="deny_backup_button_label"/>
    <string name="deny_backup_button_label">"不备份"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <add-resource type="string" name="restore_confirm_text"/>
    <string name="restore_confirm_text">"系统请求从连接的桌面计算机完整还原所有数据。允许此操作吗？\n\n如果您本人未要求还原，请阻止该操作。该操作会覆盖设备上当前的所有数据！"</string>
    <!-- Button to allow a requested full restore to occur -->
    <add-resource type="string" name="allow_restore_button_label"/>
    <string name="allow_restore_button_label">"恢复我的数据"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <add-resource type="string" name="deny_restore_button_label"/>
    <string name="deny_restore_button_label">"不恢复"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <add-resource type="string" name="current_password_text"/>
    <string name="current_password_text">"请在下方输入您的当前备份密码："</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <add-resource type="string" name="device_encryption_restore_text"/>
    <string name="device_encryption_restore_text">"请在下方输入您的设备加密密码。"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <add-resource type="string" name="device_encryption_backup_text"/>
    <string name="device_encryption_backup_text">"请在下方输入您的设备加密密码。此密码还会用于加密备份存档。"</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <add-resource type="string" name="backup_enc_password_text"/>
    <string name="backup_enc_password_text">"请输入用于加密完整备份数据的密码。如果留空，系统将会使用您当前的备份密码："</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <add-resource type="string" name="backup_enc_password_optional"/>
    <string name="backup_enc_password_optional">"如果您想为整个备份数据加密，请在下方输入密码："</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <add-resource type="string" name="backup_enc_password_required"/>
    <string name="backup_enc_password_required">"由于您的设备进行了加密，因此您需要加密备份。请在下方输入密码："</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <add-resource type="string" name="restore_enc_password_text"/>
    <string name="restore_enc_password_text">"如果恢复数据已加密，请在下方输入密码："</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <add-resource type="string" name="toast_backup_started"/>
    <string name="toast_backup_started">"开始备份..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <add-resource type="string" name="toast_backup_ended"/>
    <string name="toast_backup_ended">"备份已完成"</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <add-resource type="string" name="toast_restore_started"/>
    <string name="toast_restore_started">"开始还原..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <add-resource type="string" name="toast_restore_ended"/>
    <string name="toast_restore_ended">"恢复已结束"</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <add-resource type="string" name="toast_timeout"/>
    <string name="toast_timeout">"操作超时"</string></resources>
