<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"ஷெல்"</string>

    <!-- Title of notification indicating a bugreport is being generated. [CHAR LIMIT=50] -->
    <add-resource type="string" name="bugreport_in_progress_title"/>
    <string name="bugreport_in_progress_title">"பிழை அறிக்கை <xliff:g id="ID">#%d</xliff:g> உருவாக்கப்படுகிறது"</string>
    <!-- Title of notification indicating a bugreport has been successfully captured. [CHAR LIMIT=50] -->
    <add-resource type="string" name="bugreport_finished_title"/>
    <string name="bugreport_finished_title">"பிழை அறிக்கை <xliff:g id="ID">#%d</xliff:g> எடுக்கப்பட்டது"</string>
    <!-- Title of notification indicating a bugreport is being updated before it can be shared. [CHAR LIMIT=50] -->
    <add-resource type="string" name="bugreport_updating_title"/>
    <string name="bugreport_updating_title">"பிழை அறிக்கையில் விவரங்களைச் சேர்க்கிறது"</string>
    <!-- Content notification indicating a bugreport is being updated before it can be shared, asking the user to wait [CHAR LIMIT=50] -->
    <add-resource type="string" name="bugreport_updating_wait"/>
    <string name="bugreport_updating_wait">"காத்திருக்கவும்\u2026"</string>

    <!-- Text of notification indicating that bugreport will appear on the phone. [CHAR LIMIT=100] -->
    <add-resource type="string" name="bugreport_finished_text"/>
    <string name="bugreport_finished_text" product="watch">"பிழை அறிக்கை சிறிது நேரத்தில் மொபைலில் தோன்றும்"</string>
    <!-- Text of notification indicating that tapping will share the captured bugreport. [CHAR LIMIT=100] -->
    <add-resource type="string" name="bugreport_finished_text"/>
    <string name="bugreport_finished_text" product="default">"பிழை அறிக்கையைப் பகிர, தட்டவும்"</string>
    <!-- Text of notification indicating that swipe left will share the captured bugreport, but giving user the option to wait for the screenshot. [CHAR LIMIT=100] -->
    <add-resource type="string" name="bugreport_finished_pending_screenshot_text"/>
    <string name="bugreport_finished_pending_screenshot_text" product="watch">"ஸ்கிரீன்ஷாட் இல்லாமல் பிழை அறிக்கையைப் பகிர, தட்டவும் அல்லது ஸ்கிரீன்ஷாட் முடியும்வரை காத்திருக்கவும்"</string>
    <!-- Text of notification indicating that tapping will share the captured bugreport, but giving user the option to wait for the screenshot. [CHAR LIMIT=100] -->
    <add-resource type="string" name="bugreport_finished_pending_screenshot_text"/>
    <string name="bugreport_finished_pending_screenshot_text" product="default">"ஸ்கிரீன்ஷாட் இல்லாமல் பிழை அறிக்கையைப் பகிர, தட்டவும் அல்லது ஸ்கிரீன்ஷாட் முடியும்வரை காத்திருக்கவும்"</string>

    <!-- Body of dialog informing user about contents of a bugreport. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="bugreport_confirm"/>
    <string name="bugreport_confirm">"பிழை அறிக்கைகளில் முறைமையின் பல்வேறு பதிவுக் கோப்புகளின் தரவு (இதில் முக்கியமானவை என நீங்கள் கருதும் பயன்பாடின் உபயோகம், இருப்பிடத் தரவு போன்றவை அடங்கும்) இருக்கும். நீங்கள் நம்பும் நபர்கள் மற்றும் பயன்பாடுகளுடன் மட்டும் பிழை அறிக்கைகளைப் பகிரவும்."</string>
    <!-- Checkbox that indicates this dialog should not be shown again when the next bugreport is taken. [CHAR LIMIT=50] -->
    <add-resource type="string" name="bugreport_confirm_dont_repeat"/>
    <string name="bugreport_confirm_dont_repeat">"மீண்டும் காட்டாதே"</string>

    <!-- Title for documents backend that offers bugreports. -->
    <add-resource type="string" name="bugreport_storage_title"/>
    <string name="bugreport_storage_title">"பிழை அறிக்கைகள்"</string>

    <!-- Toast message sent when the bugreport file could be read. -->
    <add-resource type="string" name="bugreport_unreadable_text"/>
    <string name="bugreport_unreadable_text">"பிழை அறிக்கையைப் படிக்க முடியவில்லை"</string>
    <!-- Toast message sent when the bugreport details could not be added to the zip file. -->
    <add-resource type="string" name="bugreport_add_details_to_zip_failed"/>
    <string name="bugreport_add_details_to_zip_failed">"பிழை அறிக்கை விவரங்களை ஜிப் கோப்பில் சேர்க்க முடியவில்லை"</string>

    <!-- Title for bug reports received from dumpstate without a name. [CHAR LIMIT=30]-->
    <add-resource type="string" name="bugreport_unnamed"/>
    <string name="bugreport_unnamed">"பெயரிடப்படாதது"</string>

    <!-- Title of the notification action that opens the dialog for the user-defined bug report details. -->
    <add-resource type="string" name="bugreport_info_action"/>
    <string name="bugreport_info_action">"விவரங்கள்"</string>

    <!-- Title of the notification action that takes aditional screenshots. -->
    <add-resource type="string" name="bugreport_screenshot_action"/>
    <string name="bugreport_screenshot_action">"ஸ்கிரீன் ஷாட்"</string>

    <!-- Toast message sent when the a screenshot for the bug report was taken successfully. -->
    <add-resource type="string" name="bugreport_screenshot_taken"/>
    <string name="bugreport_screenshot_taken">"ஸ்கிரீன் ஷாட் எடுக்கப்பட்டது."</string>
    <!-- Toast message sent when the a screenshot for the bug report was not taken due to an error. -->
    <add-resource type="string" name="bugreport_screenshot_failed"/>
    <string name="bugreport_screenshot_failed">"ஸ்கிரீன் ஷாட்டை எடுக்க முடியவில்லை."</string>

    <!--  Title of the dialog asking for user-defined bug report details like name, title, and description. -->
    <add-resource type="string" name="bugreport_info_dialog_title"/>
    <string name="bugreport_info_dialog_title">"பிழை அறிக்கை <xliff:g id="ID">#%d</xliff:g> இன் விவரங்கள்"</string>

    <!-- Text of the label identifying the bug report name, which when set will define a suffix in the
         bug report file names. [CHAR LIMIT=30] -->
    <add-resource type="string" name="bugreport_info_name"/>
    <string name="bugreport_info_name">"கோப்புப்பெயர்"</string>
    <!-- Text of the label identifying the bug report title, which when set will define the
         Subject of the email message. [CHAR LIMIT=60] -->
    <add-resource type="string" name="bugreport_info_title"/>
    <string name="bugreport_info_title">"பிழை தலைப்பு"</string>
    <!-- Text of the label identifying the bug report description, which when set will describe
         what the bug report is about. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="bugreport_info_description"/>
    <string name="bugreport_info_description">"பிழை குறித்த சுருக்க விவரம்"</string>

    <!-- Label of button that save bugreport details.  -->
    <add-resource type="string" name="save"/>
    <string name="save">"சேமி"</string></resources>
