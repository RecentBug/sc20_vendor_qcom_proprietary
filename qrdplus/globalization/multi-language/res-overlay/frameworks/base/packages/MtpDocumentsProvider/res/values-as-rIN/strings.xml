<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- App title of MtpDocumentsProvider [CHAR LIMIT=32] -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"MTP হোষ্ট"</string>
    <!-- App title of DocumentsUI [CHAR LIMIT=32] -->
    <add-resource type="string" name="downloads_app_label"/>
    <string name="downloads_app_label">"ডাউনলোড"</string>
    <!-- Name of MTP root shown in UI. Please align the two strings (device
         model and storage name) in proper order in the language.
         [CHAR LIMIT=32] -->
    <add-resource type="string" name="root_name"/>
    <string name="root_name">"<xliff:g id="device_model" example="Nexus 9">%1$s</xliff:g> <xliff:g id="storage_name" example="Internal Storage">%2$s</xliff:g>"</string>
    <!-- Title of notification showing Files app is accessing files in a MTP device. [CHAR LIMIT=60]-->
    <add-resource type="string" name="accessing_notification_title"/>
    <string name="accessing_notification_title">"<xliff:g id="device_model" example="Nexus 9">%1$s</xliff:g>-ৰ পৰা ফাইল প্ৰৱেশাধিকাৰ কৰি আছে"</string>
    <!-- Error message shown in Files app when the connected MTP device is busy. [CHAR LIMIT=150]-->
    <add-resource type="string" name="error_busy_device"/>
    <string name="error_busy_device">"অন্য ডিভাইচটো ব্যস্ত আছে৷ আপুনি এইটো উপলব্ধ নোহোৱালৈকে ফাইল স্থানান্তৰ কৰিব নোৱাৰে৷"</string>
    <!-- Error message shown in Files app when the connected MTP device may be locked. [CHAR LIMIT=150]-->
    <add-resource type="string" name="error_locked_device"/>
    <string name="error_locked_device">"কোনো ফাইল বিচাৰি পোৱা নাই৷ অন্য ডিভাইচটো লক হ\'ব পাৰে৷ যদি হয়, তেন্তে ইয়াক আনলক্ কৰক আৰু পুনৰ চেষ্টা কৰক৷"</string></resources>
