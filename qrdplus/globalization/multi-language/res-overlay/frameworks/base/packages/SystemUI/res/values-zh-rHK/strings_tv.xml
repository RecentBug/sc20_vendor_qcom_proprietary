<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Picture-in-Picture (PIP) menu -->

    <!-- Button to close picture-in-picture (PIP) in PIP menu [CHAR LIMIT=30] -->
    <add-resource type="string" name="pip_close"/>
    <string name="pip_close">"關閉 PIP"</string>
    <!-- Button to move picture-in-picture (PIP) screen to the fullscreen in PIP menu [CHAR LIMIT=30] -->
    <add-resource type="string" name="pip_fullscreen"/>
    <string name="pip_fullscreen">"全螢幕"</string>
    <!-- Button to play the current media on picture-in-picture (PIP) [CHAR LIMIT=30] -->
    <add-resource type="string" name="pip_play"/>
    <string name="pip_play">"播放"</string>
    <!-- Button to pause the current media on picture-in-picture (PIP) [CHAR LIMIT=30] -->
    <add-resource type="string" name="pip_pause"/>
    <string name="pip_pause">"暫停"</string>
    <!-- Overlay text on picture-in-picture (PIP) to indicate that longpress HOME key to control PIP [CHAR LIMIT=52] -->
    <add-resource type="string" name="pip_hold_home"/>
    <string name="pip_hold_home">"按住"<b>"主按鈕"</b>"即可控制 PIP"</string>
    <!-- Picture-in-Picture (PIP) onboarding screen -->

    <!-- Title for picture-in-picture (PIP) onboarding screen to indicate that an user is in PIP mode. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="pip_onboarding_title"/>
    <string name="pip_onboarding_title">"畫中畫"</string>
    <!-- Description for picture-in-picture (PIP) onboarding screen to indicate that longpress HOME key to control PIP. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="pip_onboarding_description"/>
    <string name="pip_onboarding_description">"這讓您繼續觀看影片，直至您播放下一部影片。按住"<b>"主按鈕"</b>"即可控制「畫中畫」。"</string>
    <!-- Button to close picture-in-picture (PIP) onboarding screen. -->
    <add-resource type="string" name="pip_onboarding_button"/>
    <string name="pip_onboarding_button">"知道了"</string>
    <!-- Dismiss icon description -->
    <add-resource type="string" name="recents_tv_dismiss"/>
    <string name="recents_tv_dismiss">"關閉"</string>
    <!-- Package names to be blacklisted in Recents, add package names into overlay as needed -->
    <add-resource type="array" name="recents_tv_blacklist_array"/>
    <string-array name="recents_tv_blacklist_array">
    </string-array></resources>
