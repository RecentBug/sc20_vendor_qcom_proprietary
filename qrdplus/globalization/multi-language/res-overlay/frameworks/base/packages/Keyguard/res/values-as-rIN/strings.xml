    <string name="keyguard_network_locked_message">"নেটৱৰ্ক লক কৰা হৈছে"</string>
    <!-- Shown when there is no SIM card for India RJIL. -->
    <add-resource type="string" name="keyguard_missing_sim_message_RJIL"/>
    <string name="keyguard_missing_sim_message_RJIL">"কোনো SIM নাই"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message_short"/>
    <string name="keyguard_missing_sim_message_short">"কোনো SIM কাৰ্ড নাই"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="tablet">"টেবলেটত কোনো SIM কাৰ্ড নাই৷"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="default">"ফোনত কোনো SIM কাৰ্ড নাই৷"</string>
    <!-- Shown to ask the user to insert a SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions"/>
    <string name="keyguard_missing_sim_instructions">"এখন SIM কাৰ্ড সুমুৱাই দিয়ক৷"</string>
    <!-- Shown to ask the user to insert a SIM card when sim is missing or not readable. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions_long"/>
    <string name="keyguard_missing_sim_instructions_long">"SIM কাৰ্ড নাই বা পঠনযোগ্য নহয়৷ এখন SIM কাৰ্ড সুমুৱাই দিয়ক৷"</string>
    <!-- Shown when SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_message_short"/>
    <string name="keyguard_permanent_disabled_sim_message_short">"ব্যৱহাৰৰ অনুপযুক্ত SIM কাৰ্ড৷"</string>
    <!-- Shown to inform the user to SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_instructions"/>
    <string name="keyguard_permanent_disabled_sim_instructions">"আপোনাৰ SIM কাৰ্ড স্থায়ীৰূপে অক্ষম কৰা হৈছে৷\n  অন্য SIM কাৰ্ডৰ বাবে আপোনাৰ বেতাঁৰ সেৱা যোগান ধৰোঁতাৰ সৈতে সম্পৰ্ক কৰক৷"</string>
    <!-- Shown to tell the user that their SIM is locked and they must unlock it. -->
