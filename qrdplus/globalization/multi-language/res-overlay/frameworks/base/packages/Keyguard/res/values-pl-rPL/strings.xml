    <string name="kg_wrong_password">"Nieprawidłowe hasło"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"Nieprawidłowy PIN"</string>
