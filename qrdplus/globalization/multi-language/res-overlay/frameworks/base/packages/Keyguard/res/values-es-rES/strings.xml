<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- App label in the manifest  -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Bloqueo"</string>
    <!-- Instructions telling the user to enter their SIM PIN to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_code"/>
    <string name="keyguard_password_enter_pin_code">"Introduce el código PIN."</string>

    <!-- Instructions telling the user to enter their SIM PUK to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_puk_code"/>
    <string name="keyguard_password_enter_puk_code">"Escribe el PUK de la tarjeta SIM y un nuevo código PIN"</string>

    <!-- Prompt to enter SIM PUK in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_puk_prompt"/>
    <string name="keyguard_password_enter_puk_prompt">"Código PUK de la tarjeta SIM"</string>
    <!-- Prompt to enter New SIM PIN in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_pin_prompt"/>
    <string name="keyguard_password_enter_pin_prompt">"Nuevo código PIN de tarjeta SIM"</string>

    <!-- Displayed as hint in passwordEntry EditText on PasswordUnlockScreen [CHAR LIMIT=30]-->
    <add-resource type="string" name="keyguard_password_entry_touch_hint"/>
    <string name="keyguard_password_entry_touch_hint"><font size="17">"Toca para introducir contraseña"</font></string>

    <!-- Instructions telling the user to enter their text password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_password_code"/>
    <string name="keyguard_password_enter_password_code">"Introduce la contraseña para desbloquear."</string>

    <!-- Instructions telling the user to enter their PIN password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_password_code"/>
    <string name="keyguard_password_enter_pin_password_code">"Introduce el código PIN para desbloquear."</string>

    <!-- Instructions telling the user that they entered the wrong pin while trying
         to unlock the keyguard.  Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_wrong_pin_code"/>
    <string name="keyguard_password_wrong_pin_code">"Código PIN incorrecto"</string>

    <!-- Shown in the lock screen when there is SIM card IO error. -->
    <add-resource type="string" name="lockscreen_sim_error_message_short"/>
    <string name="lockscreen_sim_error_message_short">"Tarjeta no válida."</string>

    <!-- When the lock screen is showing, the phone is plugged in and the battery is fully
         charged, say that it is charged. -->
    <add-resource type="string" name="keyguard_charged"/>
    <string name="keyguard_charged">"Cargada"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, say that it's charging.  -->
    <add-resource type="string" name="keyguard_plugged_in"/>
    <string name="keyguard_plugged_in">"Cargando"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a fast charger, say that it's charging fast.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_fast"/>
    <string name="keyguard_plugged_in_charging_fast">"Cargando rápidamente"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a slow charger, say that it's charging slowly.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_slowly"/>
    <string name="keyguard_plugged_in_charging_slowly">"Cargando lentamente"</string>

    <!-- When the lock screen is showing and the battery is low, warn user to plug
         in the phone soon. -->
    <add-resource type="string" name="keyguard_low_battery"/>
    <string name="keyguard_low_battery">"Conecta el cargador."</string>

    <!-- On the keyguard screen, when pattern lock is disabled, only tell them to press menu to unlock.  This is shown in small font at the bottom. -->
    <add-resource type="string" name="keyguard_instructions_when_pattern_disabled"/>
    <string name="keyguard_instructions_when_pattern_disabled">"Ve al menú para desbloquear la pantalla."</string>

    <!-- SIM messages -->
    <!-- When the user inserts a sim card from an unsupported network, it becomes network locked -->
    <add-resource type="string" name="keyguard_network_locked_message"/>
    <string name="keyguard_network_locked_message">"Bloqueada para la red"</string>
    <!-- Shown when there is no SIM card for India RJIL. -->
    <add-resource type="string" name="keyguard_missing_sim_message_RJIL"/>
    <string name="keyguard_missing_sim_message_RJIL">"Sin tarjeta SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message_short"/>
    <string name="keyguard_missing_sim_message_short">"Falta la tarjeta SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="tablet">"No se ha insertado ninguna tarjeta SIM en el tablet."</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="default">"No se ha insertado ninguna tarjeta SIM en el teléfono."</string>
    <!-- Shown to ask the user to insert a SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions"/>
    <string name="keyguard_missing_sim_instructions">"Inserta una tarjeta SIM."</string>
    <!-- Shown to ask the user to insert a SIM card when sim is missing or not readable. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions_long"/>
    <string name="keyguard_missing_sim_instructions_long">"Falta la tarjeta SIM o no se puede leer. Introduce una tarjeta SIM."</string>
    <!-- Shown when SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_message_short"/>
    <string name="keyguard_permanent_disabled_sim_message_short">"Tarjeta SIM inutilizable"</string>
    <!-- Shown to inform the user to SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_instructions"/>
    <string name="keyguard_permanent_disabled_sim_instructions">"Tu tarjeta SIM se ha inhabilitado permanentemente.\n Para obtener otra, ponte en contacto con tu proveedor de servicios de telefonía."</string>
    <!-- Shown to tell the user that their SIM is locked and they must unlock it. -->
    <add-resource type="string" name="keyguard_sim_locked_message"/>
    <string name="keyguard_sim_locked_message">"La tarjeta SIM está bloqueada."</string>
    <!-- When the user enters a wrong sim pin too many times, it becomes PUK locked (Pin Unlock Kode) -->
    <add-resource type="string" name="keyguard_sim_puk_locked_message"/>
    <string name="keyguard_sim_puk_locked_message">"La tarjeta SIM está bloqueada con el código PUK."</string>
    <!-- For the unlock screen, When the user enters a sim unlock code, it takes a little while to check
         whether it is valid, and to unlock the sim if it is valid.  we display a
         progress dialog in the meantime.  this is the emssage. -->
    <add-resource type="string" name="keyguard_sim_unlock_progress_dialog_message"/>
    <string name="keyguard_sim_unlock_progress_dialog_message">"Desbloqueando tarjeta SIM\u2026"</string>

    <!-- Accessibility description of the PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_pin_area"/>
    <string name="keyguard_accessibility_pin_area">"Área de PIN"</string>
    <!-- Accessibility description of the SIM PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_pin_area"/>
    <string name="keyguard_accessibility_sim_pin_area">"Área de PIN de SIM"</string>
    <!-- Accessibility description of the SIM PUK password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_puk_area"/>
    <string name="keyguard_accessibility_sim_puk_area">"Área de PUK de SIM"</string>

    <!-- Accessibility description for the text view that indicates when the next alarm is set (not shown on screen). [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_next_alarm"/>
    <string name="keyguard_accessibility_next_alarm">"Próxima alarma: <xliff:g id="ALARM">%1$s</xliff:g>"</string>

    <!-- KeyguardPinView - accessibility support -->
    <!-- Description of the Delete button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_delete"/>
    <string name="keyboardview_keycode_delete">"Eliminar"</string>
    <!-- Description of the Enter button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_enter"/>
    <string name="keyboardview_keycode_enter">"Intro"</string>

    <!-- Message shown in pattern unlock after some number of unsuccessful attempts -->
    <add-resource type="string" name="kg_forgot_pattern_button_text"/>
    <string name="kg_forgot_pattern_button_text">"¿Has olvidado el patrón?"</string>
    <!-- Message shown when user enters wrong pattern -->
    <add-resource type="string" name="kg_wrong_pattern"/>
    <string name="kg_wrong_pattern">"El patrón es incorrecto"</string>
    <!-- Message shown when user enters wrong password -->
    <add-resource type="string" name="kg_wrong_password"/>
    <string name="kg_wrong_password">"Contraseña incorrecta"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"PIN incorrecto"</string>
    <!-- Countdown message shown after too many failed unlock attempts -->
    <add-resource type="string" name="kg_too_many_failed_attempts_countdown"/>
    <string name="kg_too_many_failed_attempts_countdown">"Inténtalo de nuevo en <xliff:g id="NUMBER">%d</xliff:g> segundos."</string>
    <!-- Instructions for using the pattern unlock screen -->
    <add-resource type="string" name="kg_pattern_instructions"/>
    <string name="kg_pattern_instructions">"Dibuja tu patrón de desbloqueo."</string>
    <!-- Instructions for using the SIM PIN unlock screen -->
    <add-resource type="string" name="kg_sim_pin_instructions"/>
    <string name="kg_sim_pin_instructions">"Introduce el PIN de la tarjeta SIM."</string>
    <!-- Instructions for using the SIM PIN unlock screen when there's more than one SIM -->
    <add-resource type="string" name="kg_sim_pin_instructions_multi"/>
    <string name="kg_sim_pin_instructions_multi">"Introduce el PIN de la SIM de <xliff:g id="CARRIER">%1$s</xliff:g>"</string>
    <!-- Instructions for using the PIN unlock screen -->
    <add-resource type="string" name="kg_pin_instructions"/>
    <string name="kg_pin_instructions">"Introduce el PIN."</string>
    <!-- Instructions for using the password unlock screen -->
    <add-resource type="string" name="kg_password_instructions"/>
    <string name="kg_password_instructions">"Escribe tu contraseña."</string>
    <!-- Hint shown in the PUK screen that asks the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint"/>
    <string name="kg_puk_enter_puk_hint">"La tarjeta SIM está inhabilitada. Para continuar, introduce el código PUK. Si quieres obtener más información, ponte en contacto con el operador"</string>
    <!-- Hint shown when there are multiple SIMs in the device to ask the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint_multi"/>
    <string name="kg_puk_enter_puk_hint_multi">"La SIM de <xliff:g id="CARRIER">%1$s</xliff:g> está inhabilitada. Introduce el código PUK para continuar. Ponte en contacto con el operador para obtener más información."</string>
    <!-- Hint shown in the PUK unlock screen PIN TextView -->
    <add-resource type="string" name="kg_puk_enter_pin_hint"/>
    <string name="kg_puk_enter_pin_hint">"Introduce el código PIN deseado"</string>
    <!-- Message shown when the user needs to confirm the PIN they just entered in the PUK screen -->
    <add-resource type="string" name="kg_enter_confirm_pin_hint"/>
    <string name="kg_enter_confirm_pin_hint">"Confirma el código PIN"</string>
    <!-- Message shown in dialog while the device is unlocking the SIM card -->
    <add-resource type="string" name="kg_sim_unlock_progress_dialog_message"/>
    <string name="kg_sim_unlock_progress_dialog_message">"Desbloqueando tarjeta SIM\u2026"</string>
    <!-- Message shown when the user enters an invalid SIM pin password in PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_pin_hint"/>
    <string name="kg_invalid_sim_pin_hint">"Introduce un código PIN con una longitud comprendida entre cuatro y ocho dígitos."</string>
    <!-- Message shown when the user enters an invalid PUK code in the PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_puk_hint"/>
    <string name="kg_invalid_sim_puk_hint">"El código PUK debe tener ocho números como mínimo."</string>
    <!-- Message shown when the user enters an invalid PUK code -->
    <add-resource type="string" name="kg_invalid_puk"/>
    <string name="kg_invalid_puk">"Vuelve a introducir el código PUK correcto. Si introduces un código incorrecto varias veces, se inhabilitará la tarjeta SIM."</string>
      <!-- String shown in PUK screen when PIN codes don't match -->
    <add-resource type="string" name="kg_invalid_confirm_pin_hint"/>
    <string name="kg_invalid_confirm_pin_hint" product="default">"Los códigos PIN no coinciden."</string>
    <!-- Message shown when the user exceeds the maximum number of pattern attempts -->
    <add-resource type="string" name="kg_login_too_many_attempts"/>
    <string name="kg_login_too_many_attempts">"Demasiados intentos incorrectos de crear el patrón"</string>
    <!-- Message shown in dialog when max number of attempts are reached for PIN screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_pin_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pin_attempts_dialog_message">"Has introducido un código PIN incorrecto <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. \n\nInténtalo de nuevo en <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>
    <!-- Message shown in dialog when max number of attempts are reached for password screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_password_attempts_dialog_message"/>
    <string name="kg_too_many_failed_password_attempts_dialog_message">"Has introducido una contraseña incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. \n\nInténtalo de nuevo en <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>
    <add-resource type="string" name="kg_too_many_failed_pattern_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pattern_attempts_dialog_message">"Has fallado <xliff:g id="NUMBER_0">%1$d</xliff:g> veces al dibujar tu patrón de desbloqueo. \n\nInténtalo de nuevo en <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se restablecerá el tablet, lo que borrará todos sus datos."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se restablecerá el teléfono, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. Este tablet se eliminará, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. Este teléfono se eliminará, lo que borrará todos sus datos."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se eliminará a este usuario, lo que borrará todos sus datos."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se eliminará a este usuario, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. Este usuario se eliminará, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. Este usuario se eliminará, lo que borrará todos sus datos."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se eliminará el perfil de trabajo, lo que borrará todos sus datos."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER_0">%1$d</xliff:g> veces. Si se producen <xliff:g id="NUMBER_1">%2$d</xliff:g> intentos incorrectos más, se eliminará el perfil de trabajo, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="tablet">"Has intentado desbloquear el tablet de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. El perfil de trabajo se eliminará, lo que borrará todos sus datos."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="default">"Has intentado desbloquear el teléfono de forma incorrecta <xliff:g id="NUMBER">%d</xliff:g> veces. El perfil de trabajo se eliminará, lo que borrará todos sus datos."</string>

    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="tablet">"Has fallado <xliff:g id="NUMBER_0">%1$d</xliff:g> veces al dibujar el patrón de desbloqueo. Si fallas otras <xliff:g id="NUMBER_1">%2$d</xliff:g> veces, deberás usar una cuenta de correo electrónico para desbloquear el tablet.\n\n Inténtalo de nuevo en <xliff:g id="NUMBER_2">%3$d</xliff:g> segundos."</string>
    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="default">"Has fallado <xliff:g id="NUMBER_0">%1$d</xliff:g> veces al dibujar el patrón de desbloqueo. Si fallas otras <xliff:g id="NUMBER_1">%2$d</xliff:g> veces, deberás usar una cuenta de correo electrónico para desbloquear el teléfono.\n\n Inténtalo de nuevo en <xliff:g id="NUMBER_2">%3$d</xliff:g> segundos."</string>

    <!-- Instructions telling the user that they entered the wrong SIM PIN for the last time.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_pin_code_pukked"/>
    <string name="kg_password_wrong_pin_code_pukked">"Código PIN de la tarjeta SIM incorrecto. Debes ponerte en contacto con tu operador para desbloquear el dispositivo."</string>
    <!-- Instructions telling the user that they entered the wrong SIM PIN while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Código PIN de la tarjeta SIM <xliff:g id="slotid">%d</xliff:g> incorrecto. Te queda <xliff:g id="NUMBER_0">%d</xliff:g> intento para tener que ponerte en contacto con tu operador para desbloquear el dispositivo."</item>
        <item quantity="other">"Código PIN de la tarjeta SIM <xliff:g id="slotid">%d</xliff:g> incorrecto. Te quedan <xliff:g id="NUMBER_1">%d</xliff:g> intentos."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_puk_code_dead"/>
    <string name="kg_password_wrong_puk_code_dead">"La tarjeta SIM no se puede utilizar. Ponte en contacto con tu operador."</string>
    <!-- Instructions telling the user that they entered the wrong puk while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_puk_code"/>
    <plurals name="kg_password_wrong_puk_code">
