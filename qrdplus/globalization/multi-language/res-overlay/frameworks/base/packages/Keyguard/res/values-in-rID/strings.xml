<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- App label in the manifest  -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Keyguard"</string>
    <!-- Instructions telling the user to enter their SIM PIN to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_code"/>
    <string name="keyguard_password_enter_pin_code">"Ketik kode PIN"</string>

    <!-- Instructions telling the user to enter their SIM PUK to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_puk_code"/>
    <string name="keyguard_password_enter_puk_code">"Ketik kode PIN baru dan PUK SIM"</string>

    <!-- Prompt to enter SIM PUK in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_puk_prompt"/>
    <string name="keyguard_password_enter_puk_prompt">"Kode PUK SIM"</string>
    <!-- Prompt to enter New SIM PIN in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_pin_prompt"/>
    <string name="keyguard_password_enter_pin_prompt">"Kode PIN SIM baru"</string>

    <!-- Displayed as hint in passwordEntry EditText on PasswordUnlockScreen [CHAR LIMIT=30]-->
    <add-resource type="string" name="keyguard_password_entry_touch_hint"/>
    <string name="keyguard_password_entry_touch_hint"><font size="17">"Sentuh untuk mengetikkan sandi"</font></string>

    <!-- Instructions telling the user to enter their text password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_password_code"/>
    <string name="keyguard_password_enter_password_code">"Ketik sandi untuk membuka kunci"</string>

    <!-- Instructions telling the user to enter their PIN password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_password_code"/>
    <string name="keyguard_password_enter_pin_password_code">"Ketik PIN untuk membuka kunci"</string>

    <!-- Instructions telling the user that they entered the wrong pin while trying
         to unlock the keyguard.  Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_wrong_pin_code"/>
    <string name="keyguard_password_wrong_pin_code">"Kode PIN salah."</string>

    <!-- Shown in the lock screen when there is SIM card IO error. -->
    <add-resource type="string" name="lockscreen_sim_error_message_short"/>
    <string name="lockscreen_sim_error_message_short">"Kartu Tidak Valid."</string>

    <!-- When the lock screen is showing, the phone is plugged in and the battery is fully
         charged, say that it is charged. -->
    <add-resource type="string" name="keyguard_charged"/>
    <string name="keyguard_charged">"Terisi"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, say that it's charging.  -->
    <add-resource type="string" name="keyguard_plugged_in"/>
    <string name="keyguard_plugged_in">"Mengisi daya"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a fast charger, say that it's charging fast.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_fast"/>
    <string name="keyguard_plugged_in_charging_fast">"Mengisi daya dengan cepat"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a slow charger, say that it's charging slowly.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_slowly"/>
    <string name="keyguard_plugged_in_charging_slowly">"Mengisi daya dengan lambat"</string>

    <!-- When the lock screen is showing and the battery is low, warn user to plug
         in the phone soon. -->
    <add-resource type="string" name="keyguard_low_battery"/>
    <string name="keyguard_low_battery">"Hubungkan pengisi daya."</string>

    <!-- On the keyguard screen, when pattern lock is disabled, only tell them to press menu to unlock.  This is shown in small font at the bottom. -->
    <add-resource type="string" name="keyguard_instructions_when_pattern_disabled"/>
    <string name="keyguard_instructions_when_pattern_disabled">"Tekan Menu untuk membuka."</string>

    <!-- SIM messages -->
    <!-- When the user inserts a sim card from an unsupported network, it becomes network locked -->
    <add-resource type="string" name="keyguard_network_locked_message"/>
    <string name="keyguard_network_locked_message">"Jaringan terkunci"</string>
    <!-- Shown when there is no SIM card for India RJIL. -->
    <add-resource type="string" name="keyguard_missing_sim_message_RJIL"/>
    <string name="keyguard_missing_sim_message_RJIL">"Tidak ada SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message_short"/>
    <string name="keyguard_missing_sim_message_short">"Tidak ada kartu SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="tablet">"Tidak ada kartu SIM dalam tablet."</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="default">"Tidak ada Kartu SIM di dalam ponsel."</string>
    <!-- Shown to ask the user to insert a SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions"/>
    <string name="keyguard_missing_sim_instructions">"Masukkan kartu SIM."</string>
    <!-- Shown to ask the user to insert a SIM card when sim is missing or not readable. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions_long"/>
    <string name="keyguard_missing_sim_instructions_long">"Kartu SIM tidak ada atau tidak dapat dibaca. Masukkan kartu SIM."</string>
    <!-- Shown when SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_message_short"/>
    <string name="keyguard_permanent_disabled_sim_message_short">"Kartu SIM tidak dapat digunakan."</string>
    <!-- Shown to inform the user to SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_instructions"/>
    <string name="keyguard_permanent_disabled_sim_instructions">"Kartu SIM Anda telah dinonaktifkan secara permanen.\n Hubungi penyedia layanan nirkabel Anda untuk kartu SIM lain."</string>
    <!-- Shown to tell the user that their SIM is locked and they must unlock it. -->
    <add-resource type="string" name="keyguard_sim_locked_message"/>
    <string name="keyguard_sim_locked_message">"Kartu SIM terkunci."</string>
    <!-- When the user enters a wrong sim pin too many times, it becomes PUK locked (Pin Unlock Kode) -->
    <add-resource type="string" name="keyguard_sim_puk_locked_message"/>
    <string name="keyguard_sim_puk_locked_message">"Kartu SIM terkunci PUK."</string>
    <!-- For the unlock screen, When the user enters a sim unlock code, it takes a little while to check
         whether it is valid, and to unlock the sim if it is valid.  we display a
         progress dialog in the meantime.  this is the emssage. -->
    <add-resource type="string" name="keyguard_sim_unlock_progress_dialog_message"/>
    <string name="keyguard_sim_unlock_progress_dialog_message">"Membuka kartu SIM\u2026"</string>

    <!-- Accessibility description of the PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_pin_area"/>
    <string name="keyguard_accessibility_pin_area">"Bidang PIN"</string>
    <!-- Accessibility description of the SIM PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_pin_area"/>
    <string name="keyguard_accessibility_sim_pin_area">"Bidang PIN SIM"</string>
    <!-- Accessibility description of the SIM PUK password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_puk_area"/>
    <string name="keyguard_accessibility_sim_puk_area">"Bidang PUK SIM"</string>

    <!-- Accessibility description for the text view that indicates when the next alarm is set (not shown on screen). [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_next_alarm"/>
    <string name="keyguard_accessibility_next_alarm">"Alarm berikutnya disetel untuk <xliff:g id="ALARM">%1$s</xliff:g>"</string>

    <!-- KeyguardPinView - accessibility support -->
    <!-- Description of the Delete button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_delete"/>
    <string name="keyboardview_keycode_delete">"Hapus"</string>
    <!-- Description of the Enter button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_enter"/>
    <string name="keyboardview_keycode_enter">"Enter"</string>

    <!-- Message shown in pattern unlock after some number of unsuccessful attempts -->
    <add-resource type="string" name="kg_forgot_pattern_button_text"/>
    <string name="kg_forgot_pattern_button_text">"Lupa Pola?"</string>
    <!-- Message shown when user enters wrong pattern -->
    <add-resource type="string" name="kg_wrong_pattern"/>
    <string name="kg_wrong_pattern">"Pola Salah"</string>
    <!-- Message shown when user enters wrong password -->
    <add-resource type="string" name="kg_wrong_password"/>
    <string name="kg_wrong_password">"Sandi Salah"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"PIN Salah"</string>
    <!-- Countdown message shown after too many failed unlock attempts -->
    <add-resource type="string" name="kg_too_many_failed_attempts_countdown"/>
    <string name="kg_too_many_failed_attempts_countdown">"Coba lagi dalam <xliff:g id="NUMBER">%d</xliff:g> detik."</string>
    <!-- Instructions for using the pattern unlock screen -->
    <add-resource type="string" name="kg_pattern_instructions"/>
    <string name="kg_pattern_instructions">"Gambar pola Anda"</string>
    <!-- Instructions for using the SIM PIN unlock screen -->
    <add-resource type="string" name="kg_sim_pin_instructions"/>
    <string name="kg_sim_pin_instructions">"Masukkan PIN SIM"</string>
    <!-- Instructions for using the SIM PIN unlock screen when there's more than one SIM -->
    <add-resource type="string" name="kg_sim_pin_instructions_multi"/>
    <string name="kg_sim_pin_instructions_multi">"Masukkan PIN SIM \"<xliff:g id="CARRIER">%1$s</xliff:g>\""</string>
    <!-- Instructions for using the PIN unlock screen -->
    <add-resource type="string" name="kg_pin_instructions"/>
    <string name="kg_pin_instructions">"Masukkan PIN"</string>
    <!-- Instructions for using the password unlock screen -->
    <add-resource type="string" name="kg_password_instructions"/>
    <string name="kg_password_instructions">"Masukkan Sandi"</string>
    <!-- Hint shown in the PUK screen that asks the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint"/>
    <string name="kg_puk_enter_puk_hint">"SIM telah dinonaktifkan. Masukkan kode PUK untuk melanjutkan. Hubungi operator untuk keterangan selengkapnya."</string>
    <!-- Hint shown when there are multiple SIMs in the device to ask the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint_multi"/>
    <string name="kg_puk_enter_puk_hint_multi">"SIM \"<xliff:g id="CARRIER">%1$s</xliff:g>\" kini dinonaktifkan. Masukkan kode PUK untuk melanjutkan. Hubungi operator untuk mengetahui detailnya."</string>
    <!-- Hint shown in the PUK unlock screen PIN TextView -->
    <add-resource type="string" name="kg_puk_enter_pin_hint"/>
    <string name="kg_puk_enter_pin_hint">"Masukkan kode PIN yang diinginkan"</string>
    <!-- Message shown when the user needs to confirm the PIN they just entered in the PUK screen -->
    <add-resource type="string" name="kg_enter_confirm_pin_hint"/>
    <string name="kg_enter_confirm_pin_hint">"Konfirmasi kode PIN yang diinginkan"</string>
    <!-- Message shown in dialog while the device is unlocking the SIM card -->
    <add-resource type="string" name="kg_sim_unlock_progress_dialog_message"/>
    <string name="kg_sim_unlock_progress_dialog_message">"Membuka kunci kartu SIM\u2026"</string>
    <!-- Message shown when the user enters an invalid SIM pin password in PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_pin_hint"/>
    <string name="kg_invalid_sim_pin_hint">"Ketik PIN yang terdiri dari 4 sampai 8 angka."</string>
    <!-- Message shown when the user enters an invalid PUK code in the PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_puk_hint"/>
    <string name="kg_invalid_sim_puk_hint">"Kode PUK harus terdiri dari 8 angka atau lebih."</string>
    <!-- Message shown when the user enters an invalid PUK code -->
    <add-resource type="string" name="kg_invalid_puk"/>
    <string name="kg_invalid_puk">"Masukkan kembali kode PUK yang benar. Jika berulang kali gagal, SIM akan dinonaktifkan secara permanen."</string>
      <!-- String shown in PUK screen when PIN codes don't match -->
    <add-resource type="string" name="kg_invalid_confirm_pin_hint"/>
    <string name="kg_invalid_confirm_pin_hint" product="default">"Kode PIN tidak cocok"</string>
    <!-- Message shown when the user exceeds the maximum number of pattern attempts -->
    <add-resource type="string" name="kg_login_too_many_attempts"/>
    <string name="kg_login_too_many_attempts">"Terlalu banyak upaya pola"</string>
    <!-- Message shown in dialog when max number of attempts are reached for PIN screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_pin_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pin_attempts_dialog_message">"Anda telah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali salah mengetik PIN. \n\nCoba lagi dalam <xliff:g id="NUMBER_1">%2$d</xliff:g> detik."</string>
    <!-- Message shown in dialog when max number of attempts are reached for password screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_password_attempts_dialog_message"/>
    <string name="kg_too_many_failed_password_attempts_dialog_message">"Anda telah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali salah mengetik sandi. \n\nCoba lagi dalam <xliff:g id="NUMBER_1">%2$d</xliff:g> detik."</string>
    <add-resource type="string" name="kg_too_many_failed_pattern_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pattern_attempts_dialog_message">"Anda telah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali salah menggambar pola pembuka kunci. \n\nCoba lagi dalam <xliff:g id="NUMBER_1">%2$d</xliff:g> detik."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="tablet">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, tablet ini akan disetel ulang, sehingga semua datanya akan dihapus."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="default">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, ponsel ini akan disetel ulang, sehingga semua datanya akan dihapus."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="tablet">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Tablet ini akan disetel ulang, sehingga menghapus semua datanya."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="default">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Ponsel ini akan disetel ulang, sehingga menghapus semua datanya."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="tablet">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, pengguna ini akan dihapus, sehingga semua data pengguna akan dihapus."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="default">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, pengguna ini akan dihapus, sehingga semua data pengguna akan dihapus."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="tablet">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Pengguna ini akan dihapus, sehingga menghapus semua data pengguna."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="default">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Pengguna ini akan dihapus, sehingga menghapus semua data pengguna."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="tablet">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, profil kerja akan dihapus, sehingga semua data profil akan dihapus."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="default">"Sudah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya yang tidak berhasil, profil kerja akan dihapus, sehingga semua data profil akan dihapus."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="tablet">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci tablet dengan tidak benar. Profil kerja akan dihapus, sehingga menghapus semua data profil."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="default">"Sudah <xliff:g id="NUMBER">%d</xliff:g> kali Anda berupaya membuka kunci ponsel dengan tidak benar. Profil kerja akan dihapus, sehingga menghapus semua data profil."</string>

    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="tablet">"Anda telah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali salah menggambar pola pembuka kunci. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya gagal, Anda akan diminta membuka kunci tablet menggunakan akun email.\n\nCoba lagi dalam <xliff:g id="NUMBER_2">%3$d</xliff:g> detik."</string>
    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="default">"Anda telah <xliff:g id="NUMBER_0">%1$d</xliff:g> kali salah menggambar pola pembuka kunci. Setelah <xliff:g id="NUMBER_1">%2$d</xliff:g> lagi upaya gagal, Anda akan diminta membuka kunci ponsel menggunakan akun email.\n\nCoba lagi dalam <xliff:g id="NUMBER_2">%3$d</xliff:g> detik."</string>

    <!-- Instructions telling the user that they entered the wrong SIM PIN for the last time.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_pin_code_pukked"/>
    <string name="kg_password_wrong_pin_code_pukked">"Kode PIN SIM salah. Hubungi operator untuk membuka kunci perangkat."</string>
    <!-- Instructions telling the user that they entered the wrong SIM PIN while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Kode PIN SIM <xliff:g id="slotid">%d</xliff:g> salah, sisa <xliff:g id="NUMBER_0">%d</xliff:g> percobaan sebelum Anda harus menghubungi operator untuk membuka kunci perangkat."</item>
        <item quantity="other">"Kode PIN SIM <xliff:g id="slotid">%d</xliff:g> salah, kesempatan Anda <xliff:g id="NUMBER_1">%d</xliff:g> kali lagi."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_puk_code_dead"/>
    <string name="kg_password_wrong_puk_code_dead">"SIM tidak dapat digunakan. Hubungi operator Anda."</string>
    <!-- Instructions telling the user that they entered the wrong puk while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_puk_code"/>
