<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- App label in the manifest  -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Bloqueio do teclado"</string>
    <!-- Instructions telling the user to enter their SIM PIN to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_code"/>
    <string name="keyguard_password_enter_pin_code">"Insira o código PIN"</string>

    <!-- Instructions telling the user to enter their SIM PUK to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_puk_code"/>
    <string name="keyguard_password_enter_puk_code">"Digite o PUK do SIM e o novo código PIN."</string>

    <!-- Prompt to enter SIM PUK in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_puk_prompt"/>
    <string name="keyguard_password_enter_puk_prompt">"Código PUK do SIM"</string>
    <!-- Prompt to enter New SIM PIN in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_pin_prompt"/>
    <string name="keyguard_password_enter_pin_prompt">"Novo código PIN do SIM"</string>

    <!-- Displayed as hint in passwordEntry EditText on PasswordUnlockScreen [CHAR LIMIT=30]-->
    <add-resource type="string" name="keyguard_password_entry_touch_hint"/>
    <string name="keyguard_password_entry_touch_hint"><font size="17">"Toque para inserir a senha"</font></string>

    <!-- Instructions telling the user to enter their text password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_password_code"/>
    <string name="keyguard_password_enter_password_code">"Digite a senha para desbloquear"</string>

    <!-- Instructions telling the user to enter their PIN password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_password_code"/>
    <string name="keyguard_password_enter_pin_password_code">"Insira o PIN para desbloquear"</string>

    <!-- Instructions telling the user that they entered the wrong pin while trying
         to unlock the keyguard.  Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_wrong_pin_code"/>
    <string name="keyguard_password_wrong_pin_code">"Código PIN incorreto."</string>

    <!-- Shown in the lock screen when there is SIM card IO error. -->
    <add-resource type="string" name="lockscreen_sim_error_message_short"/>
    <string name="lockscreen_sim_error_message_short">"Cartão não válido."</string>

    <!-- When the lock screen is showing, the phone is plugged in and the battery is fully
         charged, say that it is charged. -->
    <add-resource type="string" name="keyguard_charged"/>
    <string name="keyguard_charged">"Carregado"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, say that it's charging.  -->
    <add-resource type="string" name="keyguard_plugged_in"/>
    <string name="keyguard_plugged_in">"Carregando"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a fast charger, say that it's charging fast.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_fast"/>
    <string name="keyguard_plugged_in_charging_fast">"Carregando rapidamente"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a slow charger, say that it's charging slowly.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_slowly"/>
    <string name="keyguard_plugged_in_charging_slowly">"Carregando lentamente"</string>

    <!-- When the lock screen is showing and the battery is low, warn user to plug
         in the phone soon. -->
    <add-resource type="string" name="keyguard_low_battery"/>
    <string name="keyguard_low_battery">"Conecte seu carregador."</string>

    <!-- On the keyguard screen, when pattern lock is disabled, only tell them to press menu to unlock.  This is shown in small font at the bottom. -->
    <add-resource type="string" name="keyguard_instructions_when_pattern_disabled"/>
    <string name="keyguard_instructions_when_pattern_disabled">"Pressione \"Menu\" para desbloquear."</string>

    <!-- SIM messages -->
    <!-- When the user inserts a sim card from an unsupported network, it becomes network locked -->
    <add-resource type="string" name="keyguard_network_locked_message"/>
    <string name="keyguard_network_locked_message">"Rede bloqueada"</string>
    <!-- Shown when there is no SIM card for India RJIL. -->
    <add-resource type="string" name="keyguard_missing_sim_message_RJIL"/>
    <string name="keyguard_missing_sim_message_RJIL">"Sem SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message_short"/>
    <string name="keyguard_missing_sim_message_short">"Sem cartão SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="tablet">"Não há um cartão SIM no tablet."</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="default">"Não há um cartão SIM no telefone."</string>
    <!-- Shown to ask the user to insert a SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions"/>
    <string name="keyguard_missing_sim_instructions">"Insira um cartão SIM."</string>
    <!-- Shown to ask the user to insert a SIM card when sim is missing or not readable. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions_long"/>
    <string name="keyguard_missing_sim_instructions_long">"O cartão SIM não foi inserido ou não é possível lê-lo. Insira um cartão SIM."</string>
    <!-- Shown when SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_message_short"/>
    <string name="keyguard_permanent_disabled_sim_message_short">"Cartão SIM inutilizável."</string>
    <!-- Shown to inform the user to SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_instructions"/>
    <string name="keyguard_permanent_disabled_sim_instructions">"O cartão SIM foi desativado permanentemente.\nEntre em contato com seu provedor de serviços sem fio para receber outro cartão SIM."</string>
    <!-- Shown to tell the user that their SIM is locked and they must unlock it. -->
    <add-resource type="string" name="keyguard_sim_locked_message"/>
    <string name="keyguard_sim_locked_message">"O cartão SIM está bloqueado."</string>
    <!-- When the user enters a wrong sim pin too many times, it becomes PUK locked (Pin Unlock Kode) -->
    <add-resource type="string" name="keyguard_sim_puk_locked_message"/>
    <string name="keyguard_sim_puk_locked_message">"O cartão SIM está bloqueado pelo PUK."</string>
    <!-- For the unlock screen, When the user enters a sim unlock code, it takes a little while to check
         whether it is valid, and to unlock the sim if it is valid.  we display a
         progress dialog in the meantime.  this is the emssage. -->
    <add-resource type="string" name="keyguard_sim_unlock_progress_dialog_message"/>
    <string name="keyguard_sim_unlock_progress_dialog_message">"Desbloqueando o cartão SIM\u2026"</string>

    <!-- Accessibility description of the PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_pin_area"/>
    <string name="keyguard_accessibility_pin_area">"Área do PIN"</string>
    <!-- Accessibility description of the SIM PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_pin_area"/>
    <string name="keyguard_accessibility_sim_pin_area">"Área do PIN SIM"</string>
    <!-- Accessibility description of the SIM PUK password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_puk_area"/>
    <string name="keyguard_accessibility_sim_puk_area">"Área do PUK SIM"</string>

    <!-- Accessibility description for the text view that indicates when the next alarm is set (not shown on screen). [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_next_alarm"/>
    <string name="keyguard_accessibility_next_alarm">"Próximo alarme definido para <xliff:g id="ALARM">%1$s</xliff:g>"</string>

    <!-- KeyguardPinView - accessibility support -->
    <!-- Description of the Delete button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_delete"/>
    <string name="keyboardview_keycode_delete">"Excluir"</string>
    <!-- Description of the Enter button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_enter"/>
    <string name="keyboardview_keycode_enter">"Enter"</string>

    <!-- Message shown in pattern unlock after some number of unsuccessful attempts -->
    <add-resource type="string" name="kg_forgot_pattern_button_text"/>
    <string name="kg_forgot_pattern_button_text">"Esqueci o padrão"</string>
    <!-- Message shown when user enters wrong pattern -->
    <add-resource type="string" name="kg_wrong_pattern"/>
    <string name="kg_wrong_pattern">"Padrão incorreto"</string>
    <!-- Message shown when user enters wrong password -->
    <add-resource type="string" name="kg_wrong_password"/>
    <string name="kg_wrong_password">"Senha incorreta"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"PIN incorreto"</string>
    <!-- Countdown message shown after too many failed unlock attempts -->
    <add-resource type="string" name="kg_too_many_failed_attempts_countdown"/>
    <string name="kg_too_many_failed_attempts_countdown">"Tente novamente em <xliff:g id="NUMBER">%d</xliff:g> segundos."</string>
    <!-- Instructions for using the pattern unlock screen -->
    <add-resource type="string" name="kg_pattern_instructions"/>
    <string name="kg_pattern_instructions">"Desenhe seu padrão"</string>
    <!-- Instructions for using the SIM PIN unlock screen -->
    <add-resource type="string" name="kg_sim_pin_instructions"/>
    <string name="kg_sim_pin_instructions">"Digite o PIN do cartão SIM"</string>
    <!-- Instructions for using the SIM PIN unlock screen when there's more than one SIM -->
    <add-resource type="string" name="kg_sim_pin_instructions_multi"/>
    <string name="kg_sim_pin_instructions_multi">"Insira o PIN do SIM para \"<xliff:g id="CARRIER">%1$s</xliff:g>\""</string>
    <!-- Instructions for using the PIN unlock screen -->
    <add-resource type="string" name="kg_pin_instructions"/>
    <string name="kg_pin_instructions">"Digite o PIN"</string>
    <!-- Instructions for using the password unlock screen -->
    <add-resource type="string" name="kg_password_instructions"/>
    <string name="kg_password_instructions">"Digite a senha"</string>
    <!-- Hint shown in the PUK screen that asks the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint"/>
    <string name="kg_puk_enter_puk_hint">"O SIM foi desativado. Insira o código PUK para continuar. Entre em contato com a operadora para obter mais detalhes."</string>
    <!-- Hint shown when there are multiple SIMs in the device to ask the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint_multi"/>
    <string name="kg_puk_enter_puk_hint_multi">"O SIM \"<xliff:g id="CARRIER">%1$s</xliff:g>\" foi desativado. Insira o código PUK para continuar. Entre em contato com a operadora para saber mais detalhes."</string>
    <!-- Hint shown in the PUK unlock screen PIN TextView -->
    <add-resource type="string" name="kg_puk_enter_pin_hint"/>
    <string name="kg_puk_enter_pin_hint">"Digite o código PIN desejado"</string>
    <!-- Message shown when the user needs to confirm the PIN they just entered in the PUK screen -->
    <add-resource type="string" name="kg_enter_confirm_pin_hint"/>
    <string name="kg_enter_confirm_pin_hint">"Confirme o código PIN desejado"</string>
    <!-- Message shown in dialog while the device is unlocking the SIM card -->
    <add-resource type="string" name="kg_sim_unlock_progress_dialog_message"/>
    <string name="kg_sim_unlock_progress_dialog_message">"Desbloqueando o cartão SIM\u2026"</string>
    <!-- Message shown when the user enters an invalid SIM pin password in PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_pin_hint"/>
    <string name="kg_invalid_sim_pin_hint">"Digite um PIN com quatro a oito números."</string>
    <!-- Message shown when the user enters an invalid PUK code in the PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_puk_hint"/>
    <string name="kg_invalid_sim_puk_hint">"O código PUK deve ter 8 números ou mais."</string>
    <!-- Message shown when the user enters an invalid PUK code -->
    <add-resource type="string" name="kg_invalid_puk"/>
    <string name="kg_invalid_puk">"Introduza novamente o código PUK correto. Muitas tentativas malsucedidas desativarão permanentemente o SIM."</string>
      <!-- String shown in PUK screen when PIN codes don't match -->
    <add-resource type="string" name="kg_invalid_confirm_pin_hint"/>
    <string name="kg_invalid_confirm_pin_hint" product="default">"Os códigos PIN não coincidem"</string>
    <!-- Message shown when the user exceeds the maximum number of pattern attempts -->
    <add-resource type="string" name="kg_login_too_many_attempts"/>
    <string name="kg_login_too_many_attempts">"Muitas tentativas de padrão"</string>
    <!-- Message shown in dialog when max number of attempts are reached for PIN screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_pin_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pin_attempts_dialog_message">"Você digitou seu PIN incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. \n\nTente novamente em <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>
    <!-- Message shown in dialog when max number of attempts are reached for password screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_password_attempts_dialog_message"/>
    <string name="kg_too_many_failed_password_attempts_dialog_message">"Você digitou sua senha incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. \n\nTente novamente em <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>
    <add-resource type="string" name="kg_too_many_failed_pattern_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pattern_attempts_dialog_message">"Você desenhou sua sequência de desbloqueio incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. \n\nTente novamente em <xliff:g id="NUMBER_1">%2$d</xliff:g> segundos."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas, este tablet será redefinido, o que excluirá todos os seus dados."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas, este smartphone será redefinido, o que excluirá todos os seus dados."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. Este tablet será redefinido, o que excluirá todos os seus dados."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. Este smartphone será redefinido, o que excluirá todos os seus dados."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas, este usuário será removido, o que excluirá todos os dados do usuário."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas, este usuário será removido, o que excluirá todos os dados do usuário."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. Este usuário será removido, o que excluirá todos os dados do usuário."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. Este usuário será removido, o que excluirá todos os dados do usuário."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas,o perfil de trabalho será removido, o que excluirá todos os dados do perfil."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Após mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas malsucedidas, o perfil de trabalho será removido, o que excluirá todos os dados do perfil."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="tablet">"Você tentou desbloquear o tablet incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. O perfil de trabalho será removido, o que excluirá todos os dados do perfil."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="default">"Você tentou desbloquear o smartphone incorretamente <xliff:g id="NUMBER">%d</xliff:g> vezes. O perfil de trabalho será removido, o que excluirá todos os dados do perfil."</string>

    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="tablet">"Você desenhou sua sequência de desbloqueio incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Se fizer mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas incorretas, será solicitado que você use o login do Google para desbloquear seu tablet.\n\n Tente novamente em <xliff:g id="NUMBER_2">%3$d</xliff:g> segundos."</string>
    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="default">"Você desenhou sua sequência de desbloqueio incorretamente <xliff:g id="NUMBER_0">%1$d</xliff:g> vezes. Se fizer mais <xliff:g id="NUMBER_1">%2$d</xliff:g> tentativas incorretas, será solicitado que você use o login do Google para desbloquear.\n\n Tente novamente em <xliff:g id="NUMBER_2">%3$d</xliff:g> segundos."</string>

    <!-- Instructions telling the user that they entered the wrong SIM PIN for the last time.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_pin_code_pukked"/>
    <string name="kg_password_wrong_pin_code_pukked">"Código PIN do SIM incorreto. Entre em contato com a operadora para desbloquear o dispositivo."</string>
    <!-- Instructions telling the user that they entered the wrong SIM PIN while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Código PIN do SIM <xliff:g id="slotid">%d</xliff:g> incorreto. Tentativas restantes: <xliff:g id="NUMBER_1">%d</xliff:g>."</item>
        <item quantity="other">"Código PIN do SIM <xliff:g id="slotid">%d</xliff:g> incorreto. Tentativas restantes: <xliff:g id="NUMBER_1">%d</xliff:g>."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_puk_code_dead"/>
    <string name="kg_password_wrong_puk_code_dead">"O SIM está inutilizável. Entre em contato com a operadora."</string>
    <!-- Instructions telling the user that they entered the wrong puk while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_puk_code"/>
    <plurals name="kg_password_wrong_puk_code">
        <item quantity="one">"Código PUK do SIM incorreto. Tentativas restantes: <xliff:g id="NUMBER">%d</xliff:g> Caso o código correto não seja digitado, o SIM se tornará permanentemente inutilizável."</item>
        <item quantity="other">"Código PUK do SIM incorreto. Tentativas restantes: <xliff:g id="NUMBER">%d</xliff:g>. Caso o código correto não seja digitado, o SIM se tornará permanentemente inutilizável."</item>
    </plurals>
    <!-- Instructions telling the user that the operation to unlock the keyguard
