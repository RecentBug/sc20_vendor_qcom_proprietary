    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Falscher PIN-Code der SIM-Karte <xliff:g id="slotid">%d</xliff:g>. Du hast noch <xliff:g id="NUMBER_0">%d</xliff:g> Versuch, bevor du das Gerät von deinem Mobilfunkanbieter entsperren lassen musst."</item>
        <item quantity="other">"Falscher PIN-Code der SIM-Karte <xliff:g id="slotid">%d</xliff:g>. Du hast noch <xliff:g id="NUMBER_1">%d</xliff:g> Versuche."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_puk_code_dead"/>
    <string name="kg_password_wrong_puk_code_dead">"Die SIM-Karte kann nicht verwendet werden. Bitte wenden Sie sich an Ihren Mobilfunkanbieter."</string>
    <!-- Instructions telling the user that they entered the wrong puk while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_puk_code"/>
