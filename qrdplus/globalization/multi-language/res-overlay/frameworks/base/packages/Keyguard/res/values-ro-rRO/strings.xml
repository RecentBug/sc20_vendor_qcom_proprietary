<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- App label in the manifest  -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Blocarea tastaturii"</string>
    <!-- Instructions telling the user to enter their SIM PIN to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_code"/>
    <string name="keyguard_password_enter_pin_code">"Introduceţi codul PIN"</string>

    <!-- Instructions telling the user to enter their SIM PUK to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_puk_code"/>
    <string name="keyguard_password_enter_puk_code">"Introduceți codul PUK pentru cardul SIM și codul PIN nou"</string>

    <!-- Prompt to enter SIM PUK in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_puk_prompt"/>
    <string name="keyguard_password_enter_puk_prompt">"Codul PUK pentru cardul SIM"</string>
    <!-- Prompt to enter New SIM PIN in Edit Text Box in unlock screen -->
    <add-resource type="string" name="keyguard_password_enter_pin_prompt"/>
    <string name="keyguard_password_enter_pin_prompt">"Codul PIN nou pentru cardul SIM"</string>

    <!-- Displayed as hint in passwordEntry EditText on PasswordUnlockScreen [CHAR LIMIT=30]-->
    <add-resource type="string" name="keyguard_password_entry_touch_hint"/>
    <string name="keyguard_password_entry_touch_hint"><font size="17">"Atingeţi şi introduceţi parola"</font></string>

    <!-- Instructions telling the user to enter their text password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_password_code"/>
    <string name="keyguard_password_enter_password_code">"Introduceţi parola pentru a debloca"</string>

    <!-- Instructions telling the user to enter their PIN password to unlock the keyguard.
         Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_enter_pin_password_code"/>
    <string name="keyguard_password_enter_pin_password_code">"Introduceţi codul PIN pentru a debloca"</string>

    <!-- Instructions telling the user that they entered the wrong pin while trying
         to unlock the keyguard.  Displayed in one line in a large font.  -->
    <add-resource type="string" name="keyguard_password_wrong_pin_code"/>
    <string name="keyguard_password_wrong_pin_code">"Cod PIN incorect."</string>

    <!-- Shown in the lock screen when there is SIM card IO error. -->
    <add-resource type="string" name="lockscreen_sim_error_message_short"/>
    <string name="lockscreen_sim_error_message_short">"Card nevalid."</string>

    <!-- When the lock screen is showing, the phone is plugged in and the battery is fully
         charged, say that it is charged. -->
    <add-resource type="string" name="keyguard_charged"/>
    <string name="keyguard_charged">"Încărcată"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, say that it's charging.  -->
    <add-resource type="string" name="keyguard_plugged_in"/>
    <string name="keyguard_plugged_in">"Se încarcă"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a fast charger, say that it's charging fast.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_fast"/>
    <string name="keyguard_plugged_in_charging_fast">"Încărcare rapidă"</string>

    <!-- When the lock screen is showing and the phone plugged in, and the battery
         is not fully charged, and it's plugged into a slow charger, say that it's charging slowly.  -->
    <add-resource type="string" name="keyguard_plugged_in_charging_slowly"/>
    <string name="keyguard_plugged_in_charging_slowly">"Încărcare lentă"</string>

    <!-- When the lock screen is showing and the battery is low, warn user to plug
         in the phone soon. -->
    <add-resource type="string" name="keyguard_low_battery"/>
    <string name="keyguard_low_battery">"Conectați încărcătorul."</string>

    <!-- On the keyguard screen, when pattern lock is disabled, only tell them to press menu to unlock.  This is shown in small font at the bottom. -->
    <add-resource type="string" name="keyguard_instructions_when_pattern_disabled"/>
    <string name="keyguard_instructions_when_pattern_disabled">"Apăsați pe Meniu pentru a debloca."</string>

    <!-- SIM messages -->
    <!-- When the user inserts a sim card from an unsupported network, it becomes network locked -->
    <add-resource type="string" name="keyguard_network_locked_message"/>
    <string name="keyguard_network_locked_message">"Rețea blocată"</string>
    <!-- Shown when there is no SIM card for India RJIL. -->
    <add-resource type="string" name="keyguard_missing_sim_message_RJIL"/>
    <string name="keyguard_missing_sim_message_RJIL">"Niciun card SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message_short"/>
    <string name="keyguard_missing_sim_message_short">"Niciun card SIM"</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="tablet">"Tableta nu are card SIM."</string>
    <!-- Shown when there is no SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_message"/>
    <string name="keyguard_missing_sim_message" product="default">"Telefonul nu are card SIM."</string>
    <!-- Shown to ask the user to insert a SIM card. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions"/>
    <string name="keyguard_missing_sim_instructions">"Introduceți un card SIM."</string>
    <!-- Shown to ask the user to insert a SIM card when sim is missing or not readable. -->
    <add-resource type="string" name="keyguard_missing_sim_instructions_long"/>
    <string name="keyguard_missing_sim_instructions_long">"Cardul SIM lipsește sau nu poate fi citit. Introduceți un card SIM."</string>
    <!-- Shown when SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_message_short"/>
    <string name="keyguard_permanent_disabled_sim_message_short">"Card SIM inutilizabil."</string>
    <!-- Shown to inform the user to SIM card is permanently disabled. -->
    <add-resource type="string" name="keyguard_permanent_disabled_sim_instructions"/>
    <string name="keyguard_permanent_disabled_sim_instructions">"Cardul SIM este dezactivat definitiv.\n Contactați furnizorul de servicii wireless pentru a obține alt card SIM."</string>
    <!-- Shown to tell the user that their SIM is locked and they must unlock it. -->
    <add-resource type="string" name="keyguard_sim_locked_message"/>
    <string name="keyguard_sim_locked_message">"Cardul SIM este blocat."</string>
    <!-- When the user enters a wrong sim pin too many times, it becomes PUK locked (Pin Unlock Kode) -->
    <add-resource type="string" name="keyguard_sim_puk_locked_message"/>
    <string name="keyguard_sim_puk_locked_message">"Cardul SIM este blocat cu codul PUK."</string>
    <!-- For the unlock screen, When the user enters a sim unlock code, it takes a little while to check
         whether it is valid, and to unlock the sim if it is valid.  we display a
         progress dialog in the meantime.  this is the emssage. -->
    <add-resource type="string" name="keyguard_sim_unlock_progress_dialog_message"/>
    <string name="keyguard_sim_unlock_progress_dialog_message">"Se deblochează cardul SIM\u2026"</string>

    <!-- Accessibility description of the PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_pin_area"/>
    <string name="keyguard_accessibility_pin_area">"Zona codului PIN"</string>
    <!-- Accessibility description of the SIM PIN password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_pin_area"/>
    <string name="keyguard_accessibility_sim_pin_area">"Zona codului PIN al cardului SIM"</string>
    <!-- Accessibility description of the SIM PUK password view. [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_sim_puk_area"/>
    <string name="keyguard_accessibility_sim_puk_area">"Zona codului PUK al cardului SIM"</string>

    <!-- Accessibility description for the text view that indicates when the next alarm is set (not shown on screen). [CHAR_LIMIT=none] -->
    <add-resource type="string" name="keyguard_accessibility_next_alarm"/>
    <string name="keyguard_accessibility_next_alarm">"Următoarea alarmă este setată la <xliff:g id="ALARM">%1$s</xliff:g>"</string>

    <!-- KeyguardPinView - accessibility support -->
    <!-- Description of the Delete button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_delete"/>
    <string name="keyboardview_keycode_delete">"Ștergeţi"</string>
    <!-- Description of the Enter button in a KeyboardView. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="keyboardview_keycode_enter"/>
    <string name="keyboardview_keycode_enter">"Enter"</string>

    <!-- Message shown in pattern unlock after some number of unsuccessful attempts -->
    <add-resource type="string" name="kg_forgot_pattern_button_text"/>
    <string name="kg_forgot_pattern_button_text">"Model uitat"</string>
    <!-- Message shown when user enters wrong pattern -->
    <add-resource type="string" name="kg_wrong_pattern"/>
    <string name="kg_wrong_pattern">"Model greşit"</string>
    <!-- Message shown when user enters wrong password -->
    <add-resource type="string" name="kg_wrong_password"/>
    <string name="kg_wrong_password">"Parolă greşită"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"Cod PIN greşit"</string>
    <!-- Countdown message shown after too many failed unlock attempts -->
    <add-resource type="string" name="kg_too_many_failed_attempts_countdown"/>
    <string name="kg_too_many_failed_attempts_countdown">"Încercaţi din nou peste <xliff:g id="NUMBER">%d</xliff:g> (de) secunde."</string>
    <!-- Instructions for using the pattern unlock screen -->
    <add-resource type="string" name="kg_pattern_instructions"/>
    <string name="kg_pattern_instructions">"Desenaţi modelul"</string>
    <!-- Instructions for using the SIM PIN unlock screen -->
    <add-resource type="string" name="kg_sim_pin_instructions"/>
    <string name="kg_sim_pin_instructions">"Introduceţi codul PIN al cardului SIM"</string>
    <!-- Instructions for using the SIM PIN unlock screen when there's more than one SIM -->
    <add-resource type="string" name="kg_sim_pin_instructions_multi"/>
    <string name="kg_sim_pin_instructions_multi">"Introduceți codul PIN al cardului SIM pentru „<xliff:g id="CARRIER">%1$s</xliff:g>”"</string>
    <!-- Instructions for using the PIN unlock screen -->
    <add-resource type="string" name="kg_pin_instructions"/>
    <string name="kg_pin_instructions">"Introduceţi codul PIN"</string>
    <!-- Instructions for using the password unlock screen -->
    <add-resource type="string" name="kg_password_instructions"/>
    <string name="kg_password_instructions">"Introduceţi parola"</string>
    <!-- Hint shown in the PUK screen that asks the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint"/>
    <string name="kg_puk_enter_puk_hint">"Cardul SIM este acum dezactivat. Introduceţi codul PUK pentru a continua. Contactaţi operatorul pentru mai multe detalii."</string>
    <!-- Hint shown when there are multiple SIMs in the device to ask the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint_multi"/>
    <string name="kg_puk_enter_puk_hint_multi">"Cardul SIM „<xliff:g id="CARRIER">%1$s</xliff:g>” este acum dezactivat. Pentru a continua, introduceți codul PUK. Pentru detalii, contactați operatorul."</string>
    <!-- Hint shown in the PUK unlock screen PIN TextView -->
    <add-resource type="string" name="kg_puk_enter_pin_hint"/>
    <string name="kg_puk_enter_pin_hint">"Introduceţi codul PIN dorit"</string>
    <!-- Message shown when the user needs to confirm the PIN they just entered in the PUK screen -->
    <add-resource type="string" name="kg_enter_confirm_pin_hint"/>
    <string name="kg_enter_confirm_pin_hint">"Confirmaţi codul PIN dorit"</string>
    <!-- Message shown in dialog while the device is unlocking the SIM card -->
    <add-resource type="string" name="kg_sim_unlock_progress_dialog_message"/>
    <string name="kg_sim_unlock_progress_dialog_message">"Se deblochează cardul SIM..."</string>
    <!-- Message shown when the user enters an invalid SIM pin password in PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_pin_hint"/>
    <string name="kg_invalid_sim_pin_hint">"Introduceţi un cod PIN format din 4 până la 8 cifre."</string>
    <!-- Message shown when the user enters an invalid PUK code in the PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_puk_hint"/>
    <string name="kg_invalid_sim_puk_hint">"Codul PUK trebuie să aibă minimum 8 cifre."</string>
    <!-- Message shown when the user enters an invalid PUK code -->
    <add-resource type="string" name="kg_invalid_puk"/>
    <string name="kg_invalid_puk">"Reintroduceţi codul PUK corect. Încercările repetate vor dezactiva definitiv cardul SIM."</string>
      <!-- String shown in PUK screen when PIN codes don't match -->
    <add-resource type="string" name="kg_invalid_confirm_pin_hint"/>
    <string name="kg_invalid_confirm_pin_hint" product="default">"Codurile PIN nu coincid"</string>
    <!-- Message shown when the user exceeds the maximum number of pattern attempts -->
    <add-resource type="string" name="kg_login_too_many_attempts"/>
    <string name="kg_login_too_many_attempts">"Prea multe încercări de desenare a modelului"</string>
    <!-- Message shown in dialog when max number of attempts are reached for PIN screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_pin_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pin_attempts_dialog_message">"Ați introdus incorect codul PIN de <xliff:g id="NUMBER_0">%1$d</xliff:g> ori.\n\nÎncercați din nou peste <xliff:g id="NUMBER_1">%2$d</xliff:g> (de) secunde."</string>
    <!-- Message shown in dialog when max number of attempts are reached for password screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_password_attempts_dialog_message"/>
    <string name="kg_too_many_failed_password_attempts_dialog_message">"Ați introdus incorect parola de <xliff:g id="NUMBER_0">%1$d</xliff:g> ori. \n\nÎncercați din nou peste <xliff:g id="NUMBER_1">%2$d</xliff:g> (de) secunde."</string>
    <add-resource type="string" name="kg_too_many_failed_pattern_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pattern_attempts_dialog_message">"Ați desenat incorect modelul pentru deblocare de <xliff:g id="NUMBER_0">%1$d</xliff:g> ori. \n\nÎncercați din nou peste <xliff:g id="NUMBER_1">%2$d</xliff:g> (de) secunde."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="tablet">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a tabletei. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, această tabletă va fi resetată, iar toate datele acesteia vor fi șterse."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="default">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a telefonului. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, acest telefon va fi resetat, iar toate datele acestuia vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="tablet">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a tabletei. Această tabletă va fi resetată, iar toate datele acesteia vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="default">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a telefonului. Acest telefon va fi resetat, iar toate datele acestuia vor fi șterse."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="tablet">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a tabletei. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, acest utilizator va fi eliminat, iar toate datele utilizatorului vor fi șterse."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="default">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a telefonului. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, acest utilizator va fi eliminat, iar toate datele utilizatorului vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="tablet">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a tabletei. Acest utilizator va fi eliminat, iar toate datele utilizatorului vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="default">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a telefonului. Acest utilizator va fi eliminat, iar toate datele utilizatorului vor fi șterse."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="tablet">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a tabletei. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, profilul de serviciu va fi eliminat, iar toate datele profilului vor fi șterse."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="default">"Ați efectuat <xliff:g id="NUMBER_0">%1$d</xliff:g> încercări incorecte de deblocare a telefonului. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, profilul de serviciu va fi eliminat, iar toate datele profilului vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="tablet">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a tabletei. Profilul de serviciu va fi eliminat, iar toate datele profilului vor fi șterse."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="default">"Ați efectuat <xliff:g id="NUMBER">%d</xliff:g> încercări incorecte de deblocare a telefonului. Profilul de serviciu va fi eliminat, iar toate datele profilului vor fi șterse."</string>

    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="tablet">"Ați desenat incorect modelul pentru deblocare de <xliff:g id="NUMBER_0">%1$d</xliff:g> ori. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, vi se va solicita să deblocați tableta cu ajutorul unui cont de e-mail.\n\n Încercați din nou peste <xliff:g id="NUMBER_2">%3$d</xliff:g> (de) secunde."</string>
    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="default">"Ați desenat incorect modelul pentru deblocare de <xliff:g id="NUMBER_0">%1$d</xliff:g> ori. După încă <xliff:g id="NUMBER_1">%2$d</xliff:g> încercări nereușite, vi se va solicita să deblocați telefonul cu ajutorul unui cont de e-mail.\n\n Încercați din nou peste <xliff:g id="NUMBER_2">%3$d</xliff:g> (de) secunde."</string>

    <!-- Instructions telling the user that they entered the wrong SIM PIN for the last time.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_pin_code_pukked"/>
    <string name="kg_password_wrong_pin_code_pukked">"Codul PIN pentru cardul SIM este incorect. Contactați operatorul pentru a vă debloca dispozitivul."</string>
    <!-- Instructions telling the user that they entered the wrong SIM PIN while trying
         to unlock the keyguard.  Displayed in a dialog box.  -->
    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Codul PIN pentru cardul SIM <xliff:g id="slotid">%d</xliff:g> este incorect. V-a mai rămas <xliff:g id="NUMBER_0">%d</xliff:g> încercare, după care va trebui să contactați operatorul pentru a vă debloca dispozitivul."</item>
        <item quantity="other">"Codul PIN pentru cardul SIM <xliff:g id="slotid">%d</xliff:g> este incorect. V-au mai rămas <xliff:g id="NUMBER_1">%d</xliff:g> de încercări."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
         Displayed in a dialog box.  -->
    <add-resource type="string" name="kg_password_wrong_puk_code_dead"/>
    <string name="kg_password_wrong_puk_code_dead">"Cardul SIM nu poate fi utilizat. Contactați operatorul."</string>
