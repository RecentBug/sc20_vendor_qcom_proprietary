    <add-resource type="string" name="kg_password_wrong_pin_code_pukked"/>
    <string name="kg_password_wrong_pin_code_pukked">"Yanlış SIM PIN kodu. Cihazınızın kilidini açmak için artık operatörünüzle bağlantı kurmanız gerekiyor."</string>
    <!-- Instructions telling the user that they entered the wrong SIM PIN while trying
