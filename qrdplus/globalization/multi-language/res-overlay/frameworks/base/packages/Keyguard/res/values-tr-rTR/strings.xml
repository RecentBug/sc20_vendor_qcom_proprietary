    <add-resource type="plurals" name="kg_password_wrong_pin_code"/>
    <plurals name="kg_password_wrong_pin_code">
        <item quantity="one">"Yanlış SIM <xliff:g id="slotid">%d</xliff:g> PIN kodu. Cihazınızın kilidini açmak için operatörünüzle bağlantı kurmak zorunda kalmadan önce <xliff:g id="NUMBER_0">%d</xliff:g> deneme hakkınız kaldı."</item>
        <item quantity="other">"Yanlış SIM <xliff:g id="slotid">%d</xliff:g> PIN kodu, <xliff:g id="NUMBER_1">%d</xliff:g> deneme hakkınız kaldı."</item>
    </plurals>

    <!-- Instructions telling the user that they have exhausted SIM PUK retries and the SIM is now unusable.
