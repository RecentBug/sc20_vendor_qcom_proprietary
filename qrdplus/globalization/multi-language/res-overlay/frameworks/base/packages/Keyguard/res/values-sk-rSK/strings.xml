    <add-resource type="string" name="kg_pin_instructions"/>
    <string name="kg_pin_instructions">"Zadajte kód PIN"</string>
    <!-- Instructions for using the password unlock screen -->
    <add-resource type="string" name="kg_password_instructions"/>
    <string name="kg_password_instructions">"Zadajte heslo"</string>
    <!-- Hint shown in the PUK screen that asks the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint"/>
    <string name="kg_puk_enter_puk_hint">"Karta SIM je teraz zakázaná. Ak chcete pokračovať, zadajte kód PUK. Podrobné informácie získate od operátora."</string>
    <!-- Hint shown when there are multiple SIMs in the device to ask the user to enter the PUK code given to them by their provider -->
    <add-resource type="string" name="kg_puk_enter_puk_hint_multi"/>
    <string name="kg_puk_enter_puk_hint_multi">"SIM karta operátora <xliff:g id="CARRIER">%1$s</xliff:g> bola zakázaná. Ak chcete pokračovať, zadajte kód PUK. Podrobnosti získate od svojho operátora."</string>
    <!-- Hint shown in the PUK unlock screen PIN TextView -->
    <add-resource type="string" name="kg_puk_enter_pin_hint"/>
    <string name="kg_puk_enter_pin_hint">"Zadajte požadovaný kód PIN"</string>
    <!-- Message shown when the user needs to confirm the PIN they just entered in the PUK screen -->
    <add-resource type="string" name="kg_enter_confirm_pin_hint"/>
    <string name="kg_enter_confirm_pin_hint">"Potvrďte požadovaný kód PIN"</string>
    <!-- Message shown in dialog while the device is unlocking the SIM card -->
    <add-resource type="string" name="kg_sim_unlock_progress_dialog_message"/>
    <string name="kg_sim_unlock_progress_dialog_message">"Prebieha odomykanie karty SIM..."</string>
    <!-- Message shown when the user enters an invalid SIM pin password in PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_pin_hint"/>
    <string name="kg_invalid_sim_pin_hint">"Zadajte kód PIN s dĺžkou 4 až 8 číslic."</string>
    <!-- Message shown when the user enters an invalid PUK code in the PUK screen -->
    <add-resource type="string" name="kg_invalid_sim_puk_hint"/>
    <string name="kg_invalid_sim_puk_hint">"Kód PUK musí obsahovať 8 alebo viac číslic."</string>
    <!-- Message shown when the user enters an invalid PUK code -->
    <add-resource type="string" name="kg_invalid_puk"/>
    <string name="kg_invalid_puk">"Znova zadajte správny kód PUK. Opakované pokusy zakážu kartu SIM natrvalo."</string>
      <!-- String shown in PUK screen when PIN codes don't match -->
    <add-resource type="string" name="kg_invalid_confirm_pin_hint"/>
    <string name="kg_invalid_confirm_pin_hint" product="default">"Kódy PIN sa nezhodujú"</string>
    <!-- Message shown when the user exceeds the maximum number of pattern attempts -->
    <add-resource type="string" name="kg_login_too_many_attempts"/>
    <string name="kg_login_too_many_attempts">"Príliš veľa pokusov o nakreslenie vzoru"</string>
    <!-- Message shown in dialog when max number of attempts are reached for PIN screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_pin_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pin_attempts_dialog_message">"<xliff:g id="NUMBER_0">%1$d</xliff:g>-krát ste zadali nesprávny kód PIN. \n\nSkúste to znova o <xliff:g id="NUMBER_1">%2$d</xliff:g> s."</string>
    <!-- Message shown in dialog when max number of attempts are reached for password screen of keyguard -->
    <add-resource type="string" name="kg_too_many_failed_password_attempts_dialog_message"/>
    <string name="kg_too_many_failed_password_attempts_dialog_message">"<xliff:g id="NUMBER_0">%1$d</xliff:g>-krát ste zadali nesprávne heslo. \n\nSkúste to znova o <xliff:g id="NUMBER_1">%2$d</xliff:g> s."</string>
    <add-resource type="string" name="kg_too_many_failed_pattern_attempts_dialog_message"/>
    <string name="kg_too_many_failed_pattern_attempts_dialog_message">"<xliff:g id="NUMBER_0">%1$d</xliff:g>-krát ste použili nesprávny bezpečnostný vzor. \n\nSkúste to znova o <xliff:g id="NUMBER_1">%2$d</xliff:g> s."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="tablet">"Telefón ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g>) bude tento tablet obnovený a všetky údaje, ktoré sú v ňom uložené, budú odstránené."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the device will be wiped. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_wipe"/>
    <string name="kg_failed_attempts_almost_at_wipe" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g> ) bude tento telefón obnovený a všetky údaje, ktoré sú v ňom uložené, budú odstránené."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="tablet">"Tablet ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Tablet bude obnovený a všetky údaje, ktoré sú v ňom uložené, budú odstránené."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the device will now be wiped [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_wiping"/>
    <string name="kg_failed_attempts_now_wiping" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Telefón bude obnovený a všetky údaje, ktoré sú v ňom uložené, budú odstránené."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="tablet">"Tablet ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g> ) bude tento používateľ odstránený a spolu s ním všetky jeho údaje."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_user"/>
    <string name="kg_failed_attempts_almost_at_erase_user" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g>) bude tento používateľ odstránený a spolu s ním všetky jeho údaje."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="tablet">"Tablet ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Používateľ bude odstránený a spolu s ním všetky jeho údaje."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the user will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_user"/>
    <string name="kg_failed_attempts_now_erasing_user" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Používateľ bude odstránený a spolu s ním všetky jeho údaje."</string>

    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="tablet">"Tablet ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g>) bude pracovný profil odstránený a spolu s ním všetky údaje profilu."</string>
    <!-- Message shown when user is almost at the limit of password attempts where the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_erase_profile"/>
    <string name="kg_failed_attempts_almost_at_erase_profile" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER_0">%1$d</xliff:g>-krát nesprávne odomknúť. Po ďalších neúspešných pokusoch (počet: <xliff:g id="NUMBER_1">%2$d</xliff:g>) bude pracovný profil odstránený a spolu s ním všetky údaje profilu."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="tablet">"Tablet ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Pracovný profil bude odstránený spolu so všetkými údajmi."</string>
    <!-- Message shown in dialog when user has exceeded the maximum attempts and the profile will be removed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="kg_failed_attempts_now_erasing_profile"/>
    <string name="kg_failed_attempts_now_erasing_profile" product="default">"Telefón ste sa pokúsili <xliff:g id="NUMBER">%d</xliff:g>-krát nesprávne odomknúť. Pracovný profil bude odstránený spolu so všetkými údajmi."</string>

    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="tablet">"<xliff:g id="NUMBER_0">%1$d</xliff:g>-krát ste nesprávne nakreslili svoj bezpečnostný vzor. Po ďalších <xliff:g id="NUMBER_1">%2$d</xliff:g> neúspešných pokusoch sa zobrazí výzva na odomknutie tabletu pomocou e-mailového účtu.\n\n Skúste to znova o <xliff:g id="NUMBER_2">%3$d</xliff:g> s."</string>
    <!-- Message shown in dialog when user is almost at the limit where they will be
    locked out and may have to enter an alternate username/password to unlock the phone -->
    <add-resource type="string" name="kg_failed_attempts_almost_at_login"/>
    <string name="kg_failed_attempts_almost_at_login" product="default">"<xliff:g id="NUMBER_0">%1$d</xliff:g>-krát ste nesprávne nakreslili svoj bezpečnostný vzor. Po <xliff:g id="NUMBER_1">%2$d</xliff:g> ďalších neúspešných pokusoch sa zobrazí výzva na odomknutie telefónu pomocou e-mailového účtu.\n\n Skúste to znova o <xliff:g id="NUMBER_2">%3$d</xliff:g> s."</string>

    <!-- Instructions telling the user that they entered the wrong SIM PIN for the last time.
         Displayed in a dialog box.  -->
