    <string name="kg_wrong_password">"Nesprávne heslo"</string>
    <!-- Message shown when user enters wrong PIN -->
    <add-resource type="string" name="kg_wrong_pin"/>
    <string name="kg_wrong_pin">"Nesprávny kód PIN"</string>
