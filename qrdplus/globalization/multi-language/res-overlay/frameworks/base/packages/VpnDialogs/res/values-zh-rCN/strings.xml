<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <add-resource type="string" name="prompt"/>
    <string name="prompt">"网络连接请求"</string>

    <!-- Dialog message to warn about the risk of using a VPN application. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="warning"/>
    <string name="warning">"<xliff:g id="APP">%s</xliff:g>想要设置一个VPN连接（可被用于监控网络流量）。请只在您信任该来源的情况下才接受此请求。在VPN处于活动状态时，您的屏幕顶部会显示 &lt;img src=vpn_icon /&gt; 图标。"</string>

    <!-- Dialog title for built-in VPN. [CHAR LIMIT=40]  -->
    <add-resource type="string" name="legacy_title"/>
    <string name="legacy_title">"已连接VPN"</string>
    <!-- Button label to configure the current VPN session. [CHAR LIMIT=20] -->
    <add-resource type="string" name="configure"/>
    <string name="configure">"配置"</string>
    <!-- Button label to disconnect the current VPN session. [CHAR LIMIT=20] -->
    <add-resource type="string" name="disconnect"/>
    <string name="disconnect">"断开连接"</string>

    <!-- Label for the name of the current VPN session. [CHAR LIMIT=20] -->
    <add-resource type="string" name="session"/>
    <string name="session">"会话："</string>
    <!-- Label for the duration of the current VPN session. [CHAR LIMIT=20] -->
    <add-resource type="string" name="duration"/>
    <string name="duration">"时长："</string>
    <!-- Label for the network usage of data transmitted over VPN. [CHAR LIMIT=20] -->
    <add-resource type="string" name="data_transmitted"/>
    <string name="data_transmitted">"已发送："</string>
    <!-- Label for the network usage of data received over VPN. [CHAR LIMIT=20] -->
    <add-resource type="string" name="data_received"/>
    <string name="data_received">"已接收："</string>

    <!-- Formatted string for the network usage over VPN. [CHAR LIMIT=40] -->
    <add-resource type="string" name="data_value_format"/>
    <string name="data_value_format">"<xliff:g id="NUMBER_0">%1$s</xliff:g> 字节/<xliff:g id="NUMBER_1">%2$s</xliff:g> 个数据包"</string></resources>
