<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Ora Australiei Centrale – NT"</item>
        <item>"Ora Australiei Centrale – SA"</item>
        <item>"Ora Australiei de Est – QLD"</item>
        <item>"Ora Australiei de Est – NSW"</item>

        <!-- Canada -->
        <item>"Oră Zonă muntoasă – Dawson Creek"</item>
        <item>"Ora stand. centr. – Saskatchewan"</item>
        <item>"Ora standard de est – Atikokan"</item>
        <item>"Ora standard Atlantic – Quebec"</item>

        <!-- US -->
        <item>"Ora Hawaii-Aleutine – Hawaii"</item>
        <item>"Ora Hawaii-Aleutine – Alaska"</item>
        <item>"Ora stand. Pacific – Metlakatla"</item>
        <item>"Ora standard Zona muntoasă – AZ"</item>

        <!-- Mexico -->
        <item>"Ora Pacific – Baja California"</item>
        <item>"Ora Zonei muntoase – Chihuahua"</item>
        <item>"Ora centrală – Tamaulipas"</item>
        <item>"Ora Zonei muntoase – Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Ora Africii de Est"</item>
        <item>"Ora standard a Atlanticului"</item>
        <item>"Ora standard a Atlanticului"</item>
        <item>"Ora Europei de Est"</item>
        <item>"Ora Kaliningradului"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazon – Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasilia – Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint-Maarten"</item>
        <item>"Insulele Caraibe Olandeze"</item>
        <item>"Curaçao"</item>
        <item>"Sudanul de Sud"</item>
    </string-array></resources>
