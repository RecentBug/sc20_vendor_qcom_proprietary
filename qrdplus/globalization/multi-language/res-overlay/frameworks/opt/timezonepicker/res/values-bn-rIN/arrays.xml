<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"অস্ট্রেলিয় কেন্দ্রীয় - NT"</item>
        <item>"অস্ট্রেলিয় কেন্দ্রীয় - SA"</item>
        <item>"অস্ট্রেলিয় পূর্বাঞ্চলীয় - QLD"</item>
        <item>"অস্ট্রেলিয় পূর্বাঞ্চলীয় - NSW"</item>

        <!-- Canada -->
        <item>"মাউন্টেন স্ট্যান্ডার্ড - ডসন ক্রিক"</item>
        <item>"কেন্দ্রীয় মানক - সাসকাচোয়ান"</item>
        <item>"পূর্বাঞ্চলীয় মানক - অটিকোকান"</item>
        <item>"অতলান্তিক মানক - ক্যুবেক"</item>

        <!-- US -->
        <item>"হাওয়াই-আলিউশান - হাওয়াই"</item>
        <item>"হাওয়াই-আলিউশান - আলাস্কা"</item>
        <item>"প্রশান্ত মাহাসাগরীয় মানক - মেটলাকাটলা"</item>
        <item>"মাউন্টেন স্ট্যান্ডার্ড - আরিজোনা"</item>

        <!-- Mexico -->
        <item>"প্রশান্ত মাহাসাগরীয় - বাহা ক্যালিফোর্নিয়া"</item>
        <item>"মাউন্টেন - চিহুয়াহুয়া"</item>
        <item>"কেন্দ্রীয় - টামাউলিপাস"</item>
        <item>"মাউন্টেন - সোনোরা"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"পূর্ব আফ্রিকা সময়"</item>
        <item>"অতলান্তিক মানক সময়"</item>
        <item>"অতলান্তিক মানক সময়"</item>
        <item>"পূর্ব ইউরোপীয়া সময়"</item>
        <item>"কলিনিনগ্রাদ সময়"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"অ্যামাজন - রোন্ডোনিয়া"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"ব্রাজিলিয়া - পারা"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"সিন্ট মার্টিন"</item>
        <item>"ক্যারিবিয় নেদারল্যান্ডস"</item>
        <item>"কিউরাসাও"</item>
        <item>"দক্ষিণ সুদান"</item>
    </string-array></resources>
