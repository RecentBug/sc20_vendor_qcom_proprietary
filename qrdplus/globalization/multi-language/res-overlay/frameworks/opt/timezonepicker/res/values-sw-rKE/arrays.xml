<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Australia ya Kati - NT"</item>
        <item>"Australia ya Kati - SA"</item>
        <item>"Mashariki ya Australia- QLD"</item>
        <item>"Mashariki ya Australia - NSW"</item>

        <!-- Canada -->
        <item>"Saa Wastani za Mountain - Dawson Creek"</item>
        <item>"Saa Wastani za Kati - Saskatchewan"</item>
        <item>"Saa Wastani za Mashariki - Atikokan"</item>
        <item>"Saa Wastani za Atlantiki - Quebec"</item>

        <!-- US -->
        <item>"Hawaii-Aleutian - Hawaii"</item>
        <item>"Hawaii-Aleutian - Alaska"</item>
        <item>"Saa Wastani za Pasifiki - Metlakatla"</item>
        <item>"Saa Wastani za Mountain - Arizona"</item>

        <!-- Mexico -->
        <item>"Pasifiki - Baja California"</item>
        <item>"Mlima - Chihuahua"</item>
        <item>"Kati - Tamaulipas"</item>
        <item>"Mlima - Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Saa za Afrika Mashariki"</item>
        <item>"Saa Wastani za Atlantiki"</item>
        <item>"Saa Wastani za Atlantiki"</item>
        <item>"Saa za Mashariki ya Ulaya"</item>
        <item>"Saa za Kaliningrad"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazon - Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasilia - Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint Maarten"</item>
        <item>"Uholanzi wa Kikaribi"</item>
        <item>"Kurakao"</item>
        <item>"Sudan Kusini"</item>
    </string-array></resources>
