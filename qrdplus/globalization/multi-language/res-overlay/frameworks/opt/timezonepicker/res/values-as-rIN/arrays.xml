<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"অষ্ট্ৰেলিয়ান চেণ্ট্ৰেল - NT"</item>
        <item>"অষ্ট্ৰেলিয়ান চেণ্ট্ৰেল - SA"</item>
        <item>"অষ্ট্ৰেলিয়ান ইষ্টাৰ্ণ - QLD"</item>
        <item>"অষ্ট্ৰেলিয়ান ইষ্টাৰ্ণ - NSW"</item>

        <!-- Canada -->
        <item>"মাউণ্টেইন ষ্টেণ্ডাৰ্ড - ডছন ক্ৰিক"</item>
        <item>"চেণ্ট্ৰেল ষ্টেণ্ডাৰ্ড - ছাস্কেটচৱান"</item>
        <item>"ইষ্টাৰ্ণ ষ্টেণ্ডাৰ্ড - আটিকোকান"</item>
        <item>"আটলাণ্টিক ষ্টেণ্ডাৰ্ড - কিউবেক"</item>

        <!-- US -->
        <item>"হাৱাই-এলেউটিয়ান - হাৱাই"</item>
        <item>"হাৱাই-এলেউটিয়ান - আলাস্কা"</item>
        <item>"পেচিফিক ষ্টেণ্ডাৰ্ড - মেটলাকাটলা"</item>
        <item>"মাউণ্টেইন ষ্টেণ্ডাৰ্ড - আৰিজোনা"</item>

        <!-- Mexico -->
        <item>"পেচিফিক - বাজা কেলিফ’ৰ্ণিয়া"</item>
        <item>"মাউণ্টেইন - চিহুঅহুৱা"</item>
        <item>"চেণ্ট্ৰেল - টামাউলিপাছ"</item>
        <item>"মাউণ্টেইন - ছোনোৰা"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"পূৱ আফ্ৰিকাৰ সময়"</item>
        <item>"আটলাণ্টিক মান সময়"</item>
        <item>"আটলাণ্টিক মান সময়"</item>
        <item>"পূব ইউৰোপৰ সময়"</item>
        <item>"কালিনিনগ্ৰেড সময়"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"আমাজন - ৰোণ্ডোনিয়া"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"ব্ৰাছিলিয়া - পাৰা"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"ছিণ্ট মাৰ্টিন"</item>
        <item>"কেৰিবিয়ান নেদাৰলেণ্ডছ"</item>
        <item>"কাৰাকাউ"</item>
        <item>"দক্ষিণ চুডান"</item>
    </string-array></resources>
