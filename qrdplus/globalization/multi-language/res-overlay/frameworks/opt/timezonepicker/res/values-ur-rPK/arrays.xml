<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"آسٹریلین سنٹرل - NT"</item>
        <item>"آسٹریلین سنٹرل - SA"</item>
        <item>"آسٹریلین ایسٹرن - QLD"</item>
        <item>"آسٹریلین ایسٹرن - NSW"</item>

        <!-- Canada -->
        <item>"ماؤنٹین اسٹینڈرڈ - ڈاؤسن کریک"</item>
        <item>"سنٹرل اسٹینڈرڈ - سسکیچؤن"</item>
        <item>"ایسٹرن اسٹینڈرڈ - اٹیکوکن"</item>
        <item>"اٹلانٹک اسٹینڈرڈ - کیوبیک"</item>

        <!-- US -->
        <item>"ہوائی الیوشن - ہوائی"</item>
        <item>"ہوائی الیوشن - الاسکا"</item>
        <item>"پیسفک اسٹینڈرڈ - میٹلاکاٹلا"</item>
        <item>"ماؤنٹین اسٹینڈرڈ - اریزونا"</item>

        <!-- Mexico -->
        <item>"پیسفک - باجا کیلیفورنیا"</item>
        <item>"ماؤنٹین - چواوا"</item>
        <item>"سنٹرل - تامولی پاس"</item>
        <item>"ماؤنٹین - سونورا"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"مشرقی افریقہ کا وقت"</item>
        <item>"اٹلانٹک کا معیاری وقت"</item>
        <item>"اٹلانٹک کا معیاری وقت"</item>
        <item>"مشرقی یورپ کا وقت"</item>
        <item>"کلیننگراڈ کا وقت"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"ایمازن - رونڈونیا"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"برازیلیا - پھارا"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"سنٹ مارٹن"</item>
        <item>"کریبیائی نیدرلینڈز"</item>
        <item>"کیورساؤ"</item>
        <item>"جنوبی سوڈان"</item>
    </string-array></resources>
