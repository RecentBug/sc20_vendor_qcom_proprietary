<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Czas środkowoaustralijski – NT"</item>
        <item>"Czas środkowoaustralijski – SA"</item>
        <item>"Czas wschodnioaustralijski – QLD"</item>
        <item>"Czas wschodnioaustralijski – NSW"</item>

        <!-- Canada -->
        <item>"Górski czas standard. – Dawson Creek"</item>
        <item>"Środkowy czas stand. – Saskatchewan"</item>
        <item>"Wschodni czas standardowy – Atikokan"</item>
        <item>"Atlantycki czas standardowy – Quebec"</item>

        <!-- US -->
        <item>"Czas hawajsko-aleucki – Hawaje"</item>
        <item>"Czas hawajsko-aleucki – Alaska"</item>
        <item>"Czas pacyf. standard. – Metlakatla"</item>
        <item>"Górski czas standardowy – Arizona"</item>

        <!-- Mexico -->
        <item>"Czas pacyficzny – Kalifornia Dolna"</item>
        <item>"Czas górski – Chihuahua"</item>
        <item>"Czas środkowy – Tamaulipas"</item>
        <item>"Czas górski – Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Czas wschodnioafrykański"</item>
        <item>"Atlantycki czas standardowy"</item>
        <item>"Atlantycki czas standardowy"</item>
        <item>"Czas wschodnioeuropejski"</item>
        <item>"Czas Kaliningradu"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazonia – Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brazylia – Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint Maarten"</item>
        <item>"Karaiby Holenderskie"</item>
        <item>"Curaçao"</item>
        <item>"Sudan Południowy"</item>
    </string-array></resources>
