<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Stredoaustrálsky čas – NT"</item>
        <item>"Stredoaustrálsky čas – SA"</item>
        <item>"Východoaustrálsky čas – QLD"</item>
        <item>"Východoaustrálsky čas – NSW"</item>

        <!-- Canada -->
        <item>"Horský štandardný – Dawson Creek"</item>
        <item>"Centrálny štandardný – Saskatch."</item>
        <item>"Východný štandardný – Atikokan"</item>
        <item>"Atlantický štandardný – Quebec"</item>

        <!-- US -->
        <item>"Havajsko-aleutský čas – Havaj"</item>
        <item>"Havajsko-aleutský čas – Aljaška"</item>
        <item>"Tichom. štandardný – Metlakatla"</item>
        <item>"Horský štandardný čas – Arizona"</item>

        <!-- Mexico -->
        <item>"Tichomorský č. – Baja California"</item>
        <item>"Horský čas – Chihuahua"</item>
        <item>"Centrálny čas – Tamaulipas"</item>
        <item>"Horský čas – Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Východoafrický čas"</item>
        <item>"Atlantický štandardný čas"</item>
        <item>"Atlantický štandardný čas"</item>
        <item>"Východoeurópsky čas"</item>
        <item>"Kaliningradský čas"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazónia – Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brazília – Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint Maarten"</item>
        <item>"Karibské Holandsko"</item>
        <item>"Curaçao"</item>
        <item>"Južný Sudán"</item>
    </string-array></resources>
