<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Аустралијско централно – север"</item>
        <item>"Аустралијско централно – југ"</item>
        <item>"Источноаустралијско – Квинсланд"</item>
        <item>"Источноаустр. – Нови Јужни Велс"</item>

        <!-- Canada -->
        <item>"Планинско стандард. – Досон Крик"</item>
        <item>"Централ. стандардно – Саскачеван"</item>
        <item>"Источно стандардно – Атикокан"</item>
        <item>"Атланско стандардно – Квебек"</item>

        <!-- US -->
        <item>"Хавајско-алеутско – Хаваји"</item>
        <item>"Хавајско-алеутско – Аљаска"</item>
        <item>"Пацифик, стандардно – Метлакатла"</item>
        <item>"Планинско стандардно – Аризона"</item>

        <!-- Mexico -->
        <item>"Пацифик – Доња Калифорнија"</item>
        <item>"Планинско – Чихуахуа"</item>
        <item>"Централно – Тамаулипас"</item>
        <item>"Планинско – Сонора"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Источноафричко време"</item>
        <item>"Атланско стандардно време"</item>
        <item>"Атланско стандардно време"</item>
        <item>"Источноевропско време"</item>
        <item>"Калињинградско време"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Амазон – Рондонија"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Бразилија – Пара"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Свети Мартин"</item>
        <item>"Карипска Холандија"</item>
        <item>"Курасао"</item>
        <item>"Јужни Судан"</item>
    </string-array></resources>
