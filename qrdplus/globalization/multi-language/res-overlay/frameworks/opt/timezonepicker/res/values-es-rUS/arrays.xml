<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Australia Central, Territ. Norte"</item>
        <item>"Australia Central, Austr. Merid."</item>
        <item>"Australia Oriental, Queensland"</item>
        <item>"Australia Oriental, N. Gal. Sur"</item>

        <!-- Canada -->
        <item>"Estándar Montaña, Dawson Creek"</item>
        <item>"Estándar Centro, Saskatchewan"</item>
        <item>"Estándar del Este, Atikokan"</item>
        <item>"Estándar del Atlántico, Quebec"</item>

        <!-- US -->
        <item>"Hawái-Aleutiano, Hawái"</item>
        <item>"Hawái-Aleutiano, Alaska"</item>
        <item>"Estándar de Pacífico, Metlakatla"</item>
        <item>"Estándar de la Montaña, Arizona"</item>

        <!-- Mexico -->
        <item>"Pacífico, Baja California"</item>
        <item>"Montaña, Chihuahua"</item>
        <item>"Central, Tamaulipas"</item>
        <item>"Montaña, Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Hora de África Oriental"</item>
        <item>"Hora estándar del Atlántico"</item>
        <item>"Hora estándar del Atlántico"</item>
        <item>"Hora de Europa Oriental"</item>
        <item>"Hora de Kaliningrado"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazonas, Rondonia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasilia, Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint Maarten"</item>
        <item>"Caribe Neerlandés"</item>
        <item>"Curazao"</item>
        <item>"Sudán del Sur"</item>
    </string-array></resources>
