<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"‫استرالیای مرکزی - NT"</item>
        <item>"‫استرالیای مرکزی - SA"</item>
        <item>"استرالیای شرقی - QLD"</item>
        <item>"استرالیای شرقی - NSW"</item>

        <!-- Canada -->
        <item>"استاندارد کوهستان - داوسون کریک"</item>
        <item>"استاندارد مرکزی - ساسکاچوان"</item>
        <item>"استاندارد شرقی - آتیکوکان"</item>
        <item>"استاندارد اقیانوس اطلس - کبک"</item>

        <!-- US -->
        <item>"هاوایی-آلوشن - هاوایی"</item>
        <item>"هاوایی-آلوشن- آلاسکا"</item>
        <item>"استاندارد اقیانوس آرام-متلاکاتلا"</item>
        <item>"استاندارد کوهستان - آریزونا"</item>

        <!-- Mexico -->
        <item>"اقیانوس آرام - باجا کالیفرنیا"</item>
        <item>"کوهستان - شی هوا هوا"</item>
        <item>"مرکزی - تامائولیپاس"</item>
        <item>"کوهستان - سونورا"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"زمان آفریقای شرقی"</item>
        <item>"زمان استاندارد اقیانوس اطلس"</item>
        <item>"زمان استاندارد اقیانوس اطلس"</item>
        <item>"زمان اروپای شرقی"</item>
        <item>"زمان کالینینگراد"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"آمازون - روندونیا"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"برازیلیا - پارا"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"سینت مارتن"</item>
        <item>"کارائیب هلند"</item>
        <item>"کوراسائو"</item>
        <item>"سودان جنوبی"</item>
    </string-array></resources>
