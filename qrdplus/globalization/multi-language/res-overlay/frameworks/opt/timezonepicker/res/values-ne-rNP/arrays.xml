<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"अस्ट्रेलियन केन्द्रिय - NT"</item>
        <item>"अस्ट्रेलियन केन्द्रिय - SA"</item>
        <item>"अस्ट्रेलियन पूर्वी - QLD"</item>
        <item>"अस्ट्रेलियन पूर्वी - NSW"</item>

        <!-- Canada -->
        <item>"पर्वतीय मानक - डव्सन क्रीक"</item>
        <item>"केन्द्रीय मानक - सस्कत्चेवान"</item>
        <item>"पूर्वी मानक - अतिकोकान"</item>
        <item>"अटलान्टिक मानक - क्यूबेक"</item>

        <!-- US -->
        <item>"हवाई-एलेउटियन - हवाई"</item>
        <item>"हवाई-एलेउटियन - अलास्का"</item>
        <item>"प्रशान्त मानक - मेलाक्टला"</item>
        <item>"पर्वतीय मानक - एरिजोना"</item>

        <!-- Mexico -->
        <item>"प्रशान्त - बाजा कैलिफोर्निया"</item>
        <item>"पर्वतीय - चिहुआहुआ"</item>
        <item>"केन्द्रिय - टमौलिपास"</item>
        <item>"पर्वत - सोनोरा"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"पूर्वी अफ्रीका समय"</item>
        <item>"अट्लान्टिक मानक समय"</item>
        <item>"अट्लान्टिक मानक समय"</item>
        <item>"पूर्वी यूरोपीय समय"</item>
        <item>"कलिनिन्ग्राद समय"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"अमेजन - रोन्डोनियाँ"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"ब्रासीलिया - पैरा"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"सिन्ट मार्टिन"</item>
        <item>"क्यारेबियन नेदरल्यान्ड्स"</item>
        <item>"कुराकाओ"</item>
        <item>"दक्षिण सुडान"</item>
    </string-array></resources>
