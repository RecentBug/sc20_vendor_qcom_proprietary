<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"เขตเวลาออสเตรเลียกลาง - นอร์ทเทิร์นเทอริทอรี"</item>
        <item>"เขตเวลาออสเตรเลียกลาง - เซาท์ออสเตรเลีย"</item>
        <item>"เขตเวลาออสเตรเลียตะวันออก - ควีนส์แลนด์"</item>
        <item>"เขตเวลาออสเตรเลียตะวันออก - นิวเซาท์เวลส์"</item>

        <!-- Canada -->
        <item>"เขตเวลาบนภูเขา - ดอว์สันครีก"</item>
        <item>"มาตรฐานเวลากลาง - ซัสแคตเชวัน"</item>
        <item>"มาตรฐานเวลาตะวันออก - อะติโคคัง"</item>
        <item>"มาตรฐานแอตแลนติก - ควิเบก"</item>

        <!-- US -->
        <item>"ฮาวาย-หมู่เกาะอะลิวเชียน - ฮาวาย"</item>
        <item>"ฮาวาย-หมู่เกาะอะลิวเชียน - อลาสก้า"</item>
        <item>"มาตรฐานเวลาแปซิฟิก - เมทลากาตละ"</item>
        <item>"เขตเวลามาตรฐานบนภูเขา - อริโซน่า"</item>

        <!-- Mexico -->
        <item>"เขตเวลาแปซิฟิก - บาจาแคลิฟอร์เนีย"</item>
        <item>"เขตเวลาบนภูเขา - ชิวาวา"</item>
        <item>"เขตเวลากลาง - ตาเมาลีปัส"</item>
        <item>"เขตเวลาบนภูเขา - โซโนรา"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"เวลาแอฟริกาตะวันออก"</item>
        <item>"เวลามาตรฐานแอตแลนติก"</item>
        <item>"เวลามาตรฐานแอตแลนติก"</item>
        <item>"เวลายุโรปตะวันออก"</item>
        <item>"เวลาคาลินินกราด"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"อเมซอน - Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"บราซิเลีย - Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"ซินต์ มาร์เติน"</item>
        <item>"เนเธอร์แลนด์ แคริบเบียน"</item>
        <item>"คูราเซา"</item>
        <item>"ซูดานใต้"</item>
    </string-array></resources>
