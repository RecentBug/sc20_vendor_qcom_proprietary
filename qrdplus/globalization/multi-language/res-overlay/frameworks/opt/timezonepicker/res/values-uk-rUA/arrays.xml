<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Австралія, центральний – Пн.Тер."</item>
        <item>"Австралія, центральний – Пд.Авс."</item>
        <item>"Австралія, східний – Квінсленд"</item>
        <item>"Австралія, східний – Н.Пд.Уельс"</item>

        <!-- Canada -->
        <item>"Гірський стандартн. – Досон-Крік"</item>
        <item>"Центр. стандартний – Саскачеван"</item>
        <item>"Східний стандартний – Атікокан"</item>
        <item>"Атлантичний стандартний – Квебек"</item>

        <!-- US -->
        <item>"Час: Гаваї/Алеут. о-ви – Гаваї"</item>
        <item>"Час: Гаваї/Алеут. о-ви – Аляска"</item>
        <item>"Тихоокеан.стандарт. – Метлакатла"</item>
        <item>"Гірський стандартний – Аризона"</item>

        <!-- Mexico -->
        <item>"Тихоокеанський – Баха-Каліфорнія"</item>
        <item>"Гірський – Чиуауа"</item>
        <item>"Центральний – Тамауліпас"</item>
        <item>"Гірський – Сонора"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Східноафриканський час"</item>
        <item>"Атлантичний стандартний час"</item>
        <item>"Атлантичний стандартний час"</item>
        <item>"Східноєвропейський час"</item>
        <item>"Час: Калінінград"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Час: Амазонка – Рондонія"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Час: Бразилія – Пара"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Сінт-Маартен"</item>
        <item>"Нідерландські Карибські острови"</item>
        <item>"Кюрасао"</item>
        <item>"Південний Судан"</item>
    </string-array></resources>
