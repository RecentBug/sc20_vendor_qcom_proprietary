<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Central da Austrália - NT"</item>
        <item>"Central da Austrália - SA"</item>
        <item>"Leste da Austrália - QLD"</item>
        <item>"Leste da Austrália- NSW"</item>

        <!-- Canada -->
        <item>"Padrão Montanhas - Dawson Creek"</item>
        <item>"Padrão Central - Saskatchewan"</item>
        <item>"Padrão leste dos EUA - Atikokan"</item>
        <item>"Padrão leste do Canadá - Québec"</item>

        <!-- US -->
        <item>"Havaí-Aleutas - Havaí"</item>
        <item>"Havaí-Aleutas - Alasca"</item>
        <item>"Padrão do Pacífico - Metlakatla"</item>
        <item>"Padrão das Montanhas - Arizona"</item>

        <!-- Mexico -->
        <item>"Pacífico - Baja California"</item>
        <item>"Montanhas - Chihuahua"</item>
        <item>"Central - Tamaulipas"</item>
        <item>"Montanhas - Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Horário da África Oriental"</item>
        <item>"Horário Padrão do Atlântico"</item>
        <item>"Horário Padrão do Atlântico"</item>
        <item>"Horário do Leste Europeu"</item>
        <item>"Horário de Caliningrado"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazonas - Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasília - Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"São Martinho"</item>
        <item>"Países Baixos Caribenhos"</item>
        <item>"Curaçao"</item>
        <item>"Sudão do Sul"</item>
    </string-array></resources>
