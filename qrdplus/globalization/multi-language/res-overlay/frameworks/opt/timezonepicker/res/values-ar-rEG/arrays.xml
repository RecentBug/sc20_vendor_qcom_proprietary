<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"توقيت وسط أستراليا - الإقليم الشمالي"</item>
        <item>"توقيت وسط أستراليا - جنوب أستراليا"</item>
        <item>"التوقيت الشرقي لاستراليا - كوينزلاند"</item>
        <item>"توقيت شرق أستراليا - نيوساوث ويلز"</item>

        <!-- Canada -->
        <item>"توقيت جبلي - دوسن كريك"</item>
        <item>"التوقيت المركزي القياسي - ساسكاتشوان"</item>
        <item>"التوقيت الشرقي القياسي - أتيكوكان"</item>
        <item>"التوقيت الأطلنطي القياسي - كيبك"</item>

        <!-- US -->
        <item>"توقيت هاواي أليوتيان - هاواي"</item>
        <item>"توقيت هاواي أليوتيان - ألاسكا"</item>
        <item>"توقيت المحيط الهادئ القياسي - ميتلاكاتلا"</item>
        <item>"التوقيت القياسي الجبلي - أريزونا"</item>

        <!-- Mexico -->
        <item>"توقيت المحيط الهادئ - باجا كاليفورنيا"</item>
        <item>"التوقيت الجبلي - تشيهواهوا"</item>
        <item>"التوقيت المركزي - تاماوليباس"</item>
        <item>"التوقيت الجبلي - سونورا"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"توقيت شرق إفريقيا"</item>
        <item>"التوقيت الرسمي الأطلسي"</item>
        <item>"التوقيت الرسمي الأطلسي"</item>
        <item>"توقيت شرق أوروبا"</item>
        <item>"توقيت كالينجراد"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"الأمازون - روندونيا"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"برازيليا - بارا"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"سينت مارتن"</item>
        <item>"الكاريبي هولندا"</item>
        <item>"كوراساو"</item>
        <item>"جنوب السودان"</item>
    </string-array></resources>
