<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"ఆస్ట్రేలియన్ సెంట్రల్ - NT"</item>
        <item>"ఆస్ట్రేలియన్ సెంట్రల్ - SA"</item>
        <item>"ఆస్ట్రేలియన్ ఈస్ట్రన్ - QLD"</item>
        <item>"ఆస్ట్రేలియన్ ఈస్ట్రన్ - NSW"</item>

        <!-- Canada -->
        <item>"మౌంటెయిన్ స్టాండర్డ్ - డాసన్ క్రీక్"</item>
        <item>"సెంట్రల్ స్టాండర్డ్ - సస్కాచ్‌వెన్"</item>
        <item>"ఈస్ట్రన్ స్టాండర్డ్ - అటికొకన్"</item>
        <item>"అట్లాంటిక్ స్టాండర్డ్ - క్యుబెక్"</item>

        <!-- US -->
        <item>"హవాయి-అలుషియన్ - హవాయి"</item>
        <item>"హవాయి-అలుషియన్ - అలాస్కా"</item>
        <item>"పసిఫిక్ స్టాండర్డ్ - మెట్లకట్ల"</item>
        <item>"మౌంటెయిన్ స్టాండర్డ్ - అరిజోనా"</item>

        <!-- Mexico -->
        <item>"పసిఫిక్ - బాజా కాలిఫోర్నియా"</item>
        <item>"మౌంటెయిన్ - చువావా"</item>
        <item>"సెంట్రల్ - తమాలిపస్"</item>
        <item>"మౌంటెయిన్ - సొనొర"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"తూర్పు ఆఫ్రికా సమయం"</item>
        <item>"అట్లాంటిక్ ప్రామాణిక సమయం"</item>
        <item>"అట్లాంటిక్ ప్రామాణిక సమయం"</item>
        <item>"తూర్పు యూరోపియన్ సమయం"</item>
        <item>"కలినిన్‌గ్రాడ్ సమయం"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"అమెజాన్ - రాండోనియా"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"బ్రెసిలియా - పారా"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"సెయింట్ మార్టెన్"</item>
        <item>"కరీబియన్ నెదర్లాండ్స్"</item>
        <item>"క్యూరసావ్"</item>
        <item>"దక్షిణ సూడాన్"</item>
    </string-array></resources>
