<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Heure centr. Austr. (Terr. Nord)"</item>
        <item>"Heure centr. Austr. (Aus. Mérid.)"</item>
        <item>"Heure Est Australie (Queensland)"</item>
        <item>"Heure Est Austr. (N-Galles Sud)"</item>

        <!-- Canada -->
        <item>"Heure stand. mont. (Dawson Creek)"</item>
        <item>"Heure stand. cent. (Saskatchewan)"</item>
        <item>"Heure standard Est (Atikokan)"</item>
        <item>"Heure stand. Atlantique (Québec)"</item>

        <!-- US -->
        <item>"Heure Hawaï-Aléoutiennes (Hawaï)"</item>
        <item>"Heure Hawaï-Aléoutiennes (Alaska)"</item>
        <item>"Heure stand. Pacif. (Metlakatla)"</item>
        <item>"Heure stand. montagnes (Arizona)"</item>

        <!-- Mexico -->
        <item>"Heure Pacifique (Baja California)"</item>
        <item>"Heure montagnes (Chihuahua)"</item>
        <item>"Heure centr. Mexique (Tamaulipas)"</item>
        <item>"Heure montagnes (Sonora)"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Heure d\'Afrique de l\'Est"</item>
        <item>"Heure standard de l\'Atlantique"</item>
        <item>"Heure standard de l\'Atlantique"</item>
        <item>"Heure d\'Europe de l\'Est"</item>
        <item>"Heure de Kaliningrad"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazonie – Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasilia – Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Saint-Martin"</item>
        <item>"Antilles hollandaises"</item>
        <item>"Curaçao"</item>
        <item>"Soudan du Sud"</item>
    </string-array></resources>
