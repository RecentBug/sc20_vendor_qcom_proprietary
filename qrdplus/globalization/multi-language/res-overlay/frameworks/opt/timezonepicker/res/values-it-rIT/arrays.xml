<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"Australia centrale - NT"</item>
        <item>"Australia centrale - SA"</item>
        <item>"Australia orientale - QLD"</item>
        <item>"Australia orientale - NSW"</item>

        <!-- Canada -->
        <item>"Standard montagne - Dawson Creek"</item>
        <item>"Standard centrale - Saskatchewan"</item>
        <item>"Standard orientale - Atikokan"</item>
        <item>"Standard dell\'Atlantico - Quebec"</item>

        <!-- US -->
        <item>"Hawaii-Aleutine - Hawaii"</item>
        <item>"Hawaii-Aleutine - Alaska"</item>
        <item>"Standard del Pacifico - Metlakatla"</item>
        <item>"Standard delle montagne - Arizona"</item>

        <!-- Mexico -->
        <item>"Pacifico - Baja California"</item>
        <item>"Montagne - Chihuahua"</item>
        <item>"Centrale - Tamaulipas"</item>
        <item>"Montagne - Sonora"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"Ora dell\'Africa orientale"</item>
        <item>"Ora standard dell\'Atlantico"</item>
        <item>"Ora standard dell\'Atlantico"</item>
        <item>"Ora dell\'Europa orientale"</item>
        <item>"Ora di Kaliningrad"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"Amazzonia - Rondônia"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"Brasilia - Pará"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"Sint Maarten"</item>
        <item>"Isole BES"</item>
        <item>"Curaçao"</item>
        <item>"Sudan del Sud"</item>
    </string-array></resources>
