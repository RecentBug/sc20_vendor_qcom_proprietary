<resources>

    <!--
        Replacement display strings for time zones. The order should match the entries in
        timezone_rename_ids
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="timezone_rename_labels"/>
    <string-array name="timezone_rename_labels">

        <!-- Australia -->
        <item>"ऑस्ट्रेलियन मध्य - NT"</item>
        <item>"ऑस्ट्रेलियन मध्य - SA"</item>
        <item>"ऑस्ट्रेलियन पूर्वीय - QLD"</item>
        <item>"ऑस्ट्रेलियन पूर्वीय - NSW"</item>

        <!-- Canada -->
        <item>"माउंटन मानक - डॉसन क्रीक"</item>
        <item>"मध्य मानक - सास्काचेवान"</item>
        <item>"पूर्वीय मानक - अॅटिकोकन"</item>
        <item>"अटलांटिक मानक - क्वेबेक"</item>

        <!-- US -->
        <item>"हवाई-अल्युशन - हवाई"</item>
        <item>"हवाई-अल्युशन - अलास्का"</item>
        <item>"पॅसिफिक मानक - मेट्‍लाकाट्‍ला"</item>
        <item>"माउंटन मानक - ऍरिझोना"</item>

        <!-- Mexico -->
        <item>"पॅसिफिक - बाजा कॅलिफोर्निया"</item>
        <item>"माउंटन - चिहुआहुआ"</item>
        <item>"मध्‍य - तमाउलिपस"</item>
        <item>"माउंटन - सोनोरा"</item>

        <!-- Other countries with missing or undesirable labels from the framework -->
        <item>"पूर्व आफ्रिका वेळ"</item>
        <item>"अटलांटिक प्रमाणवेळ"</item>
        <item>"अटलांटिक प्रमाणवेळ"</item>
        <item>"पूर्व युरोपियन वेळ"</item>
        <item>"कलिनिनग्राड वेळ"</item>

        <!-- Brazil -->
        <!-- Non DST version of Amazon Standard Time -->
        <item>"ऍमेझॉन - रोन्‍डोनिया"</item>
        <!-- Non DST version of Brasilia Standard Time -->
        <item>"ब्रासिलिया - पॅरा"</item>
        </string-array>

    <!--
        Display strings for country names. Used only if framework doesn't have translated
        country names. The order should match the entries in backup_country_codes
        ALWAYS ADD NEW ENTRIES AT THE END. Othewise the array will not be the same size until
        all the translations are available. [CHAR LIMIT=32]
    -->
    <add-resource type="array" name="backup_country_names"/>
    <string-array name="backup_country_names">
        <item>"सिंत मार्टेन"</item>
        <item>"कॅरिबियन नेदरलँड्स"</item>
        <item>"कुरॅको"</item>
        <item>"दक्षिण सुदान"</item>
    </string-array></resources>
