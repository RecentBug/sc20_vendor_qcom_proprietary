<resources>
    <!-- Name for the default system route prior to Jellybean. [CHAR LIMIT=30] -->
    <add-resource type="string" name="mr_system_route_name"/>
    <string name="mr_system_route_name">"시스템"</string>

    <!-- Name for the user route category created when publishing routes to the system in Jellybean and above. [CHAR LIMIT=30] -->
    <add-resource type="string" name="mr_user_route_category_name"/>
    <string name="mr_user_route_category_name">"기기"</string>

    <!-- String to be shown as a tooltip of MediaRouteButton
        Cast is the standard android verb for sending content to a remote device. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_button_content_description"/>
    <string name="mr_button_content_description">"전송 버튼"</string>

    <!-- Content description of a MediaRouteButton for accessibility support when no remote device is connected.
        Cast is the standard android verb for sending content to a remote device. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_cast_button_disconnected"/>
    <string name="mr_cast_button_disconnected">"전송 버튼. 연결 해제됨"</string>

    <!-- Content description of a MediaRouteButton for accessibility support while connecting to a remote device.
        Cast is the standard android verb for sending content to a remote device. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_cast_button_connecting"/>
    <string name="mr_cast_button_connecting">"전송 버튼. 연결 중"</string>

    <!-- Content description of a MediaRouteButton for accessibility support when a remote device is connected.
        Cast is the standard android verb for sending content to a remote device. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_cast_button_connected"/>
    <string name="mr_cast_button_connected">"전송 버튼. 연결됨"</string>

    <!-- Title of the media route chooser dialog. [CHAR LIMIT=30] -->
    <add-resource type="string" name="mr_chooser_title"/>
    <string name="mr_chooser_title">"전송할 기기"</string>

    <!-- Placeholder text to show when no devices have been found. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_chooser_searching"/>
    <string name="mr_chooser_searching">"기기를 찾는 중"</string>

    <!-- Button to disconnect from a media route.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="mr_controller_disconnect"/>
    <string name="mr_controller_disconnect">"연결 해제"</string>

    <!-- Button to stop playback and disconnect from a media route. [CHAR LIMIT=30] -->
    <add-resource type="string" name="mr_controller_stop"/>
    <string name="mr_controller_stop">"전송 중지"</string>

    <!-- Content description for accessibility (not shown on the screen): dialog close button. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_close_description"/>
    <string name="mr_controller_close_description">"닫기"</string>

    <!-- Content description for accessibility (not shown on the screen): media play button. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_play"/>
    <string name="mr_controller_play">"재생"</string>

    <!-- Content description for accessibility (not shown on the screen): media pause button. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_pause"/>
    <string name="mr_controller_pause">"일시중지"</string>

    <!-- Content description for accessibility (not shown on the screen): group expand button. Pressing button shows group members of a selected route group. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_expand_group"/>
    <string name="mr_controller_expand_group">"펼치기"</string>

    <!-- Content description for accessibility (not shown on the screen): group collapse button. Pressing button hides group members of a selected route group. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_collapse_group"/>
    <string name="mr_controller_collapse_group">"접기"</string>

    <!-- Content description for accessibility (not shown on the screen): album art button. Clicking on the album art takes user to a predefined activity per media app. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_controller_album_art"/>
    <string name="mr_controller_album_art">"앨범아트"</string>

    <!-- Content description for accessibility (not shown on the screen): volume slider. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="mr_controller_volume_slider"/>
    <string name="mr_controller_volume_slider">"볼륨 슬라이더"</string>

    <!-- Placeholder text to show when no media have been selected for playback. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_controller_no_media_selected"/>
    <string name="mr_controller_no_media_selected">"선택한 미디어 없음"</string>

    <!-- Placeholder text to show when no title/description have been found for a given song/video. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_controller_no_info_available"/>
    <string name="mr_controller_no_info_available">"정보가 없습니다."</string>

    <!-- Placeholder text indicating that the user is currently casting screen. [CHAR LIMIT=50] -->
    <add-resource type="string" name="mr_controller_casting_screen"/>
    <string name="mr_controller_casting_screen">"화면 전송 중"</string></resources>
