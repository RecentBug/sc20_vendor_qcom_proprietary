<resources>
    <!-- Label for the "Done" button on the far left of action mode toolbars. -->
    <add-resource type="string" name="abc_action_mode_done"/>
    <string name="abc_action_mode_done">"Xong"</string>
    <!-- Content description for the action bar "home" affordance. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_action_bar_home_description"/>
    <string name="abc_action_bar_home_description">"Điều hướng về trang chủ"</string>
    <!-- Content description for the action bar "up" affordance. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_action_bar_up_description"/>
    <string name="abc_action_bar_up_description">"Điều hướng lên trên"</string>
    <!-- Content description for the action menu overflow button. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_action_menu_overflow_description"/>
    <string name="abc_action_menu_overflow_description">"Thêm tùy chọn"</string>

    <!-- Content description for the Toolbar icon used to collapse an expanded action mode. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_toolbar_collapse_description"/>
    <string name="abc_toolbar_collapse_description">"Thu gọn"</string>

    <!-- Formatting string for describing the action bar's title/home/up affordance.
         This is a single tappable "button" that includes the app icon, the Up indicator
         (usually a "<" chevron) and the window title text.
         %1$s is the title. %2$s is the description of what tapping/clicking the whole
         thing is going to do. -->
    <add-resource type="string" name="abc_action_bar_home_description_format"/>
    <string name="abc_action_bar_home_description_format">%1$s, %2$s</string>
    <!-- Just like action_bar_home_description_format, but this one will be used
         if the window is also providing subtitle text.
         %1$s is the title. %2$s is the subtitle. %3$s is the description of what
         tapping/clicking the whole thing is going to do. -->
    <add-resource type="string" name="abc_action_bar_home_subtitle_description_format"/>
    <string name="abc_action_bar_home_subtitle_description_format">%1$s, %2$s, %3$s</string>

    <!-- SearchView accessibility description for search button [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_searchview_description_search"/>
    <string name="abc_searchview_description_search">"Tìm kiếm"</string>
    <!-- Default hint text for the system-wide search UI's text field. [CHAR LIMIT=30] -->
    <add-resource type="string" name="abc_search_hint"/>
    <string name="abc_search_hint">"Tìm kiếm\u2026"</string>
    <!-- SearchView accessibility description for search text field [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_searchview_description_query"/>
    <string name="abc_searchview_description_query">"Tìm kiếm truy vấn"</string>
    <!-- SearchView accessibility description for clear button [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_searchview_description_clear"/>
    <string name="abc_searchview_description_clear">"Xóa truy vấn"</string>
    <!-- SearchView accessibility description for submit button [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_searchview_description_submit"/>
    <string name="abc_searchview_description_submit">"Gửi truy vấn"</string>
    <!-- SearchView accessibility description for voice button [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_searchview_description_voice"/>
    <string name="abc_searchview_description_voice">"Tìm kiếm bằng giọng nói"</string>

    <!-- ActivityChooserView - accessibility support -->
    <!-- Description of the shwoing of a popup window with activities to choose from. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_activitychooserview_choose_application"/>
    <string name="abc_activitychooserview_choose_application">"Chọn một ứng dụng"</string>
    <!-- Title for a button to expand the list of activities in ActivityChooserView [CHAR LIMIT=25] -->
    <add-resource type="string" name="abc_activity_chooser_view_see_all"/>
    <string name="abc_activity_chooser_view_see_all">"Xem tất cả"</string>
    <!-- Description of a share target (both in the list of such or the default share button) in a ShareActionProvider (share UI). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_shareactionprovider_share_with_application"/>
    <string name="abc_shareactionprovider_share_with_application">"Chia sẻ với %s"</string>
    <!-- Description of the choose target button in a ShareActionProvider (share UI). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="abc_shareactionprovider_share_with"/>
    <string name="abc_shareactionprovider_share_with">"Chia sẻ với"</string>

    <!-- Text to use when the number in a notification info is too large
         (greater than status_bar_notification_info_maxnum, defined in
         values/config.xml) and must be truncated. May need to be localized
         for most appropriate textual indicator of "more than X".
         [CHAR LIMIT=4] -->
    <add-resource type="string" name="status_bar_notification_info_overflow"/>
    <string name="status_bar_notification_info_overflow">999+</string>

    <!-- Default text for a button that can be toggled on and off. -->
    <add-resource type="string" name="abc_capital_on"/>
    <string name="abc_capital_on">"BẬT"</string>
    <!-- Default text for a button that can be toggled on and off. -->
    <add-resource type="string" name="abc_capital_off"/>
    <string name="abc_capital_off">"TẮT"</string>
    <add-resource type="string" name="search_menu_title"/>
    <string name="search_menu_title">"Tìm kiếm"</string></resources>
