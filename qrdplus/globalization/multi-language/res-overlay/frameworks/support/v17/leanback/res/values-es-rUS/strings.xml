<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- contentDescription that will be announced when go inside navigation menu of BrowseFragment-->
    <add-resource type="string" name="lb_navigation_menu_contentDescription"/>
    <string name="lb_navigation_menu_contentDescription">"Menú de navegación"</string>
    <!-- Image description for the call to search action visible when browsing content [CHAR LIMIT=40] -->
    <add-resource type="string" name="orb_search_action"/>
    <string name="orb_search_action">"Acción de búsqueda"</string>
    <!-- Hint showing in the empty search bar [CHAR LIMIT=40] -->
    <add-resource type="string" name="lb_search_bar_hint"/>
    <string name="lb_search_bar_hint">"Búsqueda"</string>
    <!-- Hint showing in the empty search bar when using the voice input [CHAR LIMIT=40] -->
    <add-resource type="string" name="lb_search_bar_hint_speech"/>
    <string name="lb_search_bar_hint_speech">"Habla para buscar"</string>
    <!-- Hint showing in the empty search bar using a provided context (usually the application name) [CHAR LIMIT=40] -->
    <add-resource type="string" name="lb_search_bar_hint_with_title"/>
    <string name="lb_search_bar_hint_with_title">"Buscar <xliff:g id="SEARCH_CONTEXT">%1$s</xliff:g>"</string>
    <!-- Hint showing in the empty search bar using a provided context (usually the application name) while in voice input mode [CHAR LIMIT=40] -->
    <add-resource type="string" name="lb_search_bar_hint_with_title_speech"/>
    <string name="lb_search_bar_hint_with_title_speech">"Habla para buscar en <xliff:g id="SEARCH_CONTEXT">%1$s</xliff:g>."</string>
    <!-- Onscreen label for the control button to fast forward media playback at a given speed multiplier -->
    <add-resource type="string" name="lb_control_display_fast_forward_multiplier"/>
    <string name="lb_control_display_fast_forward_multiplier">%1$dX</string>
    <!-- Onscreen label for the control button to rewind media playback at a given speed multiplier -->
    <add-resource type="string" name="lb_control_display_rewind_multiplier"/>
    <string name="lb_control_display_rewind_multiplier">%1$dX</string>

    <!-- Talkback label for the control button to start media playback -->
    <add-resource type="string" name="lb_playback_controls_play"/>
    <string name="lb_playback_controls_play">"REPROD."</string>
    <!-- Talkback label for the control button to pause media playback -->
    <add-resource type="string" name="lb_playback_controls_pause"/>
    <string name="lb_playback_controls_pause">"Pausar"</string>
    <!-- Talkback label for the control button to fast forward media playback -->
    <add-resource type="string" name="lb_playback_controls_fast_forward"/>
    <string name="lb_playback_controls_fast_forward">"Avanzar"</string>
    <!-- Talkback label for the control button to fast forward media playback at a given speed multiplier -->
    <add-resource type="string" name="lb_playback_controls_fast_forward_multiplier"/>
    <string name="lb_playback_controls_fast_forward_multiplier">"Avanzar %1$dX"</string>
    <!-- Talkback label for the control button to start rewind playback -->
    <add-resource type="string" name="lb_playback_controls_rewind"/>
    <string name="lb_playback_controls_rewind">"Retroceder"</string>
    <!-- Talkback label for the control button to rewind media playback at a given speed multiplier -->
    <add-resource type="string" name="lb_playback_controls_rewind_multiplier"/>
    <string name="lb_playback_controls_rewind_multiplier">"Rebobinar %1$dX"</string>
    <!-- Talkback label for the control button to skip to the next media item -->
    <add-resource type="string" name="lb_playback_controls_skip_next"/>
    <string name="lb_playback_controls_skip_next">"Ir al siguiente"</string>
    <!-- Talkback label for the control button to skip to the previous media item -->
    <add-resource type="string" name="lb_playback_controls_skip_previous"/>
    <string name="lb_playback_controls_skip_previous">"Ir al anterior"</string>
    <!-- Talkback label for the control button to display more actions -->
    <add-resource type="string" name="lb_playback_controls_more_actions"/>
    <string name="lb_playback_controls_more_actions">"Más acciones"</string>
    <!-- Talkback label for the control button to deselect the thumb up rating for the current media item -->
    <add-resource type="string" name="lb_playback_controls_thumb_up"/>
    <string name="lb_playback_controls_thumb_up">"Desmarcar \"Me gusta\""</string>
    <!-- Talkback label for the control button to select the thumb up rating for the current media item -->
    <add-resource type="string" name="lb_playback_controls_thumb_up_outline"/>
    <string name="lb_playback_controls_thumb_up_outline">"Marcar \"Me gusta\""</string>
    <!-- Talkback label for the control button to deselect the thumb down rating for the current media item -->
    <add-resource type="string" name="lb_playback_controls_thumb_down"/>
    <string name="lb_playback_controls_thumb_down">"Desmarcar \"No me gusta\""</string>
    <!-- Talkback label for the control button to select the thumb down rating for the current media item -->
    <add-resource type="string" name="lb_playback_controls_thumb_down_outline"/>
    <string name="lb_playback_controls_thumb_down_outline">"Marcar \"No me gusta\""</string>
    <!-- Talkback label for the control button to switch to a media playback mode where item playback does not repeat -->
    <add-resource type="string" name="lb_playback_controls_repeat_none"/>
    <string name="lb_playback_controls_repeat_none">"No repetir"</string>
    <!-- Talkback label for the control button to switch to a media playback mode where a set of media items will be repeated -->
    <add-resource type="string" name="lb_playback_controls_repeat_all"/>
    <string name="lb_playback_controls_repeat_all">"Repetir todo"</string>
    <!-- Talkback label for the control button to switch to a media playback mode where a single media item will be repeated -->
    <add-resource type="string" name="lb_playback_controls_repeat_one"/>
    <string name="lb_playback_controls_repeat_one">"Repetir uno"</string>
    <!-- Talkback label for the control button to enable random selection of the next media item -->
    <add-resource type="string" name="lb_playback_controls_shuffle_enable"/>
    <string name="lb_playback_controls_shuffle_enable">"Habilitar reproducción aleatoria"</string>
    <!-- Talkback label for the control button to play media items in sequential order -->
    <add-resource type="string" name="lb_playback_controls_shuffle_disable"/>
    <string name="lb_playback_controls_shuffle_disable">"Desactivar reproducción aleatoria"</string>
    <!-- Talkback label for the control button to enable playback of media items of high quality -->
    <add-resource type="string" name="lb_playback_controls_high_quality_enable"/>
    <string name="lb_playback_controls_high_quality_enable">"Habilitar calidad alta"</string>
    <!-- Talkback label for the control button to disable playback of media items of high quality -->
    <add-resource type="string" name="lb_playback_controls_high_quality_disable"/>
    <string name="lb_playback_controls_high_quality_disable">"Desactivar calidad alta"</string>
    <!-- Talkback label for the control button to enable closed captioning -->
    <add-resource type="string" name="lb_playback_controls_closed_captioning_enable"/>
    <string name="lb_playback_controls_closed_captioning_enable">"Habilitar subtítulos"</string>
    <!-- Talkback label for the control button to disable closed captioning -->
    <add-resource type="string" name="lb_playback_controls_closed_captioning_disable"/>
    <string name="lb_playback_controls_closed_captioning_disable">"Desactivar subtítulos"</string>
    <!-- Talkback label for the control button to enter picture in picture mode -->
    <add-resource type="string" name="lb_playback_controls_picture_in_picture"/>
    <string name="lb_playback_controls_picture_in_picture">"Activar el modo Imagen en imagen"</string>

    <!-- Title of standard Finish action for GuidedStepFragment -->
    <add-resource type="string" name="lb_guidedaction_finish_title"/>
    <string name="lb_guidedaction_finish_title">"Finalizar"</string>
    <!-- Title of standard Continue action for GuidedStepFragment -->
    <add-resource type="string" name="lb_guidedaction_continue_title"/>
    <string name="lb_guidedaction_continue_title">"Continuar"</string>

    <!-- Separator for date picker [CHAR LIMIT=2] -->
    <add-resource type="string" name="lb_date_separator"/>
    <string name="lb_date_separator">/</string>
    <!-- Separator for time picker [CHAR LIMIT=2] -->
    <add-resource type="string" name="lb_time_separator"/>
    <string name="lb_time_separator">:</string>

    <!-- Onboarding screen -->

    <!-- Text for "GET STARTED" button. This text should be in ALL CAPS. -->
    <add-resource type="string" name="lb_onboarding_get_started"/>
    <string name="lb_onboarding_get_started">"COMENZAR"</string>
    <!-- Content description for page navigator. -->
    <add-resource type="string" name="lb_onboarding_accessibility_next"/>
    <string name="lb_onboarding_accessibility_next">"Siguiente"</string></resources>
