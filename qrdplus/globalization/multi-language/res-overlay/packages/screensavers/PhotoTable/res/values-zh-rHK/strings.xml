<resources>
    <!-- Name of the package of photo screensavers, shown in for Settings > Apps. [CHAR LIMIT=40] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"相片螢幕保護程式"</string>

    <!-- Name of the Photo Table screensaver, which tosses photos onto a table. [CHAR LIMIT=40] -->
    <add-resource type="string" name="table_screensaver_name"/>
    <string name="table_screensaver_name">"相片表"</string>

    <!-- Name of the Photo Frame screensaver, which cross-fades between images. [CHAR LIMIT=40] -->
    <add-resource type="string" name="flipper_screensaver_name"/>
    <string name="flipper_screensaver_name">"相框"</string>

    <!-- Name of the album that contains all g+ post photos, copy from Gallery. -->
    <add-resource type="string" name="posts_album_name"/>
    <string name="posts_album_name">"訊息中附加的相片"</string>

    <!-- Name for an album that doesn't seem to have any name at all. -->
    <add-resource type="string" name="unknown_album_name"/>
    <string name="unknown_album_name">"未命名的相簿"</string>

    <!-- Name for photos baked into the app. -->
    <add-resource type="string" name="stock_photo_album_name"/>
    <string name="stock_photo_album_name">"圖庫相片"</string>

    <!-- Name for photos that are stored on this device, copy from Gallery. -->
    <add-resource type="string" name="local_source_name"/>
    <string name="local_source_name">"裝置上的相片"</string>

    <!-- Instruct the user to configure the screensaver in settings to see photos. -->
    <add-resource type="string" name="need_to_configure"/>
    <string name="need_to_configure">"未選取相片。"</string>

    <!-- Click to select all albums in the album selection activity. [CHAR LIMIT=15] -->
    <add-resource type="string" name="photodream_select_all"/>
    <string name="photodream_select_all">"全部選取"</string>

    <!-- Click to clear selection in the album selection activity. [CHAR LIMIT=15] -->
    <add-resource type="string" name="photodream_select_none"/>
    <string name="photodream_select_none">"全部取消選取"</string>

    <!-- Text shown instead of a list of albums when there are no albums with photos. [CHAR LIMIT=50] -->
    <add-resource type="string" name="no_photos"/>
    <string name="no_photos">"裝置上沒有任何相片。"</string></resources>
