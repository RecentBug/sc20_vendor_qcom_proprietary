<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Менаџер меморијског простора"</string>

    <!-- Button label for generic cancel action [CHAR LIMIT=20] -->
    <add-resource type="string" name="cancel"/>
    <string name="cancel">"Откажи"</string>
    <!-- Storage setting. Menu option for using the deletion helper. [CHAR LIMIT=30] -->
    <add-resource type="string" name="storage_menu_free"/>
    <string name="storage_menu_free">"Ослободи простор"</string>
    <!-- Activity title for deletion helper. [CHAR LIMIT=25] -->
    <add-resource type="string" name="deletion_helper_title"/>
    <string name="deletion_helper_title">"Избор ставки за уклањање"</string>
    <!-- Summary of how much storage an app is using and the number of days since last use. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_app_summary"/>
    <string name="deletion_helper_app_summary">"<xliff:g id="USED">%1$s</xliff:g> \u2022 Пре <xliff:g id="DAYS">%2$d</xliff:g> дана"</string>
    <!-- Summary of how much storage an app is using when it has not been used in the last year. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_app_summary_never_used"/>
    <string name="deletion_helper_app_summary_never_used">"<xliff:g id="used" example="1.2GB">%1$s</xliff:g> \u2022 Није искоришћено прошле године"</string>
    <!-- Summary of how much storage an app is using when its last use is unknown. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_app_summary_unknown_used"/>
    <string name="deletion_helper_app_summary_unknown_used">"<xliff:g id="USED">%1$s</xliff:g> \u2022 Није сигурно када је последњи пут коришћено"</string>
    <!-- Button which clears out storage in the deletion helper. [CHAR LIMIT=60]-->
    <add-resource type="string" name="deletion_helper_free_button"/>
    <string name="deletion_helper_free_button">"Ослободите <xliff:g id="FREEABLE">%1$s</xliff:g>"</string>

    <!-- Preference group title for the photos and videos deletion service. [CHAR LIMIT=40]-->
    <add-resource type="string" name="deletion_helper_photos_title"/>
    <string name="deletion_helper_photos_title">"Слике и видео снимци (<xliff:g id="NUM_ITEMS">%1$d</xliff:g>)"</string>

    <!-- Summary of how much backed up storage that photos and videos service can clear from the local device. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_photos_summary"/>
    <string name="deletion_helper_photos_summary">"<xliff:g id="USED">%1$s</xliff:g> \u2022 Старије од <xliff:g id="DAYS">%2$d</xliff:g> дана"</string>

    <!-- Preference title for the downloads deletion service. [CHAR LIMIT=40]-->
    <add-resource type="string" name="deletion_helper_downloads_title"/>
    <string name="deletion_helper_downloads_title">"Преузимања (<xliff:g id="NUMITEMS">%1$d</xliff:g>)"</string>

    <!-- Summary of how much stale data can be cleared from the local download folder. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_downloads_category_summary"/>
    <string name="deletion_helper_downloads_category_summary"><xliff:g id="used" example="1.2GB">%1$s</xliff:g></string>

    <!-- Summary of how much stale data can be cleared from a downloads file. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_downloads_file_summary"/>
    <string name="deletion_helper_downloads_file_summary">"<xliff:g id="USED">%1$s</xliff:g> \u2022 <xliff:g id="DAYS">%2$s</xliff:g>"</string>

    <!-- Summary for when when there is nothing in the downloads folder to clear. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_downloads_summary_empty"/>
    <string name="deletion_helper_downloads_summary_empty"><xliff:g id="used" example="1.2GB">%1$s</xliff:g></string>

    <!-- Message to warn the user before clearing space in the deletion helper. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="deletion_helper_clear_dialog_message"/>
    <string name="deletion_helper_clear_dialog_message">"<xliff:g id="CLEARABLE_BYTES">%1$s</xliff:g> ће бити уклоњено са уређаја"</string>

    <!-- Button label for the dialog prompt for clearing data in deletion helper. [CHAR LIMIT=40] -->
    <add-resource type="string" name="deletion_helper_clear_dialog_remove"/>
    <string name="deletion_helper_clear_dialog_remove">"Уклони"</string>

    <!-- Title for the dialog to up sell the storage manager. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="deletion_helper_upsell_title"/>
    <string name="deletion_helper_upsell_title">"Желите ли да омогућите аутоматско управљање меморијским простором?"</string>
    <!-- Summary for the dialog to up sell the storage manager. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="deletion_helper_upsell_summary"/>
    <string name="deletion_helper_upsell_summary">"Сада имате <xliff:g id="USED">%1$s</xliff:g> слободног меморијског простора. Желите ли да омогућите Менаџеру меморијског простора да аутоматски ослобађа простор уклањањем резервних копија садржаја са уређаја?"</string>
    <!-- Button to delay turning on the storage manager on the storage manager upsell. [CHAR LIMIT=20]-->
    <add-resource type="string" name="deletion_helper_upsell_cancel"/>
    <string name="deletion_helper_upsell_cancel">"Не, хвала"</string>
    <!-- Button to activate the storage manager on the storage manager upsell. [CHAR LIMIT=20]-->
    <add-resource type="string" name="deletion_helper_upsell_activate"/>
    <string name="deletion_helper_upsell_activate">"Укључи"</string>

    <!-- Title for the apps category in the deletion helper, showing how many apps to delete. [CHAR LIMIT=40]-->
    <add-resource type="string" name="deletion_helper_apps_group_title"/>
    <string name="deletion_helper_apps_group_title">"Апликације (<xliff:g id="NUM_ITEMS">%1$d</xliff:g>)"</string>

    <!-- Summary for the apps category in the deletion helper, showing how many space to clear. [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="deletion_helper_apps_group_summary"/>
    <string name="deletion_helper_apps_group_summary">"<xliff:g id="USED">%1$s</xliff:g> \u2022 Последњи пут коришћено пре <xliff:g id="DAYS">%2$d</xliff:g> дана"</string>

    <!-- Category title for the automatic settings in the storage manager settings. [CHAR LIMIT=40] -->
    <add-resource type="string" name="deletion_helper_automatic_title"/>
    <string name="deletion_helper_automatic_title">"Аутоматски"</string>

    <!-- Category title for the manual settings in the storage manager settings. [CHAR LIMIT=40] -->
    <add-resource type="string" name="deletion_helper_manual_title"/>
    <string name="deletion_helper_manual_title">"Ручно"</string>

    <!-- Preference menu title for accessing the deletion helper from the storage manager settings. [CHAR LIMIT=30]-->
    <add-resource type="string" name="deletion_helper_preference_title"/>
    <string name="deletion_helper_preference_title">"Одмах ослободите простор"</string>

    <!-- Automatic storage management service label. [CHAR LIMIT=40]-->
    <add-resource type="string" name="automatic_storage_manager_service_label"/>
    <string name="automatic_storage_manager_service_label">"Услуга аутоматског управљања меморијом"</string>

    <!-- Notification title for the low storage notification. [CHAR LIMIT=40]-->
    <add-resource type="string" name="automatic_storage_manager_notification_title"/>
    <string name="automatic_storage_manager_notification_title">"Телефон има мало меморијског простора"</string>
    <!-- Notification body for the low storage notification. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="automatic_storage_manager_notification_summary"/>
    <string name="automatic_storage_manager_notification_summary">"Омогућите Менаџеру меморијског простора да ослобађа простор аутоматским уклањањем старих слика и видео снимака када се меморијски простор телефона значајно смањи."</string>
    <!-- Action to dismiss the low storage notification. [CHAR LIMIT=40]-->
    <add-resource type="string" name="automatic_storage_manager_cancel_button"/>
    <string name="automatic_storage_manager_cancel_button">"Не, хвала"</string>
    <!-- Action to turn on ASM in the low storage notification. [CHAR LIMIT=40]-->
    <add-resource type="string" name="automatic_storage_manager_activate_button"/>
    <string name="automatic_storage_manager_activate_button">"Укључи"</string>

    <add-resource type="string" name="deletion_helper_photos_loading_title"/>
    <string name="deletion_helper_photos_loading_title">"Слике и видео снимци"</string>
    <add-resource type="string" name="deletion_helper_photos_loading_summary"/>
    <string name="deletion_helper_photos_loading_summary">"Проналазе се ставке\u2026"</string>

    <!-- Message to warn the user before clearing space in the deletion helper for the first time. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="deletion_helper_clear_dialog_message_first_time"/>
    <string name="deletion_helper_clear_dialog_message_first_time">"<xliff:g id="CLEARABLE_BYTES">%1$s</xliff:g> ће бити уклоњено са уређаја"</string>

    <!-- Warning when activating the automatic storage manager on legacy devices. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="automatic_storage_manager_activation_warning"/>
    <string name="automatic_storage_manager_activation_warning">"Меморијским простором сада управља Менаџер меморијског простора"</string></resources>
