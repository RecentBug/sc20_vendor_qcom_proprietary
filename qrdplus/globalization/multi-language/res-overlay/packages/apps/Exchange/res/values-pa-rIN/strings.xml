<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Long name of Microsoft Exchange account type; used by AccountManager -->
    <add-resource type="string" name="exchange_name_alternate"/>
    <string name="exchange_name_alternate">Microsoft Exchange ActiveSync</string>

    <!-- Message subject for meeting accepted response. This will be followed
        by a colon and the title of the meeting (i.e. the title of the meeting becomes
        part of the subject of the message that's sent) -->
    <add-resource type="string" name="meeting_accepted"/>
    <string name="meeting_accepted">"ਸਵੀਕਾਰ ਕੀਤਾ:  <xliff:g id="subject">%s</xliff:g>"</string>
    <!-- Message subject for meeting declined response. This is followed by
        a colon and the title of the meeting (i.e. the title of the meeting becomes
        part of the subject of the message that's sent) -->
    <add-resource type="string" name="meeting_declined"/>
    <string name="meeting_declined">"ਇਨਕਾਰ ਕਰ ਦਿੱਤਾ ਗਿਆ:  <xliff:g id="subject">%s</xliff:g>"</string>
    <!-- Message subject for meeting tentative response. This is followed by
        a colon and the title of the meeting (i.e. the title of the meeting becomes
        part of the subject of the message that's sent) -->
    <add-resource type="string" name="meeting_tentative"/>
    <string name="meeting_tentative">"ਅੰਤਰਿਮ:  <xliff:g id="subject">%s</xliff:g>"</string>
    <!-- Message subject for a canceled meeting email. This is followed by a
        colon and the title of the meeting (i.e. the title of the meeting becomes
        part of the subject of the message that's sent) -->
    <add-resource type="string" name="meeting_canceled"/>
    <string name="meeting_canceled">"ਰੱਦ ਕੀਤਾ ਗਿਆ:  <xliff:g id="subject">%s</xliff:g>"</string>
    <!-- Message subject for an updated meeting email. This is followed by a
        colon and the title of the meeting (i.e. the title of the meeting becomes
        part of the subject of the message that's sent) -->
    <add-resource type="string" name="meeting_updated"/>
    <string name="meeting_updated">"ਅੱਪਡੇਟ ਕੀਤਾ ਗਿਆ:  <xliff:g id="subject">%s</xliff:g>"</string>

    <!-- Indicate when a meeting takes place. This is presented in in bullet
        form, as in, "When: xxx" followed by "Where: xxx" -->
    <add-resource type="string" name="meeting_when"/>
    <string name="meeting_when">"ਜਦੋਂ:  <xliff:g id="when">%s</xliff:g>"</string>
    <!-- Indicate where a meeting takes place. This is presented in in bullet
        form, as in, "When: xxx" followed by "Where: xxx" -->
    <add-resource type="string" name="meeting_where"/>
    <string name="meeting_where">"ਕਿੱਥੇ:  <xliff:g id="where">%s</xliff:g>"</string>
    <!-- Indicate that a meeting is recurring. This would normally be presented
        after "When: xxx", e.g. "When: Tue, Mar 10, 2010 at 2:30pm (recurring)" -->
    <add-resource type="string" name="meeting_recurring"/>
    <string name="meeting_recurring">"ਜਦੋਂ:  <xliff:g id="eventdate" example="Tue, Mar 10, 2010 at 2:30 pm">%s</xliff:g>(ਆਵਰਤੀ)  "</string>
    <!-- Indicate that a meeting lasts all day. This would normally be presented
        after "When: xxx", e.g. "When: Tue, Mar 10, 2010 (all day)" -->
    <add-resource type="string" name="meeting_allday"/>
    <string name="meeting_allday">"ਜਦੋਂ:  <xliff:g id="eventdate" example="Tue, Mar 10, 2010 ">%s</xliff:g>(ਸਾਰਾ ਦਿਨ)  "</string>
    <!-- Indicate that a meeting lasts all day and is recurring. This would normally be presented
        after "When: xxx", e.g. "When: Tue, Mar 10, 2010 (all day, recurring)" -->
    <add-resource type="string" name="meeting_allday_recurring"/>
    <string name="meeting_allday_recurring">"ਜਦੋਂ:  <xliff:g id="eventdate" example="Tue, Mar 10, 2010 ">%s</xliff:g>(ਸਾਰਾ ਦਿਨ, ਆਵਰਤੀ)  "</string>

    <!-- The name of this APK as shown in manage applications settings. [CHAR LIMIT=30] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"ਐਕਸਚੇਂਜ ਸੇਵਾਵਾਂ"</string>

    <!-- Used as the body text of a message reporting to an attendee that a
        specific instance of a recurring meeting has been canceled -->
    <add-resource type="string" name="exception_cancel"/>
    <string name="exception_cancel">"ਇਸ ਦੇ ਲਈ ਇਹ ਇਵੈਂਟ ਰੱਦ ਕੀਤੀ ਗਈ ਹੈ: <xliff:g id="date">%s</xliff:g>"</string>
    <!-- Used as the body text of a message reporting to an attendee that a
        specific instance of a recurring meeting has been changed -->
    <add-resource type="string" name="exception_updated"/>
    <string name="exception_updated">"ਇਸ ਦੇ ਲਈ ਇਸ ਇਵੈਂਟ ਦੇ ਵੇਰਵੇ ਬਦਲ ਦਿੱਤੇ ਗਏ ਹਨ: <xliff:g id="date">%s</xliff:g>"</string>

    <!-- The following are a list of policies that the user's server might require, but that can't
        be enforced by our device.  We will list them separated by commas, as required -->
    <!-- A policy in which the device may not have a storage card [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_storage_cards"/>
    <string name="policy_dont_allow_storage_cards">"ਸਟੋਰੇਜ਼ ਕਾਰਡਾਂ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not have unsigned applications installed
        [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_unsigned_apps"/>
    <string name="policy_dont_allow_unsigned_apps">"ਨਿਸ਼ਚਿਤ ਨਾ ਕੀਤੀਆਂ ਐਪਸ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow application installation via an unsigned
        installer [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_unsigned_installers"/>
    <string name="policy_dont_allow_unsigned_installers">"ਨਿਸ਼ਚਿਤ ਨਾ ਕੀਤੀਆਂ ਐਪਸ ਦੇ ਇੰਸਟੌਲਰਾਂ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow wifi communications [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_wifi"/>
    <string name="policy_dont_allow_wifi">"Wi-Fi ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow text messaging [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_text_messaging"/>
    <string name="policy_dont_allow_text_messaging">"ਟੈਕਸਟ ਮੈਸੇਜਿੰਗ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow POP3 or IMAP email accounts [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_pop_imap"/>
    <string name="policy_dont_allow_pop_imap">"POP3 ਜਾਂ IMAP ਖਾਤਿਆਂ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may allow infrared communications [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_irda"/>
    <string name="policy_dont_allow_irda">"ਇਨਫਰਾਰੈਡ ਸੰਚਾਰ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may allow HTML email to be received [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_html"/>
    <string name="policy_dont_allow_html">"HTML ਈਮੇਲ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow the user of web browsers [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_browser"/>
    <string name="policy_dont_allow_browser">"ਬ੍ਰਾਊਜ਼ਰਾਂ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow the receipt of consumer email
        [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_consumer_email"/>
    <string name="policy_dont_allow_consumer_email">"ਖਪਤਕਾਰਾ ਈਮੇਲ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device may not allow internet connection sharing
        [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_internet_sharing"/>
    <string name="policy_dont_allow_internet_sharing">"ਇੰਟਰਨੈਟ ਸ਼ੇਅਰ ਕਰਨ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which messages must be in SMIME format [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_require_smime"/>
    <string name="policy_require_smime">"SMIME ਸੁਨੇਹੇ ਦੀ ਲੋੜ ਹੈ"</string>
    <!-- A policy in which the device's bluetooth capabilities are restricted [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_bluetooth_restricted"/>
    <string name="policy_bluetooth_restricted">"Bluetooth ਵਰਤੋਂ \'ਤੇ ਪ੍ਰਤਿਬੰਧ"</string>
    <!-- A policy in which the device blocks specified applications [CHAR LIMIT=50]-->
    <add-resource type="string" name="policy_app_blacklist"/>
    <string name="policy_app_blacklist">"ਨਿਰਧਾਰਿਤ ਐਪਸ ਨੂੰ ਅਸਵੀਕਾਰ ਕਰੋ"</string>
    <!-- A policy in which the device allows only specified applications [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_app_whitelist"/>
    <string name="policy_app_whitelist">"ਕੇਵਲ ਨਿਰਧਾਰਿਤ ਐਪਸ ਦੀ ਆਗਿਆ ਦਿਓ"</string>
    <!-- A policy in which the device limits the amount of text that can be displayed for a
        given message [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_text_truncation"/>
    <string name="policy_text_truncation">"ਟੈਕਸਟ ਈਮੇਲ ਆਕਾਰ \'ਤੇ ਪ੍ਰਤਿਬੰਧ ਕਰੋ"</string>
    <!-- A policy in which the device limits the amount of HTML text that can be displayed for a
        given message [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_html_truncation"/>
    <string name="policy_html_truncation">"HTML ਈਮੇਲ ਆਕਾਰ \'ਤੇ ਪ੍ਰਤਿਬੰਧ ਕਰੋ"</string>
    <!-- A policy in which the device requires device or sd card encryption [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_require_sd_encryption"/>
    <string name="policy_require_sd_encryption">"SD ਕਾਰਡ ਇੰਕ੍ਰਿਪਸ਼ਨ ਦੀ ਲੋੜ ਹੈ"</string>

    <!-- The following are a list of policies that the user's server requires and that are
         in force.  We will list them separated by commas, as required -->
    <!-- A policy in which attachments aren't allowed to be downloaded [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_dont_allow_attachments"/>
    <string name="policy_dont_allow_attachments">"ਅਟੈਚਮੈਂਟਾਂ ਦੀ ਆਗਿਆ ਨਾ ਦਿਓ"</string>
    <!-- A policy in which the device restricts the size of attachments that can be downloaded
        [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_max_attachment_size"/>
    <string name="policy_max_attachment_size">"ਅਟੈਚਮੈਂਟਾਂ ਦੇ ਆਕਾਰ \'ਤੇ ਪ੍ਰਤਿਬੰਧ ਕਰੋ"</string>
    <!-- A policy in which the device may only sync manually while roaming [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_require_manual_sync_roaming"/>
    <string name="policy_require_manual_sync_roaming">"ਰੋਮਿੰਗ ਦੇ ਦੌਰਾਨ ਕੇਵਲ ਮੈਨੂਅਲ ਸਿੰਕ ਦੀ ਆਗਿਆ ਦਿਓ"</string>

    <!-- The following is a policy that may or not be supported on a particular device -->
    <!-- A policy in which the device requires device or sd card encryption [CHAR LIMIT=50] -->
    <add-resource type="string" name="policy_require_encryption"/>
    <string name="policy_require_encryption">"ਡਿਵਾਈਸ ਇੰਕ੍ਰਿਪਸ਼ਨ ਦੀ ਲੋੜ ਹੈ"</string>


    <!--  The window length strings below MUST remain in sync with those in com.android.email -->
    <!-- In account setup options & account settings screens (exchange), sync window length; this
        implies loading a 'reasonable' number of messages [CHAR LIMIT=25] -->
    <add-resource type="string" name="account_setup_options_mail_window_auto"/>
    <string name="account_setup_options_mail_window_auto">"ਆਟੋਮੈਟਿਕ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_1day"/>
    <string name="account_setup_options_mail_window_1day">"ਇੱਕ ਦਿਨ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_3days"/>
    <string name="account_setup_options_mail_window_3days">"ਤਿੰਨ ਦਿਨ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_1week"/>
    <string name="account_setup_options_mail_window_1week">"ਇੱਕ ਹਫਤਾ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_2weeks"/>
    <string name="account_setup_options_mail_window_2weeks">"ਦੋ ਹਫਤੇ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_1month"/>
    <string name="account_setup_options_mail_window_1month">"ਇੱਕ ਮਹੀਨਾ"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <add-resource type="string" name="account_setup_options_mail_window_all"/>
    <string name="account_setup_options_mail_window_all">"ਸਭ"</string>

    <!-- Title for notification warning about an authentication error [CHAR LIMIT=40]  -->
    <add-resource type="string" name="auth_error_notification_title"/>
    <string name="auth_error_notification_title">"ਪ੍ਰਮਾਣੀਕਰਨ ਅਸ਼ੁੱਧੀ"</string>

    <!-- text for notification warning about an authentication error [CHAR LIMIT=60]  -->
    <add-resource type="string" name="auth_error_notification_text"/>
    <string name="auth_error_notification_text">"<xliff:g id="account">%s</xliff:g> ਦੇ ਲਈ ਖਾਤਾ ਸੈਟਿੰਗਾਂ ਦੇ ਸੰਪਾਦਨ ਲਈ ਛੋਹਵੋ"</string>

    <!-- Message displayed when there is no application to handle the settings option. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="email_settings_not_available"/>
    <string name="email_settings_not_available">"ਸੈਟਿੰਗਾਂ ਨੂੰ ਦੇਖਣ ਲਈ ਈਮੇਲ ਐਪਲੀਕੇਸ਼ਨ ਨੂੰ ਮੁੜ-ਸਮਰੱਥ ਬਣਾਓ।"</string></resources>
