<resources>
    <!-- Default Application label used on home screen with an icon -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"SIM টুলকিট"</string>
    <!-- Menu button option for ending current SIM session -->
    <add-resource type="string" name="menu_end_session"/>
    <string name="menu_end_session">"সমাপ্ত ছেচন"</string>
    <!-- Menu button option for requesting additional help information -->
    <add-resource type="string" name="help"/>
    <string name="help">"সহায়"</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <add-resource type="string" name="menu_back"/>
    <string name="menu_back">"পিছলৈ"</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <add-resource type="string" name="service_name"/>
    <string name="service_name">"সেৱাৰ নাম"</string>
    <!-- Default description for service_name when no STK service is available -->
    <add-resource type="string" name="stk_no_service"/>
    <string name="stk_no_service">"সেৱা উপলব্ধ নহয়"</string>
    <!-- OK button used to confirm a message dialog -->
    <add-resource type="string" name="button_ok"/>
    <string name="button_ok">"অ’কে"</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <add-resource type="string" name="button_cancel"/>
    <string name="button_cancel">"বাতিল কৰক"</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <add-resource type="string" name="button_yes"/>
    <string name="button_yes">"হয়"</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <add-resource type="string" name="button_no"/>
    <string name="button_no">"নহয়"</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <add-resource type="string" name="alphabet"/>
    <string name="alphabet">"বৰ্ণ"</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <add-resource type="string" name="digits"/>
    <string name="digits">"সংখ্যা (0-9, *, #, +)"</string>
    <!-- Default transition message when launching a call -->
    <add-resource type="string" name="default_call_setup_msg"/>
    <string name="default_call_setup_msg">"কল প্ৰগতিত আছে\u2026"</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <add-resource type="string" name="default_setup_call_msg"/>
    <string name="default_setup_call_msg">"কল ছেটআপ কৰি আছে"</string>

    <add-resource type="string" name="stk_app_state"/>
    <string name="stk_app_state">"এপ্লিকেশ্বন স্থিতি"</string>
    <add-resource type="string" name="enable_app"/>
    <string name="enable_app">"সক্ষম হৈছে"</string>
    <add-resource type="string" name="disable_app"/>
    <string name="disable_app">"অক্ষম হৈছে"</string>

    <!-- Title text used for system dialog from STK app -->
    <add-resource type="string" name="stk_dialog_title"/>
    <string name="stk_dialog_title">"SIM টুলকিট"</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <add-resource type="string" name="default_tone_dialog_msg"/>
    <string name="default_tone_dialog_msg">"প্লেয়িং ট\'ন"</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <add-resource type="string" name="default_open_channel_msg"/>
    <string name="default_open_channel_msg">"চেনেল খুলিবনে?"</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <add-resource type="string" name="default_send_data_msg"/>
    <string name="default_send_data_msg">"ডাটা প্ৰেৰণ কৰি আছে"</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <add-resource type="string" name="default_receive_data_msg"/>
    <string name="default_receive_data_msg">"ডাটা প্ৰাপ্ত কৰি আছে"</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <add-resource type="string" name="default_close_channel_msg"/>
    <string name="default_close_channel_msg">"চেনেল বন্ধ হৈছে"</string>
    <!-- Button texts used in STK dialog -->
    <add-resource type="string" name="stk_dialog_accept"/>
    <string name="stk_dialog_accept">"হয়"</string>
    <add-resource type="string" name="stk_dialog_reject"/>
    <string name="stk_dialog_reject">"নহয়"</string>
    <add-resource type="string" name="no_sim_card_inserted"/>
    <string name="no_sim_card_inserted">"অনুগ্ৰহ কৰি SIM টুলকিট প্ৰাৰম্ভ কৰিবলৈ SIM আন্তঃসংযোগ কৰক।"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <add-resource type="string" name="stk_intent_permission"/>
    <string name="stk_intent_permission">"Allow STK ইণ্টেনটৰ অনুমতি দিয়ক "</string></resources>
