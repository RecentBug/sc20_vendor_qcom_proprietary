        <item>"Обустављено"</item>
        <!-- Status message of Wi-Fi when it is disconnecting from a network. -->
        <item>"Прекидање везе са мрежом <xliff:g id="NETWORK_NAME">%1$s</xliff:g>\u2026"</item>
