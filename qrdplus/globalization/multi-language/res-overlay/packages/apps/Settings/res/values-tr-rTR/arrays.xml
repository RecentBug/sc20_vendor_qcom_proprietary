        <item>"Taranıyor\u2026"</item>
        <!-- Status message of Wi-Fi when it is connecting to a network. -->
        <item>"<xliff:g id="NETWORK_NAME">%1$s</xliff:g> ağına bağlanılıyor\u2026"</item>
