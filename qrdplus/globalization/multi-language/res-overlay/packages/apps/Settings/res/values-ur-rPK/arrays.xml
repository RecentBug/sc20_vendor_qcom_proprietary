        <item>"<xliff:g id="NETWORK_NAME">%1$s</xliff:g> سے مربوط ہو گیا"</item>
        <!-- Status message of Wi-Fi when it is suspended. -->
        <item>"معطل شدہ"</item>
        <!-- Status message of Wi-Fi when it is disconnecting from a network. -->
