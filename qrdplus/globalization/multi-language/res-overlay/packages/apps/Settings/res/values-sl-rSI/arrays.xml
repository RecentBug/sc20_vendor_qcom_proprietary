        <item>"Povezava z omrežjem <xliff:g id="NETWORK_NAME">%1$s</xliff:g> je vzpostavljena"</item>
        <!-- Status message of Wi-Fi when it is suspended. -->
        <item>"Začasno ustavljeno"</item>
        <!-- Status message of Wi-Fi when it is disconnecting from a network. -->
        <item>"Prekinjanje povezave z omrežjem <xliff:g id="NETWORK_NAME">%1$s</xliff:g> \u2026"</item>
        <!-- Status message of Wi-Fi when it is disconnected. -->
        <item>"Prekinjena povezava"</item>
        <!-- Status message of Wi-Fi when it is a failure. -->
        <item>"Ni uspelo"</item>
        <!-- Status message of Wi-Fi when it is blocked. -->
        <item>"Blokirano"</item>
        <!-- Status message of Wi-Fi when connectiong is being verified. -->
        <item>"Začasno izogibanje slabi povezavi"</item>
