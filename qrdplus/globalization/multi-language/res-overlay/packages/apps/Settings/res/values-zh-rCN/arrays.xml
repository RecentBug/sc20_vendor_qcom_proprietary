        <item>"已连接到 <xliff:g id="NETWORK_NAME">%1$s</xliff:g>"</item>
        <!-- Status message of Wi-Fi when it is suspended. -->
        <item>"已暂停"</item>
        <!-- Status message of Wi-Fi when it is disconnecting from a network. -->
        <item>"正在断开与 <xliff:g id="NETWORK_NAME">%1$s</xliff:g> 的连接..."</item>
