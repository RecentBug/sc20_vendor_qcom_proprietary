    <add-resource type="string" name="tap_again_to_complete"/>
    <string name="tap_again_to_complete">"ແຕະອີກຄັ້ງເພື່ອດຳເນີນການໂດຍໃຊ້ <xliff:g id="APP">%1$s</xliff:g>"</string>
