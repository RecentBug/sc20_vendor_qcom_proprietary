    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Huduma ya Nfc"</string>
    <add-resource type="string" name="nfcUserLabel"/>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="accessibility_nfc_enabled"/>
    <string name="accessibility_nfc_enabled">"NFC imewezeshwa."</string>
    <add-resource type="string" name="tap_to_beam"/>
    <string name="tap_to_beam">"Gonga ili usambaze"</string>

    <add-resource type="string" name="beam_progress"/>
    <string name="beam_progress">"Boriti zinazoingia..."</string>
    <add-resource type="string" name="beam_outgoing"/>
    <string name="beam_outgoing">"Inasambaza..."</string>
    <add-resource type="string" name="beam_complete"/>
    <string name="beam_complete">"Kusambaza kumekamilika"</string>
    <add-resource type="string" name="beam_failed"/>
    <string name="beam_failed">"Usambazaji haukukamilika"</string>
    <add-resource type="string" name="beam_canceled"/>
    <string name="beam_canceled">"Boriti imeghairiwa"</string>
    <add-resource type="string" name="cancel"/>
    <string name="cancel">"Ghairi"</string>
    <add-resource type="string" name="beam_tap_to_view"/>
    <string name="beam_tap_to_view">"Gonga ili uangalie"</string>
    <add-resource type="string" name="beam_handover_not_supported"/>
    <string name="beam_handover_not_supported">"Kifaa cha upokeaji hakihimili uhamishaji wa faili kubwa kupitia kwenye boriti."</string>
    <add-resource type="string" name="beam_try_again"/>
    <string name="beam_try_again">"Unganisha vifaa tena"</string>
    <add-resource type="string" name="beam_busy"/>
    <string name="beam_busy">"Boriti lina shughuli kwa sasa. Jaribu tena wakatu uhamishaji wa awali umekamilika."</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <add-resource type="string" name="device"/>
    <string name="device">"kifaa"</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connecting_peripheral"/>
    <string name="connecting_peripheral">"Kuunganisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connected_peripheral"/>
    <string name="connected_peripheral">"Imeunganusha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connect_peripheral_failed"/>
    <string name="connect_peripheral_failed">"Haikuweza kuunganisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <add-resource type="string" name="disconnecting_peripheral"/>
    <string name="disconnecting_peripheral">"Kutenganisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <add-resource type="string" name="disconnected_peripheral"/>
    <string name="disconnected_peripheral">"Imetenganisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <add-resource type="string" name="pairing_peripheral"/>
    <string name="pairing_peripheral">"Kupatanisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <add-resource type="string" name="pairing_peripheral_failed"/>
    <string name="pairing_peripheral_failed">"Haikuweza kupatanisha <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <add-resource type="string" name="failed_to_enable_bt"/>
    <string name="failed_to_enable_bt">"Haikuweza kuwezesha Bluetooth"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <add-resource type="string" name="confirm_pairing"/>
    <string name="confirm_pairing">"Una uhakika unataka kuoanisha kifaa cha Bluetooth cha <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <add-resource type="string" name="pair_yes"/>
    <string name="pair_yes">"Ndiyo"</string>
    <add-resource type="string" name="pair_no"/>
    <string name="pair_no">"Hapana"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_pay"/>
    <string name="tap_again_to_pay">"Gonga tena ili kulipa kwa <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_complete"/>
    <string name="tap_again_to_complete">"Gonga tena ili kukamilisha kwa <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn't be completed with the
         indicated app -->
    <add-resource type="string" name="transaction_failure"/>
    <string name="transaction_failure">"Ununuzi huu haukuweza kukamilishwa kwa <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn't be completed with the
         indicated app -->
    <add-resource type="string" name="could_not_use_app"/>
    <string name="could_not_use_app">"Haikuweza kutumia <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <add-resource type="string" name="pay_with"/>
    <string name="pay_with">"Lipa kwa"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <add-resource type="string" name="complete_with"/>
    <string name="complete_with">"Kamilisha kwa"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <add-resource type="string" name="default_pay_app_removed"/>
    <string name="default_pay_app_removed">"Huduma unayopendelea ya kugonga na kulipa iliondolewa. Ungependa kuchagua nyingine?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <add-resource type="string" name="ask_nfc_tap"/>
    <string name="ask_nfc_tap">"Gonga kifaa kingine ili kukamilisha"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <add-resource type="string" name="wifi_connect"/>
    <string name="wifi_connect">"Unganisha"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <add-resource type="string" name="status_unable_to_connect"/>
    <string name="status_unable_to_connect">"Haiwezi kuunganisha kwenye mtandao"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <add-resource type="string" name="status_wifi_connected"/>
    <string name="status_wifi_connected">"Imeunganishwa"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <add-resource type="string" name="title_connect_to_network"/>
    <string name="title_connect_to_network">"Unganisha kwenye mtandao"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <add-resource type="string" name="prompt_connect_to_network"/>
    <string name="prompt_connect_to_network">"Ungependa kuunganisha kwenye mtandao wa <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <add-resource type="string" name="beam_requires_nfc_enabled"/>
    <string name="beam_requires_nfc_enabled">"Boriti ya Android inahitaji NFC iwe imewashwa. Je, unataka kuiwasha?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <add-resource type="string" name="android_beam"/>
    <string name="android_beam">"Mweka wa Android"</string></resources>
