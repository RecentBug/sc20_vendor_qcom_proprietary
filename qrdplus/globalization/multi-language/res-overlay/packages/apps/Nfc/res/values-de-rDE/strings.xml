    <!-- String referring to the device being connected when there is no name for the device -->
    <add-resource type="string" name="device"/>
    <string name="device">"Gerät"</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connecting_peripheral"/>
    <string name="connecting_peripheral">"Verbinden mit <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connected_peripheral"/>
    <string name="connected_peripheral">"Verbindung mit <xliff:g id="device_name">%1$s</xliff:g> hergestellt"</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <add-resource type="string" name="connect_peripheral_failed"/>
    <string name="connect_peripheral_failed">"Keine Verbindung zu <xliff:g id="device_name">%1$s</xliff:g> möglich"</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <add-resource type="string" name="disconnecting_peripheral"/>
    <string name="disconnecting_peripheral">"Verbindung mit <xliff:g id="device_name">%1$s</xliff:g> wird getrennt"</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <add-resource type="string" name="disconnected_peripheral"/>
    <string name="disconnected_peripheral">"Verbindung mit <xliff:g id="device_name">%1$s</xliff:g> wurde getrennt"</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <add-resource type="string" name="pairing_peripheral"/>
    <string name="pairing_peripheral">"Koppeln mit <xliff:g id="device_name">%1$s</xliff:g>"</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <add-resource type="string" name="pairing_peripheral_failed"/>
    <string name="pairing_peripheral_failed">"Kopplung mit <xliff:g id="device_name">%1$s</xliff:g> fehlgeschlagen"</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <add-resource type="string" name="failed_to_enable_bt"/>
    <string name="failed_to_enable_bt">"Bluetooth konnte nicht aktiviert werden."</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <add-resource type="string" name="confirm_pairing"/>
    <string name="confirm_pairing">"Möchten Sie das Bluetooth-Gerät <xliff:g id="DEVICE_NAME">%1$s</xliff:g> wirklich koppeln?"</string>
    <add-resource type="string" name="pair_yes"/>
    <string name="pair_yes">"Ja"</string>
    <add-resource type="string" name="pair_no"/>
    <string name="pair_no">"Nein"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_pay"/>
    <string name="tap_again_to_pay">"Halten Sie Ihr Gerät erneut ans Terminal, um mit <xliff:g id="APP">%1$s</xliff:g> zu bezahlen."</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <add-resource type="string" name="tap_again_to_complete"/>
    <string name="tap_again_to_complete">"Halten Sie Ihr Gerät erneut ans Terminal, um den Vorgang mit <xliff:g id="APP">%1$s</xliff:g> durchzuführen."</string>
    <!-- String that is telling the user that his NFC transaction couldn't be completed with the
         indicated app -->
    <add-resource type="string" name="transaction_failure"/>
    <string name="transaction_failure">"Diese Transaktion konnte nicht mit <xliff:g id="APP">%1$s</xliff:g> durchgeführt werden."</string>
    <!-- String that is telling the user that his NFC transaction couldn't be completed with the
         indicated app -->
    <add-resource type="string" name="could_not_use_app"/>
    <string name="could_not_use_app">"Fehler bei der Verwendung von <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <add-resource type="string" name="pay_with"/>
    <string name="pay_with">"Zahlen mit"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <add-resource type="string" name="complete_with"/>
    <string name="complete_with">"Durchführen mit"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <add-resource type="string" name="default_pay_app_removed"/>
    <string name="default_pay_app_removed">"Der von Ihnen bevorzugte Dienst für das mobile Bezahlen wurde entfernt. Möchten Sie einen anderen auswählen?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <add-resource type="string" name="ask_nfc_tap"/>
    <string name="ask_nfc_tap">"Zum Abschluss auf ein anderes Gerät tippen"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <add-resource type="string" name="wifi_connect"/>
    <string name="wifi_connect">"Verbinden"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <add-resource type="string" name="status_unable_to_connect"/>
    <string name="status_unable_to_connect">"Verbindung zum Netzwerk konnte nicht hergestellt werden."</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <add-resource type="string" name="status_wifi_connected"/>
    <string name="status_wifi_connected">"Verbunden"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <add-resource type="string" name="title_connect_to_network"/>
    <string name="title_connect_to_network">"Mit Netzwerk verbinden"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <add-resource type="string" name="prompt_connect_to_network"/>
    <string name="prompt_connect_to_network">"Verbindung zum Netzwerk <xliff:g id="NETWORK_SSID">%1$s</xliff:g> herstellen?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <add-resource type="string" name="beam_requires_nfc_enabled"/>
    <string name="beam_requires_nfc_enabled">"Android Beam erfordert zur Aktivierung die Nahfeldkommunikation (NFC). Möchten Sie sie aktivieren?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <add-resource type="string" name="android_beam"/>
    <string name="android_beam">"Android Beam"</string></resources>
