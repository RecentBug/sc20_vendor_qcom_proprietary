<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- The title of the NFC tag application -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"টেগসমূহ"</string>

    <!-- The title for the activity that shows a tag that was just scanned -->
    <add-resource type="string" name="title_scanned_tag"/>
    <string name="title_scanned_tag">"নতুন টেগ সংগ্ৰহ কৰিছে"</string>

    <!-- The title for the activity that shows a tag that was stored from a past scan -->
    <add-resource type="string" name="title_existing_tag"/>
    <string name="title_existing_tag">"টেগ"</string>

    <!-- The title displayed for unknown tag types -->
    <add-resource type="string" name="tag_unknown"/>
    <string name="tag_unknown">"অজ্ঞাত টেগৰ প্ৰকাৰ"</string>

    <!-- The title displayed for an empty tag -->
    <add-resource type="string" name="tag_empty"/>
    <string name="tag_empty">"খালী টেগ খালী"</string>

    <!-- Button label indicating that the user wants to delete a tag -->
    <add-resource type="string" name="button_done"/>
    <string name="button_done">"সম্পন্ন"</string>

    <!-- String displayed for an action to send a text message to a phone number -->
    <add-resource type="string" name="action_text"/>
    <string name="action_text">"<xliff:g id="phone_number">%s</xliff:g>-লৈ পঠিয়াওক"</string>

    <!-- String displayed for an action to call a phone number -->
    <add-resource type="string" name="action_call"/>
    <string name="action_call">"<xliff:g id="phone_number">%s</xliff:g>-ত কল কৰক"</string>

    <!-- Message for prompting on an empty tag -->
    <add-resource type="string" name="empty_tag_prompt"/>
    <string name="empty_tag_prompt">"এই টেগ খালী।"</string>

    <!-- Action description indicating the user can import a scanned vCard -->
    <add-resource type="string" name="import_vcard"/>
    <string name="import_vcard">"^1 আমদানি কৰক"</string>

    <!-- String describing MIME content when it's shared with other application -->
    <add-resource type="string" name="mime_display_name"/>
    <string name="mime_display_name">"NFC টেগৰ পৰা ডাটা"</string>

    <!-- Description string for vCard messages -->
    <add-resource type="string" name="vcard_title"/>
    <string name="vcard_title">"^1-ৰ বাবে সম্পৰ্ক তথ্য"</string></resources>
