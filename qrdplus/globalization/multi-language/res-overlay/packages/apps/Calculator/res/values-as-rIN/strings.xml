<resources>

    <!-- Name of the application. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"কেলকুলেটৰ"</string>

    <!-- Error displayed when expression evaluates to NaN. [CHAR_LIMIT=14] -->
    <add-resource type="string" name="error_nan"/>
    <string name="error_nan">"এটা নম্বৰ নহয়"</string>
    <!-- Error displayed when expression contains a syntax error. [CHAR_LIMIT=14] -->
    <add-resource type="string" name="error_syntax"/>
    <string name="error_syntax">"ত্ৰুটি"</string>

    <!-- Abbrev. name of cosine function (e.g. "cos(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_cos"/>
    <string name="fun_cos">cos</string>
    <!-- Natural logarithm function (e.g. "ln(2)"). [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_ln"/>
    <string name="fun_ln">"ইন"</string>
    <!-- Logarithm function (e.g. "log(10)"). [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_log"/>
    <string name="fun_log">"লগ"</string>
    <!-- Abbrev. name of sine function (e.g. "sin(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_sin"/>
    <string name="fun_sin">sin</string>
    <!-- Abbrev. name of tangent function (e.g. "tan(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_tan"/>
    <string name="fun_tan">"স্পৰ্শক"</string>

    <!-- Clear operation to clear the currently entered expression. [CHAR_LIMIT=3] -->
    <add-resource type="string" name="clr"/>
    <string name="clr">"পৰিষ্কাৰ"</string>
    <!-- Delete operation to remove last entered token. [CHAR_LIMIT=3] -->
    <add-resource type="string" name="del"/>
    <string name="del">"বিলোপ"</string>

    <!-- Content Descriptions -->


    <!-- Content description for 'e' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_e"/>
    <string name="desc_const_e">"য়ুলেৰ নম্বৰ"</string>
    <!-- Content description for 'π' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_pi"/>
    <string name="desc_const_pi">"পাই"</string>

    <!-- Content description for '.' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_dec_point"/>
    <string name="desc_dec_point">"পইণ্ট"</string>

    <!-- Content description for '(' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_lparen"/>
    <string name="desc_lparen">"বাওঁ বন্ধনী"</string>
    <!-- Content description for ')' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_rparen"/>
    <string name="desc_rparen">"সোঁ বন্ধনী"</string>

    <!-- Content description for 'cos' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_cos"/>
    <string name="desc_fun_cos">"কোচিন"</string>
    <!-- Content description for 'ln' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_ln"/>
    <string name="desc_fun_ln">"স্বাভাৱিক ল’গাৰিদম"</string>
