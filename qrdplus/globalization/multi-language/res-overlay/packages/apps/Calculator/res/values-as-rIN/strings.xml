    <add-resource type="string" name="desc_op_sub"/>
    <string name="desc_op_sub">"বিয়োগ"</string>

    <!-- Content description for "clr" button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_clr"/>
    <string name="desc_clr">"পৰিষ্কাৰ কৰক"</string>
    <!-- Content description for "del" button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_del"/>
    <string name="desc_del">"বিলোপ কৰক"</string>
    <!-- Content description for '=' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_eq"/>
    <string name="desc_eq">"সমান"</string></resources>
