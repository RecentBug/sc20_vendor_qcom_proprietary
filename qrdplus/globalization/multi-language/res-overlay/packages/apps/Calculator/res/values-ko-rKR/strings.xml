<resources>

    <!-- Name of the application. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"계산기"</string>

    <!-- Error displayed when expression evaluates to NaN. [CHAR_LIMIT=14] -->
    <add-resource type="string" name="error_nan"/>
    <string name="error_nan">"숫자 아님"</string>
    <!-- Error displayed when expression contains a syntax error. [CHAR_LIMIT=14] -->
    <add-resource type="string" name="error_syntax"/>
    <string name="error_syntax">"오류"</string>

    <!-- Abbrev. name of cosine function (e.g. "cos(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_cos"/>
    <string name="fun_cos">cos</string>
    <!-- Natural logarithm function (e.g. "ln(2)"). [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_ln"/>
    <string name="fun_ln">"ln"</string>
    <!-- Logarithm function (e.g. "log(10)"). [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_log"/>
    <string name="fun_log">"log"</string>
    <!-- Abbrev. name of sine function (e.g. "sin(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_sin"/>
    <string name="fun_sin">sin</string>
    <!-- Abbrev. name of tangent function (e.g. "tan(π)". [CHAR_LIMIT=3] -->
    <add-resource type="string" name="fun_tan"/>
    <string name="fun_tan">"tan"</string>

    <!-- Clear operation to clear the currently entered expression. [CHAR_LIMIT=3] -->
    <add-resource type="string" name="clr"/>
    <string name="clr">"clr"</string>
    <!-- Delete operation to remove last entered token. [CHAR_LIMIT=3] -->
    <add-resource type="string" name="del"/>
    <string name="del">"del"</string>

    <!-- Content Descriptions -->


    <!-- Content description for 'e' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_e"/>
    <string name="desc_const_e">"오일러의 수"</string>
    <!-- Content description for 'π' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_pi"/>
    <string name="desc_const_pi">"원주율(파이)"</string>

    <!-- Content description for '.' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_dec_point"/>
    <string name="desc_dec_point">"소수점"</string>

    <!-- Content description for '(' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_lparen"/>
    <string name="desc_lparen">"왼쪽 괄호"</string>
    <!-- Content description for ')' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_rparen"/>
    <string name="desc_rparen">"오른쪽 괄호"</string>

    <!-- Content description for 'cos' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_cos"/>
    <string name="desc_fun_cos">"코사인(cosine)"</string>
    <!-- Content description for 'ln' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_ln"/>
    <string name="desc_fun_ln">"자연로그"</string>
    <!-- Content description for 'log' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_log"/>
    <string name="desc_fun_log">"로그"</string>
    <!-- Content description for 'sin' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_sin"/>
    <string name="desc_fun_sin">"사인(sine)"</string>
    <!-- Content description for 'tan' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_tan"/>
    <string name="desc_fun_tan">"탄젠트(tangent)"</string>

    <!-- Content description for '+' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_add"/>
    <string name="desc_op_add">"더하기"</string>
    <!-- Content description for '÷' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_div"/>
    <string name="desc_op_div">"나누기"</string>
    <!-- Content description for '!' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_fact"/>
    <string name="desc_op_fact">"팩토리얼"</string>
    <!-- Content description for '×' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_mul"/>
    <string name="desc_op_mul">"곱하기"</string>
    <!-- Content description for '^' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_pow"/>
    <string name="desc_op_pow">"거듭제곱"</string>
    <!-- Content description for '√' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_sqrt"/>
    <string name="desc_op_sqrt">"제곱근"</string>
    <!-- Content description for '-' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_sub"/>
    <string name="desc_op_sub">"빼기"</string>

    <!-- Content description for "clr" button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_clr"/>
    <string name="desc_clr">"지우기"</string>
    <!-- Content description for "del" button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_del"/>
    <string name="desc_del">"삭제"</string>
    <!-- Content description for '=' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_eq"/>
    <string name="desc_eq">"등호"</string></resources>
