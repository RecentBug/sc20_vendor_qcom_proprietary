<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"ਕੁੰਜੀ ਚੇਨ"</string>

    <!-- Dialog title when no certificates were found -->
    <add-resource type="string" name="title_no_certs"/>
    <string name="title_no_certs">"ਕੋਈ ਸਰਟੀਫਿਕੇਟ ਨਹੀਂ ਮਿਲੇ"</string>

    <!-- Dialog title when at least one certificate was found -->
    <add-resource type="string" name="title_select_cert"/>
    <string name="title_select_cert">"ਸਰਟੀਫਿਕੇਟ ਚੁਣੋ"</string>

    <!-- Used at top of dialog to identify requesting application -->
    <add-resource type="string" name="requesting_application"/>
    <string name="requesting_application">"ਐਪ %s ਨੇ ਇੱਕ ਸਰਟੀਫਿਕੇਟ ਦੀ ਬੇਨਤੀ ਕੀਤੀ ਹੈ। ਇੱਕ ਸਰਟੀਫਿਕੇਟ ਚੁਣਨ ਨਾਲ ਇਹ ਐਪ ਨੂੰ ਹੁਣ ਅਤੇ ਭਵਿੱਖ ਵਿੱਚ ਸਰਵਰਾਂ ਨਾਲ ਇਹ ਪਛਾਣ ਵਰਤਣ ਦੀ ਆਗਿਆ ਦਿੰਦਾ ਹੈ।"</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <add-resource type="string" name="requesting_server"/>
    <string name="requesting_server">"ਐਪ ਨੇ ਬੇਨਤੀ ਕਰਨ ਵਾਲੇ ਸਰਵਰ ਦੀ ਪਛਾਣ %s ਦੇ ਤੌਰ ਤੇ ਕੀਤੀ ਹੈ, ਪਰੰਤੂ ਤੁਹਾਨੂੰ ਐਪ ਨੂੰ ਸਰਟੀਫਿਕੇਟ ਤੱਕ ਕੇਵਲ ਤਾਂ ਹੀ ਪਹੁੰਚ ਦੇਣੀ ਚਾਹੀਦੀ ਹੈ ਜੇਕਰ ਤੁਸੀਂ ਐਪ ਤੇ ਭਰੋਸਾ ਕਰਦੇ ਹੋ।"</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <add-resource type="string" name="install_new_cert_message"/>
    <string name="install_new_cert_message">"ਤੁਸੀਂ ਬਾਹਰੀ ਸਟੋਰੇਜ ਵਿੱਚ ਸਥਿਤ ਇੱਕ %1$s ਜਾਂ ਇੱਕ %2$s ਐਕਸਟੈਂਸ਼ਨ ਨਾਲ ਇੱਕ PKCS#12 ਫਾਈਲ ਵਿੱਚੋਂ ਸਰਟੀਫਿਕੇਟ ਇੰਸੌਟਲ ਕਰ ਸਕਦੇ ਹੋ।"</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <add-resource type="string" name="install_new_cert_button_label"/>
    <string name="install_new_cert_button_label">"ਇੰਸਟੌਲ ਕਰੋ"</string>

    <!-- Label of button to send the allow the application certificate request -->
    <add-resource type="string" name="allow_button"/>
    <string name="allow_button">"ਆਗਿਆ ਦਿਓ"</string>

    <!-- Label of button to send the deny the application certificate request -->
    <add-resource type="string" name="deny_button"/>
    <string name="deny_button">"ਅਸਵੀਕਾਰ ਕਰੋ"</string></resources>
