<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Button label on Wallpaper picker screen; user selects this button to set a specific wallpaper -->
    <add-resource type="string" name="wallpaper_instructions"/>
    <string name="wallpaper_instructions">"وال پیپر سیٹ کریں"</string>
    <!-- Error message when an image is selected as a wallpaper,
         but the wallpaper picker cannot load it -->
    <add-resource type="string" name="image_load_fail"/>
    <string name="image_load_fail">"تصویر کو لوڈ نہیں کیا جا سکا"</string>
    <!-- Error message when an image is selected as a wallpaper,
         but the wallpaper cropper cannot load it. The user will
         usually see this when using another app and trying to set
         an image as the wallpaper -->
    <add-resource type="string" name="wallpaper_load_fail"/>
    <string name="wallpaper_load_fail">"تصویر کو وال پیپر کے بطور لوڈ نہیں کیا جا سکا"</string>
    <!-- Error message when an image is selected as a wallpaper,
         but something goes wrong when the user clicks "Set wallpaper" -->
    <add-resource type="string" name="wallpaper_set_fail"/>
    <string name="wallpaper_set_fail">"تصویر کو بطور وال پیپر سیٹ نہیں کیا جا سکا"</string>
    <!-- Shown when wallpapers are selected in Wallpaper picker -->
    <!-- String indicating how many media item(s) is(are) selected
            eg. 1 selected [CHAR LIMIT=30] -->
    <add-resource type="plurals" name="number_of_items_selected"/>
    <plurals name="number_of_items_selected">
        <item quantity="zero">"‏%1$d کو منتخب کیا گیا"</item>
        <item quantity="one">"‏%1$d کو منتخب کیا گیا"</item>
        <item quantity="other">"‏%1$d کو منتخب کیا گیا"</item>
    </plurals>
    <!-- Accessibility string used as a label for a particular wallpaper in the Wallpaper Picker list.
         e.g. "Wallpaper 3 of 10" -->
    <add-resource type="string" name="wallpaper_accessibility_name"/>
    <string name="wallpaper_accessibility_name">"‏وال پیپر ‎%1$d از ‎%2$d"</string>
    <!-- Accessibility string used to announce that a wallpaper has been selected. -->
    <add-resource type="string" name="announce_selection"/>
    <string name="announce_selection">"<xliff:g id="LABEL">%1$s</xliff:g> کو منتخب کیا گیا"</string>

    <!-- Label on button to delete wallpaper(s) -->
    <add-resource type="string" name="wallpaper_delete"/>
    <string name="wallpaper_delete">"حذف کریں"</string>
    <!-- Label on button in Wallpaper Picker to pick an image -->
    <add-resource type="string" name="pick_image"/>
    <string name="pick_image">"میری تصاویر"</string>
    <!-- Option in "Select wallpaper from" dialog box -->
    <add-resource type="string" name="pick_wallpaper"/>
    <string name="pick_wallpaper">"وال پیپرز"</string>
    <!-- Title of activity for cropping wallpapers -->
    <add-resource type="string" name="crop_wallpaper"/>
    <string name="crop_wallpaper">"وال پیپر کو تراشیں"</string>

    <!-- Option for setting the wallpaper only on the home screen. -->
    <add-resource type="string" name="which_wallpaper_option_home_screen"/>
    <string name="which_wallpaper_option_home_screen">"ہوم اسکرین"</string>
    <!-- Option for setting the wallpaper only on the lock screen. -->
    <add-resource type="string" name="which_wallpaper_option_lock_screen"/>
    <string name="which_wallpaper_option_lock_screen">"مقفل اسکرین"</string>
    <!-- Option for setting the wallpaper on both the home screen and lock screen. -->
    <add-resource type="string" name="which_wallpaper_option_home_screen_and_lock_screen"/>
    <string name="which_wallpaper_option_home_screen_and_lock_screen">"ہوم اسکرین اور مقفل اسکرین"</string></resources>
