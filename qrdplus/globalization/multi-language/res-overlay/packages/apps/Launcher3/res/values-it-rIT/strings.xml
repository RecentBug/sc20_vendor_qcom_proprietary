    <string name="shortcut_not_available">"La scorciatoia non è disponibile"</string>

    <!-- Widgets -->
    <!-- Message to tell the user to press and hold on a widget to add it [CHAR_LIMIT=50] -->
    <add-resource type="string" name="long_press_widget_to_add"/>
    <string name="long_press_widget_to_add">"Tocca e tieni premuto per scegliere un widget."</string>
