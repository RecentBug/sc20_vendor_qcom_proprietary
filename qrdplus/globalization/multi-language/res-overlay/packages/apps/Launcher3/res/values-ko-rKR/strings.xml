    <add-resource type="string" name="abandoned_promise_explanation"/>
    <string name="abandoned_promise_explanation">"이 아이콘의 앱이 설치되어 있지 않습니다. 아이콘을 삭제하거나 앱을 검색하여 수동으로 설치하세요."</string>
    <!-- Title for an app which is being downloaded. -->
    <add-resource type="string" name="app_downloading_title"/>
    <string name="app_downloading_title">"<xliff:g id="NAME">%1$s</xliff:g> 다운로드 중, <xliff:g id="PROGRESS">%2$s</xliff:g> 완료"</string>
