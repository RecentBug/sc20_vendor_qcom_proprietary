    <add-resource type="string" name="folder_tap_to_rename"/>
    <string name="folder_tap_to_rename">"Yeni adın kaydedilmesi için hafifçe dokunun"</string>
    <!-- Indication that folder closed -->
    <add-resource type="string" name="folder_closed"/>
    <string name="folder_closed">"Klasör kapatıldı"</string>
