    <string name="shortcut_not_available">"Kısayol kullanılamıyor"</string>

    <!-- Widgets -->
    <!-- Message to tell the user to press and hold on a widget to add it [CHAR_LIMIT=50] -->
    <add-resource type="string" name="long_press_widget_to_add"/>
    <string name="long_press_widget_to_add">"Widget seçmek için dokunun ve basılı tutun."</string>
