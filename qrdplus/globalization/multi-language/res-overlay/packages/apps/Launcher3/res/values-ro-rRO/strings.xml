    <add-resource type="string" name="permdesc_install_shortcut"/>
    <string name="permdesc_install_shortcut">"Permite unei aplicații să adauge comenzi rapide fără intervenția utilizatorului."</string>
    <!-- Permission short label -->
    <add-resource type="string" name="permlab_read_settings"/>
    <string name="permlab_read_settings">"citește setări și comenzi rapide pentru ecranul de pornire"</string>
    <!-- Permission description -->
    <add-resource type="string" name="permdesc_read_settings"/>
    <string name="permdesc_read_settings">"Permite aplicației să citească setările și comenzile rapide din ecranul de pornire."</string>
    <!-- Permission short label -->
    <add-resource type="string" name="permlab_write_settings"/>
    <string name="permlab_write_settings">"scrie setări și comenzi rapide pentru ecranul de pornire"</string>
    <!-- Permission description -->
    <add-resource type="string" name="permdesc_write_settings"/>
    <string name="permdesc_write_settings">"Permite aplicației să modifice setările și comenzile rapide din ecranul de pornire."</string>

    <!-- Toast shown on clicking a direct call shortcut. [CHAR_LIMIT=80] -->
    <add-resource type="string" name="msg_no_phone_permission"/>
    <string name="msg_no_phone_permission">"<xliff:g id="APP_NAME">%1$s</xliff:g> nu are permisiunea de a apela"</string>

    <!-- Widgets: -->


    <!-- Text to show user in place of a gadget when we can't display it properly -->
    <add-resource type="string" name="gadget_error_text"/>
    <string name="gadget_error_text">"Problemă la încărcarea widgetului"</string>

    <!-- Text to show user in place of a gadget when it is not yet initialized. -->
    <add-resource type="string" name="gadget_setup_text"/>
    <string name="gadget_setup_text">"Configurați"</string>

    <!-- Text to inform the user that they can't uninstall a system application -->
    <add-resource type="string" name="uninstall_system_app_text"/>
    <string name="uninstall_system_app_text">"Aceasta este o aplicație de sistem și nu poate fi dezinstalată."</string>

    <!-- Default folder title -->
    <add-resource type="string" name="folder_hint_text"/>
    <string name="folder_hint_text">"Dosar fără nume"</string>

    <!-- Accessibility -->
    <!-- The format string for when an app is temporarily disabled. -->
    <add-resource type="string" name="disabled_app_label"/>
    <string name="disabled_app_label">"S-a dezactivat <xliff:g id="APP_NAME">%1$s</xliff:g>"</string>


    <!-- The format string for default page scroll text [CHAR_LIMIT=none] -->
    <add-resource type="string" name="default_scroll_format"/>
    <string name="default_scroll_format">"Pagina %1$d din %2$d"</string>
