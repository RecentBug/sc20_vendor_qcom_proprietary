    <add-resource type="string" name="abandoned_promise_explanation"/>
    <string name="abandoned_promise_explanation">"Aplicația pentru această pictogramă nu este instalată. Puteți să ștergeți pictograma sau să căutați aplicația și s-o instalați manual."</string>
    <!-- Title for an app which is being downloaded. -->
    <add-resource type="string" name="app_downloading_title"/>
