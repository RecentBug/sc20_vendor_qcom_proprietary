    <add-resource type="string" name="app_info_drop_target_label"/>
    <string name="app_info_drop_target_label">"Info aplicație"</string>

    <!-- Permissions: -->

    <!-- Permission short label -->
    <add-resource type="string" name="permlab_install_shortcut"/>
    <string name="permlab_install_shortcut">"instalează comenzi rapide"</string>
