    <add-resource type="string" name="abandoned_promise_explanation"/>
    <string name="abandoned_promise_explanation">"Програму для цього значка не встановлено. Можна видалити значок або знайти програму та встановити її вручну. "</string>
    <!-- Title for an app which is being downloaded. -->
    <add-resource type="string" name="app_downloading_title"/>
    <string name="app_downloading_title">"<xliff:g id="NAME">%1$s</xliff:g> завантажується, <xliff:g id="PROGRESS">%2$s</xliff:g>"</string>
    <!-- Title for an app whose download has been started. -->
    <add-resource type="string" name="app_waiting_download_title"/>
    <string name="app_waiting_download_title">"<xliff:g id="NAME">%1$s</xliff:g> очікує на завантаження"</string>

<!-- Strings for accessibility actions -->
    <!-- Accessibility action to add an app to workspace. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_add_to_workspace"/>
    <string name="action_add_to_workspace">"Додати на головний екран"</string>

