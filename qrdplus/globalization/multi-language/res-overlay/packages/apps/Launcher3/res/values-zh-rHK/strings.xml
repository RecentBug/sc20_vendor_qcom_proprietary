<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->


    <!-- Application name -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Launcher3"</string>
    <!-- Default folder name -->
    <add-resource type="string" name="folder_name"/>
    <string name="folder_name"></string>
    <!-- Work folder name -->
    <add-resource type="string" name="work_folder_name"/>
    <string name="work_folder_name">"工作"</string>
    <!-- Displayed when user selects a shortcut for an app that was uninstalled [CHAR_LIMIT=none]-->
    <add-resource type="string" name="activity_not_found"/>
    <string name="activity_not_found">"尚未安裝應用程式。"</string>
    <!-- Displayed when user selects a shortcut for an app that is current not available [CHAR_LIMIT=none]-->
    <add-resource type="string" name="activity_not_available"/>
    <string name="activity_not_available">"目前無法使用這個應用程式"</string>
    <!-- SafeMode shortcut error string -->
    <add-resource type="string" name="safemode_shortcut_error"/>
    <string name="safemode_shortcut_error">"在安全模式中無法使用「已下載的應用程式」功能"</string>
    <!-- SafeMode widget error string -->
    <add-resource type="string" name="safemode_widget_error"/>
    <string name="safemode_widget_error">"在安全模式中無法使用小工具"</string>
    <!-- Message shown when a shortcut is not available. It could have been temporarily disabled and may start working again after some time. -->
    <add-resource type="string" name="shortcut_not_available"/>
    <string name="shortcut_not_available">"沒有可用的捷徑"</string>

    <!-- Widgets -->
    <!-- Message to tell the user to press and hold on a widget to add it [CHAR_LIMIT=50] -->
    <add-resource type="string" name="long_press_widget_to_add"/>
    <string name="long_press_widget_to_add">"輕觸並按住小工具即可選取。"</string>
    <!-- Accessibility spoken hint message in widget picker, which allows user to add a widget. Custom action is the label for additional accessibility actions available in this mode [CHAR_LIMIT=100] -->
    <add-resource type="string" name="long_accessible_way_to_add"/>
    <string name="long_accessible_way_to_add">"連扲兩下，然後扲住，就可以新增小工具，或者執行自訂操作。"</string>
    <!-- The format string for the dimensions of a widget in the drawer -->
    <!-- There is a special version of this format string for Farsi -->
    <add-resource type="string" name="widget_dims_format"/>
    <string name="widget_dims_format">%1$d \u00d7 %2$d</string>
    <!-- Accessibility spoken message format for the dimensions of a widget in the drawer -->
    <add-resource type="string" name="widget_accessible_dims_format"/>
    <string name="widget_accessible_dims_format">"%1$d 闊，%2$d 高"</string>

    <!-- All Apps -->
    <!-- Search bar text in the apps view. [CHAR_LIMIT=50] -->
    <add-resource type="string" name="all_apps_search_bar_hint"/>
    <string name="all_apps_search_bar_hint">"搜尋應用程式"</string>
    <!-- Loading apps text. [CHAR_LIMIT=50] -->
    <add-resource type="string" name="all_apps_loading_message"/>
    <string name="all_apps_loading_message">"正在載入應用程式\u2026"</string>
    <!-- No-search-results text. [CHAR_LIMIT=50] -->
    <add-resource type="string" name="all_apps_no_search_results"/>
    <string name="all_apps_no_search_results">"無法找到與「<xliff:g id="QUERY">%1$s</xliff:g>」相符的應用程式"</string>
    <!-- Label for the button which allows the user to get app search results. [CHAR_LIMIT=50] -->
    <add-resource type="string" name="all_apps_search_market_message"/>
    <string name="all_apps_search_market_message">"搜尋更多應用程式"</string>

    <!-- Drag and drop -->

    <!-- Error message when user has filled a home screen -->
    <add-resource type="string" name="out_of_space"/>
    <string name="out_of_space">"主畫面已無空間。"</string>
    <!-- Error message when user has filled the hotseat -->
    <add-resource type="string" name="hotseat_out_of_space"/>
    <string name="hotseat_out_of_space">"我的收藏寄存區沒有足夠空間"</string>

    <!-- All applications label -->
    <add-resource type="string" name="all_apps_button_label"/>
    <string name="all_apps_button_label">"應用程式清單"</string>
    <!-- Label for button in all applications label to go back home (to the workspace / desktop)
         for accessibilty (spoken when the button gets focus). -->
    <add-resource type="string" name="all_apps_home_button_label"/>
    <string name="all_apps_home_button_label">"主畫面"</string>

    <!-- Label for remove drop target. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="remove_drop_target_label"/>
    <string name="remove_drop_target_label">"移除"</string>
    <!-- Label for uninstall drop target. [CHAR_LIMIT=20]-->
    <add-resource type="string" name="uninstall_drop_target_label"/>
    <string name="uninstall_drop_target_label">"解除安裝"</string>
    <!-- Label for app info drop target. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="app_info_drop_target_label"/>
    <string name="app_info_drop_target_label">"應用程式資料"</string>

    <!-- Permissions: -->

    <!-- Permission short label -->
    <add-resource type="string" name="permlab_install_shortcut"/>
    <string name="permlab_install_shortcut">"安裝捷徑"</string>
    <!-- Permission description -->
    <add-resource type="string" name="permdesc_install_shortcut"/>
    <string name="permdesc_install_shortcut">"允許應用程式無需使用者許可也可新增捷徑。"</string>
    <!-- Permission short label -->
    <add-resource type="string" name="permlab_read_settings"/>
    <string name="permlab_read_settings">"讀取主畫面的設定和捷徑"</string>
    <!-- Permission description -->
    <add-resource type="string" name="permdesc_read_settings"/>
    <string name="permdesc_read_settings">"允許應用程式讀取主畫面中的設定和捷徑。"</string>
    <!-- Permission short label -->
    <add-resource type="string" name="permlab_write_settings"/>
    <string name="permlab_write_settings">"寫入主畫面的設定和捷徑"</string>
    <!-- Permission description -->
    <add-resource type="string" name="permdesc_write_settings"/>
    <string name="permdesc_write_settings">"允許應用程式更改主畫面中的設定和捷徑。"</string>

    <!-- Toast shown on clicking a direct call shortcut. [CHAR_LIMIT=80] -->
    <add-resource type="string" name="msg_no_phone_permission"/>
    <string name="msg_no_phone_permission">"不允許 <xliff:g id="APP_NAME">%1$s</xliff:g> 撥打電話"</string>

    <!-- Widgets: -->


    <!-- Text to show user in place of a gadget when we can't display it properly -->
    <add-resource type="string" name="gadget_error_text"/>
    <string name="gadget_error_text">"載入小工具時發生問題"</string>

    <!-- Text to show user in place of a gadget when it is not yet initialized. -->
    <add-resource type="string" name="gadget_setup_text"/>
    <string name="gadget_setup_text">"設定"</string>

    <!-- Text to inform the user that they can't uninstall a system application -->
    <add-resource type="string" name="uninstall_system_app_text"/>
    <string name="uninstall_system_app_text">"這是系統應用程式，無法將其解除安裝。"</string>

    <!-- Default folder title -->
    <add-resource type="string" name="folder_hint_text"/>
    <string name="folder_hint_text">"未命名的資料夾"</string>

    <!-- Accessibility -->
    <!-- The format string for when an app is temporarily disabled. -->
    <add-resource type="string" name="disabled_app_label"/>
    <string name="disabled_app_label">"「<xliff:g id="APP_NAME">%1$s</xliff:g>」已停用"</string>


    <!-- The format string for default page scroll text [CHAR_LIMIT=none] -->
    <add-resource type="string" name="default_scroll_format"/>
    <string name="default_scroll_format">"第 %1$d 頁，共 %2$d 頁"</string>
    <!-- The format string for Workspace page scroll text [CHAR_LIMIT=none] -->
    <add-resource type="string" name="workspace_scroll_format"/>
    <string name="workspace_scroll_format">"主畫面 %1$d，共 %2$d 個"</string>
    <!-- Description for a new page on homescreen[CHAR_LIMIT=none] -->
    <add-resource type="string" name="workspace_new_page"/>
    <string name="workspace_new_page">"新主畫面頁面"</string>

    <!-- Folder accessibility -->
    <!-- The format string for when a folder is opened, speaks the dimensions -->
    <add-resource type="string" name="folder_opened"/>
    <string name="folder_opened">"資料夾已開啟 (<xliff:g id="WIDTH">%1$d</xliff:g> x <xliff:g id="HEIGHT">%2$d</xliff:g>)"</string>
    <!-- Instruction that clicking outside will close folder -->
    <add-resource type="string" name="folder_tap_to_close"/>
    <string name="folder_tap_to_close">"輕按即可關閉資料夾"</string>
    <!-- Instruction that clicking outside will commit folder rename -->
    <add-resource type="string" name="folder_tap_to_rename"/>
    <string name="folder_tap_to_rename">"輕按即可儲存新名稱"</string>
    <!-- Indication that folder closed -->
    <add-resource type="string" name="folder_closed"/>
    <string name="folder_closed">"已關閉資料夾"</string>
    <!-- Folder renamed format -->
    <add-resource type="string" name="folder_renamed"/>
    <string name="folder_renamed">"資料夾已重新命名為「<xliff:g id="NAME">%1$s</xliff:g>」"</string>
    <!-- Folder name format -->
    <add-resource type="string" name="folder_name_format"/>
    <string name="folder_name_format">"資料夾: <xliff:g id="NAME">%1$s</xliff:g>"</string>

    <!-- Strings for the customization mode -->
    <!-- Text for widget add button -->
    <add-resource type="string" name="widget_button_text"/>
    <string name="widget_button_text">"小工具"</string>
    <!-- Text for wallpaper change button -->
    <add-resource type="string" name="wallpaper_button_text"/>
    <string name="wallpaper_button_text">"桌布"</string>
    <!-- Text for settings button -->
    <add-resource type="string" name="settings_button_text"/>
    <string name="settings_button_text">"設定"</string>
    <!-- Message shown when a feature is disabled by the administrator -->
    <add-resource type="string" name="msg_disabled_by_admin"/>
    <string name="msg_disabled_by_admin">"已由您的管理員停用"</string>
    <!-- Text for custom accessibility action to go to the overview mode, where users can look and change the overall UI of the launcher. -->
    <add-resource type="string" name="accessibility_action_overview"/>
    <string name="accessibility_action_overview">"概覽"</string>

    <!-- Strings for settings -->
    <!-- Title for Allow Rotation setting. [CHAR LIMIT=50] -->
    <add-resource type="string" name="allow_rotation_title"/>
    <string name="allow_rotation_title">"允許主畫面旋轉"</string>
    <!-- Text explaining when the home screen will get rotated. [CHAR LIMIT=100] -->
    <add-resource type="string" name="allow_rotation_desc"/>
    <string name="allow_rotation_desc">"當手機旋轉時"</string>
    <!-- Text explaining that rotation is disabled in Display settings. 'Display' refers to the Display section in system settings [CHAR LIMIT=100] -->
    <add-resource type="string" name="allow_rotation_blocked_desc"/>
    <string name="allow_rotation_blocked_desc">"「目前顯示屏」設定不允許旋轉"</string>

    <!-- Label on an icon that references an uninstalled package, for which we have no information about when it might be installed. [CHAR_LIMIT=15] -->
    <add-resource type="string" name="package_state_unknown"/>
    <string name="package_state_unknown">"不明"</string>

    <!-- Button for abandoned promises dialog, to removes this abandoned promise icon. -->
    <add-resource type="string" name="abandoned_clean_this"/>
    <string name="abandoned_clean_this">"移除"</string>
    <!-- Button for abandoned promise dialog, to search in the market for the missing package. -->
    <add-resource type="string" name="abandoned_search"/>
    <string name="abandoned_search">"搜尋"</string>
    <!-- Title for abandoned promise dialog. -->
    <add-resource type="string" name="abandoned_promises_title"/>
    <string name="abandoned_promises_title">"尚未安裝這個應用程式"</string>
    <!-- Explanation for abandoned promise dialog. "The first 'it' refers to the shortcut icon.
    The second "it" refers to the app. -->
    <add-resource type="string" name="abandoned_promise_explanation"/>
    <string name="abandoned_promise_explanation">"您尚未安裝這個圖示代表的應用程式。您可以移除這個圖示，也可以搜尋該應用程式並手動安裝。"</string>
    <!-- Title for an app which is being downloaded. -->
    <add-resource type="string" name="app_downloading_title"/>
    <string name="app_downloading_title">"正在下載 <xliff:g id="NAME">%1$s</xliff:g>，已完成 <xliff:g id="PROGRESS">%2$s</xliff:g>"</string>
    <!-- Title for an app whose download has been started. -->
    <add-resource type="string" name="app_waiting_download_title"/>
    <string name="app_waiting_download_title">"正在等待安裝 <xliff:g id="NAME">%1$s</xliff:g>"</string>

<!-- Strings for accessibility actions -->
    <!-- Accessibility action to add an app to workspace. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_add_to_workspace"/>
    <string name="action_add_to_workspace">"新增至主畫面"</string>

    <!-- Accessibility action to move item to the current location. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_move_here"/>
    <string name="action_move_here">"移動項目至這裡"</string>

    <!-- Accessibility confirmation for item added to workspace. -->
    <add-resource type="string" name="item_added_to_workspace"/>
    <string name="item_added_to_workspace">"已將項目加入至主畫面"</string>

    <!-- Accessibility confirmation for item removed. -->
    <add-resource type="string" name="item_removed"/>
    <string name="item_removed">"項目已移除"</string>

    <!-- Accessibility action to move an item on the workspace. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_move"/>
    <string name="action_move">"移動項目"</string>

    <!-- Accessibility description to move item to empty cell. -->
    <add-resource type="string" name="move_to_empty_cell"/>
    <string name="move_to_empty_cell">"移動至第 <xliff:g id="NUMBER_0">%1$s</xliff:g> 行第 <xliff:g id="NUMBER_1">%2$s</xliff:g> 列"</string>

    <!-- Accessibility description to move item inside a folder. -->
    <add-resource type="string" name="move_to_position"/>
    <string name="move_to_position">"移動至位置 <xliff:g id="NUMBER">%1$s</xliff:g>"</string>

    <!-- Accessibility description to move item to the hotseat. -->
    <add-resource type="string" name="move_to_hotseat_position"/>
    <string name="move_to_hotseat_position">"移動至喜愛的位置 <xliff:g id="NUMBER">%1$s</xliff:g>"</string>

    <!-- Accessibility confirmation for item move. -->
    <add-resource type="string" name="item_moved"/>
    <string name="item_moved">"已移動項目"</string>

    <!-- Accessibility description to move item into an existing folder. -->
    <add-resource type="string" name="add_to_folder"/>
    <string name="add_to_folder">"加入資料夾: <xliff:g id="NAME">%1$s</xliff:g>"</string>

    <!-- Accessibility description to move item into an existing folder containing an app. -->
    <add-resource type="string" name="add_to_folder_with_app"/>
    <string name="add_to_folder_with_app">"加入至資料夾: <xliff:g id="NAME">%1$s</xliff:g>"</string>

    <!-- Accessibility confirmation for item added to folder. -->
    <add-resource type="string" name="added_to_folder"/>
    <string name="added_to_folder">"項目已加入資料夾"</string>

    <!-- Accessibility description to create folder with another item. -->
    <add-resource type="string" name="create_folder_with"/>
    <string name="create_folder_with">"使用以下項目建立資料夾: <xliff:g id="NAME">%1$s</xliff:g>"</string>

    <!-- Accessibility confirmation for folder created. -->
    <add-resource type="string" name="folder_created"/>
    <string name="folder_created">"已建立資料夾"</string>

    <!-- Accessibility action to move an item from folder to workspace. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_move_to_workspace"/>
    <string name="action_move_to_workspace">"移動至主畫面"</string>

    <!-- Accessibility action to move an homescreen to the left. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_move_screen_left"/>
    <string name="action_move_screen_left">"向左移動螢幕"</string>

    <!-- Accessibility action to move an homescreen to the right. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_move_screen_right"/>
    <string name="action_move_screen_right">"向右移動螢幕"</string>

    <!-- Accessibility confirmation when a screen was moved. -->
    <add-resource type="string" name="screen_moved"/>
    <string name="screen_moved">"已移動螢幕"</string>

    <!-- Accessibility action to resize a widget. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_resize"/>
    <string name="action_resize">"重新調整大小"</string>

    <!-- Accessibility action to increase width of a widget. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_increase_width"/>
    <string name="action_increase_width">"增加闊度"</string>

    <!-- Accessibility action to increase height of a widget. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_increase_height"/>
    <string name="action_increase_height">"增加高度"</string>

    <!-- Accessibility action to decrease width of a widget. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_decrease_width"/>
    <string name="action_decrease_width">"減少闊度"</string>

    <!-- Accessibility action to decrease height of a widget. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_decrease_height"/>
    <string name="action_decrease_height">"減少高度"</string>

    <!-- Accessibility confirmation for widget resize. -->
    <add-resource type="string" name="widget_resized"/>
    <string name="widget_resized">"已調整小工具的大小至闊 <xliff:g id="NUMBER_0">%1$s</xliff:g> 高 <xliff:g id="NUMBER_1">%2$s</xliff:g>"</string>

    <!-- Accessibility action to show quick actions menu for an icon. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="action_deep_shortcut"/>
    <string name="action_deep_shortcut">"捷徑"</string>

    <!-- Accessibility description for the shortcuts menu shown for an app. -->
    <add-resource type="string" name="shortcuts_menu_description"/>
    <string name="shortcuts_menu_description">"「<xliff:g id="APP_NAME">%2$s</xliff:g>」嘅 <xliff:g id="NUMBER_OF_SHORTCUTS">%1$d</xliff:g> 個捷徑"</string></resources>
