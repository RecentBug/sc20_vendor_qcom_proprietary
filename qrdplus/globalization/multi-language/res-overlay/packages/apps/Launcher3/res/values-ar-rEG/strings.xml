    <add-resource type="string" name="abandoned_promise_explanation"/>
    <string name="abandoned_promise_explanation">"لم يتم تثبيت تطبيق لهذا الرمز. يمكنك إزالته أو البحث عن التطبيق وتثبيته يدويًا."</string>
    <!-- Title for an app which is being downloaded. -->
    <add-resource type="string" name="app_downloading_title"/>
    <string name="app_downloading_title">"جارٍ تنزيل <xliff:g id="NAME">%1$s</xliff:g>، اكتمل <xliff:g id="PROGRESS">%2$s</xliff:g>"</string>
