<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- used in various places to indicate there is one song for a given artist or album -->
    <add-resource type="string" name="onesong"/>
    <string name="onesong">"1 പാട്ട്"</string>
    <!-- used in various places to indicate there is some number other than one songs for a given artist or album. -->
    <add-resource type="plurals" name="Nsongs"/>
    <plurals name="Nsongs">
        <item quantity="one">"<xliff:g id="count">%d</xliff:g> ഗാനം"</item>
        <item quantity="other">"<xliff:g id="count">%d</xliff:g> പാട്ടുകൾ"</item>
    </plurals>

    <!-- shows how many songs on the album are by the selected artist, out of how many total, if those two numbers are different -->
    <add-resource type="plurals" name="Nsongscomp"/>
    <plurals name="Nsongscomp">
        <item quantity="one">"<xliff:g id="count_for_artist">%2$d</xliff:g> / <xliff:g id="total_count">%1$d</xliff:g> ഗാനം"</item>
        <item quantity="other">"<xliff:g id="count_for_artist">%2$d</xliff:g> / <xliff:g id="total_count">%1$d</xliff:g> പാട്ടുകൾ"</item>
    </plurals>

    <!-- Used in artists list view, indicates how many albums exist for a given artist. -->
    <add-resource type="plurals" name="Nalbums"/>
    <plurals name="Nalbums">
        <!-- number of albums is one -->
        <item quantity="one">"1 ആൽബം"</item>
        <!-- number of albums is not equal to one -->
        <item quantity="other">"<xliff:g id="count">%d</xliff:g> ആൽബങ്ങൾ"</item>
    </plurals>

    <!-- Menu item that takes the user back to the top level screen of the music player -->
    <add-resource type="string" name="goto_start"/>
    <string name="goto_start">"ലൈബ്രറി"</string>
    <!-- Menu item that switches the music player in to party shuffle mode -->
    <add-resource type="string" name="party_shuffle"/>
    <string name="party_shuffle">"പാർട്ടി ഷഫിൾ"</string>
    <!-- Menu item that switches the music player out of party shuffle mode -->
    <add-resource type="string" name="party_shuffle_off"/>
    <string name="party_shuffle_off">"പാർട്ടി ഷഫിൾ ഓഫ്"</string>
    <!-- Menu item that deletes the currently selected item, which might be a single song, or a collection of songs.
         The user will be prompted to confirm before deletion actually takes place -->
    <add-resource type="string" name="delete_item"/>
    <string name="delete_item">"ഇല്ലാതാക്കുക"</string>
