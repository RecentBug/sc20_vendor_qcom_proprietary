<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- used in various places to indicate there is one song for a given artist or album -->
    <add-resource type="string" name="onesong"/>
    <string name="onesong">"1 بتھہ "</string>
    <!-- used in various places to indicate there is some number other than one songs for a given artist or album. -->
    <add-resource type="plurals" name="Nsongs"/>
    <plurals name="Nsongs">
        <item quantity="one">"<xliff:g id="count">%d</xliff:g> بتھہ "</item>
        <item quantity="other">"<xliff:g id="count">%d</xliff:g> بتھہ "</item>
    </plurals>

    <!-- shows how many songs on the album are by the selected artist, out of how many total, if those two numbers are different -->
    <add-resource type="plurals" name="Nsongscomp"/>
    <plurals name="Nsongscomp">
        <item quantity="one">"<xliff:g id="count_for_artist">%2$d</xliff:g> ہند <xliff:g id="total_count">%1$d</xliff:g> بتھہ "</item>
        <item quantity="other">"<xliff:g id="count_for_artist">%2$d</xliff:g> ہند <xliff:g id="total_count">%1$d</xliff:g> بتھہ"</item>
    </plurals>

    <!-- Used in artists list view, indicates how many albums exist for a given artist. -->
    <add-resource type="plurals" name="Nalbums"/>
    <plurals name="Nalbums">
        <!-- number of albums is one -->
        <item quantity="one">"1 البم "</item>
        <!-- number of albums is not equal to one -->
        <item quantity="other">"<xliff:g id="count">%d</xliff:g> البمز "</item>
    </plurals>

