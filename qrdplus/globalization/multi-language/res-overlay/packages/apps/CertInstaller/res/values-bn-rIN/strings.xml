<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"সারটিফিকেট ইন্সটলার"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"একটি সার্টিফিকেট বাছুন"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"সার্টিফিকেটের সারমর্ম নির্ণয় করুন"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"নিষ্কাশন চলছে\u2026"</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"%s থেকে সারমর্ম নির্ণয় করুন"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"সার্টিফিকেটের নাম করুন"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"সার্টিফিকেটের নাম:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"সার্টিফিকেটের সারমর্ম নির্ণয় করার জন্য পাসওয়ার্ড টাইপ করুন।"</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"এই প্যাকেজে রয়েছে:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"PKCS12 কীস্টোরে সার্টিফিকেট।"</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"একজন ইউজারের কী"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"একজন ইউজারের সার্টিফিকেট"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"একটি CA সার্টিফিকেট"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d CA সার্টিফিকেট"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"সঠিক পাসওয়ার্ড টাইপ করুন।"</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"পাসওয়ার্ড টাইপ করুন।"</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"নাম টাইপ করুন।"</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"অনুগ্রহ করে একটি নাম এন্টার করুন যাতে শুধুমাত্র অক্ষর ও সংখ্যা থাকে।"</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"সার্টিফিকেটটি সংরক্ষণ করতে অক্ষম করুন। প্রমানপত্র স্টোরেজ সক্ষম নয় বা সঠিকভাবে সক্রিয় করা হয়েছে।"</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"সার্টিফিকেট সংস্থাপন করা নেই।"</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"সংস্থাপন করার মত কোনও সার্টিফিকেট নেই।"</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"সার্টিফিকেট অবৈধ।"</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="credential">%s</xliff:g> সংস্থাপন করা হয়েছে।"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"ইনষ্টল করা যায় নি কারণ সার্টিফিকেটের মাপ খুব বড়।"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"ইনষ্টল করা যায় নি কারণ সার্টিফিকেট ফাইলের অবস্থান খুঁজে বার করা যায় নি।"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"ইনষ্টল করা যায় নি কারণ সার্টিফিকেট ফাইল পড়া যায় নি।"</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"USB স্টোরেজে কোনও সার্টিফিকেট ফাইল পাওয়া যায়নি।"</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"SD কার্ডে কোনও সার্টিফিকেট ফাইল পাওয়া যায়নি।"</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"USB স্টোরেজ লভ্য নয়।"</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"SD কার্ড বর্তমান নেই।"</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"কেবলমাত্র এই উপকরণের মালিক সার্টিফিকেট ইনষ্টল করতে পারেন।"</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"প্রমাণপত্রর ব্যবহার:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN এবং অ্যাপস"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Wi-Fi প্রোফাইল"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"%s-এর বিবরণ"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"বিশদ বিবরণ"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"ইনস্টল করুন"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"ইনস্টল হচ্ছে"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"বাতিল"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"বাতিল করা"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"কেউ নয়"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"নাম: %1$s\nFQDN: %2$s\nরোমিং কন্সোর্টিয়াম: %3$s\nএলাকা: %4$s\nপ্রমাণীকরণ পদ্ধতি: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"ব্যবহারকারীর নাম: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"ক্লায়েন্ট সার্টিফিকেট:\n%1$s\nকী: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"ট্রাস্ট সার্টিফিকেট:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"শংসাপত্র ইনস্টল করা হয়েছে"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Wi-Fi-তে সংরক্ষণ করা নেটওয়ার্কগুলিতে <xliff:g id="name">%1$s</xliff:g> শংসাপত্র যোগ করা হয়েছে।"</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"হয়ে গেছে"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"<xliff:g id="name">%1$s</xliff:g>-এর মাধ্যমে উপলভ্য নেটওয়ার্কগুলিতে সংযোগ স্থাপন করার জন্য Wi-Fi শংসাপত্র ইনস্টল করুন।"</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"ডাউনলোড করা ফাইলে কিছু সমস্যা আছে এবং ইনস্টল করা যাবে না। আপনি সঠিক উৎস থেকে ফাইলটি ডাউনলোড করেছেন কিনা তা সুনিশ্চিত করুন।"</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Wi-Fi শংসাপত্রগুলি ইনস্টল করা যায়নি। ফাইলটিকে আবার ডাউনলোড করার চেষ্টা করুন।"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"ইনস্টলেশন বাতিল করা হয়েছে"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"ইনস্টল করা যায়নি"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Wi-Fi চালু করে আবার চেষ্টা করুন।"</string></resources>
