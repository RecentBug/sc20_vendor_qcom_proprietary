<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Program instalacyjny certyfikatu"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Wybierz certyfikat"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Wyodrębnij certyfikat"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Wyodrębnianie..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Wyodrębnij z %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Nadaj certyfikatowi nazwę"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Nazwa certyfikatu:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Wprowadź hasło, aby wyodrębnić certyfikaty."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Pakiet zawiera:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Certyfikaty w magazynie kluczy PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"jeden klucz użytkownika"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"jeden certyfikat użytkownika"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"jeden certyfikat urzędu certyfikacji"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"Certyfikaty urzędu certyfikacji %d"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Wpisz poprawne hasło."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Wpisz hasło."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Wpisz nazwę."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Wpisz nazwę, która zawiera tylko litery i cyfry."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Nie można zapisać certyfikatu. Magazyn danych logowania nie został włączony lub poprawnie zainicjowany."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Certyfikat nie został zainstalowany."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Brak certyfikatu do zainstalowania."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Certyfikat jest nieprawidłowy."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> jest zainstalowany."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Nie można zainstalować, ponieważ rozmiar certyfikatu jest zbyt duży."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Nie można zainstalować, ponieważ nie można znaleźć pliku certyfikatu."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Nie można zainstalować, ponieważ nie można odczytać pliku certyfikatu."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Nie znaleziono pliku certyfikatu na nośniku USB."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Na karcie SD nie znaleziono pliku certyfikatu."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Nośnik USB jest niedostępny."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Brak karty SD."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Certyfikaty może instalować tylko właściciel tego urządzenia."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Przeznaczenie tych danych logowania:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN i aplikacje"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Profil Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Szczegóły: %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Szczegóły"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Zainstaluj"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Instalowanie"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Anuluj"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Zamknij"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Brak"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Nazwa: %1$s\nPełna nazwa domenowa: %2$s\nKonsorcja roamingowe: %3$s\nObszar: %4$s\nMetoda uwierzytelniania: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Nazwa użytkownika: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Certyfikat klienta:\n%1$s\nKlucz: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Certyfikat zaufania:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Zainstalowano dane logowania"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Dane logowania do <xliff:g id="NAME">%1$s</xliff:g> zostały dodane do zapisanych sieci Wi-Fi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"GOTOWE"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Zainstaluj dane logowania do sieci Wi-Fi, by łączyć się z sieciami dostępnymi przez <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"W pobranym pliku występują błędy i nie można go zainstalować. Upewnij się, że został on pobrany z właściwego źródła."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Nie można zainstalować danych logowania do sieci Wi-Fi. Pobierz plik ponownie."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Instalacja została anulowana"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Nie można zainstalować"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Włącz Wi-Fi i spróbuj ponownie."</string></resources>
