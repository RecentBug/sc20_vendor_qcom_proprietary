    <string name="wifi_installer_download_error">"Faili iliyopakuliwa ina matatizo na haiwezi kusakinishwa. Hakikisha kuwa ulipakua faili hiyo kutoka chanzo sahihi."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Haiwezi kusakinisha kitambulisho cha Wi-Fi. Jaribu kupakua faili tena."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Usakinishaji umeghairiwa"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Haiwezi kusakinisha"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Washa Wi-Fi kisha ujaribu tena."</string></resources>
