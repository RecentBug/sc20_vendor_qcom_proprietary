<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Zertifikats-Installer"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Zertifikat auswählen"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Zertifikat extrahieren"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Zertifikate werden extrahiert..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Zertifikate aus %s extrahieren"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Zertifikat benennen"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Zertifikatname:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Geben Sie das Passwort zum Extrahieren der Zertifikate ein."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Das Paket enthält Folgendes:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Zertifikate im PKCS12-Keystore"</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"ein Nutzerschlüssel"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"ein Nutzerzertifikat"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"ein CA-Zertifikat"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d CA-Zertifikate"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Geben Sie das richtige Passwort ein."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Geben Sie das Passwort ein."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Geben Sie einen Namen ein."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Geben Sie einen Namen ein, der ausschließlich aus Buchstaben und Zahlen besteht."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Das Zertifikat konnte nicht gespeichert werden. Der Speicher für die Anmeldeinformationen wurde nicht aktiviert oder falsch initialisiert."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Das Zertifikat ist nicht installiert."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Kein zu installierendes Zertifikat"</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Das Zertifikat ist ungültig."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> ist installiert."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Installation nicht möglich, weil das Zertifikat zu groß ist"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Installation nicht möglich, weil die Zertifikatdatei nicht gefunden wurde"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Installation nicht möglich, weil die Zertifikatdatei nicht gelesen werden konnte"</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Keine Zertifikatdatei auf USB-Speicher gefunden."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Auf der SD-Karte wurde keine Zertifikats-Datei gefunden."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Kein USB-Speicher verfügbar"</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"SD-Karte ist nicht vorhanden."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Nur der Eigentümer dieses Geräts kann Zertifikate installieren."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Verwendung der Anmeldedaten:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN und Apps"</item>
        <item>"WLAN"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"WLAN-Profile"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Details für %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Details"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Installieren"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Installation"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Abbrechen"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Schließen"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Keine"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Name: %1$s\nFQDN: %2$s\nRoaming-Konsortien: %3$s\nBereich: %4$s\nAuthentifizierungsmethode: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Nutzername: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Clientzertifikat:\n%1$s\nSchlüssel: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Vertrauenswürdiges Zertifikat:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Anmeldedaten installiert"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Die Anmeldedaten für <xliff:g id="NAME">%1$s</xliff:g> wurden zu den gespeicherten WLANs hinzugefügt."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"Fertig"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Installieren Sie WLAN-Anmeldedaten, um eine Verbindung mit Netzwerken herzustellen, die über <xliff:g id="NAME">%1$s</xliff:g> verfügbar sind."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"Die heruntergeladene Datei enthält Fehler und kann nicht installiert werden. Vergewissern Sie sich, dass Sie die Datei von der korrekten Quelle heruntergeladen haben."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Die WLAN-Anmeldedaten können nicht installiert werden. Laden Sie die Datei erneut herunter."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Installation abgebrochen"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Installation nicht möglich"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Aktivieren Sie WLAN und versuchen Sie es erneut."</string></resources>
