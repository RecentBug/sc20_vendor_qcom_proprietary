<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Програм за инсталирање сертификата"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Избор сертификата"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Издвајање сертификата"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Издвајање..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Издвајање из датотеке %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Именовање сертификата"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Назив сертификата:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Унесите лозинку да бисте издвојили сертификате."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Пакет обухвата:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Сертификати у PKCS12 складишту за кључ."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"кључ за једног корисника"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"сертификат за једног корисника"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"један CA сертификат"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d CA сертификати"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Унесите тачну лозинку."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Унесите лозинку."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Унесите име."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Унесите име састављено само од слова и бројева."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Није могуће сачувати сертификат. Складиште акредитива није омогућено или није правилно покренуто."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Сертификат није инсталиран."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Нема сертификата за инсталирање."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Сертификат је неважећи."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> је инсталиран."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Инсталација није могућа јер је сертификат превелик."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Инсталација није могућа јер није могуће пронаћи датотеку сертификата."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Инсталација није могућа јер није могуће читати датотеку сертификата."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"У USB меморији није пронађена ниједна датотека са сертификатом."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"На SD картици није пронађена ниједна датотека сертификата."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"USB меморија није доступна."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"SD картица није уметнута."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Само власник овог уређаја може да инсталира сертификате."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Коришћење акредитива:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN и апликације"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Профил за Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Детаљи за %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Детаљи"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Инсталирај"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Инсталира се"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Откажи"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Одбаци"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Ниједно"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Назив: %1$s\nПуно име домена: %2$s\nКонзорцијуми за роминг: %3$s\nИменована област: %4$s\nМетод потврде аутентичности: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Корисничко име: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Сертификат клијента:\n%1$s\nКључ: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Поуздани сертификат:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Акредитив је инсталиран"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Акредитив за <xliff:g id="NAME">%1$s</xliff:g> је додат у сачуване Wi-Fi мреже."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"ГОТОВО"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Инсталирајте акредитив за Wi-Fi да бисте се повезали на мреже доступне преко <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"Постоје проблеми у преузетој датотеци и она не може да се инсталира. Проверите да ли сте датотеку преузели из одговарајућег извора."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Инсталирање акредитива за Wi-Fi није могуће. Покушајте да поново преузмете датотеку."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Инсталација је отказана"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Није могуће инсталирати"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Укључите Wi-Fi и покушајте поново."</string></resources>
