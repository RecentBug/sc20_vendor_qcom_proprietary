<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Inštalátor certifikátov"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Vyberte certifikát"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Extrahovať certifikát"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Prebieha extrahovanie..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Extrahovať z %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Zadajte názov certifikátu"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Názov certifikátu:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Ak chcete extrahovať certifikáty, zadajte heslo."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Balíček obsahuje:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Certifikáty v úložisku kľúčov PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"kľúč pre jedného používateľa"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"certifikát pre jedného používateľa"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"jeden certifikát CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d certifikátov CA"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Zadajte správne heslo."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Zadajte heslo."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Zadajte názov"</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Zadajte názov, ktorý obsahuje iba písmená a čísla."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Certifikát sa nepodarilo uložiť. Úložisko poverení nie je povolené alebo nie je správne inicializované."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Certifikát nie je nainštalovaný."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Nebol nájdený žiadny certifikát na inštaláciu."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Certifikát je neplatný."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"Bolo nainštalované poverenie <xliff:g id="CREDENTIAL">%s</xliff:g>."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Inštalácia zlyhala, pretože súbor certifikátu je príliš veľký."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Inštalácia zlyhala, pretože sa nepodarilo nájsť súbor s certifikátom."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Inštalácia zlyhala, pretože sa nepodarilo prečítať súbor s certifikátom."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"V ukladacom priestore USB nebol nájdený žiadny súbor certifikátu."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Na karte SD nebol nájdený žiadny certifikát."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Ukl. priestor USB je nedostup."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Karta SD nie je k dispozícii."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Certifikáty môže inštalovať iba vlastník tohto zariadenia."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Použitie poverení:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN a aplikácie"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Profil Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Podrobné informácie o sieti %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Podrobné informácie"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Inštalovať"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Inštalácia"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Zrušiť"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Odmietnuť"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Žiadne"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Názov: %1$s\nPresne stanovený názov domény (FQDN): %2$s\nRoamingové konzorciá: %3$s\nOblasť: %4$s\nMetóda overenia totožnosti: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Používateľské meno: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Certifikát klienta:\n%1$s\nKľúč: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Certifikát dôveryhodnosti:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Poverenia boli nainštalované"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Poverenia rozhrania <xliff:g id="NAME">%1$s</xliff:g> boli pridané do uložených sietí Wi-Fi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"HOTOVO"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Nainštaluje poverenia siete Wi-Fi na pripojenie k sieťam dostupným prostredníctvom rozhrania <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"V stiahnutom súbore sa vyskytli problémy a nedá sa nainštalovať. Skontrolujte, či ste ho stiahli zo správneho zdroja."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Poverenia do siete Wi-Fi nie je možné nainštalovať. Skúste znova stiahnuť súbor."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Inštalácia bola zrušená"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Nedá sa nainštalovať"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Zapnite Wi-Fi a skúste to znova."</string></resources>
