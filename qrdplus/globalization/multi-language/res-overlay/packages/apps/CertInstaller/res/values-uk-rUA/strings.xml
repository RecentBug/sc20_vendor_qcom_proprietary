<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Прогр. встан. серт-тів"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Вибрати сертифікат"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Отримати сертифікат"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Видобування..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Отримати з %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Дайте назву сертифікату"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Назва сертифіката:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Введіть пароль, щоб видобути сертифікати."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Пакет містить:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Сертиф-ти в сховищі ключів PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"один ключ корист."</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"один сертифікат корист."</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"один сертифікат ЦС"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d сертифікати ЦА"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Введіть правильний пароль."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Введіть пароль."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Введіть ім\'я."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Введіть назву, яка містить лише літери та цифри."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Не вдалося зберегти сертифікат. Сховище облікових даних не ввімкнено або не ініціалізовано належним чином."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Сертифікат не встановлено."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Немає сертиф. для встановл."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Сертифікат недійсний."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> встановлено."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Не вдалося встановити, оскільки розмір сертифіката завеликий."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Не вдалося встановити, оскільки неможливо знайти файл сертифіката."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Не вдалося встановити, оскільки неможливо зчитати файл сертифіката."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"На носії USB не знайдено жодного файлу сертифікату."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"На карті SD не знайд. жодного файлу сертиф."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Носій USB недоступний."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Відсутня карта SD."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Лише власник цього пристрою може встановлювати сертифікати."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Використання облікових даних:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"Мережа VPN і програми"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Профіль мережі Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"%s: деталі"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Деталі"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Установити"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Встановлення"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Скасувати"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Закрити"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Немає"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Назва: %1$s\nFQDN: %2$s\nКонсорціуми з питань роумінгу: %3$s\nКраїна: %4$s\nМетод автентифікації: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Ім’я користувача: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Сертифікат клієнта:\n%1$s\nКлюч: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Сертифікат довіреної особи чи компанії:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Облікові дані встановлено"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Облікові дані для мережі \"<xliff:g id="NAME">%1$s</xliff:g>\" додано до збережених мереж Wi-Fi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"ГОТОВО"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Установіть облікові дані для Wi-Fi, щоб під’єднуватися до доступних мереж через мережу \"<xliff:g id="NAME">%1$s</xliff:g>\"."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"Не вдається встановити завантажений файл. Переконайтеся, що ви завантажили його з потрібного джерела."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Не вдається встановити облікові дані для Wi-Fi. Завантажте файл знову."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Встановлення скасовано"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Не встановлено"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Увімкніть Wi-Fi та повторіть спробу."</string></resources>
