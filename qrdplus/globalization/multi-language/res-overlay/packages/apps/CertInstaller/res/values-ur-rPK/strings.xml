    <string name="install_done_title">"اسنادات کو انسٹال کر دیا گیا"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"‏<xliff:g id="NAME">%1$s</xliff:g> اسنادات کو Wi-Fi کی جانب سے محفوظ کردہ نیٹ ورکس میں شامل کر دیا گیا ہے۔"</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"ہوگیا"</string>
