    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"ಪ್ರಮಾಣಪತ್ರವನ್ನು ಸ್ಥಾಪಿಸಲಾಗಿಲ್ಲ."</string>
