<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Program de instalare a certificatului"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Alegeţi un certificat"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Extrageţi certificatul"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Se extrage..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Extrageţi din %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Denumiţi certificatul"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Nume certificat:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Introduceţi parola pentru a extrage certificatele."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Pachetul conţine:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Certificate în depozitul de chei PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"o cheie pentru utilizator"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"un certificat pentru utilizator"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"un certificat CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d certificate CA"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Introduceţi parola corectă."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Introduceţi parola."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Introduceţi un nume."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Introduceţi un nume care conţine doar litere şi numere."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Nu se poate salva certificatul. Spaţiul de stocare a certificatelor nu este activat sau nu este corect iniţializat."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Certificatul nu este instalat."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Niciun certificat de instalat."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Certificatul este nevalid."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> este instalat."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Nu se poate instala, deoarece dimensiunea certificatului este prea mare."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Nu se poate instala, deoarece fişierul certificat nu a putut fi găsit."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Nu se poate instala, deoarece fişierul certificat nu a putut fi citit."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Nu a fost găsit niciun fişier certificat în stocarea USB."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Nu s-a găsit niciun fişier certificat pe cardul SD."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Stocarea USB nedisponibilă."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Nu există card SD."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Numai proprietarul acestui dispozitiv poate instala certificate."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Utilizarea datelor de conectare:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN și aplicații"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Profil Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Detalii pentru %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Detalii"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Instalați"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Se instalează"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Anulați"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Închideți"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Niciuna"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Nume: %1$s\nFQDN: %2$s\nRoaming Consortiums: %3$s\nRealm: %4$s\nMetodă de autentificare: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Nume de utilizator: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Certificat client:\n%1$s\nCheie: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Certificat de încredere:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Datele de conectare au fost instalate"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Datele de conectare la <xliff:g id="NAME">%1$s</xliff:g> au fost adăugate la rețelele Wi-Fi salvate."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"TERMINAT"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Instalați datele de conectare la Wi-Fi pentru a vă conecta la rețelele disponibile prin <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"Fișierul descărcat are probleme și nu poate fi instalat. Asigurați-vă că ați descărcat fișierul din sursa corespunzătoare."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Datele de conectare la Wi-Fi nu pot fi instalate. Încercați să descărcați din nou fișierul."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Instalarea a fost anulată"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Nu se poate instala"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Activați conexiunea Wi-Fi și încercați din nou."</string></resources>
