    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> इंस्‍टॉल किया हुआ है."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"प्रमाणपत्र का आकार बहुत बड़ा होने के कारण इंस्‍टॉल नहीं कर सका."</string>
