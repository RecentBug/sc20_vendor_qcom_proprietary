    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d CA प्रमाणपत्र"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"सही पासवर्ड लिखें."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"पासवर्ड लिखें."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"नाम लिखें."</string>
