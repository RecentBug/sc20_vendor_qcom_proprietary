<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Instalador de certificado"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Escolher um certificado"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Extrair certificado"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Extraindo..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Extrair de %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Nomear o certificado"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Nome do certificado:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Digite a senha para extrair os certificados."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"O pacote contém:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Certificados no armazenamento de chaves PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"uma chave de usuário"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"um certificado de usuário"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"um certificado CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d certificados CA"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Digite a senha correta."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Digite a senha."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Digite um nome."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Digite um nome que contenha apenas letras e números."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Não foi possível salvar o certificado. Armazenamento de credenciais desativado ou não inicializado corretamente."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"O certificado não está instalado."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Não há certificado para instalar."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"O certificado é inválido."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"A <xliff:g id="CREDENTIAL">%s</xliff:g> está instalada"</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Não foi possível concluir a instalação, pois o tamanho do certificado é muito grande."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"A instalação não pôde ser concluída, pois não foi possível localizar o arquivo do certificado."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"A instalação não pôde ser concluída, pois não foi possível ler o arquivo do certificado."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Nenhum arquivo de certificado foi encontrado no armazenamento USB."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Nenhum arquivo de certificado foi encontrado no cartão SD."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Armazenam. USB não disponível."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"O cartão SD não está presente."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Somente o dono do dispositivo pode instalar certificados."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Uso da credencial:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN e apps"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Perfil de Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Detalhes sobre %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Detalhes"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Instalar"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Instalando"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Cancelar"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Dispensar"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Nenhum"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Nome: %1$s\nFQDN: %2$s\nConsórcios de roaming: %3$s\nRealm: %4$s\nMétodo de autenticação: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Nome de usuário: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Certificados do cliente:\n%1$s\nChave: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Certificado de confiança:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Credenciais instaladas"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"As credenciais de <xliff:g id="NAME">%1$s</xliff:g> foram adicionadas a redes Wi-Fi salvas."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"CONCLUÍDO"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Instalar credenciais de Wi-Fi para se conectar a redes disponíveis via <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"O arquivo transferido por download tem problemas e não pode ser instalado. Certifique-se de que você tenha feito o download do arquivo da fonte correta."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Não foi possível instalar as credenciais de Wi-Fi. Tente fazer o download do arquivo novamente."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Instalação cancelada"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Não é possível realizar a instalação"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Ative o Wi-Fi e tente novamente."</string></resources>
