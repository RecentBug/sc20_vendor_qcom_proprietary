<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Sertifika Yükleyicisi"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Sertifika seçin"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Sertifikayı çıkart"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Çıkarılıyor..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"%s dosyasından ayıkla"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Sertifikayı adlandırın"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Sertifika adı:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Sertifikaları çıkarmak için şifreyi yazın."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Paket şunları içeriyor:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"PKCS12 anahtar deposundaki sertifikalar."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"bir kullanıcı anahtarı"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"bir kullanıcı sertifikası"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"bir CA sertifikası"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d CA sertifikası"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Doğru şifreyi yazın."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Şifreyi yazın."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Bir ad yazın."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Yalnızca harflerden ve rakamlardan oluşan bir ad yazın."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Sertifika kaydedilemedi. Kimlik bilgileri deposu etkinleştirilmemiş veya düzgün bir şekilde başlatılmamış."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Sertifika yüklenmemiş."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Yüklenecek sertifika yok."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Sertifika geçersiz."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> yüklendi."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Sertifika boyutu çok büyük olduğundan yüklenemedi."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Sertifika dosyası bulunamadığından yüklenemedi."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Sertifika dosyası okunamadığından yüklenemedi."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"USB bellekte hiçbir sertifika dosyası bulunamadı."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"SD kartta hiçbir sertifika dosyası bulunamadı."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"USB bellek kullanılamıyor."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"SD kart yok."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Sadece bu cihazın sahibi sertifikaları yükleyebilir."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Kimlik bilgisinin kullanımı:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN ve uygulamalar"</item>
        <item>"Kablosuz"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Kablosuz Ağ Profili"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"%s ayrıntıları"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Ayrıntılar"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Yükle"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Yükleniyor"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"İptal"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Kapat"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Yok"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Ad: %1$s\nTam Nitelikli Alan Adı: %2$s\nDolaşım Konsorsiyumları: %3$s\nYetki Alanı: %4$s\nKimlik doğrulama yöntemi: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Kullanıcı adı: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"İstemci sertifikası:\n%1$s\nAnahtar: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Güven sertifikası:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Kimlik bilgileri yüklendi"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"<xliff:g id="NAME">%1$s</xliff:g> kimlik bilgileri kayıtlı kablosuz ağlara eklendi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"BİTTİ"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"<xliff:g id="NAME">%1$s</xliff:g> aracılığıyla kullanılabilen ağlara bağlanmak için kablosuz kimlik bilgilerini yükleyin."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"İndirilen dosyada sorunlar var ve yüklenemiyor. Dosyayı doğru kaynaktan indirdiğinizden emin olun."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Kablosuz kimlik bilgileri yüklenemiyor. Dosyayı tekrar indirmeyi deneyin."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Yükleme işlemi iptal edildi"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Yüklenemiyor"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Kablosuz bağlantıyı açın ve tekrar deneyin."</string></resources>
