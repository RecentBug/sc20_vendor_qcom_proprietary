    <string name="install_done">"<xliff:g id="NAME">%1$s</xliff:g> நற்சான்றிதழ், சேமித்த வைஃபை நெட்வொர்க்குகளில் சேர்க்கப்பட்டது."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"முடிந்தது"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
