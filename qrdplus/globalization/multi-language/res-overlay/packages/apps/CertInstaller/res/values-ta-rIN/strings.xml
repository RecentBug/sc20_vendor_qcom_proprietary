    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"இறக்கப்பட்ட கோப்பில் சிக்கல்கள் உள்ளதால், அதை நிறுவ முடியாது. சரியான மூலத்திலிருந்து கோப்பை இறக்கியதை உறுதிப்படுத்திக்கொள்ளவும்."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"வைஃபை நற்சான்றிதழை நிறுவ முடியவில்லை. கோப்பை மீண்டும் இறக்க முயற்சிக்கவும்."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"நிறுவுதல் ரத்துசெய்யப்பட்டது"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"நிறுவ முடியவில்லை"</string>
