<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Installer ng Certificate"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Pumili ng certificate"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"I-extract ang certificate"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Ine-extract\u2026"</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"I-extract mula sa %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Pangalanan ang certificate"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Pangalan ng certificate:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"I-type ang password upang i-extract ang mga certificate."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Naglalaman ang package ng:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Mga certificate sa PKCS12 keystore."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"isang key ng user"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"isang certificate ng user"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"isang certficate ng CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"%d Mga certificate ng CA"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"I-type ang tamang password."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"I-type ang password."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Mag-type ng pangalan."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Mag-type ng pangalan na naglalaman lamang ng mga titik at numero."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Hindi mai-save ang certificate. Hindi pinagana o maayos na nasimulan ang storage ng kredensyal."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Hindi naka-install ang certificate."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Walang ii-install na certificate."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Di-wasto ang certificate."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"Naka-install ang <xliff:g id="CREDENTIAL">%s</xliff:g>."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Hindi ma-install dahil masyadong malaki ang certificate."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Hindi ma-install dahil hindi mahanap ang file ng certificate."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Hindi mai-install dahil hindi mabasa ang file ng certificate."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Walang nakitang file ng certificate sa imbakan na USB."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Walang nakitang file ng certificate sa SD card."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Hindi available imbakan na USB"</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Wala ang SD card."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Ang may-ari lang ng device na ito ang maaaring mag-install ng mga certificate."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Paggamit ng kredensyal:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN at apps"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Profile ng Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Mga detalye para sa %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Mga Detalye"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"I-install"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Nag-i-install"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Kanselahin"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"I-dismiss"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Wala"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Pangalan: %1$s\nFQDN: %2$s\nMga Roaming Consortium: %3$s\nRealm: %4$s\nParaan ng pagpapatotoo: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"User name: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Client certificate:\n%1$s\nKey: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Trust certificate:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Na-install ang mga credential"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Naidagdag ang mga credential ng <xliff:g id="NAME">%1$s</xliff:g> sa mga naka-save na network ng Wi-Fi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"TAPOS NA"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"I-install ang mga credential ng Wi-Fi upang kumonekta sa mga network na available sa pamamagitan ng <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"May problema ang mga na-download na file at hindi ma-install ang mga ito. Tiyaking na-download mo ang file mula sa tamang pinagmulan."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Hindi ma-install ang mga credential ng Wi-Fi. Subukang muli ang pagda-download ng file."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Kinansela ang pag-install"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Hindi makapag-install"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"I-on ang Wi-Fi at subukang muli."</string></resources>
