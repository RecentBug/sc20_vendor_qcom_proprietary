    <string name="name_credential_dialog_title">"ప్రమాణపత్రానికి పేరు పెట్టండి"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"ప్రమాణపత్రం పేరు:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"ప్రమాణపత్రాలను సంగ్రహించడానికి పాస్‌వర్డ్‌ను టైప్ చేయండి."</string>
