    <string name="p12_description">"PKCS12 కీస్టోర్‌లోని ప్రమాణపత్రాలు."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"ఒక వినియోగదారు కీ"</string>
