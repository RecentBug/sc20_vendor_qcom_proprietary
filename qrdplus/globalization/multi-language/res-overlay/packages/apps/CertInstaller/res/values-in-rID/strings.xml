<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Pemasang Sertifikat"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Pilih sertifikat"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Ekstrak sertifikat"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Mengekstrak..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Ekstrak dari %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Beri nama sertifikat"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Nama sertifikat:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Ketikkan sandi untuk mengekstrak sertifikat."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Paket terdiri dari:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Sertifikat dalam PKCS12 keystore."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"satu kunci pengguna"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"satu sertifikat pengguna"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"satu Sertifikat CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"Sertifikat CA %d"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Ketik sandi yang benar."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Ketikkan sandi."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Ketikkan nama."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Ketikkan nama yang hanya berisi huruf dan angka."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Tidak dapat menyimpan sertifikat. Penyimpanan kredensial tidak diaktifkan atau dimulai dengan semestinya."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Sertifikat tidak terpasang."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Tidak ada sertifikat untuk memasang."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Sertifiakt tidak valid."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> telah dipasang."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Tidak dapat memasang karena ukuran sertifikat terlalu besar."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Tidak dapat memasang karena file sertifikat tidak dapat ditemukan."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Tidak dapat memasang karena file sertifikat tidak dapat dibaca."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"Tidak ada file sertifikat dalam penyimpanan USB."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"Tidak ada file sertifikat di dalam kartu SD."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"Penyimpanan USB tidak tersedia."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"Kartu SD tidak ada."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Hanya pemilik perangkat ini yang dapat memasang sertifikat."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Penggunaan kredensial:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN dan aplikasi"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Profil Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"Detail untuk %s"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Detail"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Pasang"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Memasang"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Batal"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Tutup"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Tidak ada"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Nama: %1$s\nFQDN: %2$s\nKonsorsium Roaming: %3$s\nRanah: %4$s\nMetode autentikasi: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Nama pengguna: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Sertifikat klien:\n%1$s\nKunci: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Sertifikat kepercayaan:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Kredensial yang terpasang"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Kredensial <xliff:g id="NAME">%1$s</xliff:g> telah ditambahkan ke jaringan Wi-Fi yang tersimpan."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"SELESAI"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Pasang kredensial Wi-Fi untuk menyambungkan ke jaringan yang tersedia melalui <xliff:g id="NAME">%1$s</xliff:g>."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"File yang diunduh bermasalah dan tidak dapat dipasang. Pastkan Anda mengunduh file dari sumber yang benar."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Tidak dapat memasang kredensial Wi-Fi. Coba unduh file lagi."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Pemasangan dibatalkan"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Tidak dapat memasang"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Aktifkan Wi-Fi dan coba lagi."</string></resources>
