    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"ไม่สามารถติดตั้งได้เนื่องจากไม่สามารถอ่านไฟล์ใบรับรอง"</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"ไม่พบไฟล์ใบรับรองในที่เก็บข้อมูล USB"</string>
