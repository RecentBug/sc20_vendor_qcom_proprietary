<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"Программа установки сертификата"</string>

    <!-- Title of the file picker screen -->
    <add-resource type="string" name="pick_file_title"/>
    <string name="pick_file_title">"Выбор сертификата"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <add-resource type="string" name="pkcs12_password_dialog_title"/>
    <string name="pkcs12_password_dialog_title">"Извлечение сертификата"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <add-resource type="string" name="extracting_pkcs12"/>
    <string name="extracting_pkcs12">"Загрузка..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <add-resource type="string" name="pkcs12_file_password_dialog_title"/>
    <string name="pkcs12_file_password_dialog_title">"Извлечение из %s"</string>
    <!-- Title of dialog to name a credential -->
    <add-resource type="string" name="name_credential_dialog_title"/>
    <string name="name_credential_dialog_title">"Укажите название сертификата"</string>
    <!-- Description for the credential name input box -->
    <add-resource type="string" name="credential_name"/>
    <string name="credential_name">"Название сертификата:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <add-resource type="string" name="credential_password"/>
    <string name="credential_password">"Введите пароль для извлечения сертификатов."</string>
    <add-resource type="string" name="credential_info"/>
    <string name="credential_info">"Пакет содержит:"</string>
    <add-resource type="string" name="p12_description"/>
    <string name="p12_description">"Сертификат находится в хранилище ключей PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_userkey"/>
    <string name="one_userkey">"один ключ пользователя"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_usercrt"/>
    <string name="one_usercrt">"один пользовательский сертификат"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="one_cacrt"/>
    <string name="one_cacrt">"один сертификат центра сертификации"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <add-resource type="string" name="n_cacrts"/>
    <string name="n_cacrts">"Сертификатов центра сертификации: %d"</string>

    <add-resource type="string" name="password_error"/>
    <string name="password_error">"Введен неверный пароль."</string>
    <add-resource type="string" name="password_empty_error"/>
    <string name="password_empty_error">"Введите пароль."</string>
    <add-resource type="string" name="name_empty_error"/>
    <string name="name_empty_error">"Введите название."</string>
    <add-resource type="string" name="name_char_error"/>
    <string name="name_char_error">"Можно использовать только буквы и цифры."</string>
    <add-resource type="string" name="unable_to_save_cert"/>
    <string name="unable_to_save_cert">"Не удалось сохранить сертификат, поскольку хранилище учетных данных не включено или не инициализировано."</string>
    <add-resource type="string" name="cert_not_saved"/>
    <string name="cert_not_saved">"Сертификат не установлен."</string>
    <add-resource type="string" name="no_cert_to_saved"/>
    <string name="no_cert_to_saved">"Отсутствует сертификат для установки."</string>
    <add-resource type="string" name="invalid_cert"/>
    <string name="invalid_cert">"Сертификат недействителен."</string>

    <!-- toast message -->
    <add-resource type="string" name="cert_is_added"/>
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> установлен."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_too_large_error"/>
    <string name="cert_too_large_error">"Установка не выполнена, поскольку сертификат слишком большой."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_missing_error"/>
    <string name="cert_missing_error">"Установка не выполнена, поскольку не удалось найти файл сертификата."</string>
    <!-- toast message -->
    <add-resource type="string" name="cert_read_error"/>
    <string name="cert_read_error">"Установка не выполнена, поскольку не удалось открыть файл сертификата."</string>

    <!-- Shown when the certificate file can't be found.  [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="nosdcard">"На USB-накопителе отсутствует файл сертификата."</string>
    <add-resource type="string" name="no_cert_file_found"/>
    <string name="no_cert_file_found" product="default">"На SD-карте отсутствует файл сертификата."</string>
    <!-- Shown when USB storage can't be found to look for a certificate.  [CHAR LIMIT=30] -->
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="nosdcard">"USB-накопитель недоступен."</string>
    <add-resource type="string" name="sdcard_not_present"/>
    <string name="sdcard_not_present" product="default">"SD-карта отсутствует."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="only_primary_user_allowed"/>
    <string name="only_primary_user_allowed">"Сертификаты может устанавливать только владелец устройства."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <add-resource type="string" name="credential_usage_label"/>
    <string name="credential_usage_label">"Использовать аккаунт:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <add-resource type="array" name="credential_usage"/>
    <string-array name="credential_usage">
        <item>"VPN и приложения"</item>
        <item>"Wi-Fi"</item>
    </string-array>

    <add-resource type="string" name="wifi_title"/>
    <string name="wifi_title">"Профиль Wi-Fi"</string>
    <add-resource type="string" name="wifi_detail_title"/>
    <string name="wifi_detail_title">"%s: информация"</string>
    <add-resource type="string" name="wifi_detail_label"/>
    <string name="wifi_detail_label">"Информация"</string>
    <add-resource type="string" name="wifi_install_label"/>
    <string name="wifi_install_label">"Установить"</string>
    <!-- Toast message to be displayed when installing Hotspot 2.0 credentials [CHAR LIMIT=40] -->
    <add-resource type="string" name="wifi_installing_label"/>
    <string name="wifi_installing_label">"Устанавливается"</string>
    <add-resource type="string" name="wifi_cancel_label"/>
    <string name="wifi_cancel_label">"Отмена"</string>
    <add-resource type="string" name="wifi_dismiss_label"/>
    <string name="wifi_dismiss_label">"Закрыть"</string>
    <add-resource type="string" name="wifi_no_config"/>
    <string name="wifi_no_config">"Нет"</string>
    <add-resource type="string" name="wifi_config_text"/>
    <string name="wifi_config_text">"Имя: %1$s\nFQDN: %2$s\nРоуминг: %3$s\nОбласть: %4$s\nСпособ авторизации: EAP-%5$s\n"</string>
    <add-resource type="string" name="wifi_ttls_config_text"/>
    <string name="wifi_ttls_config_text">"Имя пользователя: %s\n"</string>
    <add-resource type="string" name="wifi_tls_config_text"/>
    <string name="wifi_tls_config_text">"Сертификат клиента:\n%1$s\nКлюч: %2$s\n"</string>
    <add-resource type="string" name="wifi_sim_config_text"/>
    <string name="wifi_sim_config_text">SIM: %s\n</string>
    <add-resource type="string" name="wifi_trust_config_text"/>
    <string name="wifi_trust_config_text">"Cертификат доверия:\n%s\n"</string>

    <add-resource type="string" name="install_done_title"/>
    <string name="install_done_title">"Установка завершена"</string>
    <add-resource type="string" name="install_done"/>
    <string name="install_done">"Учетные данные (<xliff:g id="NAME">%1$s</xliff:g>) добавлены в базу сохраненных сетей Wi-Fi."</string>
    <add-resource type="string" name="done_label"/>
    <string name="done_label">"ГОТОВО"</string>
    <add-resource type="string" name="wifi_installer_detail"/>
    <string name="wifi_installer_detail">"Чтобы подключаться к сетям Wi-Fi, доступным через <xliff:g id="NAME">%1$s</xliff:g>, установите сертификат."</string>
    <add-resource type="string" name="wifi_installer_download_error"/>
    <string name="wifi_installer_download_error">"Невозможно установить сертификат. Убедитесь, что вы скачали его из надежного источника."</string>
    <add-resource type="string" name="wifi_installer_fail"/>
    <string name="wifi_installer_fail">"Не удалось установить сертификат доступа к Wi-Fi. Попробуйте скачать файл ещё раз."</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi_title"/>
    <string name="wifi_installer_fail_no_wifi_title">"Установка отменена"</string>
    <add-resource type="string" name="wifi_installer_fail_title"/>
    <string name="wifi_installer_fail_title">"Не удалось выполнить установку"</string>
    <add-resource type="string" name="wifi_installer_fail_no_wifi"/>
    <string name="wifi_installer_fail_no_wifi">"Включите Wi-Fi и повторите попытку."</string></resources>
