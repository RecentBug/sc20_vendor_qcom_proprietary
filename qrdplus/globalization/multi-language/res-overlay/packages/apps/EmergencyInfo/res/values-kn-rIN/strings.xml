    <!-- Title of dialog shown to the user to confirm that he understands that emergency information is shown in emergency dialer [CHAR LIMIT=30] -->
    <add-resource type="string" name="user_emergency_info_title"/>
    <string name="user_emergency_info_title">"ತುರ್ತು ಮಾಹಿತಿ"</string>
    <!-- Message of dialog shown to user to confirm that he understands that emergency information is shown in emergency dialer [CHAR LIMIT=none] -->
    <add-resource type="string" name="user_emergency_info_consent"/>
    <string name="user_emergency_info_consent">"ಇಲ್ಲಿ ನಮೂದಿಸುವ ಮಾಹಿತಿಯು ತುರ್ತುಪರಿಸ್ಥಿತಿಯಲ್ಲಿ ಮೊದಲು ಸಹಾಯಕ್ಕೆ ಬರುವವರಿಗೆ ಪ್ರಯೋಜನಕಾರಿಯಾಗಿರುತ್ತದೆ. ಇದು ನಿಮ್ಮ ಸಾಧನದಲ್ಲಿ ಮಾತ್ರ ಸಂಗ್ರಹವಾಗಿರುತ್ತದೆ. ಇದನ್ನು ತುರ್ತು ಡಯಲರ್‌ನಲ್ಲಿ, ನಿಮ್ಮ ಫೋನ್‌ ಅನ್‌ಲಾಕ್ ಮಾಡದೆಯೇ, ಯಾರು ಬೇಕಾದರೂ ಓದಬಹುದು."</string>
    <!-- Label for button in confirmation dialog when the user clicks on emergency information [CHAR LIMIT=15] -->
    <add-resource type="string" name="emergency_info_continue"/>
    <string name="emergency_info_continue">"ಮುಂದುವರಿಸು"</string>
    <!-- Message shown when no emergency information has been provided by the user [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_info_provided"/>
    <string name="no_info_provided">"ಫೋನ್‌ನ ಮಾಲೀಕರ ಕುರಿತು ಯಾವುದೇ ಮಾಹಿತಿ ಇಲ್ಲ"</string>
    <!-- Extra message to indicate to the user that they can add emergency information [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="tap_pencil"/>
    <string name="tap_pencil">"ಇದು ನಿಮ್ಮ ಫೋನ್ ಆಗಿದ್ದಲ್ಲಿ, ತುರ್ತು ಸಂದರ್ಭದಲ್ಲಿ ಇಲ್ಲಿ ಪ್ರದರ್ಶಿಸಲಾಗುವ ಮಾಹಿತಿಯನ್ನು ಸೇರಿಸಲು ಪೆನ್ಸಿಲ್ ಐಕಾನ್ ಅನ್ನು ಟ್ಯಾಪ್ ಮಾಡಿ"</string>
    <!-- Label for action item on the menu of the editing screen [CHAR LIMIT=15] -->
