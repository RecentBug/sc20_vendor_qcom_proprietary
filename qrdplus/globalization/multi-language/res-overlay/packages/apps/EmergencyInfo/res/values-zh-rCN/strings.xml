    <add-resource type="string" name="clear"/>
    <string name="clear">"清除"</string>
    <!-- Message of dialog shown to clear all information and contacts [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="clear_all_message"/>
    <string name="clear_all_message">"要清除所有信息和联系人吗？"</string></resources>
