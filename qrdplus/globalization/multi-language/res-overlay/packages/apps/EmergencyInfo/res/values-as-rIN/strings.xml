<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The name of the app. [CHAR_LIMIT=30] -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"জৰুৰীকালিন তথ্য"</string>
    <!-- The description of the pencil icon, for talkback. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="edit_info"/>
