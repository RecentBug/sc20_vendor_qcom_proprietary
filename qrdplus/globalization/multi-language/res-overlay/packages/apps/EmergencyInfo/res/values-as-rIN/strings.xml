    <add-resource type="string" name="no_info_provided"/>
    <string name="no_info_provided">"ফোনৰ গৰাকীৰ বিষয়ে কোনো তথ্য নাই"</string>
    <!-- Extra message to indicate to the user that they can add emergency information [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="tap_pencil"/>
    <string name="tap_pencil">"যদি এইটো আপোনাৰ ফোন, তেন্তে কোনো জৰুৰী অৱস্থাৰ সময়ত ইয়াত প্ৰদৰ্শিত হোৱা তথ্য যোগ কৰিবলৈ পেঞ্চিল চিহ্নত টোকৰ মাৰক"</string>
