    <add-resource type="string" name="user_emergency_info_title"/>
    <string name="user_emergency_info_title">"জৰুৰীকালিন তথ্য"</string>
    <!-- Message of dialog shown to user to confirm that he understands that emergency information is shown in emergency dialer [CHAR LIMIT=none] -->
    <add-resource type="string" name="user_emergency_info_consent"/>
    <string name="user_emergency_info_consent">"আপুনি ইয়াত প্ৰৱেশ কৰা তথ্যই এটা জৰুৰীকালিনত প্ৰথমে সঁহাৰিদাতাক সহায় কৰিব পাৰে। ইয়াক কেৱল আপোনাৰ ডিভাইচত সংগ্ৰহ কৰা হৈছে আৰু আপোনাৰ ফোন আনলক নকৰাকৈয়ে যিকোনোয়ে ইয়াক জৰুৰীকালিন ডায়েলাৰৰ পৰা পঢ়িব পাৰে।"</string>
