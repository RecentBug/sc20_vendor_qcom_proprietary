    <add-resource type="string" name="tab_title_info"/>
    <string name="tab_title_info">"তথ্য"</string>
    <!-- The title of the emergency contacts tab. [CHAR_LIMIT=15] -->
    <add-resource type="string" name="tab_title_contacts"/>
    <string name="tab_title_contacts">"সম্পৰ্কসমূহ"</string>
    <!-- The title of the name preference. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="name"/>
    <string name="name">"নাম"</string>
    <!-- The default preference subtitle text to show when the name is unknown. [CHAR_LIMIT=40] -->
