    <add-resource type="string" name="remove_contact"/>
    <string name="remove_contact">"জৰুৰীকালীন সম্পর্কৰ পৰা <xliff:g id="contact_name" example="John Doe">%1$s</xliff:g>আঁতৰাবনে?  "</string>
    <!-- The description of the remove an emergency contact, for talkback. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="remove_contact_icon_text"/>
    <string name="remove_contact_icon_text">"সম্পৰ্ক আঁতৰাই পেলাওক"</string>
