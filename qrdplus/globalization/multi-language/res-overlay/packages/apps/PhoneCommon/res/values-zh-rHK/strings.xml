    <add-resource type="string" name="callFailed_cb_enabled"/>
    <string name="callFailed_cb_enabled">"啟用通話限制時，無法撥出電話。"</string>
    <!-- In-call screen: call failure reason (call denied because domain specific access control is on) -->
    <add-resource type="string" name="callFailed_dsac_restricted"/>
    <string name="callFailed_dsac_restricted">"通話受存取控制權限制。"</string>
