    <add-resource type="string" name="callFailed_dsac_restricted_emergency"/>
    <string name="callFailed_dsac_restricted_emergency">"緊急通話受存取控制權限制。"</string>
    <!-- In-call screen: call failure reason (Normal call denied because domain specific access control is on)-->
    <add-resource type="string" name="callFailed_dsac_restricted_normal"/>
    <string name="callFailed_dsac_restricted_normal">"正常通話受存取控制權限制。"</string>
