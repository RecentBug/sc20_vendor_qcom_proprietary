    <add-resource type="string" name="callFailed_outOfService"/>
    <string name="callFailed_outOfService">"無法連線至流動網絡"</string>
    <!-- In-call screen: call failure reason (call denied because of current FDN setting) -->
    <add-resource type="string" name="callFailed_fdn_only"/>
    <string name="callFailed_fdn_only">"撥出通話受 FDN 限制。"</string>
