<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"အသံသွင်းစက်"</string>
    <add-resource type="string" name="folder_name"/>
    <string name="folder_name">"အသံဖမ်းနေခြင်း"</string>
    <!-- Screen title before and during recording -->
    <add-resource type="string" name="record_your_message"/>
    <string name="record_your_message">"သင့်စာတိုကို အသံသွင်းနေပါပြီ"</string>
    <!-- Screen title after recording -->
    <add-resource type="string" name="message_recorded"/>
    <string name="message_recorded">"စာတိုကို အသံသွင်းယူပြီးပါပြီ"</string>
    <!-- Screen title while playing back a recording -->
    <add-resource type="string" name="review_message"/>
    <string name="review_message">"စာတိုအား ပြန်လည်ဆန်းစစ်ရန်"</string>
    <!-- Label shown during recording -->
    <add-resource type="string" name="recording"/>
    <string name="recording">"အသံသွင်းယူနေပါပြီ"</string>
    <!-- Label shown when recording is paused -->
    <add-resource type="string" name="recording_paused"/>
    <string name="recording_paused">"ဆိုင်းငံ့ရန်"</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <add-resource type="string" name="recording_stopped"/>
    <string name="recording_stopped">"အသံသွင်းယူခြင်း ရပ်သွားပါပြီ"</string>
    <!-- label shown when there is not enough space to record something -->
    <add-resource type="string" name="storage_is_full"/>
    <string name="storage_is_full">"သိုလှောင်မှုနေရာ ပြည့်နေပါပြီ"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <add-resource type="string" name="max_length_reached"/>
    <string name="max_length_reached">"အများဆုံး အရှည်အတိုင်းအတာကို ရောက်ရှိပါပြီ"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <add-resource type="string" name="insert_sd_card"/>
    <string name="insert_sd_card" product="nosdcard">"USBသိုလှောင်မှုကိုထိုင်ပါ"</string>
    <!-- label shown when there is no sd card available to record to -->
    <add-resource type="string" name="insert_sd_card"/>
    <string name="insert_sd_card" product="default">"SD ကတ်ကို ထည့်သွင်းပါ"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <add-resource type="string" name="min_available"/>
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> မိနစ် ရှိသေးသည်"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <add-resource type="string" name="sec_available"/>
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g>စက္ကန့် ရှိသေးသည်"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <add-resource type="string" name="accept"/>
    <string name="accept">"ပြီးပါပြီ"</string>
    <!-- button to discard the current recording and return to the caller -->
    <add-resource type="string" name="discard"/>
    <string name="discard">"ထားလိုက်တော့"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <add-resource type="string" name="button_ok"/>
    <string name="button_ok">"ကောင်းပြီ"</string>
    <!-- label shown before the user has recorded anything -->
    <add-resource type="string" name="press_record"/>
    <string name="press_record">"ဖိပါ: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP 6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP နှင့် အသံဖမ်းရန် ဖိပါ"</string>
    <add-resource type="string" name="press_record2"/>
    <string name="press_record2">"Record ကို နှိပ်ပါ"</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <add-resource type="string" name="press_record_ssr"/>
    <string name="press_record_ssr">"ဖိပါ: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP 6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP နှင့် အသံဖမ်းရန် ဖိပါ"</string>

    <!-- the name under which recordings will be visible in the media database is formatted like this -->
    <add-resource type="string" name="audio_db_title_format"/>
    <string name="audio_db_title_format"><xliff:g id="format">yyyy-MM-dd HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this 'artist' name -->
    <add-resource type="string" name="audio_db_artist_name"/>
    <string name="audio_db_artist_name">"သင်ဖမ်းထားသော အသံသွင်းမှုများ"</string>
    <!-- all recordings will show up in the media database with this 'album' name -->
    <add-resource type="string" name="audio_db_album_name"/>
    <string name="audio_db_album_name">"အသံသွင်းမှုများ"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <add-resource type="string" name="audio_db_playlist_name"/>
    <string name="audio_db_playlist_name">"ကျွန်ုပ်၏ အသံသွင်းမှုများ"</string>

    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="error_sdcard_access"/>
    <string name="error_sdcard_access" product="nosdcard">"USB သိုလှောင်ကတ်ကို ချိတ်ဆက်ကြည့်ရှု၍မရပါ"</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <add-resource type="string" name="error_sdcard_access"/>
    <string name="error_sdcard_access" product="default">"SD ကတ်ကို ချိတ်ဆက်ကြည့်ရှု၍မရပါ"</string>
    <!-- shown as the message in a dialog when the recording is interrupted internally -->
    <add-resource type="string" name="error_record_interrupted"/>
    <string name="error_record_interrupted">"မှတ်တမ်းတင်မှု အနှောင့်အယှက်ဖြစ်သွားသည်"</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <add-resource type="string" name="error_app_internal"/>
    <string name="error_app_internal">"အပလီကေးရှင်းအတွင်းမှ အမှား"</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <add-resource type="string" name="error_app_unsupported"/>
    <string name="error_app_unsupported">"ပံ့ပိုးမှု မပေးသော စီစဉ်မှုပုံစံ"</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <add-resource type="string" name="error_mediadb_new_record"/>
    <string name="error_mediadb_new_record">"အသံသွင်းထားမှုကို သိမ်းဆည်း၍မရပါ"</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <add-resource type="string" name="error_mediadb_incall"/>
    <string name="error_mediadb_incall">"ဖုန်းခေါ်နေစဉ်သာ၊ ဤရွေးချယ်မှုကို ပံ့ပိုးပေးပါသည်"</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <add-resource type="string" name="error_mediadb_aacincall"/>
    <string name="error_mediadb_aacincall">"AAC အသံခေါ်ဆိုမှုဖြင့် အသံဖမ်းထားခြင်းကို မပံ့ပိုးပေးပါ"</string>
    <!-- shown as the dialog title when user selects recording format -->
    <add-resource type="string" name="format_setting"/>
    <string name="format_setting">"ဖိုင်အမျိုးအစား"</string>
    <add-resource type="string" name="format_setting_aac_item"/>
    <string name="format_setting_aac_item">AAC</string>
    <add-resource type="string" name="format_setting_amr_item"/>
    <string name="format_setting_amr_item">AMR</string>
    <add-resource type="string" name="format_setting_3gpp_item"/>
    <string name="format_setting_3gpp_item">3GPP</string>
    <add-resource type="string" name="format_setting_wav_item"/>
    <string name="format_setting_wav_item">WAV</string>
    <add-resource type="string" name="storage_setting"/>
    <string name="storage_setting">"သိုလှောင်ခန်း နေရာ"</string>
    <add-resource type="string" name="storage_setting_sdcard_item"/>
    <string name="storage_setting_sdcard_item">"SD ကတ်"</string>
    <add-resource type="string" name="storage_setting_local_item"/>
    <string name="storage_setting_local_item">"ဖုန်း သိုလှောင်ခန်း"</string>
    <add-resource type="string" name="no_phonestorage"/>
    <string name="no_phonestorage">"ဖုန်း သိုလှောင်ခန်းကို ဖြုတ်ပြီးပါပြီ"</string>
    <add-resource type="string" name="keyboard"/>
    <string name="keyboard">"ကီးဘုတ်"</string>
    <add-resource type="string" name="file_deleted"/>
    <string name="file_deleted">"ဖိုင်ကို ဖျက်ပြီးပါပြီ!"</string>
    <add-resource type="string" name="in_call_record_error"/>
    <string name="in_call_record_error">"အဝင်ခေါ်ဆိုမှု ဖမ်းယူခြင်း အမှားဖြစ်ပွားသည်။"</string>
    <!-- Label shown during recording -->
    <add-resource type="string" name="recording_now"/>
    <string name="recording_now">"ယခု မှတ်တမ်းတင်နေသည် ..."</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <add-resource type="string" name="file_saved"/>
    <string name="file_saved">"ဖိုင်ကို သိမ်းဆည်းပြီးပါပြီ"</string>
    <add-resource type="string" name="file_saved_interrupt"/>
    <string name="file_saved_interrupt">"မှတ်တမ်းတင်မှုကို ရပ်ပြီးတော့ သိမ်းထားပြီးပါပြီ"</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <add-resource type="string" name="file_discard"/>
    <string name="file_discard">"ဖိုင်ကို စွန့်ပစ်ပြီးပါပြီ"</string></resources>
