<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"검색"</string>
    <add-resource type="string" name="search_land"/>
    <string name="search_land">"부동산 검색"</string>
    <!-- Toast text shown when the app fails to remove an item from
         the search history. [CHAR_LIMIT=60] -->
    <add-resource type="string" name="remove_from_history_failed"/>
    <string name="remove_from_history_failed">"기록에서 항목을 삭제하지 못했습니다."</string>

    <!-- Source selector -->
    <add-resource type="string" name="corpus_selection_heading"/>
    <string name="corpus_selection_heading">"검색"</string>

    <!-- Search settings menu item -->
    <add-resource type="string" name="menu_settings"/>
    <string name="menu_settings">"검색 설정"</string>

    <!-- Title for 'system search' category of search settings -->
    <add-resource type="string" name="system_search_category_title"/>
    <string name="system_search_category_title">"휴대전화"</string>

    <!-- Title and summary used for the option to clear search shortcuts from the system
         search history -->
    <add-resource type="string" name="clear_shortcuts_summary"/>
    <string name="clear_shortcuts_summary">"최근에 선택한 추천 검색어에 대한 바로가기 삭제"</string>

    <!-- Help menu item. Opens a web page with information about how to use Quick Search Box.
         [CHAR LIMIT=20] -->
    <add-resource type="string" name="menu_help"/>
    <string name="menu_help">"도움말"</string>

    <!-- Search source label for the Google search source. -->
    <add-resource type="string" name="google_search_label"/>
    <string name="google_search_label">Google</string>

    <!-- This is the hint text shown in the search widget, before text is entered. -->
    <!-- This translation MUST MATCH the string "search_hint" which is found in
         Home/res/values/strings.xml -->
    <add-resource type="string" name="google_search_hint"/>
    <string name="google_search_hint">"Google 검색"</string>

    <!-- Search settings description for the Google search source. [CHAR LIMIT=50] -->
    <add-resource type="string" name="google_search_description"/>
    <string name="google_search_description">"Google 검색 제안사항"</string>

    <!-- Title for Voice Search hints bubble -->
    <add-resource type="string" name="voice_search_hint_title"/>
    <string name="voice_search_hint_title">"추천 검색어:"</string></resources>
