<resources>

    <!-- Name of the application. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"કેલ્ક્યુલેટર"</string>

    <!--
      Decimal separator (e.g. "1.23" or "1,23").

      This should be whatever character is appropriate for use as a decimal sign for a given locale
      when used with Arabic numerals (i.e. 0, 1, 2, 3, ...)

      [CHAR_LIMIT=1]
      -->
    <add-resource type="string" name="dec_point"/>
    <string name="dec_point">"."</string>

    <!-- Abbrev. name of degree mode [CHAR_LIMIT=4] -->
    <add-resource type="string" name="mode_deg"/>
    <string name="mode_deg">"deg"</string>
    <!-- Abbrev. name of radian mode. [CHAR_LIMIT=4] -->
    <add-resource type="string" name="mode_rad"/>
    <string name="mode_rad">"rad"</string>
    <!-- Clear button to clear the currently entered expression. [CHAR_LIMIT=4] -->
    <add-resource type="string" name="clr"/>
    <string name="clr">"clr"</string>
    <!--
      Announced when all characters are removed from Formula, e.g. after clear. [CHAR_LIMIT=NONE]
      -->
    <add-resource type="string" name="cleared"/>
    <string name="cleared">"સાફ કર્યા"</string>
    <!-- Delete button to remove last entered token. [CHAR_LIMIT=4] -->
    <add-resource type="string" name="del"/>
    <string name="del">"del"</string>

    <!-- Content description for 'e' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_e"/>
    <string name="desc_const_e">"યુલરનો નંબર"</string>
    <!-- Content description for 'π' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_const_pi"/>
    <string name="desc_const_pi">"પાઇ"</string>

    <!-- Content description for '.' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_dec_point"/>
    <string name="desc_dec_point">"પોઇન્ટ"</string>

    <!-- Content description for '(' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_lparen"/>
    <string name="desc_lparen">"ડાબો કૌંસ"</string>
    <!-- Content description for ')' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_rparen"/>
    <string name="desc_rparen">"જમણો કૌંસ"</string>

    <!-- Content description for 'cos' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_cos"/>
    <string name="desc_fun_cos">"કોટિજ્યા"</string>
    <!-- Content description for 'ln' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_ln"/>
    <string name="desc_fun_ln">"પ્રાકૃતિક લઘુગણક"</string>
    <!-- Content description for 'log' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_log"/>
    <string name="desc_fun_log">"લઘુગણક"</string>
    <!-- Content description for 'sin' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_sin"/>
    <string name="desc_fun_sin">"જ્યા"</string>
    <!-- Content description for 'tan' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_tan"/>
    <string name="desc_fun_tan">"સ્પર્શક"</string>
    <!-- Content description for 'arccos' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_arccos"/>
    <string name="desc_fun_arccos">"વ્યસ્ત કોટિજ્યા"</string>
    <!-- Content description for 'arcsin' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_arcsin"/>
    <string name="desc_fun_arcsin">"વ્યસ્ત જ્યા"</string>
    <!-- Content description for 'arctan' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_arctan"/>
    <string name="desc_fun_arctan">"વ્યસ્ત સ્પર્શક"</string>

    <!--
      Content description for 10^ button. This button is used to raise 10 to the subsequently
      entered power. E.g. <10^>3 would yield 1000.
      [CHAR_LIMIT=NONE]
      -->
    <add-resource type="string" name="desc_fun_10pow"/>
    <string name="desc_fun_10pow">"દસની ઘાત"</string>
    <!-- Content description for e^ (exponential function) button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_fun_exp"/>
    <string name="desc_fun_exp">"ઘાતાંકીય કાર્ય"</string>
    <!--
      Content description for ^2 button. This button is used to square the previously entered
      number. E.g. 3<^2> would yield 9.
      [CHAR_LIMIT=NONE]
      -->
    <add-resource type="string" name="desc_op_sqr"/>
    <string name="desc_op_sqr">"વર્ગાકાર"</string>

    <!-- Content description for '+' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_add"/>
    <string name="desc_op_add">"સરવાળો"</string>
    <!-- Content description for '÷' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_div"/>
    <string name="desc_op_div">"ભાગાકાર"</string>
    <!-- Content description for '!' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_fact"/>
    <string name="desc_op_fact">"ફેક્ટોરિઅલ"</string>
    <!-- Content description for '×' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_mul"/>
    <string name="desc_op_mul">"ગુણાકાર"</string>
    <!-- Content description for '%' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_pct"/>
    <string name="desc_op_pct">"ટકા"</string>
    <!-- Content description for '^' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_pow"/>
    <string name="desc_op_pow">"ઘાત"</string>
    <!-- Content description for '√' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_sqrt"/>
    <string name="desc_op_sqrt">"વર્ગમૂળ"</string>
    <!-- Content description for '-' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_op_sub"/>
    <string name="desc_op_sub">"બાદબાકી"</string>

    <!-- Content description for degree mode. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_mode_deg"/>
    <string name="desc_mode_deg">"ડિગ્રી મોડ"</string>
    <!-- Content description for radian mode. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_mode_rad"/>
    <string name="desc_mode_rad">"સમત્રિજ્યાકોણ મોડ"</string>

    <!-- Content description for switch to degree button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_switch_deg"/>
    <string name="desc_switch_deg">"ડિગ્રી પર સ્વિચ કરો"</string>
    <!-- Content description for switch to radian button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_switch_rad"/>
    <string name="desc_switch_rad">"સમત્રિજ્યાકોણ પર સ્વિચ કરો"</string>

    <!-- Content description for '=' button. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_eq"/>
    <string name="desc_eq">"બરાબર"</string>
    <!-- Content description for "clr" button. Deletes entire formula. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_clr"/>
    <string name="desc_clr">"સાફ કરો"</string>
    <!--
      Content description for "del" button. Deletes single character. Should differ
      from desc_clr. [CHAR_LIMIT=NONE]
      -->
    <add-resource type="string" name="desc_del"/>
    <string name="desc_del">"કાઢી નાખો"</string>
    <!-- Content description for "inv" button to show inverse functions. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_inv_off"/>
    <string name="desc_inv_off">"વ્યુત્ક્રમ કાર્ય બતાવો"</string>
    <!-- Content description for "inv" button to hide inverse functions. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="desc_inv_on"/>
    <string name="desc_inv_on">"વ્યુત્ક્રમ કાર્ય છુપાવો"</string>

    <!-- Error displayed when expression evaluates to an undefined result. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="error_nan"/>
    <string name="error_nan">"કોઈ સંખ્યા નથી"</string>
    <!-- Error displayed when expression contains a syntax error. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="error_syntax"/>
    <string name="error_syntax">"ખોટી પદાવલિ"</string>
    <!-- Error displayed when evaluation is manually aborted. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="error_aborted"/>
    <string name="error_aborted">"રદ કરેલ"</string>
    <!-- Error displayed when excessive precision is required. [CHAR_LIMIT=20] -->
    <add-resource type="string" name="error_overflow"/>
    <string name="error_overflow">"અનંત?"</string>
    <!--
      Error displayed when division by zero is detected.

      If the translation does not fit within the given limit, then a shortened version must
      be used, e.g. "Division by 0". Exceeding the limit will result in a truncated string.

      [CHAR_LIMIT=20]
      -->
    <add-resource type="string" name="error_zero_divide"/>
    <string name="error_zero_divide">"0 દ્વારા ભાગાકાર કરી શકતાં નથી"</string>

    <!-- Toast shown when text is copied to the clipboard. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="text_copied_toast"/>
    <string name="text_copied_toast">"ટેક્સ્ટ કૉપિ કરી"</string>

    <!-- Dialog message when a computation is cancelled by the user. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="cancelled"/>
    <string name="cancelled">"ગણતરી રદ કરી"</string>
    <!-- Dialog message when a computation times out. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="timeout"/>
    <string name="timeout">"સમય સમાપ્તિ. મૂલ્ય અનિશ્ચિત અથવા અવ્યાખ્યાયિત હોઈ શકે છે."</string>
    <!--
      Button label to allow future computations with a longer timeout.

      When evaluating a result takes too long, we show a "timed out" dialog. This button allows
      the user to increase the maximum amount of time we attempt to evaluate the expression before
      "timing out".

      [CHAR_LIMIT=40]
      -->
    <add-resource type="string" name="ok_remove_timeout"/>
    <string name="ok_remove_timeout">"લાંબી સમયસમાપ્તિનો ઉપયોગ કરો"</string>
    <!-- Button label to dismiss informative text message. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="dismiss"/>
    <string name="dismiss">"છોડી દો"</string>
    <!-- Appended indicator (for "leading" display) that result is exact. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="exact"/>
    <string name="exact">"(ચોક્કસ)"</string>
    <!-- Indicator (for "leading" display) that result is inexact. [CHAR_LIMIT=NONE] -->
    <add-resource type="string" name="approximate"/>
    <string name="approximate">"(છેલ્લા અંકમાં ±1)"</string>

    <!-- Menu option to display result including leading digits. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="menu_leading"/>
    <string name="menu_leading">"આગળ અંકો સાથે જવાબ"</string>
    <!-- Menu option to display result as fraction. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="menu_fraction"/>
    <string name="menu_fraction">"અપૂર્ણાંક તરીકે જવાબ"</string>
    <!-- Menu option to view the app's open source licenses. [CHAR_LIMIT=40] -->
    <add-resource type="string" name="menu_licenses"/>
    <string name="menu_licenses">"ખુલ્લા સ્ત્રોત લાઇસેંસેસ"</string></resources>
