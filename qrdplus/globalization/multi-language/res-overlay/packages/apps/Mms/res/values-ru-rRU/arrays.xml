<resources>
    <!-- String to match as no subject and filter out as a subject. For example, if the
         subject string is "no subject", we won't display that. We'll pretend
         no subject string was delivered. -->
    <add-resource type="array" name="empty_subject_strings"/>
    <string-array name="empty_subject_strings">
        <item>"без темы"</item>
    </string-array>

    <add-resource type="array" name="pre_define_message_template"/>
    <string-array name="pre_define_message_template">
        <item>"Я перезвоню вам позже."</item>
        <item>"Я на конференции, перезвоню вам позже."</item>
        <item>"Позвоните позже."</item>
        <item>"Вы не могли бы мне перезвонить?"</item>
        <item>"Где вы?"</item>
        <item>"Что вы делаете?"</item>
        <item>"Не волнуйтесь! Я в порядке."</item>
        <item>"Еду домой."</item>
        <item>"Скоро буду."</item>
    </string-array>

    <add-resource type="array" name="pref_entries_creation_mode"/>
    <string-array name="pref_entries_creation_mode">
        <item>"Ограниченный"</item>
        <item>"Внимание"</item>
        <item>"Свободно"</item>
    </string-array>

</resources>
