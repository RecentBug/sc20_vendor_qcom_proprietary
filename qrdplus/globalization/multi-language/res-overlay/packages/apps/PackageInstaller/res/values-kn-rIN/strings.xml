    <add-resource type="string" name="dlg_app_replacement_title"/>
    <string name="dlg_app_replacement_title">"ಅಪ್ಲಿಕೇಶನ್ ಬದಲಿಸುವುದೇ?"</string>
    <add-resource type="string" name="dlg_app_replacement_statement"/>
    <string name="dlg_app_replacement_statement">"ನೀವು ಸ್ಥಾಪಿಸುತ್ತಿರುವ ಅಪ್ಲಿಕೇಶನ್ ಮತ್ತೊಂದು ಅಪ್ಲಿಕೇಶನ್‌ಗೆ ಬದಲಾಯಿಸುತ್ತದೆ.\n\nನಿಮ್ಮ ಹಿಂದಿನ ಎಲ್ಲಾ ಬಳಕೆದಾರ ಡೇಟಾವನ್ನು ಉಳಿಸಲಾಗುತ್ತದೆ."</string>
    <!-- Dialog attributes when replacing system app -->
    <add-resource type="string" name="dlg_sys_app_replacement_statement"/>
    <string name="dlg_sys_app_replacement_statement">"ಇದು ಸಿಸ್ಟಂ ಅಪ್ಲಿಕೇಶನ್ ಆಗಿದೆ.\n\nನಿಮ್ಮ ಹಿಂದಿನ ಎಲ್ಲಾ ಬಳಕೆದಾರ ಡೇಟಾವನ್ನು ಉಳಿಸಲಾಗುತ್ತದೆ."</string>
    <add-resource type="string" name="out_of_space_dlg_title"/>
    <string name="out_of_space_dlg_title">"ಖಾಲಿ ಇಲ್ಲ"</string>
    <add-resource type="string" name="out_of_space_dlg_text"/>
    <string name="out_of_space_dlg_text">"<xliff:g id="APP_NAME">%1$s</xliff:g> ಅನ್ನು ಸ್ಥಾಪಿಸಲು ಸಾಧ್ಯವಿಲ್ಲ. ಕೊಂಚ ಸ್ಥಳವನ್ನು ಖಾಲಿ ಮಾಡಿ. ಮತ್ತೆ ಪ್ರಯತ್ನಿಸಿ."</string>
