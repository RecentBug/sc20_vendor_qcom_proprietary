<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the dialog button to deny a permission grant and never ask the user again. -->
    <add-resource type="string" name="grant_dialog_button_deny_dont_ask_again"/>
    <string name="grant_dialog_button_deny_dont_ask_again">"اجازه ندارد و دیگر سؤال نشود"</string>

    <!-- Instructional text telling the user how to change permission grants later. -->
    <add-resource type="string" name="grant_dialog_how_to_change"/>
    <string name="grant_dialog_how_to_change">"‏می‌توانید بعداً آن را در تنظیمات &gt; برنامه‌ها تغییر دهید"</string>

    <!-- Template for the current permission from the total number of permissions. -->
    <add-resource type="string" name="current_permission_template"/>
    <string name="current_permission_template">"<xliff:g id="CURRENT_PERMISSION_INDEX">%1$s</xliff:g> / <xliff:g id="PERMISSION_COUNT">%2$s</xliff:g>"</string>

    <!-- Preference row title for showing system apps. -->
    <add-resource type="string" name="preference_show_system_apps"/>
    <string name="preference_show_system_apps">"نمایش‌ برنامه‌های سیستم"</string>

    <!--decor title displayed as the page title for different TV permission screens-->
    <add-resource type="string" name="app_permissions_decor_title"/>
    <string name="app_permissions_decor_title">"مجوزهای برنامه"</string>
    <add-resource type="string" name="manage_permissions_decor_title"/>
    <string name="manage_permissions_decor_title">"مجوزهای برنامه"</string>
    <add-resource type="string" name="permission_apps_decor_title"/>
    <string name="permission_apps_decor_title">"مجوزهای <xliff:g id="PERMISSION">%1$s</xliff:g>"</string>
    <add-resource type="string" name="additional_permissions_decor_title"/>
    <string name="additional_permissions_decor_title">"مجوزهای بیشتر"</string>
    <add-resource type="string" name="system_apps_decor_title"/>
    <string name="system_apps_decor_title">"مجوزهای <xliff:g id="PERMISSION">%1$s</xliff:g>"</string></resources>
