<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the dialog button to deny a permission grant and never ask the user again. [CHAR LIMIT=29]-->
    <add-resource type="string" name="grant_dialog_button_deny_dont_ask_again"/>
    <string name="grant_dialog_button_deny_dont_ask_again">"Відхилити й більше не запитувати"</string>

    <!-- Template for the current permission from the total number of permissions. -->
    <add-resource type="string" name="current_permission_template"/>
    <string name="current_permission_template">"<xliff:g id="CURRENT_PERMISSION_INDEX">%1$s</xliff:g> з <xliff:g id="PERMISSION_COUNT">%2$s</xliff:g>"</string>

    <!-- Preference row title for showing system apps. -->
    <add-resource type="string" name="preference_show_system_apps"/>
    <string name="preference_show_system_apps">"Показати системні додатки"</string>

    <!-- Summary of a permission switch when it's enforced by policy [CHAR LIMIT=17] -->
    <add-resource type="string" name="permission_summary_enforced_by_policy"/>
    <string name="permission_summary_enforced_by_policy">"Не можна змінити"</string>

    <!-- Generic text to indicate a yes. [CHAR LIMIT=10] -->
    <add-resource type="string" name="generic_yes"/>
    <string name="generic_yes">"Так"</string>

    <!-- Generic text to indicate Cancel. [CHAR LIMIT=10] -->
    <add-resource type="string" name="generic_cancel"/>
    <string name="generic_cancel">"Скасувати"</string></resources>
