    <!-- Message for installing a new app that requires some permissions [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question"/>
    <string name="install_confirm_question">"இந்தப் பயன்பாட்டை நிறுவ விரும்புகிறீர்களா? அது இதற்கான அணுகலைப் பெறும்:"</string>
    <!-- Message for installing a new app that does not require permissions [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question_no_perms"/>
    <string name="install_confirm_question_no_perms">"இந்தப் பயன்பாட்டை நிறுவ விரும்புகிறீர்களா? இதற்கு எந்தத் தனிப்பட்ட அணுகலும் தேவையில்லை."</string>
    <!-- Message for updating an existing app [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question_update"/>
    <string name="install_confirm_question_update">"முன்பே உள்ள இந்தப் பயன்பாட்டில் புதுப்பிப்பை நிறுவ விரும்புகிறீர்களா? ஏற்கனவே உள்ள உங்கள் தரவை இழக்க மாட்டீர்கள். புதுப்பிக்கப்பட்ட பயன்பாடு இதற்கான அணுகலைப் பெறும்:"</string>
    <!-- Message for updating an existing system app [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question_update_system"/>
    <string name="install_confirm_question_update_system">"உள்ளமைக்கப்பட்ட பயன்பாட்டில் புதுப்பிப்பை நிறுவ விரும்புகிறீர்களா? ஏற்கனவே உள்ள உங்கள் தரவை இழக்க மாட்டீர்கள். புதுப்பிக்கப்பட்ட பயன்பாடு இதற்கான அணுகலைப் பெறும்:"</string>
    <!-- Message for updating an existing app, no permissions [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question_update_no_perms"/>
    <string name="install_confirm_question_update_no_perms">"முன்பே உள்ள இந்தப் பயன்பாட்டில் புதுப்பிப்பை நிறுவ விரும்புகிறீர்களா? ஏற்கனவே உள்ள உங்கள் தரவை இழக்கமாட்டீர்கள். இதற்குத் தனிப்பட்ட அணுகல் எதுவும் தேவையில்லை."</string>
    <!-- Message for updating an existing system app, no permissions [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="install_confirm_question_update_system_no_perms"/>
    <string name="install_confirm_question_update_system_no_perms">"உள்ளமைக்கப்பட்ட பயன்பாட்டில் புதுப்பிப்பை நிறுவ விரும்புகிறீர்களா? ஏற்கனவே உள்ள உங்கள் தரவை இழக்க மாட்டீர்கள். இதற்குத் தனிப்பட்ட அணுகல் எதுவும் தேவையில்லை."</string>
    <add-resource type="string" name="install_failed"/>
    <string name="install_failed">"பயன்பாடு நிறுவப்படவில்லை."</string>
    <!-- Reason displayed when installation fails because the package was blocked
        from being installed (e.g., device policy, verification, ...) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_blocked"/>
    <string name="install_failed_blocked">"இந்தத் தொகுப்பு நிறுவுவதிலிருந்து தடுக்கப்பட்டது."</string>
    <!-- Reason displayed when installation fails because the package conflicts with
        an existing application (e.g., incompatible certificates) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_conflict"/>
    <string name="install_failed_conflict">"இந்தத் தொகுப்பு அதே பெயரில் ஏற்கனவே உள்ள தொகுப்புடன் முரண்படுகிறது."</string>
    <!-- Reason displayed when installation fails because the package is incompatible with
       the current tablet (e.g., missing native code for the current ABI, newer SDK, ...) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_incompatible"/>
    <string name="install_failed_incompatible" product="tablet">"உங்கள் டேப்லெட்டுடன் இந்தப் பயன்பாடு இணங்கவில்லை."</string>
    <!-- Reason displayed when installation fails because the package is incompatible with
       the current TV (e.g., missing native code for the current ABI, newer SDK, ...) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_incompatible"/>
    <string name="install_failed_incompatible" product="tv">"உங்கள் டிவியுடன் இந்தப் பயன்பாடு இணங்கவில்லை."</string>
    <!-- Reason displayed when installation fails because the package is incompatible with
       the current phone (e.g., missing native code for the current ABI, newer SDK, ...) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_incompatible"/>
    <string name="install_failed_incompatible" product="default">"உங்கள் மொபைலுடன் இந்தப் பயன்பாடு இணங்கவில்லை."</string>
    <!-- Reason displayed when installation fails because the installation package itself is invalid
        in some way (e.g., corrupt) [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_invalid_apk"/>
    <string name="install_failed_invalid_apk">"தொகுப்பு சிதைந்ததுபோல் தெரிகிறது."</string>
    <!-- Message presented when an application could not be installed on the tablet for some reason. [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_msg"/>
    <string name="install_failed_msg" product="tablet">"<xliff:g id="APP_NAME">%1$s</xliff:g> பயன்பாட்டை உங்கள் டேப்லெட்டில் நிறுவ முடியாது."</string>
    <!-- Message presented when an application could not be installed on the TV for some reason. [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_msg"/>
    <string name="install_failed_msg" product="tv">"<xliff:g id="app_name">%1$s</xliff:g>-ஐ உங்கள் TV-இல் நிறுவ முடியவில்லை."</string>
    <!-- Message presented when an application could not be installed on the phone for some reason. [CHAR LIMIT=100] -->
    <add-resource type="string" name="install_failed_msg"/>
    <string name="install_failed_msg" product="default">"<xliff:g id="APP_NAME">%1$s</xliff:g> பயன்பாட்டை உங்கள் மொபைலில் நிறுவ முடியாது."</string>
