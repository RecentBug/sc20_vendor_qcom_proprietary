    <!-- Message presented in a dialog box when a package is requested to be installed, but the "Allow unknown applications" checkbox is not enabled. [CHAR LIMIT=100] -->
    <add-resource type="string" name="unknown_apps_dlg_text"/>
    <string name="unknown_apps_dlg_text" product="tablet">"பாதுகாப்பிற்கு, தெரியாத மூலங்களிலிருந்து பெறப்படும் பயன்பாடுகளின் நிறுவலைத் தடுக்கும்படி உங்கள் டேப்லெட் அமைக்கப்பட்டுள்ளது."</string>
    <!-- Message presented in a dialog box when a package is requested to be installed, but the "Allow unknown applications" checkbox is not enabled. [CHAR LIMIT=100] -->
    <add-resource type="string" name="unknown_apps_dlg_text"/>
    <string name="unknown_apps_dlg_text" product="tv">"பாதுகாப்புக்காக, தெரியாத மூலங்களிலிருந்து பெறப்படும் பயன்பாடுகளை நிறுவுவதைத் முடக்குமாறு உங்கள் TV அமைக்கப்படுகிறது."</string>
    <!-- Message presented in a dialog box when a package is requested to be installed, but the "Allow unknown applications" checkbox is not enabled. [CHAR LIMIT=100] -->
    <add-resource type="string" name="unknown_apps_dlg_text"/>
    <string name="unknown_apps_dlg_text" product="default">"பாதுகாப்பிற்கு, தெரியாத மூலங்களிலிருந்து பெறப்படும் பயன்பாடுகளின் நிறுவலைத் தடுக்கும்படி உங்கள் மொபைல் அமைக்கப்பட்டுள்ளது."</string>
    <!-- Message presented in a dialog box when the device administrator restricts the installation of apps from unknown sources. [CHAR LIMIT=none] -->
    <add-resource type="string" name="unknown_apps_admin_dlg_text"/>
    <string name="unknown_apps_admin_dlg_text">"தெரியாத மூலங்களிலிருந்து பெறப்படும் பயன்பாடுகளை நிறுவ, உங்கள் நிர்வாகி அனுமதிப்பதில்லை."</string>

    <add-resource type="string" name="ok"/>
