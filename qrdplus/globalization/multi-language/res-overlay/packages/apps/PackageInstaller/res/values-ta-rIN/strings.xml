    <add-resource type="string" name="manage_applications"/>
    <string name="manage_applications">"பயன்பாடுகளை நிர்வகி"</string>
    <add-resource type="string" name="dlg_app_replacement_title"/>
    <string name="dlg_app_replacement_title">"பயன்பாட்டை மாற்றவா?"</string>
    <add-resource type="string" name="dlg_app_replacement_statement"/>
    <string name="dlg_app_replacement_statement">"நிறுவும் பயன்பாடு மற்றொரு பயன்பாட்டை மாற்றிவிடும்.\n\nமுந்தைய பயனர் தரவு அனைத்தும் சேமிக்கப்படும்."</string>
    <!-- Dialog attributes when replacing system app -->
    <add-resource type="string" name="dlg_sys_app_replacement_statement"/>
    <string name="dlg_sys_app_replacement_statement">"இது அமைப்பின் பயன்பாடாகும்.\n\nஉங்கள் முந்தைய பயனர் தரவு அனைத்தும் சேமிக்கப்படும்."</string>
    <add-resource type="string" name="out_of_space_dlg_title"/>
