    <add-resource type="string" name="dlg_ok"/>
    <string name="dlg_ok">"சரி"</string>
    <add-resource type="string" name="app_not_found_dlg_title"/>
    <string name="app_not_found_dlg_title">"பயன்பாடு கண்டறியப்படவில்லை"</string>
    <add-resource type="string" name="app_not_found_dlg_text"/>
    <string name="app_not_found_dlg_text">"நிறுவிய பயன்பாடுகளின் பட்டியலில் பயன்பாடு இல்லை."</string>
    <add-resource type="string" name="uninstall_application_title"/>
    <string name="uninstall_application_title">"பயன்பாட்டை நிறுவல் நீக்கு"</string>
    <add-resource type="string" name="uninstall_update_title"/>
    <string name="uninstall_update_title">"புதுப்பிப்பை நிறுவல் நீக்கு"</string>
    <add-resource type="string" name="uninstall_activity_text"/>
    <string name="uninstall_activity_text">"<xliff:g id="ACTIVITY_NAME">%1$s</xliff:g> ஆனது பின்வரும் பயன்பாட்டின் பகுதியாகும்:"</string>
    <add-resource type="string" name="uninstall_application_text"/>
    <string name="uninstall_application_text">"இந்தப் பயன்பாட்டை நிறுவல் நீக்க விரும்புகிறீர்களா?"</string>
    <add-resource type="string" name="uninstall_application_text_all_users"/>
    <string name="uninstall_application_text_all_users">"இந்தப் பயன்பாட்டை "<b>"எல்லா"</b>" பயனர்களுக்கும் நிறுவல் நீக்க விரும்புகிறீர்களா? பயன்பாடும், அதன் தரவும் சாதனத்தில் உள்ள "<b>"எல்லா"</b>" பயனர்களிடமிருந்தும் அகற்றப்படும்."</string>
    <add-resource type="string" name="uninstall_application_text_user"/>
    <string name="uninstall_application_text_user">"<xliff:g id="USERNAME">%1$s</xliff:g> பயனருக்கான இந்தப் பயன்பாட்டை நிறுவல்நீக்க விரும்புகிறீர்களா?"</string>
    <add-resource type="string" name="uninstall_update_text"/>
    <string name="uninstall_update_text">"ஆரம்பநிலைப் பதிப்பாக இந்தப் பயன்பாட்டை மாற்றியமைக்கவா? எல்லா தரவும் அகற்றப்படும்."</string>
    <add-resource type="string" name="uninstall_update_text_multiuser"/>
    <string name="uninstall_update_text_multiuser">"ஆரம்பநிலைப் பதிப்பாக இந்தப் பயன்பாட்டை மாற்றியமைக்கவா? எல்லா தரவும் அகற்றப்படும். பணிச் சுயவிவரங்களுடன் உள்ளவர்கள் உட்பட இந்தச் சாதனத்தின் எல்லா பயனர்களையும் இது பாதிக்கும்."</string>
    <add-resource type="string" name="uninstalling"/>
    <string name="uninstalling">"நிறுவலை நீக்குகிறது\u2026"</string>
    <add-resource type="string" name="uninstall_done"/>
    <string name="uninstall_done">"நிறுவல் நீக்குவது முடிந்தது."</string>
    <add-resource type="string" name="uninstall_failed"/>
    <string name="uninstall_failed">"நிறுவல் நீக்குவதில் தோல்வி."</string>
    <!-- String presented to the user when uninstalling a package failed because the target package
        is a current device administrator [CHAR LIMIT=80] -->
    <add-resource type="string" name="uninstall_failed_device_policy_manager"/>
    <string name="uninstall_failed_device_policy_manager">"இந்தத் தொகுப்பு, செயலில் உள்ள சாதன நிர்வாகி என்பதால் நிறுவல் நீக்க முடியாது."</string>
    <!-- String presented to the user when uninstalling a package failed because the target package
        is a current device administrator for some user [CHAR LIMIT=100] -->
    <add-resource type="string" name="uninstall_failed_device_policy_manager_of_user"/>
    <string name="uninstall_failed_device_policy_manager_of_user">"இந்தத் தொகுப்பு <xliff:g id="USERNAME">%1$s</xliff:g> பயனருக்கான செயலில் உள்ள சாதன நிர்வாகியாக இருப்பதால், நிறுவல் நீக்க முடியாது."</string>
    <!-- String presented to the admin user when uninstalling a package for all users failed
    because a profile owner has marked the target package as not able to be uninstalled
    [CHAR LIMIT=120] -->
    <add-resource type="string" name="uninstall_all_blocked_profile_owner"/>
    <string name="uninstall_all_blocked_profile_owner">"இது சில பயனர்கள்/சுயவிவரங்களுக்குத் தேவைப்படுவதால், நிறுவல்நீக்க முடியாது, பிறருக்கு நிறுவல் நீக்கப்பட்டது"</string>
    <!-- String presented to the user when uninstalling a package failed because a profile owner
         has marked the target package as not able to be uninstalled [CHAR LIMIT=120] -->
    <add-resource type="string" name="uninstall_blocked_profile_owner"/>
    <string name="uninstall_blocked_profile_owner">"உங்கள் சுயவிவரத்திற்கு இந்தப் பயன்பாடு தேவைப்படுவதால், அதை நிறுவல்நீக்க முடியாது, பிறருக்கு நிறுவல் நீக்கப்பட்டது."</string>
    <!-- String presented to the user when uninstalling a package failed because a device owner
