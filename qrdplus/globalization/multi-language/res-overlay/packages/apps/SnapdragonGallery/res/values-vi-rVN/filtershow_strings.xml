    <add-resource type="string" name="dual_camera_effects_intro"/>
    <string name="dual_camera_effects_intro">"Cho phép bạn áp dụng các hiệu ứng nền, như Chuyển động và Phác thảo.Bạn cũng có thể kết hợp hai ảnh với nhau để tạo hiệu ứng thú vị.Tính năng này hiệu quả nhất khi chụp chủ thể chính của ảnh ở khoảng cách gần."</string>
    <add-resource type="string" name="dualcam_no_segment_toast"/>
