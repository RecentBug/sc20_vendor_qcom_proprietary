<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  <!-- General provisioning strings used for both device owner and work profile provisioning -->

  <!-- App label displayed on the notification shown to indicate that the provisioning process was previously started but not completed. [CHAR LIMIT=45] -->
  <add-resource type="string" name="app_label"/>
    <string name="app_label">"Configurarea profilului de serviciu"</string>

  <!-- Title of the provisioning error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="provisioning_error_title"/>
    <string name="provisioning_error_title">"Hopa!"</string>

  <!-- Work profile provisioning flow UI. -->

  <!-- User consent -->
  <!-- Title of all work profile setup screens. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="setup_work_profile"/>
    <string name="setup_work_profile">"Configurați profilul de serviciu"</string>
  <!-- The text that asks for user consent to create a work profile [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="company_controls_workspace"/>
    <string name="company_controls_workspace">"Organizația dvs. controlează acest profil și menține siguranța acestuia. Dvs. controlați restul conținutului de pe dispozitiv."</string>
  <!-- The text that asks for user consent to set up a work device [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="company_controls_device"/>
    <string name="company_controls_device">"Organizația dvs. va controla acest dispozitiv și îi va asigura protecția."</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="the_following_is_your_mdm"/>
    <string name="the_following_is_your_mdm">"Următoarea aplicație va trebui să vă acceseze profilul:"</string>
  <!-- String shown above the icon of the app that will control the work device after it is set up. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="the_following_is_your_mdm_for_device"/>
    <string name="the_following_is_your_mdm_for_device">"Dispozitivul va fi gestionat de următoarea aplicație:"</string>
  <!-- String for positive button on user consent dialog for creating a profile [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="next"/>
    <string name="next">"Înainte"</string>
  <!-- Progress text shown when setting up work profile [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="setting_up_workspace"/>
    <string name="setting_up_workspace">"Se configurează profilul de serviciu..."</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the profile they are creating. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="admin_has_ability_to_monitor_profile"/>
    <string name="admin_has_ability_to_monitor_profile">"Administratorul poate să monitorizeze și să gestioneze setările, accesul la nivelul companiei, aplicațiile, permisiunile și datele asociate cu profilul, inclusiv activitatea în rețea și informațiile despre locația dispozitivului, istoricul apelurilor și istoricul căutărilor persoanelor de contact."</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the device. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="admin_has_ability_to_monitor_device"/>
    <string name="admin_has_ability_to_monitor_device">"Administratorul poate să monitorizeze și să gestioneze setările, accesul la nivelul companiei, aplicațiile, permisiunile, funcțiile de protecție împotriva furtului și datele asociate acestui dispozitiv, inclusiv activitatea în rețea și informațiile despre locația dispozitivului."</string>
  <!-- Text in a pop-up that asks the user to confirm that theft protection will only be enabled after a password-protected screen lock was chosen. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="theft_protection_disabled_warning"/>
    <string name="theft_protection_disabled_warning">"Pentru a folosi funcțiile de protecție împotriva furtului, trebuie să aveți o blocare a ecranului protejată prin parolă pentru dispozitiv."</string>
  <!-- String telling the user how to get more information about their work profile-->
  <add-resource type="string" name="contact_your_admin_for_more_info"/>
    <string name="contact_your_admin_for_more_info">"Contactați administratorul pentru mai multe informații, inclusiv politicile de confidențialitate ale organizației."</string>
  <!-- Text on the link that will forward the user to a website with more information about work profiles [CHAR LIMIT=30]-->
  <add-resource type="string" name="learn_more_link"/>
    <string name="learn_more_link">"Aflați mai multe"</string>
  <!-- Text on negative button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <add-resource type="string" name="cancel_setup"/>
    <string name="cancel_setup">"Anulați"</string>
  <!-- Text on positive button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <add-resource type="string" name="ok_setup"/>
    <string name="ok_setup">"Ok"</string>
  <!-- Text of the checkbox to consent to the mobile device management app taking control of the device. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="user_consent_msg"/>
    <string name="user_consent_msg">"Sunt de acord"</string>

  <!-- Default profile name used for the creation of work profiles. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="default_managed_profile_name"/>
    <string name="default_managed_profile_name">"Profilul de serviciu"</string>

  <!-- Default name used for the creation of the first meat user on a device with system-user split. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="default_first_meat_user_name"/>
    <string name="default_first_meat_user_name">"Utilizatorul principal"</string>

  <!-- Title on the dialog that prompts the user to confirm that they really want to delete their existing work profile-->
  <add-resource type="string" name="delete_profile_title"/>
    <string name="delete_profile_title">"Eliminați profilul de serviciu?"</string>
  <!-- Opening string on the dialog that prompts the user to confirm that they really want to delete
       their existing work profile. The administration app icon and name appear after the final
       colon. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="opening_paragraph_delete_profile_unknown_company"/>
    <string name="opening_paragraph_delete_profile_unknown_company">"Deja există un profil de serviciu, iar acesta este gestionat de:"</string>
    <!-- Opening string on the dialog that prompts the user to confirm that they really want to delete
         their existing work profile. The administration app icon and name appear after the final
         colon, the %s is replaced by the domain name of the organization that owns the work
         profile. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="opening_paragraph_delete_profile_known_company"/>
    <string name="opening_paragraph_delete_profile_known_company">"Acest profil de serviciu este gestionat pentru %s folosind:"</string>
  <!-- String on the dialog that links through to more information about the profile management application. -->
  <add-resource type="string" name="read_more_delete_profile"/>
    <string name="read_more_delete_profile">"Înainte de a continua, "<a href="#read_this_link">"citiți aici"</a>"."</string>
  <!-- String on the dialog that prompts the user to confirm that they really want to delete their existing work profile-->
  <add-resource type="string" name="sure_you_want_to_delete_profile"/>
    <string name="sure_you_want_to_delete_profile">"În cazul în care continuați, toate aplicațiile și datele din acest profil vor fi șterse."</string>
  <!-- String on the button that triggers the deletion of a work profile.-->
  <add-resource type="string" name="delete_profile"/>
    <string name="delete_profile">"Ștergeți"</string>
  <!-- String on the button that cancels out of the deletion of a work profile.-->
  <add-resource type="string" name="cancel_delete_profile"/>
    <string name="cancel_delete_profile">"Anulați"</string>

  <!-- String shown on prompt for user to encrypt and reboot the device, in case of profile setup [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="encrypt_device_text_for_profile_owner_setup"/>
    <string name="encrypt_device_text_for_profile_owner_setup">"Pentru a continua configurarea profilului de serviciu, va trebui să criptați dispozitivul. Acest proces poate dura un timp."</string>
  <!-- String shown on prompt for user to encrypt and reboot the device, in case of device setup [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="encrypt_device_text_for_device_owner_setup"/>
    <string name="encrypt_device_text_for_device_owner_setup">"Pentru a continua configurarea dispozitivului, va trebui să îl criptați. Acest proces poate dura un timp."</string>
  <!-- String shown on button for user to continue to settings to encrypt the device. [CHAR LIMIT=20]-->
  <add-resource type="string" name="encrypt_device_launch_settings"/>
    <string name="encrypt_device_launch_settings">"Criptați"</string>

  <!-- Title for reminder notification to resume provisioning after encryption [CHAR LIMIT=30] -->
  <add-resource type="string" name="continue_provisioning_notify_title"/>
    <string name="continue_provisioning_notify_title">"Criptare completă"</string>
  <!-- Body for reminder notification to resume provisioning after encryption [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="continue_provisioning_notify_text"/>
    <string name="continue_provisioning_notify_text">"Atingeți pentru a continua configurarea profilului de serviciu"</string>

  <!-- Explains the failure and what to do to next. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="managed_provisioning_error_text"/>
    <string name="managed_provisioning_error_text">"Nu s-a putut configura profilul de serviciu. Contactați departamentul IT sau încercați din nou mai târziu."</string>
  <!-- Error string displayed if this device doesn't support work profiles. -->
  <add-resource type="string" name="managed_provisioning_not_supported"/>
    <string name="managed_provisioning_not_supported">"Dispozitivul dvs. nu acceptă profiluri de serviciu"</string>
  <!-- Error string displayed if the user that initiated the provisioning is not the user owner. -->
  <add-resource type="string" name="user_cannot_have_work_profile"/>
    <string name="user_cannot_have_work_profile">"Acest utilizator nu poate avea un profil de serviciu"</string>
  <!-- Error string displayed if provisioning was initiated on a device with a Device Owner -->
  <add-resource type="string" name="device_owner_exists"/>
    <string name="device_owner_exists">"Profilurile de serviciu nu pot fi configurate pe un dispozitiv gestionat de un administrator"</string>
  <!-- Error string displayed if maximum user limit is reached -->
  <add-resource type="string" name="maximum_user_limit_reached"/>
    <string name="maximum_user_limit_reached">"Profilul de serviciu nu a putut fi creat, deoarece ați atins numărul maxim de utilizatori pe dispozitiv. Eliminați cel puțin un utilizator și încercați din nou."</string>
  <!-- Error string displayed if the selected launcher doesn't support work profiles. -->
  <add-resource type="string" name="managed_provisioning_not_supported_by_launcher"/>
    <string name="managed_provisioning_not_supported_by_launcher">"Profilul dvs. de lucru nu este acceptat de aplicația de lansare. Va trebui să comutați la o aplicație de lansare compatibilă."</string>
  <!-- Button text for the button that cancels provisioning  -->
  <add-resource type="string" name="cancel_provisioning"/>
    <string name="cancel_provisioning">"Anulați"</string>
  <!-- Button text for the button that opens the launcher picker  -->
  <add-resource type="string" name="pick_launcher"/>
    <string name="pick_launcher">"OK"</string>

  <!-- Managed user provisioning flow UI. -->
  <add-resource type="string" name="user_setup_incomplete"/>
    <string name="user_setup_incomplete">"Configurarea utilizatorului nu s-a finalizat"</string>

  <!-- Device owner provisioning flow UI. -->

  <!-- Default username for the user of the owned device. [CHAR LIMIT=45] -->
  <add-resource type="string" name="default_owned_device_username"/>
    <string name="default_owned_device_username">"Utilizatorul dispozitivului de serviciu"</string>

  <!-- Title of the work device setup screen. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="setup_work_device"/>
    <string name="setup_work_device">"Configurați dispozitivul de serviciu"</string>
  <!-- Progress text indicating that the setup data is processed. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_data_process"/>
    <string name="progress_data_process">"Se procesează datele de configurare..."</string>
  <!-- Progress text indicating that wifi is set up. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_connect_to_wifi"/>
    <string name="progress_connect_to_wifi">"Se conectează la Wi-Fi..."</string>
  <!-- Progress text indicating that the device admin package is being downloaded. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_download"/>
    <string name="progress_download">"Se descarcă aplicația de administrare..."</string>
  <!-- Progress text indicating that the device admin package is being installed. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_install"/>
    <string name="progress_install">"Se instalează aplicația de administrare..."</string>
  <!-- Progress text indicating that the device admin package is set as owner. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_set_owner"/>
    <string name="progress_set_owner">"Se setează proprietarul dispozitivului..."</string>

  <!-- Message of the error dialog in case a device does not support encryption, but encryption is necessary. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="preprovisioning_error_encryption_not_supported"/>
    <string name="preprovisioning_error_encryption_not_supported">"Dispozitivul dvs. nu acceptă criptarea. Contactați departamentul IT."</string>
  <!-- Message of the cancel dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_cancel_message"/>
    <string name="device_owner_cancel_message">"Opriți configurarea și ștergeți datele de pe dispozitiv?"</string>
  <!-- Cancel button text of the cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_cancel_cancel"/>
    <string name="device_owner_cancel_cancel">"Anulați"</string>
  <!-- OK button text of the error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_error_ok"/>
    <string name="device_owner_error_ok">"Ok"</string>
  <!-- Reset button text of the error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_error_reset"/>
    <string name="device_owner_error_reset">"Resetați"</string>
  <!-- Message of the error dialog in case of an unspecified error. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_general"/>
    <string name="device_owner_error_general">"Configurarea nu s-a putut efectua. Contactați departamentul IT."</string>
  <!-- Message of the error dialog when already provisioned. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_already_provisioned"/>
    <string name="device_owner_error_already_provisioned">"Dispozitivul este deja configurat."</string>
  <!-- Message of the error dialog when setting up wifi failed. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_wifi"/>
    <string name="device_owner_error_wifi">"Nu se poate conecta la Wi-Fi"</string>
  <!-- Message of the error dialog when passing factory reset protection fails. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_frp"/>
    <string name="device_owner_error_frp">"Acest dispozitiv este blocat prin protecția împotriva revenirii la setările din fabrică. Contactați departamentul IT."</string>
  <!-- Message informing the user that the factory reset protection partition is being erased [CHAR LIMIT=30] -->
  <add-resource type="string" name="frp_clear_progress_title"/>
    <string name="frp_clear_progress_title">"Se șterge"</string>
  <!-- Progress screen text shown while erasing the factory reset protection partition [CHAR LIMIT=75] -->
  <add-resource type="string" name="frp_clear_progress_text"/>
    <string name="frp_clear_progress_text">"Așteptați..."</string>
  <!-- Message of the error dialog when data integrity check fails. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_hash_mismatch"/>
    <string name="device_owner_error_hash_mismatch">"Nu se poate utiliza aplicația de administrare din cauza unei erori de sumă de control. Contactați departamentul IT."</string>
  <!-- Message of the error dialog when device owner apk could not be downloaded. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_download_failed"/>
    <string name="device_owner_error_download_failed">"Eroare la descărcarea aplicației de administrare"</string>
  <!-- Message of the error dialog when package to install is invalid. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_package_invalid"/>
    <string name="device_owner_error_package_invalid">"Nu se poate utiliza aplicația de administrare. Este deteriorată sau lipsesc componente. Contactați departamentul IT."</string>
  <!-- Message of the error dialog when package could not be installed. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_installation_failed"/>
    <string name="device_owner_error_installation_failed">"Eroare la instalarea aplicației de administrare"</string>
  <!-- Message of the cancel dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="profile_owner_cancel_message"/>
    <string name="profile_owner_cancel_message">"Opriți configurarea?"</string>
  <!-- Cancel button text of the cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="profile_owner_cancel_cancel"/>
    <string name="profile_owner_cancel_cancel">"Nu"</string>
  <!-- OK button text of the error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="profile_owner_cancel_ok"/>
    <string name="profile_owner_cancel_ok">"Da"</string>
  <!-- Message of the cancel progress dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="profile_owner_cancelling"/>
    <string name="profile_owner_cancelling">"Se anulează..."</string>
  <!-- Title of the work profile setup cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="work_profile_setup_later_title"/>
    <string name="work_profile_setup_later_title">"Opriți configurarea profilului?"</string>
  <!-- Message of the work profile setup cancel dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="work_profile_setup_later_message"/>
    <string name="work_profile_setup_later_message">"Puteți configura profilul de serviciu mai târziu din aplicația de administrare a dispozitivelor folosită de organizația dvs."</string>
  <!-- Cancel button text of the work profile setup cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="work_profile_setup_continue"/>
    <string name="work_profile_setup_continue">"Continuați"</string>
  <!-- OK button text of the work profile setup cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="work_profile_setup_stop"/>
    <string name="work_profile_setup_stop">"Opriți"</string>

  <!-- CertService -->
  <!-- Title of a status bar notification for a service which installs CA certificates to secondary users. [CHAR LIMIT=45] -->
  <add-resource type="string" name="provisioning"/>
    <string name="provisioning">"Se asigură accesul"</string>
  <!-- Content of a status bar notification for a service which installs CA certificates to secondary users. [CHAR LIMIT=45] -->
  <add-resource type="string" name="copying_certs"/>
