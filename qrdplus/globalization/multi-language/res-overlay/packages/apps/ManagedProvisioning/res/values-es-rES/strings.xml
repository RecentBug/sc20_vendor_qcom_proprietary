  <!-- Accessibility description for the device setup screen showing encryption info. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="setup_device_encryption"/>
    <string name="setup_device_encryption">"Configura tu dispositivo. Cifrado"</string>
  <!-- Accessibility description for the device setup screen showing progress. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="setup_device_progress"/>
    <string name="setup_device_progress">"Configura tu dispositivo. Mostrando progreso"</string>
  <!-- Accessibility description for the learn more link in user consent dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="learn_more_label"/>
    <string name="learn_more_label">"Botón para obtener más información"</string>
  <!-- Accessibility description for the company icon. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="mdm_icon_label"/>
    <string name="mdm_icon_label">"Icono de <xliff:g id="ICON_LABEL">%1$s</xliff:g>"</string>

  <!-- TODO: Following strings have to be updated. -->
  <!-- Strings used in the dialog asking for user confirmation to cancel the setup process and factory reset the device. -->
  <!-- Title shown in the dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="cancel_setup_and_factory_reset_dialog_title"/>
    <string name="cancel_setup_and_factory_reset_dialog_title">"¿Cancelar configuración y restablecer datos?"</string>
  <!-- Message shown in the dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="cancel_setup_and_factory_reset_dialog_msg"/>
    <string name="cancel_setup_and_factory_reset_dialog_msg">"Al finalizar esta configuración, se restablecerán los datos de fábrica de tu dispositivo y volverás a la primera pantalla."</string>
  <!-- Cancel button text of the dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="cancel_setup_and_factory_reset_dialog_cancel"/>
    <string name="cancel_setup_and_factory_reset_dialog_cancel">"Cancelar"</string>
  <!-- OK button text of the dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="cancel_setup_and_factory_reset_dialog_ok"/>
    <string name="cancel_setup_and_factory_reset_dialog_ok">"Restablecer dispositivo"</string></resources>
