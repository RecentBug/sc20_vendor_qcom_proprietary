  <add-resource type="string" name="company_controls_device"/>
    <string name="company_controls_device">"Twoja organizacja będzie mieć kontrolę nad tym urządzeniem i będzie dbać o jego bezpieczeństwo."</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <add-resource type="string" name="the_following_is_your_mdm"/>
    <string name="the_following_is_your_mdm">"Aplikacja, która będzie potrzebować dostępu do tego profilu:"</string>
