  <add-resource type="string" name="encrypt_device_text_for_profile_owner_setup"/>
    <string name="encrypt_device_text_for_profile_owner_setup">"Aby kontynuować konfigurowanie profilu do pracy, musisz zaszyfrować swoje urządzenie. Może to trochę potrwać."</string>
