  <!-- Progress text indicating that wifi is set up. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_connect_to_wifi"/>
    <string name="progress_connect_to_wifi">"Wi-Fi-এ সংযোগ স্থাপন করা হচ্ছে\u2026"</string>
  <!-- Progress text indicating that the device admin package is being downloaded. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_download"/>
    <string name="progress_download">"প্রশাসক অ্যাপ ডাউনলোড করা হচ্ছে\u2026"</string>
  <!-- Progress text indicating that the device admin package is being installed. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_install"/>
    <string name="progress_install">"প্রশাসক অ্যাপ ইনস্টল করা হচ্ছে\u2026"</string>
  <!-- Progress text indicating that the device admin package is set as owner. [CHAR LIMIT=45] -->
  <add-resource type="string" name="progress_set_owner"/>
    <string name="progress_set_owner">"ডিভাইসের মালিক সেট করা হচ্ছে\u2026"</string>

  <!-- Message of the error dialog in case a device does not support encryption, but encryption is necessary. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="preprovisioning_error_encryption_not_supported"/>
    <string name="preprovisioning_error_encryption_not_supported">"আপনার ডিভাইস এনক্রিপশান সমর্থন করে না৷ আপনার আইটি বিভাগের সাথে যোগাযোগ করুন৷"</string>
  <!-- Message of the cancel dialog. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_cancel_message"/>
    <string name="device_owner_cancel_message">"আপনার ডিভাইসের ডেটা সেটআপ করা থামাতে ও মুছতে চান?"</string>
  <!-- Cancel button text of the cancel dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_cancel_cancel"/>
    <string name="device_owner_cancel_cancel">"বাতিল করুন"</string>
  <!-- OK button text of the error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_error_ok"/>
    <string name="device_owner_error_ok">"ঠিক আছে"</string>
  <!-- Reset button text of the error dialog. [CHAR LIMIT=45] -->
  <add-resource type="string" name="device_owner_error_reset"/>
    <string name="device_owner_error_reset">"পুনরায় সেট করুন"</string>
  <!-- Message of the error dialog in case of an unspecified error. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_general"/>
    <string name="device_owner_error_general">"আপনার ডিভাইস সেট আপ হয়নি। আপনার IT বিভগে যোগাযোগ করুন। "</string>
  <!-- Message of the error dialog when already provisioned. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_already_provisioned"/>
    <string name="device_owner_error_already_provisioned">"এই ডিভাইসটিকে ইতিমধ্যেই সেটআপ করা হয়েছে।"</string>
  <!-- Message of the error dialog when setting up wifi failed. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_wifi"/>
    <string name="device_owner_error_wifi">"Wi-Fi এ সংযোগ করা যায়নি"</string>
  <!-- Message of the error dialog when passing factory reset protection fails. [CHAR LIMIT=NONE] -->
  <add-resource type="string" name="device_owner_error_frp"/>
    <string name="device_owner_error_frp">"এই ডিভাইসটিকে ফ্যাক্টরি পুনঃসেট সুরক্ষা দ্বারা লক করা হয়েছে। আপনার IT বিভাগের সঙ্গে যোগাযোগ করুন।"</string>
  <!-- Message informing the user that the factory reset protection partition is being erased [CHAR LIMIT=30] -->
  <add-resource type="string" name="frp_clear_progress_title"/>
    <string name="frp_clear_progress_title">"মোছা হচ্ছে"</string>
