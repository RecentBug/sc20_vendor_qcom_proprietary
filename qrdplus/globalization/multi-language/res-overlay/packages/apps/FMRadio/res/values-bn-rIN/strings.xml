<resources>
    <!-- Application name shown in Launcher [CHAR LIMIT=20] -->
    <add-resource type="string" name="app_name"/>
    <string name="app_name">"FM রেডিও"</string>
    <!-- Toast message shown after we refresh radio stations successfully [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_station_searched"/>
    <string name="toast_station_searched">"উপলভ্য স্টেশনগুলি অনুসন্ধান করা হয়েছে:"</string>
    <!-- Toast message shown after we refresh radio stations failed [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_cannot_search"/>
    <string name="toast_cannot_search">"উপলভ্য স্টেশনগুলি অনুসন্ধান করা যাচ্ছে না।"</string>
    <!-- Title of a menu item to change FM sound output to speaker [CHAR LIMIT=20] -->
    <add-resource type="string" name="optmenu_speaker"/>
    <string name="optmenu_speaker">"স্পীকার"</string>
    <!-- Title of a menu item to change FM sound output to earphone [CHAR LIMIT=20] -->
    <add-resource type="string" name="optmenu_earphone"/>
    <string name="optmenu_earphone">"ইয়ারফোন"</string>
    <!-- Dialog "Save" button to save a new station name [CHAR LIMIT=20] -->
    <add-resource type="string" name="save"/>
    <string name="save">"সংরক্ষণ করুন"</string>
    <!-- Toast message shown when we record without sdcard [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_sdcard_missing"/>
    <string name="toast_sdcard_missing">"আপনার SD কার্ড পাওয়া যাচ্ছে না!"</string>
    <!-- Toast message shown when we record but sdcard has no free space [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_sdcard_insufficient_space"/>
    <string name="toast_sdcard_insufficient_space">"অপর্যাপ্ত SD কার্ড স্থান!"</string>
    <!-- Toast message shown when record or playback record has some internal error
         [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="toast_recorder_internal_error"/>
    <string name="toast_recorder_internal_error">"রেকর্ডারে একটি অভ্যন্তরীণ ত্রুটি ঘটেছে!"</string>
    <!-- Dialog "Save" button to save the FM recording [CHAR LIMIT=20] -->
    <add-resource type="string" name="btn_save_recording"/>
    <string name="btn_save_recording">"সংরক্ষণ করুন"</string>
    <!-- Dialog "Discard" button to discard the FM recording [CHAR LIMIT=20] -->
    <add-resource type="string" name="btn_discard_recording"/>
    <string name="btn_discard_recording">"বাতিল করুন"</string>
    <!-- EditText hint for save recording dialog [CHAR LIMIT=50] -->
    <add-resource type="string" name="edit_recording_name_hint"/>
    <string name="edit_recording_name_hint">"এখানে ফাইল নাম টাইপ করুন"</string>
    <!-- Toast message shown when we save the recording name which is already exists
         [CHAR LIMIT=50] -->
    <add-resource type="string" name="already_exists"/>
    <string name="already_exists">"ইতিমধ্যে বিদ্যমান"</string>
    <!-- Toast message shown when FM power up failed [CHAR LIMIT=30] -->
    <add-resource type="string" name="not_available"/>
    <string name="not_available">"বর্তমানে উপলভ্য নয়"</string>
    <!-- Talkback message for previous station button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_prev_station"/>
    <string name="talk_prev_station">"পূর্ববর্তী স্টেশন"</string>
    <!-- Talkback message for decrease frequency button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_decrease"/>
    <string name="talk_decrease">"ফ্রিকোয়েন্সি হ্রাস করুন"</string>
    <!-- Talkback message for increase frequency button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_increase"/>
    <string name="talk_increase">"ফ্রিকোয়েন্সি বৃদ্ধি করুন"</string>
    <!-- Talkback message for next station button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_next_station"/>
    <string name="talk_next_station">"পরবর্তী স্টেশন"</string>
    <!-- Talkback message for next station button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_add_favorite"/>
    <string name="talk_add_favorite">"পছন্দসইতে যোগ করুন"</string>
    <!-- Talkback message for start recording button [CHAR LIMIT=30] -->
    <add-resource type="string" name="talk_start_recording"/>
    <string name="talk_start_recording">"রেকর্ডিং শুরু করুন"</string>
    <!-- Talkback message for favorit card more menu [CHAR LIMIT=30] -->
    <add-resource type="string" name="more"/>
    <string name="more">"আরও"</string>
    <!-- The tips shown when we start recording [CHAR LIMIT=30] -->
    <add-resource type="string" name="recording_tips"/>
    <string name="recording_tips">"রেকর্ড হচ্ছে\u2026"</string>
    <!-- The text "FM" shown in FM main screen [CHAR LIMIT=30] -->
    <add-resource type="string" name="fm_symbol"/>
    <string name="fm_symbol">FM</string>
    <!-- Talkback message for the floating button which will play or stop FM sound
         [CHAR LIMIT=30] -->
    <add-resource type="string" name="fm_turn_on"/>
    <string name="fm_turn_on">"FM রেডিও প্লে করুন বা থামান"</string>
    <!-- Title of a menu item to start recording FM sound [CHAR LIMIT=30] -->
    <add-resource type="string" name="fm_over_start_record"/>
    <string name="fm_over_start_record">"রেকর্ডিং শুরু করুন"</string>
    <!-- Title of a menu item to see the saved recordings in play music  [CHAR LIMIT=30] -->
    <add-resource type="string" name="fm_over_record_list"/>
    <string name="fm_over_record_list">"রেকর্ডিংগুলি সংরক্ষণ হয়েছে"</string>
    <!-- Title of a menu item to refresh the station list  [CHAR LIMIT=30] -->
    <add-resource type="string" name="fm_station_list_menu_refresh"/>
    <string name="fm_station_list_menu_refresh">"রিফ্রেশ করুন"</string>
    <!-- The tips shown when without an earphone or plug out the earphone [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="fm_no_headset_text"/>
    <string name="fm_no_headset_text">"শোনা শুরু করতে, হেডফোনগুলি প্ল্যাগ ইন করুন (অ্যান্টেনা হিসাবে ব্যবহৃত হয়)।"</string>
    <!-- Actionbar title for station list. It will show in the action bar [CHAR LIMIT=50] -->
    <add-resource type="string" name="station_title"/>
    <string name="station_title">"রেডিও স্টেশনগুলি"</string>
    <!-- The tips shown when we refresh in station list [CHAR LIMIT=50] -->
    <add-resource type="string" name="station_searching_tips"/>
    <string name="station_searching_tips">"স্টেশনগুলি প্রাপ্ত হচ্ছে"</string>
    <!-- The tips shown when we refresh in station list but no stations found [CHAR LIMIT=50] -->
    <add-resource type="string" name="station_empty_text"/>
    <string name="station_empty_text">"কোনও স্টেশন পাওয়া যায়নি"</string>
    <!-- It means "minutes" that shown in record screen [CHAR LIMIT=2] -->
    <add-resource type="string" name="minutes_label"/>
    <string name="minutes_label">"M"</string>
    <!-- It means "seconds" that shown in record screen [CHAR LIMIT=2] -->
    <add-resource type="string" name="seconds_label"/>
    <string name="seconds_label">"S"</string>
    <!-- The default number for record screen. It will show "00M 00S" [CHAR LIMIT=2] -->
    <add-resource type="string" name="timer_default"/>
    <string name="timer_default">00</string>
    <!-- Actionbar title for recording FM. It will show in the action bar [CHAR LIMIT=20] -->
    <add-resource type="string" name="record_title"/>
    <string name="record_title">"রেকর্ড করা হচ্ছে"</string>
    <!-- The text shown in notification when currently is recording [CHAR LIMIT=50] -->
    <add-resource type="string" name="record_notification_message"/>
    <string name="record_notification_message">"রেকর্ডিং চলছে"</string>
    <!-- Stop recording button shown when currently is recording [CHAR LIMIT=20] -->
    <add-resource type="string" name="stop_record"/>
    <string name="stop_record">"রেকর্ডিং বন্ধ করুন"</string>
    <!-- Title of save dialog shown when stop recording [CHAR LIMIT=40] -->
    <add-resource type="string" name="save_dialog_title"/>
    <string name="save_dialog_title">"রেকর্ডিং সংরক্ষণ করবেন?"</string>
    <!-- Tips of save dialog shown when stop recording [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="save_dialog_caption"/>
    <string name="save_dialog_caption">"রেকর্ডিংগুলি শুনতে, সঙ্গীত প্লে করুন অ্যাপটি খুলুন।"</string>
    <!-- Title of favorite area shown when we have favorite stations [CHAR LIMIT=50] -->
    <add-resource type="string" name="favorites"/>
    <string name="favorites">"পছন্দসই"</string>
    <!-- Title of a menu in favorite card more menu. It means rename a station [CHAR LIMIT=50] -->
    <add-resource type="string" name="rename"/>
    <string name="rename">"পুনঃনামকরণ"</string>
    <!-- EditText hint for rename station dialog [CHAR LIMIT=50] -->
    <add-resource type="string" name="station_rename_hint"/>
    <string name="station_rename_hint">"স্টেশন নাম লিখুন"</string>
    <!-- Title of a menu in favorite card more menu. It means remove a station from favorite
         [CHAR LIMIT=50] -->
    <add-resource type="string" name="remove_favorite"/>
    <string name="remove_favorite">"পছন্দসই অপসারণ করুন"</string>
    <!-- Snackbar message shown after we save a recording [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_record_saved"/>
    <string name="toast_record_saved">"রেকর্ডিং সংরক্ষণ করা হয়েছে"</string>
    <!-- Snackbar message shown after we discard a recording [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_record_not_saved"/>
    <string name="toast_record_not_saved">"রেকর্ডিং সংরক্ষণ করা হয়নি"</string>
    <!-- Button in snackbar shown we save a recording. It will enter play music to listen recording
         [CHAR LIMIT=12] -->
    <add-resource type="string" name="toast_listen"/>
    <string name="toast_listen">"শুনুন"</string>
    <!-- Snackbar message shown after we rename a favorite station [CHAR LIMIT=50] -->
    <add-resource type="string" name="toast_station_renamed"/>
    <string name="toast_station_renamed">"স্টেশনটির পুনঃনামকরণ করা হয়েছে"</string></resources>
