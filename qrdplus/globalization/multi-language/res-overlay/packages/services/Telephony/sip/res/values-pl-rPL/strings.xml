<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title displayed SIP settings coming after other call settings in the call features screen. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_settings"/>
    <string name="sip_settings">"Ustawienia dzwonienia przez internet"</string>
    <!-- Title displayed SIP account settings in the sip settings category. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_accounts"/>
    <string name="sip_accounts">"Konta SIP"</string>
    <!-- Title displayed SIP account settings in the sip settings category. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_accounts_title"/>
    <string name="sip_accounts_title">"Konta"</string>
    <!-- Title displayed for the button of receiving incoming SIP calls flag. Enabled for receiving calls. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_receive_calls"/>
    <string name="sip_receive_calls">"Odbieraj połączenia przychodzące"</string>
    <!-- Help user to understand that it will require more battery usage if the 'receiving incoming call' is enabled. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_receive_calls_summary"/>
    <string name="sip_receive_calls_summary">"Skraca czas pracy baterii"</string>
    <!-- Title for the dialog of selecting the way we handle an outgoing call. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_call_options_title"/>
    <string name="sip_call_options_title">"Dzwoń przez internet"</string>
    <!-- Title for the dialog of selecting the way we handle an outgoing call (Wi-Fi only). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_call_options_wifi_only_title"/>
    <string name="sip_call_options_wifi_only_title">"Dzwoń przez internet (tylko Wi-Fi)"</string>

    <!-- Item of the SIP call option dialog: for routing all outgoing calls via SIP. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_call_options_entry_1"/>
    <string name="sip_call_options_entry_1">"Przy wszystkich połączeniach, gdy sieć transmisji danych jest dostępna"</string>
    <!-- Item of the SIP call option dialog: for routing a outgoing call via SIP if the destination is a SIP URI. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_call_options_entry_2"/>
    <string name="sip_call_options_entry_2">"Tylko internetowe"</string>
    <!-- Item of the SIP call (Wi-Fi only) option dialog: for routing all outgoing calls via SIP. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_call_options_wifi_only_entry_1"/>
    <string name="sip_call_options_wifi_only_entry_1">"Wszystkie połączenia"</string>

    <!-- Title for the button to add a new sip account. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="add_sip_account"/>
    <string name="add_sip_account">"Dodaj konto"</string>
    <!-- Title for the button to remove a sip account. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="remove_sip_account"/>
    <string name="remove_sip_account">"Usuń konto"</string>
    <!-- String of the category which lists all sip accounts. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_account_list"/>
    <string name="sip_account_list">"Konta SIP"</string>
    <!-- Toast message to indicate that the system is saving the account. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="saving_account"/>
    <string name="saving_account">"Zapisuję konto\u2026"</string>
    <!-- Toast message to indicate that the system is removing the account. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="removing_account"/>
    <string name="removing_account">"Usuwam konto\u2026"</string>

    <!-- Menu item when the menu button is pressed in the sip account editor. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_menu_save"/>
    <string name="sip_menu_save">"Zapisz"</string>
    <!-- Menu item when the menu button is pressed in the sip account editor. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_menu_discard"/>
    <string name="sip_menu_discard">"Odrzuć"</string>

    <!-- Title of the alert dialog. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="alert_dialog_close"/>
    <string name="alert_dialog_close">"Zamknij profil"</string>
    <!-- Title of the button to show in a alert dialog. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="alert_dialog_ok"/>
    <string name="alert_dialog_ok">"OK"</string>
    <!-- Title of the button to show in a alert dialog. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="close_profile"/>
    <string name="close_profile">"Zamknij"</string>

    <!-- Text of SIP-call registration status, checking current registration status with SIP service [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_checking_status"/>
    <string name="registration_status_checking_status">"Sprawdzam stan..."</string>
    <!-- Text of SIP-call registration status, registering with SIP server in order to receive calls on this account [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_registering"/>
    <string name="registration_status_registering">"Rejestruję\u2026"</string>
    <!-- Text of SIP-call registration status, still trying to register with SIP server [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_still_trying"/>
    <string name="registration_status_still_trying">"Nadal próbuję\u2026"</string>
    <!-- Text of SIP-call registration status, currently not receiving calls on this account [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_not_receiving"/>
    <string name="registration_status_not_receiving">"Połączenia nie są odbierane."</string>
    <!-- Text of SIP-call registration status, no data connection [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_no_data"/>
    <string name="registration_status_no_data">"Rejestracja konta została zatrzymana, ponieważ nie masz połączenia internetowego."</string>
    <!-- Text of SIP-call registration status, no Wi-Fi connection [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_no_wifi_data"/>
    <string name="registration_status_no_wifi_data">"Rejestracja konta została zatrzymana, ponieważ nie masz połączenia Wi-Fi."</string>
    <!-- Text of SIP-call registration status, registration process is not running due to some error [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_not_running"/>
    <string name="registration_status_not_running">"Rejestracja konta nie powiodła się."</string>
    <!-- Text of SIP-call registration status, currently we can receive calls on this account [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_done"/>
    <string name="registration_status_done">"Odbieranie połączeń."</string>
    <!-- Text of SIP-call registration status, registration failed with a reason; will try again later [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_failed_try_later"/>
    <string name="registration_status_failed_try_later">"Rejestracja konta nie powiodła się: (<xliff:g id="REGISTRATION_ERROR_MESSAGE">%s</xliff:g>). Spróbujemy ponownie później."</string>
    <!-- Text of SIP-call registration status, registration failed due to invalid credentials [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_invalid_credentials"/>
    <string name="registration_status_invalid_credentials">"Rejestracja konta nie powiodła się: nieprawidłowa nazwa użytkownika lub hasło."</string>
    <!-- Text of SIP-call registration status, registration failed due to unknown host exception [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="registration_status_server_unreachable"/>
    <string name="registration_status_server_unreachable">"Rejestracja konta nie powiodła się: sprawdź nazwę serwera."</string>
    <!-- Text for describing the account's owner and status. For example, 'Active, in use by Google Talk', it means that the account managed by SIP service was registered by the application 'Google Talk' and the status is active. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="third_party_account_summary"/>
    <string name="third_party_account_summary">"To konto jest obecnie używane przez aplikację <xliff:g id="ACCOUNT_OWNER">%s</xliff:g>."</string>


    <!-- Title of the sip editor screen. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_edit_title"/>
    <string name="sip_edit_title">"Szczeg. kont SIP"</string>
    <!-- Title of the sip editor screen. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="sip_edit_new_title"/>
    <string name="sip_edit_new_title">"Szczeg. kont SIP"</string>

    <!-- Text of the domain address preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="domain_address_title"/>
    <string name="domain_address_title">"Serwer"</string>
    <!-- Text of the username preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="username_title"/>
    <string name="username_title">"Nazwa użytkownika"</string>
    <!-- Text of the password preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="password_title"/>
    <string name="password_title">"Hasło"</string>
    <!-- Text of the display name preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="display_name_title"/>
    <string name="display_name_title">"Wyświetlana nazwa"</string>
    <!-- Text of the outbound proxy address preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="proxy_address_title"/>
    <string name="proxy_address_title">"Proxy połączeń wychodzących"</string>
    <!-- Text of the sip server/proxy port number. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="port_title"/>
    <string name="port_title">"Numer portu"</string>
    <!-- Text of the transport type preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="transport_title"/>
    <string name="transport_title">"Typ transportu"</string>
    <!-- Text of the keepalive preference. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="send_keepalive_title"/>
    <string name="send_keepalive_title">"Utrzymuj połączenie"</string>
    <!-- Text of the advanced settings section. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="advanced_settings"/>
    <string name="advanced_settings">"Ustawienia opcjonalne"</string>
    <!-- Text of the username used in authentication. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="auth_username_title"/>
    <string name="auth_username_title">"Nazwa do uwierzytelniania"</string>
    <!-- Help text of the auth_username field. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="auth_username_summary"/>
    <string name="auth_username_summary">"Nazwa użytkownika używana do uwierzytelniania"</string>

    <!-- Initial status of the username preference is '<Not set>'. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="default_preference_summary_username"/>
    <string name="default_preference_summary_username">"&lt;Nie ustawiono&gt;"</string>
    <!-- Initial status of the password preference is '<Not set>'. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="default_preference_summary_password"/>
    <string name="default_preference_summary_password">"&lt;Nie ustawiono&gt;"</string>
    <!-- Initial status of the domain address preference is '<Not set>'. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="default_preference_summary_domain_address"/>
    <string name="default_preference_summary_domain_address">"&lt;Nie ustawiono&gt;"</string>
    <!-- Default value for the display-name preference summary. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="display_name_summary"/>
    <string name="display_name_summary">"&lt;Taka jak nazwa użytkownika&gt;"</string>
    <!-- Default value for the outbound-proxy-address preference summary. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="optional_summary"/>
    <string name="optional_summary">"&lt;Opcjonalny&gt;"</string>


    <!-- Hint to show the advanced settings section. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="advanced_settings_show"/>
    <string name="advanced_settings_show">"▷ Dotknij, by pokazać wszystko"</string>
    <!-- Hint to hide the advanced settings section. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="advanced_settings_hide"/>
    <string name="advanced_settings_hide">"▽ Dotknij, by ukryć wszystko"</string>

    <!-- Text in an alert dialog telling the user that all input fields can not be left blank. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="all_empty_alert"/>
    <string name="all_empty_alert">"Wpisz szczegóły nowego konta SIP"</string>
    <!-- Text in an alert dialog telling the user that some input field (name of which is also part of the strings being translated above) is required and can not be left blank. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="empty_alert"/>
    <string name="empty_alert">"Pole <xliff:g id="INPUT_FIELD_NAME">%s</xliff:g> jest wymagane i nie może pozostać puste."</string>
    <!-- Showing that port number is out of range in an alert dialog. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="not_a_valid_port"/>
    <string name="not_a_valid_port">"Numer portu powinien mieć wartość od 1000 do 65 534."</string>

    <!-- Message of the alert dialog to notify user that there is no Internet connectivity. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_internet_available"/>
    <string name="no_internet_available">"Aby zadzwonić przez internet, sprawdź najpierw połączenie internetowe."</string>
    <!-- Message of the alert dialog to notify user that there is no Wi-Fi connectivity. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_wifi_available"/>
    <string name="no_wifi_available">"Aby dzwonić przez internet, musisz połączyć się z siecią Wi-Fi (użyj ustawień w sekcji Sieci zwykłe i bezprzewodowe)."</string>
    <!-- Message of the alert dialog to notify user that SIP call is not supported. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="no_voip"/>
    <string name="no_voip">"Brak obsługi połączeń przez internet"</string>

    <!-- Option for whether the SIP service should send keepalive messages. [CHAR LIMIT=40] -->
    <add-resource type="string" name="sip_system_decide"/>
    <string translatable="true" name="sip_system_decide">"Automat."</string>
    <!-- Option for whether the SIP service should send keepalive messages. [CHAR LIMIT=40] -->
    <add-resource type="string" name="sip_always_send_keepalive"/>
    <string translatable="true" name="sip_always_send_keepalive">"Zawsze wysyłaj"</string>

    <!-- Label for SIP connection service. -->
    <add-resource type="string" name="sip_connection_service_label"/>
    <string name="sip_connection_service_label">"Wbudowana funkcja telefonii internetowej"</string></resources>
