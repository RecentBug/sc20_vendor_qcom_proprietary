    <add-resource type="string" name="handle_restricted"/>
    <string name="handle_restricted">RESTRICTED</string>

    <add-resource type="string" name="toast_personal_call_msg"/>
    <string name="toast_personal_call_msg">"전화를 걸 때 개인 다이얼러 사용"</string>
    <add-resource type="string" name="toast_make_video_call_failed"/>
