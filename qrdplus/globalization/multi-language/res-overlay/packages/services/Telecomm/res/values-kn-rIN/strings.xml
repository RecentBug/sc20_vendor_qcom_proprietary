<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Official label of the Telecomm/Phone app, as seen in "Manage Applications"
         and other settings UIs. This is the "app name" used in notification, recents,
         and app info screens. -->
    <add-resource type="string" name="telecommAppLabel"/>
    <string name="telecommAppLabel" product="default">"ಕರೆ ನಿರ್ವಹಣೆ"</string>

    <!-- Title used for the activity for placing a call. This name appears
         in activity disambig dialogs -->
    <add-resource type="string" name="userCallActivityLabel"/>
    <string name="userCallActivityLabel" product="default">"ಫೋನ್"</string>

    <!-- Name for an "unknown" caller. -->
    <add-resource type="string" name="unknown"/>
    <string name="unknown">"ಅಜ್ಞಾತ"</string>

    <!-- Notification strings -->
    <!-- Missed call notification label, used when there's exactly one missed call -->
    <add-resource type="string" name="notification_missedCallTitle"/>
    <string name="notification_missedCallTitle">"ಮಿಸ್ಡ್‌ ಕಾಲ್‌"</string>
    <!-- Missed work call notification label, used when there's exactly one missed call [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="notification_missedWorkCallTitle"/>
    <string name="notification_missedWorkCallTitle">"ಮಿಸ್ಡ್‌ ಕೆಲಸದ ಕರೆ"</string>
    <!-- Missed call notification label, used when there are two or more missed calls -->
    <add-resource type="string" name="notification_missedCallsTitle"/>
    <string name="notification_missedCallsTitle">"ತಪ್ಪಿದ ಕರೆಗಳು"</string>
    <!-- Missed call notification message used when there are multiple missed calls -->
    <add-resource type="string" name="notification_missedCallsMsg"/>
    <string name="notification_missedCallsMsg">"<xliff:g id="NUM_MISSED_CALLS">%s</xliff:g> ತಪ್ಪಿದ ಕರೆಗಳು"</string>
    <!-- Missed call notification message used for a single missed call, including
         the caller-id info from the missed call -->
    <add-resource type="string" name="notification_missedCallTicker"/>
    <string name="notification_missedCallTicker">"<xliff:g id="MISSED_CALL_FROM">%s</xliff:g> ಅವರಿಂದ ಮಿಸ್ಡ್‌ ಕಾಲ್‌"</string>
    <!-- Message for "call back" Action, which is displayed in the missed call notificaiton.
         The user will be able to call back to the person or the phone number.
         [CHAR LIMIT=18] -->
    <add-resource type="string" name="notification_missedCall_call_back"/>
    <string name="notification_missedCall_call_back">"ಮರಳಿ ಕರೆ ಮಾಡಿ"</string>
    <!-- Message for "reply via sms" action, which is displayed in the missed call notification.
         The user will be able to send text messages using the phone number.
         [CHAR LIMIT=18] -->
    <add-resource type="string" name="notification_missedCall_message"/>
    <string name="notification_missedCall_message">"ಸಂದೇಶ"</string>

    <!-- Content description of the call muted notification icon for
         accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="accessibility_call_muted"/>
    <string name="accessibility_call_muted">"ಕರೆಯನ್ನು ಮ್ಯೂಟ್ ಮಾಡಲಾಗಿದೆ."</string>

    <!-- Content description of the speakerphone enabled notification icon for
         accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="accessibility_speakerphone_enabled"/>
    <string name="accessibility_speakerphone_enabled">"ಸ್ಪೀಕರ್‌ಫೋನ್ ಸಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ."</string>

    <!-- Canned response for the "Respond via SMS" feature for incoming calls. [CHAR LIMIT=35] -->
    <add-resource type="string" name="respond_via_sms_canned_response_1"/>
    <string name="respond_via_sms_canned_response_1">"ಇದೀಗ ಮಾತನಾಡಲು ಸಾಧ್ಯವಿಲ್ಲ. ಏನು ಸಮಾಚಾರ?"</string>
    <!-- Canned response for the "Respond via SMS" feature for incoming calls. [CHAR LIMIT=35] -->
    <add-resource type="string" name="respond_via_sms_canned_response_2"/>
    <string name="respond_via_sms_canned_response_2">"ನಾನು ಮರಳಿ ನಿಮಗೆ ಕರೆ ಮಾಡುತ್ತೇನೆ."</string>
    <!-- Canned response for the "Respond via SMS" feature for incoming calls. [CHAR LIMIT=35] -->
    <add-resource type="string" name="respond_via_sms_canned_response_3"/>
    <string name="respond_via_sms_canned_response_3">"ನಾನು ನಂತರ ನಿಮಗೆ ಕರೆ ಮಾಡುತ್ತೇನೆ."</string>
    <!-- Canned response for the "Respond via SMS" feature for incoming calls. [CHAR LIMIT=35] -->
    <add-resource type="string" name="respond_via_sms_canned_response_4"/>
    <string name="respond_via_sms_canned_response_4">"ಇದೀಗ ಮಾತನಾಡಲು ಸಾಧ್ಯವಿಲ್ಲ. ನನಗೆ ನಂತರ ಕರೆ ಮಾಡುವಿರಾ?"</string>

    <!-- Title of settings screen for managing the "Respond via SMS" feature. [CHAR LIMIT=30] -->
    <add-resource type="string" name="respond_via_sms_setting_title"/>
    <string name="respond_via_sms_setting_title">"ತ್ವರಿತ ಪ್ರತಿಕ್ರಿಯೆ"</string>
    <!-- Slightly more verbose title of settings screen for managing the
         "Respond via SMS" feature. [CHAR LIMIT=30] -->
    <add-resource type="string" name="respond_via_sms_setting_title_2"/>
    <string name="respond_via_sms_setting_title_2">"ತ್ವರಿತ ಪ್ರತಿಕ್ರಿಯೆಗಳನ್ನು ಸಂಪಾದಿಸಿ"</string>
    <!-- Settings summary string for the "Respond via SMS" feature. [CHAR LIMIT=40] -->
    <add-resource type="string" name="respond_via_sms_setting_summary"/>
    <string name="respond_via_sms_setting_summary"></string>
    <!-- Dialog title when changing a string for the "Respond via SMS" feature. [CHAR LIMIT=30] -->
    <add-resource type="string" name="respond_via_sms_edittext_dialog_title"/>
    <string name="respond_via_sms_edittext_dialog_title">"ತ್ವರಿತ ಪ್ರತಿಕ್ರಿಯೆ"</string>

    <!-- "Respond via SMS": Confirmation message shown after sending
        a text response. [CHAR LIMIT=40] -->
    <add-resource type="string" name="respond_via_sms_confirmation_format"/>
    <string name="respond_via_sms_confirmation_format">"<xliff:g id="PHONE_NUMBER">%s</xliff:g> ಗೆ ಸಂದೇಶ ಕಳುಹಿಸಲಾಗಿದೆ."</string>

    <!-- Title of settings screen that allows user to enable and disable phone-accounts.
         Each method for placing a call (SIM1, SIM2, SIP account, etc) has a phone-account.
         Phone-accounts that are created by third party apps can be disabled and enabled by user.
         [CHAR LIMIT=30] -->
    <add-resource type="string" name="enable_account_preference_title"/>
    <string name="enable_account_preference_title">"ಕರೆ ಮಾಡುವ ಖಾತೆಗಳು"</string>

    <!-- Message indicating that the user is not allowed to make non-emergency outgoing phone calls
         due to a user restriction -->
    <add-resource type="string" name="outgoing_call_not_allowed_user_restriction"/>
    <string name="outgoing_call_not_allowed_user_restriction">"ತುರ್ತು ಕರೆಗಳನ್ನು ಮಾಡಲು ಮಾತ್ರ ಅವಕಾಶವಿದೆ."</string>

    <!-- Message indicating that the user is not allowed to make non-emergency outgoing phone calls
         due to the lack of the CALL_PHONE permission -->
    <add-resource type="string" name="outgoing_call_not_allowed_no_permission"/>
    <string name="outgoing_call_not_allowed_no_permission">"ಈ ಅಪ್ಲಿಕೇಶನ್ ಫೋನ್ ಅನುಮತಿಯಿಲ್ಲದೆ ಹೊರಹೋಗುವ ಕರೆಗಳನ್ನು ಮಾಡಲು ಸಾಧ್ಯವಿಲ್ಲ."</string>

    <!-- Call failure message displayed in an error dialog used to indicate that a phone number was not provided -->
    <add-resource type="string" name="outgoing_call_error_no_phone_number_supplied"/>
    <string name="outgoing_call_error_no_phone_number_supplied">"ಕರೆಯನ್ನು ಮಾಡಲು, ಮಾನ್ಯವಾದ ಸಂಖ್ಯೆಯನ್ನು ನಮೂದಿಸಿ."</string>

    <!-- Message shown when the user tries to make a video call when already in a video call. -->
    <add-resource type="string" name="duplicate_video_call_not_allowed"/>
    <string name="duplicate_video_call_not_allowed">"ಕರೆಯನ್ನು ಈ ಸಮಯದಲ್ಲಿ ಸೇರಿಸಲಾಗುವುದಿಲ್ಲ."</string>

    <!-- missing voicemail number -->
    <!-- Title of the "Missing voicemail number" dialog -->
    <add-resource type="string" name="no_vm_number"/>
    <string name="no_vm_number">"ಧ್ವನಿಮೇಲ್‌ ಸಂಖ್ಯೆಯು ಕಾಣೆಯಾಗಿದೆ"</string>
    <!-- Body text of the "Missing voicemail number" dialog -->
    <add-resource type="string" name="no_vm_number_msg"/>
    <string name="no_vm_number_msg">"SIM ಕಾರ್ಡ್‌ನಲ್ಲಿ ಯಾವುದೇ ಧ್ವನಿಮೇಲ್‌ ಸಂಖ್ಯೆಯನ್ನು ಸಂಗ್ರಹಿಸಿಲ್ಲ."</string>
    <!-- Button label on the "Missing voicemail number" dialog -->
    <add-resource type="string" name="add_vm_number_str"/>
    <string name="add_vm_number_str">"ಸಂಖ್ಯೆಯನ್ನು ಸೇರಿಸಿ"</string>

    <!-- Title of dialog used to comfirm whether the user intends to change the default dialer
            application [CHAR LIMIT=55]-->
    <add-resource type="string" name="change_default_dialer_dialog_title"/>
    <string name="change_default_dialer_dialog_title">"<xliff:g id="NEW_APP">%s</xliff:g> ಅನ್ನು ನಿಮ್ಮ ಡಿಫಾಲ್ಟ್ ಫೋನ್ ಅಪ್ಲಿಕೇಶನ್ ಆಗಿ ಮಾಡುವುದೇ?"</string>
    <!-- Confirmation text that a user taps on to change the Default Phone App-->
    <add-resource type="string" name="change_default_dialer_dialog_affirmative"/>
    <string name="change_default_dialer_dialog_affirmative">"ಡಿಫಾಲ್ಟ್ ಹೊಂದಿಸಿ"</string>
    <!-- Cancel text that a user taps on to not change the Default Phone App-->
    <add-resource type="string" name="change_default_dialer_dialog_negative"/>
    <string name="change_default_dialer_dialog_negative">"ರದ್ದುಮಾಡು"</string>
    <!-- Warning message indicating what may happen if a user allows a 3rd party app to become the default dialer.-->
    <add-resource type="string" name="change_default_dialer_warning_message"/>
    <string name="change_default_dialer_warning_message">"<xliff:g id="NEW_APP">%s</xliff:g> ಗೆ ನಿಮ್ಮ ಕರೆಗಳ ಎಲ್ಲಾ ಅಂಶಗಳನ್ನು ನಿಯಂತ್ರಿಸಲು ಮತ್ತು ಕರೆಗಳನ್ನು ಮಾಡಲು ಸಾಧ್ಯವಾಗುತ್ತದೆ. ನೀವು ವಿಶ್ವಾಸವಿರಿಸಿರುವಂತಹ ಅಪ್ಲಿಕೇಶನ್‌ಗಳನ್ನು ಮಾತ್ರ ನಿಮ್ಮ ಡಿಫಾಲ್ಟ್ ಅಪ್ಲಿಕೇಶನ್ ಆಗಿ ಹೊಂದಿಸಬೇಕು."</string>

    <!-- Blocked numbers -->
    <add-resource type="string" name="blocked_numbers"/>
    <string name="blocked_numbers">"ನಿರ್ಬಂಧಿಸಲಾದ ಸಂಖ್ಯೆಗಳು"</string>
    <!-- Text shown at the beginning of the blocked numbers screen to explain what the screen is about. -->
    <add-resource type="string" name="blocked_numbers_msg"/>
    <string name="blocked_numbers_msg">"ನಿರ್ಬಂಧಿಸಲಾದ ಸಂಖ್ಯೆಗಳಿಂದ ಕರೆಗಳು ಅಥವಾ ಪಠ್ಯ ಸಂದೇಶಗಳನ್ನು ನೀವು ಸ್ವೀಕರಿಸುವುದಿಲ್ಲ."</string>
    <!-- Button to add a blocked number. -->
    <add-resource type="string" name="block_number"/>
    <string name="block_number">"ಸಂಖ್ಯೆ ಸೇರಿಸಿ"</string>
    <!-- Body of dialog to confirm unblocking a number. -->
    <add-resource type="string" name="unblock_dialog_body"/>
    <string name="unblock_dialog_body">"<xliff:g id="NUMBER_TO_BLOCK">%1$s</xliff:g> ನಿರ್ಬಂಧ ತೆಗೆಯುವುದೇ?"</string>
    <!-- Button to unblock a number. -->
    <add-resource type="string" name="unblock_button"/>
    <string name="unblock_button">"ನಿರ್ಬಂಧ ತೆಗೆಯಿರಿ"</string>
    <!-- Body of dialog to block a number.  -->
    <add-resource type="string" name="add_blocked_dialog_body"/>
    <string name="add_blocked_dialog_body">"ಇದರಿಂದ ಬರುವ ಕರೆಗಳು ಮತ್ತು ಪಠ್ಯ ಸಂದೇಶಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ"</string>
    <!-- Hint shown in the edit text box for adding a blocked number -->
    <add-resource type="string" name="add_blocked_number_hint"/>
    <string name="add_blocked_number_hint">"ಫೋನ್ ಸಂಖ್ಯೆ"</string>
    <!-- Button to block a number. -->
    <add-resource type="string" name="block_button"/>
    <string name="block_button">"ನಿರ್ಬಂಧಿಸು"</string>
    <!-- String shown to users unable to manage blocked numbers because they are not owners of the
        device. -->
    <add-resource type="string" name="non_primary_user"/>
    <string name="non_primary_user">"ಸಾಧನ ಮಾಲೀಕರು ಮಾತ್ರ ನಿರ್ಬಂಧಿಸಿದ ಸಂಖ್ಯೆಗಳನ್ನು ವೀಕ್ಷಿಸಬಹುದು ಮತ್ತು ನಿರ್ವಹಿಸಬಹುದು."</string>
    <!-- Description of icon to delete blocked number. -->
    <add-resource type="string" name="delete_icon_description"/>
    <string name="delete_icon_description">"ನಿರ್ಬಂಧ ತೆಗೆಯಿರಿ"</string>
    <!-- Title for butter bar shown when call blocking is disabled. -->
    <add-resource type="string" name="blocked_numbers_butter_bar_title"/>
    <string name="blocked_numbers_butter_bar_title">"ನಿರ್ಬಂಧಿಸುವಿಕೆ ತಾತ್ಕಾಲಿಕವಾಗಿ ಆಫ್ ಮಾಡಲಾಗಿದೆ"</string>
    <!-- Explanation for butter bar shown when call blocking is disabled. -->
    <add-resource type="string" name="blocked_numbers_butter_bar_body"/>
    <string name="blocked_numbers_butter_bar_body">"ನೀವು ತುರ್ತು ಸಂಖ್ಯೆಯನ್ನು ಡಯಲ್‌ ಮಾಡಿದ ಅಥವಾ ಪಠ್ಯ ಸಂದೇಶ ಕಳುಹಿಸಿದ ನಂತರ, ತುರ್ತು ಸೇವೆಗಳಲ್ಲಿ ನಿಮ್ಮನ್ನು ಸಂಪರ್ಕಿಸಬಹುದು ಎಂಬುದನ್ನು ಖಚಿತಪಡಿಸಿಕೊಳ್ಳಲು ನಿರ್ಬಂಧಿಸುವಿಕೆ ಆಫ್‌ ಮಾಡಲಾಗಿದೆ."</string>
    <!-- Button to re-enable blocking shown in butter bar shown when call blocking is disabled. -->
    <add-resource type="string" name="blocked_numbers_butter_bar_button"/>
    <string name="blocked_numbers_butter_bar_button">"ಇದೀಗ ಮರು ಸಕ್ರಿಯಗೊಳಿಸಿ"</string>
    <!-- Message to show when a number is blocked. -->
    <add-resource type="string" name="blocked_numbers_number_blocked_message"/>
    <string name="blocked_numbers_number_blocked_message">"<xliff:g id="BLOCKED_NUMBER">%1$s</xliff:g> ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ"</string>
    <!-- Message to show when a number is unblocked. -->
    <add-resource type="string" name="blocked_numbers_number_unblocked_message"/>
    <string name="blocked_numbers_number_unblocked_message">"<xliff:g id="UNBLOCKED_NUMBER">%1$s</xliff:g> ನಿರ್ಬಂಧ ತೆಗೆಯಲಾಗಿದೆ"</string>
    <!-- Message to show when a number cannot be blocked because it is associated with emergency services.. -->
    <add-resource type="string" name="blocked_numbers_block_emergency_number_message"/>
    <string name="blocked_numbers_block_emergency_number_message">"ತುರ್ತು ಸಂಖ್ಯೆಯನ್ನು ನಿರ್ಬಂಧಿಸಲು ಸಾಧ್ಯವಿಲ್ಲ."</string>
    <!-- Message to show when a number is already blocked. -->
    <add-resource type="string" name="blocked_numbers_number_already_blocked_message"/>
    <string name="blocked_numbers_number_already_blocked_message">"<xliff:g id="BLOCKED_NUMBER">%1$s</xliff:g> ಅನ್ನು ಈಗಾಗಲೇ ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ."</string>

