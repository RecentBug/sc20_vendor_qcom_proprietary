<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->

    <!-- Application name -->
    <add-resource type="string" name="application_name"/>
    <string name="application_name">"Opções de plano de fundo interativo"</string>
    <!-- Title for the screen that lets the user choose a live wallpaper to use
         for the system. -->
    <add-resource type="string" name="live_wallpaper_picker_title"/>
    <string name="live_wallpaper_picker_title">"Selecionar Plano de fundo interativo"</string>
    <!-- Label for the live wallpaper picker [CHAR_LIMIT=40]-->
    <add-resource type="string" name="live_wallpaper_picker_label"/>
    <string name="live_wallpaper_picker_label">"Planos de fundo interativos"</string>
    <!-- Title for the screen that lets the user preview a live wallpaper. -->
    <add-resource type="string" name="live_wallpaper_preview_title"/>
    <string name="live_wallpaper_preview_title">"Visualização do p. de fundo interativo"</string>
    <!-- List item for configuring the current wallpaper -->
    <add-resource type="string" name="configure_wallpaper"/>
    <string name="configure_wallpaper">"Configurações\u2026"</string>
    <!-- Button label on Wallpaper Gallery screen; user selects this button to set a specific wallpaper -->
    <add-resource type="string" name="wallpaper_instructions"/>
    <string name="wallpaper_instructions">"Definir plano de fundo"</string>
    <!-- Warning message, no live wallpapers available. -->
    <add-resource type="string" name="live_wallpaper_empty"/>
    <string name="live_wallpaper_empty">"Não há planos de fundo interativos."</string>
    <!-- Button label, action, sets the currently selected wallpaper. -->
    <add-resource type="string" name="set_live_wallpaper"/>
    <string name="set_live_wallpaper">"Definir plano de fundo"</string>
    <!-- Label, title and author of the live wallpaper -->
    <add-resource type="string" name="wallpaper_title_and_author"/>
    <string name="wallpaper_title_and_author">"<xliff:g id="TITLE">%1$s</xliff:g> por <xliff:g id="AUTHOR">%2$s</xliff:g>"</string>
    <!-- Message, tells the user the selected live wallpaper is loading. -->
    <add-resource type="string" name="live_wallpaper_loading"/>
    <string name="live_wallpaper_loading">"Carregando plano de fundo interativo..."</string>

    <!-- Option for setting the wallpaper only on the home screen. -->
    <add-resource type="string" name="which_wallpaper_option_home_screen"/>
    <string name="which_wallpaper_option_home_screen">"Tela inicial"</string>
    <!-- Option for setting the wallpaper on both the home screen and lock screen. -->
    <add-resource type="string" name="which_wallpaper_option_home_screen_and_lock_screen"/>
    <string name="which_wallpaper_option_home_screen_and_lock_screen">"Tela inicial e tela de bloqueio"</string>

    <!-- Content description for a button letting the user collapse a visual panel showing info
         about the wallpaper being previewed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="collapse_attribution_panel"/>
    <string name="collapse_attribution_panel">"Recolher painel de informações de plano de fundo"</string>

    <!-- Content description for a button letting the user expand a visual panel showing info about
         the wallpaper being previewed. [CHAR LIMIT=none] -->
    <add-resource type="string" name="expand_attribution_panel"/>
    <string name="expand_attribution_panel">"Expandir painel de informações de plano de fundo"</string></resources>
