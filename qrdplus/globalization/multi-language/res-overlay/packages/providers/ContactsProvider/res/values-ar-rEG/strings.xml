<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app's shared user ID (and thus for all other
         apps sharing the uid). -->
    <add-resource type="string" name="sharedUserLabel"/>
    <string name="sharedUserLabel">"‏تطبيقات Android المركزية"</string>

    <!-- This is the label for the application that stores contacts data -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"تخزين جهات الاتصال"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <add-resource type="string" name="provider_label"/>
    <string name="provider_label">"جهات الاتصال"</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <add-resource type="string" name="upgrade_out_of_memory_notification_ticker"/>
    <string name="upgrade_out_of_memory_notification_ticker">"تتطلب ترقية جهات الاتصال مزيدًا من الذاكرة."</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <add-resource type="string" name="upgrade_out_of_memory_notification_title"/>
    <string name="upgrade_out_of_memory_notification_title">"ترقية وحدة التخزين لجهات الاتصال"</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage [CHAR_LIMIT=50] -->
    <add-resource type="string" name="upgrade_out_of_memory_notification_text"/>
    <string name="upgrade_out_of_memory_notification_text">"انقر لإكمال الترقية."</string>

    <!-- The name of the default contact directory -->
    <add-resource type="string" name="default_directory"/>
    <string name="default_directory">"جهات الاتصال"</string>

    <!-- The name of the invisible local contact directory -->
    <add-resource type="string" name="local_invisible_directory"/>
    <string name="local_invisible_directory">"غير ذلك"</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can't allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <add-resource type="string" name="voicemail_from_column"/>
    <string name="voicemail_from_column">"بريد صوتي من "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_title"/>
    <string name="debug_dump_title">"نسخ قاعدة بيانات جهات الاتصال"</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_database_message"/>
    <string name="debug_dump_database_message">"أنت على وشك 1) إنشاء نسخة من قاعدة بياناتك التي تتضمن جميع المعلومات المرتبطة بجهات الاتصال وجميع سجلات المكالمات إلى وحدة التخزين الداخلية و2) إرسالها بالبريد الإلكتروني. تذكر حذف النسخة بمجرد إتمام نسخها من الجهاز أو تلقي الرسالة الإلكترونية."</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_delete_button"/>
    <string name="debug_dump_delete_button">"الحذف الآن"</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_start_button"/>
    <string name="debug_dump_start_button">"البداية"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_email_sender_picker"/>
    <string name="debug_dump_email_sender_picker">"اختر أحد البرامج لإرسال الملف"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_email_subject"/>
    <string name="debug_dump_email_subject">"قاعدة بيانات جهات الاتصال مرفقة"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_dump_email_body"/>
    <string name="debug_dump_email_body">"قاعدة بيانات جهات اتصالي والتي تحتوي على جميع المعلومات ذات الصلة بجهات الاتصال مرفقة. يرجى التعامل معها بحرص."</string>

    <add-resource type="string" name="group_title_co_workers"/>
    <string name="group_title_co_workers">"زملاء العمل"</string>
    <add-resource type="string" name="group_title_family"/>
    <string name="group_title_family">"العائلة"</string>
    <add-resource type="string" name="group_title_friends"/>
    <string name="group_title_friends">"الأصدقاء"</string>

    <!--preset contact info: name,number,email-->
    <add-resource type="array" name="preload_contact_detail_info"/>
    <string-array name="preload_contact_detail_info">
        <item>"Segr.Telefonica"</item>
        <item>4133</item>
        <item></item>
    </string-array></resources>
