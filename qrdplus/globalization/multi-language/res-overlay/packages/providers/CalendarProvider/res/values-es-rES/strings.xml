<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <add-resource type="string" name="calendar_storage"/>
    <string name="calendar_storage">"Almacenamiento en el calendario"</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <add-resource type="string" name="calendar_default_name"/>
    <string name="calendar_default_name">"Predeterminado"</string>

    <!-- Caption on secret calendar info -->
    <add-resource type="string" name="calendar_info"/>
    <string name="calendar_info">"Información de calendario"</string>

    <!-- Error message for secret calendar info -->
    <add-resource type="string" name="calendar_info_error"/>
    <string name="calendar_info_error">"Error"</string>

    <!-- No calendars message for secret calendar info -->
    <add-resource type="string" name="calendar_info_no_calendars"/>
    <string name="calendar_info_no_calendars">"No hay calendarios."</string>

    <!-- Event count message for secret calendar info -->
    <add-resource type="string" name="calendar_info_events"/>
    <string name="calendar_info_events">"Eventos: <xliff:g id="EVENTS">%1$d</xliff:g>"</string>

    <!-- Event and dirty event count message for secret calendar info -->
    <add-resource type="string" name="calendar_info_events_dirty"/>
    <string name="calendar_info_events_dirty">"Eventos: <xliff:g id="EVENTS_0">%1$d</xliff:g>; no guardados: <xliff:g id="DIRTY_EVENTS">%2$d</xliff:g>"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <add-resource type="string" name="provider_label"/>
    <string name="provider_label">"Calendario"</string>

    <!-- Debug tool - dialog - delete file now [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_delete_button"/>
    <string name="debug_tool_delete_button">"Eliminar ahora"</string>

    <!-- Debug tool - dialog - start copying [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_start_button"/>
    <string name="debug_tool_start_button">"Iniciar"</string>

    <!-- Debug tool - Main message shown to the user when starting up the debug tool [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_message"/>
    <string name="debug_tool_message">"Vas a 1) hacer una copia de tu base de datos de calendario en la tarjeta SD o en el almacenamiento USB a la que puede acceder cualquier aplicación y a 2) enviarla por correo electrónico. Recuerda eliminarla en cuanto se haya copiado correctamente fuera del dispositivo o se haya recibido el mensaje."</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_email_sender_picker"/>
    <string name="debug_tool_email_sender_picker">"Seleccionar un programa para enviar el archivo"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_email_subject"/>
    <string name="debug_tool_email_subject">"Base de datos de calendario adjunta"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <add-resource type="string" name="debug_tool_email_body"/>
    <string name="debug_tool_email_body">"Mi base de datos de calendario con todas mis citas y mi información personal va adjunta a este mensaje. Utiliza esta información con cuidado."</string></resources>
