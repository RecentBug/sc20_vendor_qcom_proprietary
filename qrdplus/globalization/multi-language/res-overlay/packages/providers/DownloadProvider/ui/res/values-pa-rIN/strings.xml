<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The name of the application that appears in the launcher [CHAR LIMIT=15] -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"ਡਾਊਨਲੋਡਸ"</string>
    <!-- The title that appears at the top of the activity listing downloads sorted by date [CHAR LIMIT=40] -->
    <add-resource type="string" name="download_title_sorted_by_date"/>
    <string name="download_title_sorted_by_date">"ਡਾਊਨਲੋਡਸ - ਤਾਰੀਖ ਮੁਤਾਬਕ ਛਾਂਟਿਆ"</string>
    <!-- The title that appears at the top of the activity listing downloads sorted by size [CHAR LIMIT=40] -->
    <add-resource type="string" name="download_title_sorted_by_size"/>
    <string name="download_title_sorted_by_size">"ਡਾਊਨਲੋਡਸ - ਆਕਾਰ ਮੁਤਾਬਕ ਛਾਂਟਿਆ"</string>

    <!-- Appears in lieu of the list of downloads if there are no downloads to view
         [CHAR LIMIT=200] -->
    <add-resource type="string" name="no_downloads"/>
    <string name="no_downloads">"ਕੋਈ ਡਾਊਨਲੋਡਸ ਨਹੀਂ।"</string>

    <!-- Default title for an item in the download list for which no title was provided by the app.
         [CHAR LIMIT=20] -->
    <add-resource type="string" name="missing_title"/>
    <string name="missing_title">"&lt;ਅਗਿਆਤ&gt;"</string>

    <!-- Button to display sort-by-size option [CHAR LIMIT=25] -->
    <add-resource type="string" name="button_sort_by_size"/>
    <string name="button_sort_by_size">"ਆਕਾਰ ਮੁਤਾਬਕ ਛਾਂਟੋ"</string>
    <!-- Button to display sort-by-date option [CHAR LIMIT=25] -->
    <add-resource type="string" name="button_sort_by_date"/>
    <string name="button_sort_by_date">"ਤਾਰੀਖ ਮੁਤਾਬਕ ਛਾਂਟੋ"</string>

    <!-- Status messages -->

    <!-- Status indicating that the download has been queued to start in the future.  Appears for an
         individual item in the download list.  [CHAR LIMIT=11] -->
    <add-resource type="string" name="download_queued"/>
    <string name="download_queued">"ਕਤਾਰਬੱਧ"</string>
    <!-- Status indicating that the system is currently downloading the file.  Appears for an
         individual item in the download list.  [CHAR LIMIT=11] -->
    <add-resource type="string" name="download_running"/>
    <string name="download_running">"ਪ੍ਰਗਤੀ ਵਿੱਚ"</string>
    <!-- Status indicating that the download has completed successfully. Appears for an individual
         item in the download list.  [CHAR LIMIT=11] -->
    <add-resource type="string" name="download_success"/>
    <string name="download_success">"ਪੂਰਾ"</string>
    <!-- Status indicating that the download has ended without completing successfully.  Appears for
         an individual item in the download list.  [CHAR LIMIT=11] -->
    <add-resource type="string" name="download_error"/>
    <string name="download_error">"ਅਸਫਲ"</string>

    <!-- Dialog/toast messages -->

    <!-- Title of dialog that is shown when the user clicks a download for which no file is
         available, either because the download hasn't started or because the download failed
         [CHAR LIMIT=25] -->
    <add-resource type="string" name="dialog_title_not_available"/>
    <string name="dialog_title_not_available">"ਡਾਊਨਲੋਡ ਨਹੀਂ ਕਰ ਸਕਿਆ"</string>
    <!-- Text for dialog when user clicks on a download that failed [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_failed_body"/>
    <string name="dialog_failed_body">"ਕੀ ਤੁਸੀਂ ਬਾਅਦ ਵਿੱਚ ਫਾਈਲ ਨੂੰ ਡਾਊਨਲੋਡ ਕਰਨ ਦੀ ਦੁਬਾਰਾ ਕੋਸ਼ਿਸ਼ ਕਰਨਾ ਚਾਹੁੰਦੇ ਹੋ ਜਾਂ ਇਸਨੂੰ ਕਤਾਰ ਵਿੱਚੋਂ ਮਿਟਾਉਣਾ ਚਾਹੁੰਦੇ ਹੋ?"</string>
    <!-- Title for dialog when user clicks on a download that has not yet begun, but will be started
         in the future. [CHAR LIMIT=25] -->
    <add-resource type="string" name="dialog_title_queued_body"/>
    <string name="dialog_title_queued_body">"ਕਤਾਰ ਵਿੱਚ ਫਾਈਲ"</string>
    <!-- Text for dialog when user clicks on a download that has not yet begun, but will be started
         in the future. [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_queued_body"/>
    <string name="dialog_queued_body">"ਫਾਈਲ ਭਵਿੱਖੀ ਡਾਊਨਲੋਡ ਲਈ ਕਤਾਰਬੱਧ ਨਹੀਂ ਹੈ ਇਸਲਈ ਇਹ ਅਜੇ ਉਪਲਬਧ ਨਹੀਂ ਹੈ।"</string>
    <!-- Text for dialog when user clicks on a completed download but the file is missing
         [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_file_missing_body"/>
    <string name="dialog_file_missing_body">"ਡਾਊਨਲੋਡ ਫਾਈਲ ਨਹੀਂ ਲੱਭੀ ਜਾ ਸਕਦੀ।"</string>
    <!-- Text for dialog when user clicks on a download that failed due to insufficient space on
         external storage [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_insufficient_space_on_external"/>
    <string name="dialog_insufficient_space_on_external">"ਡਾਊਨਲੋਡ ਪੂਰਾ ਨਹੀਂ ਕਰ ਸਕਦਾ। ਬਾਹਰੀ ਡਾਊਨਲੋਡ ਸਟੋਰੇਜ ਤੇ ਕਾਫ਼ੀ ਸਪੇਸ ਨਹੀਂ ਹੈ।"</string>
    <!-- Text for dialog when user clicks on a download that failed due to insufficient space on
         the internal download cache [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_insufficient_space_on_cache"/>
    <string name="dialog_insufficient_space_on_cache">"ਡਾਊਨਲੋਡ ਪੂਰਾ ਨਹੀਂ ਕਰ ਸਕਦਾ। ਅੰਦਰੂਨੀ ਡਾਊਨਲੋਡ ਸਟੋਰੇਜ ਤੇ ਕਾਫ਼ੀ ਸਪੇਸ ਨਹੀਂ ਹੈ।"</string>
    <!-- Text for dialog when user clicks on a download that failed because it was interrupted and
         the server doesn't support resuming downloads [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_cannot_resume"/>
    <string name="dialog_cannot_resume">"ਡਾਊਨਲੋਡ ਰੋਕੀ ਗਈ ਸੀ ਅਤੇ ਦੁਬਾਰਾ ਸ਼ੁਰੂ ਨਹੀਂ ਕੀਤੀ ਜਾ ਸਕਦੀ।"</string>
    <!-- Text for dialog when user clicks on a download that failed because the requested
         destination file already exists [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_file_already_exists"/>
    <string name="dialog_file_already_exists">"ਡਾਊਨਲੋਡ ਨਹੀਂ ਕਰ ਸਕਦਾ। ਡੈਸਟੀਨੇਸ਼ਨ ਫਾਈਲ ਪਹਿਲਾਂ ਹੀ ਮੌਜੂਦ ਹੈ।"</string>
    <!-- Text for dialog when user clicks on a download that failed because it was requested to go
         on the external media, which is not mounted [CHAR LIMIT=200] -->
    <add-resource type="string" name="dialog_media_not_found"/>
    <string name="dialog_media_not_found">"ਡਾਊਨਲੋਡ ਨਹੀਂ ਕਰ ਸਕਦਾ। ਬਾਹਰੀ ਮੀਡੀਆ ਉਪਲਬਧ ਨਹੀਂ ਹੈ।"</string>
    <!-- Text for a toast appearing when a user clicks on a completed download, informing the user
         that there is no application on the device that can open the file that was downloaded
         [CHAR LIMIT=200] -->
    <add-resource type="string" name="download_no_application_title"/>
    <string name="download_no_application_title">"ਫਾਈਲ ਨਹੀਂ ਖੋਲ੍ਹ ਸਕਦਾ"</string>

    <!-- Buttons -->

    <!-- Text for button to remove the entry for a download that has not yet begun or that has
         failed [CHAR LIMIT=25] -->
    <add-resource type="string" name="remove_download"/>
    <string name="remove_download">"ਹਟਾਓ"</string>
    <!-- Text for button to delete a download that has completed, or to delete multiple download
         entries [CHAR LIMIT=25] -->
    <add-resource type="string" name="delete_download"/>
    <string name="delete_download">"ਮਿਟਾਓ"</string>
    <!-- Text for button to keep a download that has not yet begun [CHAR LIMIT=25] -->
    <add-resource type="string" name="keep_queued_download"/>
    <string name="keep_queued_download">"ਰੱਖੋ"</string>
    <!-- Text for button to cancel a download that is currently in progress [CHAR LIMIT=25] -->
    <add-resource type="string" name="cancel_running_download"/>
    <string name="cancel_running_download">"ਰੱਦ ਕਰੋ"</string>
    <!-- Text for button appearing in a dialog to restart a download, either one that failed or one
         for which the downloaded file is now missing [CHAR LIMIT=25] -->
    <add-resource type="string" name="retry_download"/>
    <string name="retry_download">"ਦੁਬਾਰਾ ਕੋਸ਼ਿਸ਼ ਕਰੋ"</string>
    <!-- Text for button to start a download over the mobile connection now, even though it's over
        the carrier-specified recommended maximum size for downloads over the mobile connection
        [CHAR LIMIT=25] -->
    <add-resource type="string" name="start_now_download"/>
    <string name="start_now_download">"ਹੁਣ ਸ਼ੁਰੂ ਕਰੋ"</string>
    <!-- Text for button appearing in the pop-up selection menu to deselect all currently selected
    downloads in the download list [CHAR LIMIT=25] -->
    <add-resource type="string" name="deselect_all"/>
    <string name="deselect_all">"ਸਾਰਿਆਂ ਨੂੰ ਅਚੋਣਵਾਂ ਕਰੋ"</string>
    <!-- Text for menu to select all downloads in the download list [CHAR LIMIT=25] -->
    <add-resource type="string" name="select_all"/>
    <string name="select_all">"ਸਾਰੇ ਚੁਣੋ"</string>
    <!-- number of downloads selected [CHAR LIMIT=50] -->
    <add-resource type="string" name="selected_count"/>
    <string name="selected_count">"<xliff:g id="TOTAL">%2$d</xliff:g> ਵਿੱਚੋਂ <xliff:g id="NUMBER">%1$d</xliff:g> ਨੂੰ ਚੁਣਿਆ ਗਿਆ"</string>
    <!-- title shown when displaying list of apps to choose from when share is clicked on
        [CHAR LIMIT = 20] -->
    <add-resource type="string" name="download_share_dialog"/>
    <string name="download_share_dialog">"ਇਸ ਰਾਹੀਂ ਸ਼ੇਅਰ ਕਰੋ"</string></resources>
