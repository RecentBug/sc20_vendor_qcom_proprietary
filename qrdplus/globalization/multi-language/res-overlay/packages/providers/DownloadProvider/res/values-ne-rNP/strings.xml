<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- This is the name of the Download Manager application. -->
    <add-resource type="string" name="app_label"/>
    <string name="app_label">"डाउनलोड प्रबन्धक"</string>

    <!-- This is the short description of a permission associated with the
        Android Download Manager. It is displayed as part of the description
        of any application that was granted that permission.
        This specific permission controls access to the Download Manager by
        applications that initiate downloads. -->
    <add-resource type="string" name="permlab_downloadManager"/>
    <string name="permlab_downloadManager">"डाउनलोड प्रबन्धक पहुँच गर्नुहोस्"</string>
    <!-- This is the long description of a permission associated with the
        Android Download Manager. It is displayed as part of the description
        of any application that was granted that permission.
        This specific permission controls access to the Download Manager by
        applications that initiate downloads. -->
    <add-resource type="string" name="permdesc_downloadManager"/>
    <string name="permdesc_downloadManager">"फाइल डाउनलोड गर्नका लागि अनुप्रयोगलाई डाउनलोड प्रबन्धकको पहुँचको अनुमति दिन्छ। हानिकारक अनुप्रयोगहरूले यसलाई डाउनलोडहरू अवरूद्ध गर्न र निजी जानकारी पहुँच गर्न यसको प्रयोग गर्न सक्छन्।"</string>

    <!-- This is the short description of a permission associated with the
        Android Download Manager. It is displayed as part of the description
        of any application that was granted that permission.
        This specific permission controls access to some advanced (and
        dangerous) features from the Download Manager that are needed by
        system applications but aren't necessary for regular applications
        that just initiate plain downloads. -->
    <add-resource type="string" name="permlab_downloadManagerAdvanced"/>
    <string name="permlab_downloadManagerAdvanced">"उन्नत डाउनलोड प्रबन्धक प्रकार्यहरू"</string>
    <!-- This is the long description of a permission associated with the
        Android Download Manager. It is displayed as part of the description
        of any application that was granted that permission.
        This specific permission controls access to some advanced (and
        dangerous) features from the Download Manager that are needed by
        system applications but aren't necessary for regular applications
        that just initiate plain downloads. -->
    <add-resource type="string" name="permdesc_downloadManagerAdvanced"/>
    <string name="permdesc_downloadManagerAdvanced">"अनुप्रयोगलाई डाउनलोड प्रबन्धकको उन्नत प्रकार्यहरू पहुँच गर्न अनुमति दिन्छ। हानिकारक अनुप्रयोगहरूले यसलाई डाउनलोड अवरूद्ध गरी निजी जानकारी पहुँच गर्न प्रयोग गर्न सक्छ।"</string>

    <add-resource type="string" name="permlab_downloadCompletedIntent"/>
    <string name="permlab_downloadCompletedIntent">"डाउनलोड सूचनाहरू पठाउनुहोस्।"</string>
    <!-- This is the long description of a permission associated with the
        Android Download Manager. It is displayed as part of the description
        of any application that was granted that permission.
        This specific permission allows an application to tell other
        applications that their downloads have completed. -->
    <add-resource type="string" name="permdesc_downloadCompletedIntent"/>
    <string name="permdesc_downloadCompletedIntent">"अनुप्रयोगलाई समाप्त भएका डाउनलोडहरूका सूचनाहरू पठाउन अनुमति दिन्छ। हानिकारक अनुप्रयोगहरूले फाइल डाउनलोड गर्ने अन्य अनुप्रयोगलाई झुक्याउनका लागि यसलाई प्रयोग गर्न सक्छन्।"</string>

    <!-- Title for permission to see all downloads to EXTERNAL [CHAR LIMIT=25] -->
    <add-resource type="string" name="permlab_seeAllExternal"/>
    <string name="permlab_seeAllExternal" product="nosdcard">"सबै डाउनलोडहरूलाई USB भण्डारणमा हेर्नुहोस्"</string>
    <!-- Title for permission to see all downloads to EXTERNAL -->
    <add-resource type="string" name="permlab_seeAllExternal"/>
    <string name="permlab_seeAllExternal" product="default">"सबै डाउनलोडहरूलाई SD कार्डमा हेर्नुहोस्"</string>
    <!-- Description for the permission to see all downloads to EXTERNAL -->
    <add-resource type="string" name="permdesc_seeAllExternal"/>
    <string name="permdesc_seeAllExternal">"जुनसुकै अनुप्रयोगले डाउनलोड गरेको भए पनि अनुप्रयोगलाई सम्पूर्ण डाउनलोडहरूलाई SD कार्डमा देख्ने अनुमति दिन्छ ।"</string>

    <!-- The label for the permission to download files to the download cache
    that can't be automatically deleted by the download manager to free up
    space [CHAR LIMIT=50] -->
    <add-resource type="string" name="permlab_downloadCacheNonPurgeable"/>
    <string name="permlab_downloadCacheNonPurgeable">"डाउनलोड केसमा ठाउँ ओगट्नुहोस्"</string>
    <!-- The full sentence description for the permission to download files to
    the download cache that can't be automatically deleted by the download
    manager to free up space [CHAR LIMIT=160] -->
    <add-resource type="string" name="permdesc_downloadCacheNonPurgeable"/>
    <string name="permdesc_downloadCacheNonPurgeable">"अनुप्रयोगलाई फाइलहरूलाई डाउनलोड केसमा डाउनलोड गर्न अनुमति दिन्छ, जुन डाउनलोड प्रबन्धकलाई थप ठाउँ चाहिएको खण्डमा स्वचालित रूपमा हटाउन सकिदैन।"</string>

    <!-- The label for the permission to download files through the download
    manager without any notification being shown to the user [CHAR LIMIT=50] -->
    <add-resource type="string" name="permlab_downloadWithoutNotification"/>
    <string name="permlab_downloadWithoutNotification">"बिना सूचना फाइलहरू डाउनलोड गर्नुहोस्"</string>

    <!-- The full sentence description for the permission to download files
    through the download manager without any notification being shown to the
    user [CHAR LIMIT=160] -->
    <add-resource type="string" name="permdesc_downloadWithoutNotification"/>
    <string name="permdesc_downloadWithoutNotification">"अनुप्रयोगलाई डाउनलोड प्रबन्धकका माध्ययमद्वारा प्रयोगकर्तामा सूचना नदेखाई फाइलहरू डाउनलोड गर्ने अनुमति दिन्छ।"</string>

    <!-- The label for the permission to access all downloads in the download
    manager, not just those owned by the calling user [CHAR LIMIT=50] -->
    <add-resource type="string" name="permlab_accessAllDownloads"/>
    <string name="permlab_accessAllDownloads">"सम्पूर्ण प्रणाली डाउनलोडहरू पहुँच गर्नुहोस्"</string>

    <!-- The full sentence description for the permission to access all
    downloads in the download manager, not just those owned by the calling user
    [CHAR LIMIT=160] -->
    <add-resource type="string" name="permdesc_accessAllDownloads"/>
    <string name="permdesc_accessAllDownloads">"अनुप्रयोगलाई कुनै पनि अनुप्रयोगले प्रणालीमा पहल गरेको सम्पूर्ण डाउनलोडहरू हेर्ने र परिमार्जन गर्ने अनुमति दिन्छ।"</string>


    <!-- This is the title that is used when displaying the notification
    for a download that doesn't have a title associated with it. -->
    <add-resource type="string" name="download_unknown_title"/>
    <string name="download_unknown_title">"&lt;शीर्षकविहिन&gt;"</string>

    <!-- When a download completes, a notification is displayed, and this
        string is used to indicate that the download successfully completed.
        Note that such a download could have been initiated by a variety of
        applications, including (but not limited to) the browser, an email
        application, a content marketplace. -->
    <add-resource type="string" name="notification_download_complete"/>
    <string name="notification_download_complete">"डाउनलोड पुरा भयो।"</string>

    <!-- When a download completes, a notification is displayed, and this
        string is used to indicate that the download failed.
        Note that such a download could have been initiated by a variety of
        applications, including (but not limited to) the browser, an email
        application, a content marketplace. -->
    <add-resource type="string" name="notification_download_failed"/>
    <string name="notification_download_failed">"डाउनलोड असफल"</string>

    <!-- When a download is paused because it's too large to download over a
        mobile connection, and Wi-Fi is unavailable, this string is displayed in
        the system notification for the running download, beneath the download
        title and description.  Note that such a download could have been
        initiated by a variety of applications, including (but not limited to)
        the browser, an email application, a content marketplace.
        [CHAR LIMIT=24] -->
    <add-resource type="string" name="notification_need_wifi_for_size"/>
    <string name="notification_need_wifi_for_size">"डाउनलोड आकारलाई Wi-Fi चाहिन्छ।"</string>

    <!-- Notification shown when a download has been paused because a user policy
        has blocked network access to applications running in background. [CHAR LIMIT=24] -->
    <add-resource type="string" name="notification_paused_in_background"/>
    <string name="notification_paused_in_background">"पृष्ठभूमिमा रोकिएको छ।"</string>

    <!-- Title for dialog when a download exceeds the carrier-specified maximum size of downloads
        over the mobile network and Wi-Fi is required.  The user has the choice to either queue the
        download to start next time Wi-Fi is available or cancel the download altogether. [CHAR
        LIMIT=50] -->
    <add-resource type="string" name="wifi_required_title"/>
    <string name="wifi_required_title">"अपरेटर सञ्जालका लागि डाउनलोड ज्यादै ठूलो"</string>

    <!-- Text for dialog when a download exceeds the carrier-specified maximum size of downloads
        over the mobile network and Wi-Fi is required.  The user has the choice to either queue the
        download to start next time Wi-Fi is available or cancel the download altogether. [CHAR
        LIMIT=200] -->
    <add-resource type="string" name="wifi_required_body"/>
    <string name="wifi_required_body">"तपाईँले Wi-Fi प्रयोग गर्नु पर्छ यस <xliff:g id="SIZE">%s </xliff:g> डाउनलोड समाप्त गर्न। \n\nछुनुहोस् <xliff:g id="QUEUE_TEXT">%s </xliff:g> यस डाउनलोडलाई सुरु गर्न अर्को पटक तपाईँ Wi-Fi सञ्जालामा जडित भए पछि।"</string>

    <!-- Title for dialog when a download exceeds the carrier-specified recommended maximum size of
        downloads over the mobile network, and the user may choose to start the download over mobile
        anyway or to queue for download to start next time a Wi-Fi connection is available [CHAR
        LIMIT=50] -->
    <add-resource type="string" name="wifi_recommended_title"/>
    <string name="wifi_recommended_title">"पछि डाउनलोडका लागि लाममा राख्ने हो?"</string>

    <!-- Text for dialog when a download exceeds the carrier-specified recommended maximum size of
        downloads over the mobile network, and the user may choose to start the download over mobile
        anyway or to queue for download to start next time a Wi-Fi connection is available [CHAR
        LIMIT=200] -->
    <add-resource type="string" name="wifi_recommended_body"/>
    <string name="wifi_recommended_body">"यस <xliff:g id="SIZE">%s </xliff:g> डाउनलोडलाई सुरु गर्दा तपाईँको ब्याट्रिको काल छोट्टिन सक्छ र/वा तपाईँको मोबाइल डेटा जडानको अधिकतम प्रयोग भई तपाईँको डेटा योजना अनुसार मोबाइल अपरेटरले थप शुल्क लिन सक्छ। \n\n छुनुहोस् <xliff:g id="QUEUE_TEXT">%s</xliff:g>तल यस डाउनलोडलाई सुरु गर्न अर्को पटक तपाईँ Wi-Fi सन्जालमा जडित हुने बेला।"</string>


    <!-- Text for button to queue a download to start next time Wi-Fi is available [CHAR LIMIT=25]
        -->
    <add-resource type="string" name="button_queue_for_wifi"/>
    <string name="button_queue_for_wifi">"लाम"</string>

    <!-- Text for button to cancel a download because it's too large to proceed over the mobile
         network and the user does not want to queue it for Wi-Fi [CHAR LIMIT=25] -->
    <add-resource type="string" name="button_cancel_download"/>
    <string name="button_cancel_download">"रद्द गर्नुहोस्"</string>

    <!-- Text for button to start a download over the mobile connection now, even though it's over
         the carrier-specified recommended maximum size for downloads over the mobile connection
         [CHAR LIMIT=25] -->
    <add-resource type="string" name="button_start_now"/>
    <string name="button_start_now">"अहिले सुरु गर्नुहोस्"</string>

    <!-- Title summarizing active downloads. [CHAR LIMIT=32] -->
    <add-resource type="plurals" name="notif_summary_active"/>
    <plurals name="notif_summary_active">
        <item quantity="one">"१ फाइल डाउनलोड हुँदैछ"</item>
        <item quantity="other">"<xliff:g id="NUMBER">%d</xliff:g> फाइलहरू डाउनलोड हुँदैछन्"</item>
    </plurals>

    <!-- Title summarizing waiting downloads. [CHAR LIMIT=32] -->
    <add-resource type="plurals" name="notif_summary_waiting"/>
    <plurals name="notif_summary_waiting">
        <item quantity="one">"१ फाइल डाउनलोड हुन पालो कुर्दै"</item>
        <item quantity="other">"<xliff:g id="NUMBER">%d</xliff:g> फाइलहरू डाउनलोड हुन पालो कुर्दै"</item>
    </plurals>

    <!-- Time remaining until download is complete. [CHAR LIMIT=32] -->
    <add-resource type="string" name="download_remaining"/>
    <string name="download_remaining">"<xliff:g id="DURATION">%s</xliff:g> बाँकी छ"</string>

    <!-- Text for a toast appearing when a user clicks on a completed download, informing the user
         that there is no application on the device that can open the file that was downloaded
         [CHAR LIMIT=200] -->
    <add-resource type="string" name="download_no_application_title"/>
    <string name="download_no_application_title">"फाइल खोल्न सक्दैन"</string>

    <!-- Label describing Downloads as a storage root [CHAR LIMIT=32] -->
    <add-resource type="string" name="root_downloads"/>
    <string name="root_downloads">"डाउनलोडहरू"</string>

    <!-- Status indicating that the download has been queued to start in the future.  Appears for an
         individual item in the download list.  [CHAR LIMIT=24] -->
    <add-resource type="string" name="download_queued"/>
    <string name="download_queued">"लाममा राखियो"</string>
    <!-- Status indicating that the system is currently downloading the file.  Appears for an
         individual item in the download list.  [CHAR LIMIT=24] -->
    <add-resource type="string" name="download_running"/>
    <string name="download_running">"प्रगतिमा"</string>
    <!-- Status indicating that the download has ended without completing successfully.  Appears for
         an individual item in the download list.  [CHAR LIMIT=24] -->
    <add-resource type="string" name="download_error"/>
    <string name="download_error">"असफल"</string>

    <!-- Representation of download progress percentage when running. [CHAR LIMIT=24] -->
    <add-resource type="string" name="download_running_percent"/>
    <string name="download_running_percent">"प्रगति हुँदै, <xliff:g id="PERCENTAGE">%s</xliff:g>"</string></resources>
