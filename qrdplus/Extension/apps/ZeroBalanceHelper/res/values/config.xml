<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Zero Balance timer value config -->
    <integer name="operator_config_timeout">1200000</integer>
    <!-- Zero Balance unblock single probe timer value config -->
    <integer name="operator_config_single_ping_timeout">300000</integer>
    <!--Number of retry value to find out zero balance is reached-->
    <integer name="num_ping_try_to_block">3</integer>
    <!--Number of retry value to find out SIM is recharged-->
    <integer name="num_ping_try_to_unblock">1</integer>
    <!--Delay value before evaluating network domancy state while checking Zerobalace-->
    <integer name="tx_rx_block_delay">10000</integer>
    <!--Delay value before evaluating network domancy state while checking SIM recharge-->
    <integer name="tx_rx_unblock_delay">60000</integer>
    <!--Config flag to load whitelist app matching metadata tag-->
    <bool name="whitelist_apps_add_enable">true</bool>
    <!--Metadata tag key and value strings to load whitelist apps-->
    <string name="exception_apps_key">"PrepaidSIMWhiteListApp"</string>
    <string name="exception_apps_val">"AllowDataAccess"</string>
</resources>
