<CleanerRoot xmlns:android="http://schemas.android.com/apk/res/android">
    <cleaner name="@string/application_cache">
        <path>/data/data</path>
        <storage>data</storage>
        <pattern>Application Cache</pattern>
    </cleaner>
    <cleaner name="@string/garbage_files">
        <path>/storage</path>
        <storage>shared</storage>
        <pattern>sdcard/*.log, sdcard/*.bak, sdcard/*.tmp</pattern>
    </cleaner>
    <cleaner name="@string/unavalible_apk">
        <path>/storage</path>
        <storage>shared</storage>
        <pattern>Invalid APK</pattern>
    </cleaner>
</CleanerRoot>
