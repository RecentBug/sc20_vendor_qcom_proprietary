    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="StartTheme" parent="@android:style/Theme.Material.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:actionModeBackground">@color/actionModeTitleBarColor</item>
        <item name="android:actionOverflowButtonStyle">@style/OverflowActionMode</item>
        <item name="android:actionButtonStyle">@style/ActionButtonStyle</item>
        <item name="android:actionBarSize">64dp</item>
        <item name="android:statusBarColor">@color/statusBarColorCode</item>
    </style>
    <style name="OverflowActionMode" parent="@android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@drawable/overflow_vector</item>
        <item name="android:minWidth">0dip</item>
        <item name="android:paddingStart">8dip</item>
        <item name="android:paddingEnd">10dip</item>
    </style>
    <style name="ActionButtonStyle" parent="@android:style/Widget.Material.Light.ActionButton">
        <item name="android:minWidth">0dip</item>
        <item name="android:paddingStart">14dip</item>
        <item name="android:paddingEnd">14dip</item>
    </style>
    <style name="EditTheme" parent="@android:style/Theme.Material.Light">
        <item name="android:actionBarStyle">@style/MyEditActionBar</item>
        <!-- <item name="android:actionButtonStyle">@style/ActionButtonStyle</item> -->
        <item name="android:actionBarSize">64dp</item>
        <item name="android:statusBarColor">@color/editStatusBarColorCode</item>
    </style>
    <style name="MyEditActionBar" parent="@android:style/Widget.Material.Light.ActionBar.Solid">
        <item name="android:background">@color/editTitleBackgroundColor</item>
        <item name="android:titleTextStyle">@style/editThemeTitle</item>
    </style>
    <style name="MyActionBar" parent="@android:style/Widget.Material.Light.ActionBar.Solid">
        <item name="android:background">@color/titlebackgroundcolor</item>
        <item name="android:titleTextStyle">@style/ThemeTitle</item>
    </style>
    <style name="ThemeTitle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="editThemeTitle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">@color/editTitleTextColor</item>
        <item name="android:textSize">20sp</item>
    </style>
    <color name="selectedStatusBarColorCode">#C6CACD</color>
    <color name="statusBarColorCode">#E6B111</color>
    <color name="editStatusBarColorCode">#C9CFD3</color>
    <color name="editTitleBackgroundColor">#eceff1</color>
    <color name="titlebackgroundcolor">#f6be1a</color>
    <color name="listSelctorColor">#A4A4A4</color>
    <color name="pagebackgroundColor">#fafafa</color>
    <color name="emptySloganTextColor">#bdbdbd</color>
    <color name="editTitleTextColor">#dd000000</color>
    <color name="actionModeTitleBarColor">#e0e0e0</color>
    <color name="listviewDividerColor">#1E000000</color>
</resources>
